{
  return p.intersects(topPlane,notablePoints,topPlanePoints,bounds,bottomPlane,eitherBound) || p.intersects(bottomPlane,notablePoints,bottomPlanePoints,bounds,topPlane,eitherBound) || p.intersects(leftPlane,notablePoints,leftPlanePoints,bounds,topPlane,bottomPlane)|| p.intersects(rightPlane,notablePoints,rightPlanePoints,bounds,topPlane,bottomPlane);
}
