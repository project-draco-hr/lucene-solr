{
  return p.intersects(planetModel,topPlane,notablePoints,topPlanePoints,bounds,bottomPlane,eitherBound) || p.intersects(planetModel,bottomPlane,notablePoints,bottomPlanePoints,bounds,topPlane,eitherBound) || p.intersects(planetModel,leftPlane,notablePoints,leftPlanePoints,bounds,topPlane,bottomPlane)|| p.intersects(planetModel,rightPlane,notablePoints,rightPlanePoints,bounds,topPlane,bottomPlane);
}
