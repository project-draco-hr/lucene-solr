{
  final int insideRectangle=isShapeInsideBBox(path);
  if (insideRectangle == SOME_INSIDE)   return OVERLAPS;
  final boolean insideShape=path.isWithin(ULHC);
  if (insideRectangle == ALL_INSIDE && insideShape)   return OVERLAPS;
  if (path.intersects(topPlane,bottomPlane,eitherBound) || path.intersects(bottomPlane,topPlane,eitherBound) || path.intersects(leftPlane,topPlane,bottomPlane)|| path.intersects(rightPlane,topPlane,bottomPlane)) {
    return OVERLAPS;
  }
  if (insideRectangle == ALL_INSIDE) {
    return WITHIN;
  }
  if (insideShape) {
    return CONTAINS;
  }
  return DISJOINT;
}
