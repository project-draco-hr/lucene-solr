{
  final int insideRectangle=isShapeInsideBBox(path);
  if (insideRectangle == SOME_INSIDE) {
    return OVERLAPS;
  }
  final boolean insideShape=path.isWithin(ULHC);
  if (insideRectangle == ALL_INSIDE && insideShape) {
    return OVERLAPS;
  }
  if (path.intersects(topPlane,topPlanePoints,bottomPlane,eitherBound) || path.intersects(bottomPlane,bottomPlanePoints,topPlane,eitherBound) || path.intersects(leftPlane,leftPlanePoints,topPlane,bottomPlane)|| path.intersects(rightPlane,rightPlanePoints,topPlane,bottomPlane)) {
    return OVERLAPS;
  }
  if (insideRectangle == ALL_INSIDE) {
    return WITHIN;
  }
  if (insideShape) {
    return CONTAINS;
  }
  return DISJOINT;
}
