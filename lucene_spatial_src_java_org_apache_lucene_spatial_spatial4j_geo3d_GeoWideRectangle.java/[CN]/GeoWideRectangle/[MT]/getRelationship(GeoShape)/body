{
  if (path.intersects(topPlane,bottomPlane,eitherBound) || path.intersects(bottomPlane,topPlane,eitherBound) || path.intersects(leftPlane,topPlane,bottomPlane)|| path.intersects(rightPlane,topPlane,bottomPlane))   return OVERLAPS;
  if (isWithin(path.getInteriorPoint()))   return WITHIN;
  if (path.isWithin(centerPoint))   return CONTAINS;
  return DISJOINT;
}
