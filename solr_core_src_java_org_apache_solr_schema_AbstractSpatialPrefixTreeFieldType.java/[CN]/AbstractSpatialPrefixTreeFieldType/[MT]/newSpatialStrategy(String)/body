{
  T strat=newPrefixTreeStrategy(fieldName);
  if (distErrPct != null)   strat.setDistErrPct(distErrPct);
  if (defaultFieldValuesArrayLen != null)   strat.setDefaultFieldValuesArrayLen(defaultFieldValuesArrayLen);
  log.info(this.toString() + " strat: " + strat+ " maxLevels: "+ grid.getMaxLevels());
  return strat;
}
