{
  for (int i=0; i < bucket.size(); i++) {
    String key=bucket.getName(i);
    Object val=bucket.getVal(i);
    if ("count".equals(key)) {
      count+=((Number)val).longValue();
      continue;
    }
    if ("val".equals(key)) {
      continue;
    }
    FacetMerger merger=getMerger(key,val);
    if (merger != null) {
      merger.merge(val);
    }
  }
}
