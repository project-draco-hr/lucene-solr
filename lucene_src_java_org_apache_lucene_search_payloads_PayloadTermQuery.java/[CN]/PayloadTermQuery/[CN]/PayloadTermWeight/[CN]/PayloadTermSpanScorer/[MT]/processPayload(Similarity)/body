{
  final DocsAndPositionsEnum postings=termSpans.getPostings();
  if (postings.hasPayload()) {
    payload=postings.getPayload();
    if (payload != null) {
      payloadScore=function.currentScore(doc,term.field(),spans.start(),spans.end(),payloadsSeen,payloadScore,docScorer.computePayloadFactor(doc,spans.start(),spans.end(),payload));
    }
 else {
      payloadScore=function.currentScore(doc,term.field(),spans.start(),spans.end(),payloadsSeen,payloadScore,1F);
    }
    payloadsSeen++;
  }
 else {
  }
}
