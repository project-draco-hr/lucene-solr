{
  if (indexOptions == null) {
    return 0;
  }
 else {
switch (indexOptions) {
case DOCS_ONLY:
      return 1;
case DOCS_AND_FREQS:
    return 2;
case DOCS_AND_FREQS_AND_POSITIONS:
  return 3;
case DOCS_AND_FREQS_AND_POSITIONS_AND_OFFSETS:
return 4;
default :
throw new AssertionError("unhandled IndexOptions: " + indexOptions);
}
}
}
