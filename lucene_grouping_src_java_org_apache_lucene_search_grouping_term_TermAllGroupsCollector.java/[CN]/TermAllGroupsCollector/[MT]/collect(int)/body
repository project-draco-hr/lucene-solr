{
  if (doc > index.docID()) {
    index.advance(doc);
  }
  int key;
  if (doc == index.docID()) {
    key=index.ordValue();
  }
 else {
    key=-1;
  }
  if (!ordSet.exists(key)) {
    ordSet.put(key);
    final BytesRef term;
    if (key == -1) {
      term=null;
    }
 else {
      term=BytesRef.deepCopyOf(index.lookupOrd(key));
    }
    groups.add(term);
  }
}
