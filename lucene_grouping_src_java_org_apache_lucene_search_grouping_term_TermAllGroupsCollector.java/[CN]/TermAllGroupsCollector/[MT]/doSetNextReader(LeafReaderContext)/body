{
  index=DocValues.getSorted(context.reader(),groupField);
  ordSet.clear();
  for (  BytesRef countedGroup : groups) {
    if (countedGroup == null) {
      ordSet.put(-1);
    }
 else {
      int ord=index.lookupTerm(countedGroup);
      if (ord >= 0) {
        ordSet.put(ord);
      }
    }
  }
}
