{
  Throwable firstExc=null;
  for (  final LeafReader r : getSequentialSubReaders()) {
    try {
      r.decRef();
    }
 catch (    Throwable t) {
      if (firstExc == null) {
        firstExc=t;
      }
    }
  }
  if (writer != null) {
    try {
      writer.decRefDeleter(segmentInfos);
    }
 catch (    AlreadyClosedException ex) {
    }
  }
  IOUtils.reThrow(firstExc);
}
