{
  StringBuffer sb=new StringBuffer();
  sb.append('(');
  if (hasAttributes()) {
    Iterator it=getAttributeImplsIterator();
    if (it.hasNext()) {
      sb.append(it.next().toString());
    }
    while (it.hasNext()) {
      sb.append(',');
      sb.append(it.next().toString());
    }
  }
  sb.append(')');
  return sb.toString();
}
