{
  StringBuffer sb=new StringBuffer();
  sb.append('(');
  if (hasAttributes()) {
    if (currentState == null) {
      computeCurrentState();
    }
    for (State state=currentState; state != null; state=state.next) {
      if (state != currentState)       sb.append(',');
      sb.append(state.attribute.toString());
    }
  }
  sb.append(')');
  return sb.toString();
}
