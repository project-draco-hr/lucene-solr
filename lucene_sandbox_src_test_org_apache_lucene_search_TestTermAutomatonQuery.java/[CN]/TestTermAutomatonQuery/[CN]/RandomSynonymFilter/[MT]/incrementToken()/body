{
  if (synNext) {
    clearAttributes();
    posIncAtt.setPositionIncrement(0);
    termAtt.append("" + ((char)97 + random().nextInt(3)));
    synNext=false;
    return true;
  }
  if (input.incrementToken()) {
    if (random().nextInt(10) == 8) {
      synNext=true;
    }
    return true;
  }
 else {
    return false;
  }
}
