{
  if (q.getClass() == MatchNoDocsQuery.class)   return;
  if (!(q instanceof SpanQuery))   throw new AssertionError("Expected SpanQuery: " + q.toString(getFieldName()));
  float boost=1f;
  if (q instanceof SpanBoostQuery) {
    SpanBoostQuery bq=(SpanBoostQuery)q;
    boost=bq.getBoost();
    q=bq.getQuery();
  }
  addSpanQueryWeighted((SpanQuery)q,boost);
}
