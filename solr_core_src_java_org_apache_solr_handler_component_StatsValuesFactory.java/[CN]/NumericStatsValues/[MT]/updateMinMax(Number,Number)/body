{
  if (computeMin) {
    if (null != min) {
      double minD=min.doubleValue();
      this.min=(null == this.min) ? minD : Math.min(this.min.doubleValue(),minD);
    }
  }
  if (computeMax) {
    if (null != max) {
      double maxD=max.doubleValue();
      this.max=(null == this.max) ? maxD : Math.max(this.max.doubleValue(),maxD);
    }
  }
}
