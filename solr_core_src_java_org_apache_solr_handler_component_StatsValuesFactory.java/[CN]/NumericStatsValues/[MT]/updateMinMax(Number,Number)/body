{
  if (computeMin) {
    if (null != min) {
      double minD=min.doubleValue();
      if (null == this.min || minD < this.minD) {
        this.min=this.minD=minD;
      }
    }
  }
  if (computeMax) {
    if (null != max) {
      double maxD=max.doubleValue();
      if (null == this.max || this.maxD < maxD) {
        this.max=this.maxD=maxD;
      }
    }
  }
}
