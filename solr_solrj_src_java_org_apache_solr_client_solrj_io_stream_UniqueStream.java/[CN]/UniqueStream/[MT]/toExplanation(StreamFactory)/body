{
  return new StreamExplanation(getStreamNodeId().toString()).withChildren(new Explanation[]{originalStream.toExplanation(factory)}).withFunctionName(factory.getFunctionName(this.getClass())).withImplementingClass(this.getClass().getName()).withExpressionType(ExpressionType.STREAM_DECORATOR).withExpression(toExpression(factory,false).toString()).withHelper(originalEqualitor.toExplanation(factory));
}
