{
  Tuple tuple=tupleStream.read();
  if (tuple.EOF) {
    return tuple;
  }
  if (currentTuple == null) {
    currentTuple=tuple;
    return tuple;
  }
 else {
    while (true) {
      int i=comp.compare(currentTuple,tuple);
      if (i == 0) {
        tuple=tupleStream.read();
        if (tuple.EOF) {
          return tuple;
        }
      }
 else {
        this.currentTuple=tuple;
        return tuple;
      }
    }
  }
}
