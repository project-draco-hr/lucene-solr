{
  Tuple tuple=stream.read();
  if (tuple.EOF) {
    return tuple;
  }
  if (currentTuple == null) {
    currentTuple=tuple;
    return tuple;
  }
 else {
    while (true) {
      if (eq.test(currentTuple,tuple)) {
        tuple=stream.read();
        if (tuple.EOF) {
          return tuple;
        }
      }
 else {
        this.currentTuple=tuple;
        return tuple;
      }
    }
  }
}
