{
  this.mockControl.expectAndDefaultReturn(this.mockedRequest.getPathInfo(),testPathSuccess);
  this.mockControl.replay();
  assertNotNull(this.provider.getServiceSkeletonInvoker(this.mockedRequest));
  this.mockControl.verify();
  this.mockControl.reset();
  this.mockControl.expectAndDefaultReturn(this.mockedRequest.getPathInfo(),testPathFail);
  this.mockControl.replay();
  try {
    assertNotNull(this.provider.getServiceSkeletonInvoker(this.mockedRequest));
    fail("Service should not be found");
  }
 catch (  NoSuchServiceException e) {
  }
  this.mockControl.verify();
  this.mockControl.reset();
}
