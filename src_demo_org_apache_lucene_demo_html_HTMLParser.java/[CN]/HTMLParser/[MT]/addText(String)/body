{
  if (inStyle)   return;
  if (inTitle)   title.append(text);
 else {
    addToSummary(text);
    if (!titleComplete && !(title.length() == 0)) {
synchronized (this) {
        titleComplete=true;
        notifyAll();
      }
    }
  }
  length+=text.length();
  pipeOut.write(text);
  afterSpace=false;
}
