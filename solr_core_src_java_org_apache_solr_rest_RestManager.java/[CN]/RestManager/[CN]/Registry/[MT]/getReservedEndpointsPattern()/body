{
  StringBuilder builder=new StringBuilder();
  builder.append("(");
  boolean notFirst=false;
  for (  String reservedEndpoint : reservedEndpoints) {
    if (notFirst) {
      builder.append("|");
    }
 else {
      notFirst=true;
    }
    builder.append(reservedEndpoint);
  }
  builder.append(")(?:|/.*)");
  return Pattern.compile(builder.toString());
}
