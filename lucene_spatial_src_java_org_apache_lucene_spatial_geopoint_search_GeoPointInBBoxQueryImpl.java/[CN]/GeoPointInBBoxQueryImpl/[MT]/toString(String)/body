{
  final StringBuilder sb=new StringBuilder();
  sb.append(getClass().getSimpleName());
  sb.append(':');
  if (!getField().equals(field)) {
    sb.append(" field=");
    sb.append(getField());
    sb.append(':');
  }
  return sb.append(" Lower Left: [").append(minLat).append(',').append(minLon).append(']').append(" Upper Right: [").append(maxLat).append(',').append(maxLon).append("]").toString();
}
