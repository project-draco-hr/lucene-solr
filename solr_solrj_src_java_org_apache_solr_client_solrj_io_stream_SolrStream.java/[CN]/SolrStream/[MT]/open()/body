{
  if (cache == null) {
    client=new HttpSolrClient.Builder(baseUrl).build();
  }
 else {
    client=cache.getHttpSolrClient(baseUrl);
  }
  try {
    tupleStreamParser=constructParser(client,loadParams(params));
  }
 catch (  Exception e) {
    throw new IOException("params " + params,e);
  }
}
