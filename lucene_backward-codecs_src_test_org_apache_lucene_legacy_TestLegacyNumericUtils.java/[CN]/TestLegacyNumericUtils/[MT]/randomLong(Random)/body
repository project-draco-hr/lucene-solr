{
  long val;
switch (random.nextInt(4)) {
case 0:
    val=1L << (random.nextInt(63));
  break;
case 1:
val=-1L << (random.nextInt(63));
break;
default :
val=random.nextLong();
}
val+=random.nextInt(5) - 2;
if (random.nextBoolean()) {
if (random.nextBoolean()) val+=random.nextInt(100) - 50;
if (random.nextBoolean()) val=~val;
if (random.nextBoolean()) val=val << 1;
if (random.nextBoolean()) val=val >>> 1;
}
return val;
}
