{
  if (getClass() != o.getClass())   return false;
  JoinQuery other=(JoinQuery)o;
  return this.fromField.equals(other.fromField) && this.toField.equals(other.toField) && this.getBoost() == other.getBoost() && this.q.equals(other.q) && (this.fromIndex == other.fromIndex || this.fromIndex != null && this.fromIndex.equals(other.fromIndex));
}
