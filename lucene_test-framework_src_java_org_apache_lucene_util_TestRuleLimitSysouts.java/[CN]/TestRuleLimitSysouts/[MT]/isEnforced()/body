{
  Class<?> target=RandomizedTest.getContext().getTargetClass();
  if (LuceneTestCase.VERBOSE || LuceneTestCase.INFOSTREAM || target.isAnnotationPresent(Monster.class)|| target.isAnnotationPresent(SuppressSysoutChecks.class)) {
    return false;
  }
  if (!target.isAnnotationPresent(Limit.class)) {
    return false;
  }
  return true;
}
