{
  final String[] ar=dir.list();
  for (int i=0; i < ar.length; i++) {
    OutputStream os=createFile(ar[i]);
    InputStream is=dir.openFile(ar[i]);
    int len=(int)is.length();
    byte[] buf=new byte[len];
    is.readBytes(buf,0,len);
    os.writeBytes(buf,len);
    is.close();
    os.close();
  }
}
