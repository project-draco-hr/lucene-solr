{
  try {
    Token t=(Token)super.clone();
    if (termBuffer != null) {
      t.termBuffer=null;
      t.setTermBuffer(termBuffer,0,termLength);
    }
    if (payload != null) {
      t.setPayload((Payload)payload.clone());
    }
    return t;
  }
 catch (  CloneNotSupportedException e) {
    throw new RuntimeException(e);
  }
}
