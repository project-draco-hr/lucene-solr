{
  StringBuffer sb=new StringBuffer();
  sb.append("(");
  initTermBuffer();
  if (termBuffer == null)   sb.append("null");
 else   sb.append(termBuffer,0,termLength);
  sb.append("," + startOffset + ","+ endOffset);
  if (!type.equals("word"))   sb.append(",type=" + type);
  if (positionIncrement != 1)   sb.append(",posIncr=" + positionIncrement);
  sb.append(")");
  return sb.toString();
}
