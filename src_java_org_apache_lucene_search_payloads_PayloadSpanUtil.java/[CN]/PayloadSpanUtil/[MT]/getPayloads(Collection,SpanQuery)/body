{
  Spans spans=query.getSpans(reader);
  while (spans.next() == true) {
    if (spans.isPayloadAvailable()) {
      Collection payload=spans.getPayload();
      Iterator it=payload.iterator();
      while (it.hasNext()) {
        byte[] bytes=(byte[])it.next();
        payloads.add(bytes);
      }
    }
  }
}
