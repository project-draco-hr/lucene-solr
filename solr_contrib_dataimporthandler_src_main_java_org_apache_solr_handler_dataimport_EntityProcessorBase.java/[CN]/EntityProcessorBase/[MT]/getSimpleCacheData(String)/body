{
  List<Map<String,Object>> rows=simpleCache.get(query);
  if (rows != null) {
    dataSourceRowCache=new ArrayList<Map<String,Object>>(rows);
    return getFromRowCacheTransformed();
  }
 else {
    rows=getAllNonCachedRows();
    if (rows.isEmpty()) {
      return null;
    }
 else {
      dataSourceRowCache=new ArrayList<Map<String,Object>>(rows);
      simpleCache.put(query,rows);
      return getFromRowCacheTransformed();
    }
  }
}
