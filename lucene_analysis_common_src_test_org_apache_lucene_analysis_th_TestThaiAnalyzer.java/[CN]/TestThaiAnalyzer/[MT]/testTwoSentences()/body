{
  Analyzer analyzer=new ThaiAnalyzer(CharArraySet.EMPTY_SET);
  assertAnalyzesTo(analyzer,"This is a test. ???????????????????????????????????????????????????????????????????????????",new String[]{"this","is","a","test","?????????","?????????","?????????","????????????","????????????","?????????","?????????","??????"},new int[]{0,5,8,10,16,19,22,25,29,33,36,39},new int[]{4,7,9,14,19,22,25,29,33,36,39,41});
  analyzer.close();
}
