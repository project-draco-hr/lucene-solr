{
  ThaiAnalyzer analyzer=new ThaiAnalyzer();
  TokenStream ts=analyzer.tokenStream("dummy","?????????????????????");
  assertTokenStreamContents(ts,new String[]{"????????????","?????????"});
  ts=analyzer.tokenStream("dummy","?????????????????????");
  ts.addAttribute(FlagsAttribute.class);
  assertTokenStreamContents(ts,new String[]{"????????????","?????????"});
}
