{
  Directory indexDir=newDirectory();
  TaxonomyWriter tw=new LuceneTaxonomyWriter(indexDir);
  fillTaxonomy(tw);
  tw.close();
  tw=new LuceneTaxonomyWriter(indexDir);
  TaxonomyReader tr=new LuceneTaxonomyReader(indexDir);
  checkWriterParent(tr,tw);
  tw.close();
  tr.close();
  indexDir.close();
}
