{
  Directory indexDir=new RAMDirectory();
  TaxonomyWriter tw=new LuceneTaxonomyWriter(indexDir);
  fillTaxonomy(tw);
  checkPaths(tw);
  fillTaxonomy(tw);
  checkPaths(tw);
  tw.close();
  tw=new LuceneTaxonomyWriter(indexDir);
  checkPaths(tw);
  fillTaxonomy(tw);
  checkPaths(tw);
  tw.close();
}
