{
  this.name=name;
  this.field_sf=field_sf;
  this.facet_sf=facet_sf;
  this.calcDistinct=calcDistinct;
  topLevelReader=searcher.getAtomicReader();
  valueSource=facet_sf.getType().getValueSource(facet_sf,null);
