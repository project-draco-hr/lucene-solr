{
  DIHCache cache=null;
  try {
    @SuppressWarnings("unchecked") Class<DIHCache> cacheClass=DocBuilder.loadClass(cacheImplName,context.getSolrCore());
    Constructor<DIHCache> constr=cacheClass.getConstructor();
    cache=constr.newInstance();
    cache.open(context);
  }
 catch (  Exception e) {
    throw new DataImportHandlerException(DataImportHandlerException.SEVERE,"Unable to load Cache implementation:" + cacheImplName,e);
  }
  return cache;
}
