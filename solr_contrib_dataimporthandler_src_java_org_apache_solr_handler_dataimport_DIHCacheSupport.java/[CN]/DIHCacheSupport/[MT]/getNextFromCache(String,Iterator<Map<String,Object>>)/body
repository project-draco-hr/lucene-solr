{
  try {
    if (rowIterator == null)     return null;
    if (rowIterator.hasNext())     return rowIterator.next();
    return null;
  }
 catch (  Exception e) {
    SolrException.log(log,"getNextFromCache() failed for query '" + query + "'",e);
    wrapAndThrow(DataImportHandlerException.WARN,e);
    return null;
  }
}
