{
  T newOutput;
  if (current.length == 0) {
    newOutput=addedOutput;
  }
 else   if (addedOutput == NO_OUTPUT) {
    output[current.length]=output[current.length - 1];
    return;
  }
 else {
    newOutput=fst.outputs.add(output[current.length - 1],addedOutput);
  }
  output[current.length]=newOutput;
}
