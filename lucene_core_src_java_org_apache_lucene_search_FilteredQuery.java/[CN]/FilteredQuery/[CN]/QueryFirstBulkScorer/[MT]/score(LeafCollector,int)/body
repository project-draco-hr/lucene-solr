{
  collector.setScorer(scorer);
  if (scorer.docID() == -1) {
    scorer.nextDoc();
  }
  while (true) {
    final int scorerDoc=scorer.docID();
    if (scorerDoc < maxDoc) {
      if (filterBits.get(scorerDoc)) {
        collector.collect(scorerDoc);
      }
      scorer.nextDoc();
    }
 else {
      break;
    }
  }
  return scorer.docID() != Scorer.NO_MORE_DOCS;
}
