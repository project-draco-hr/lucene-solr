{
  CategoryPath p=new CategoryPath(1000,1000);
  assertNull(p.getComponent(0));
  assertNull(p.getComponent(1));
  assertNull(p.getComponent(-1));
  for (int i=0; i <= 100; i++) {
    p.add(Integer.toString(i));
    for (int j=0; j <= i; j++) {
      assertEquals(j,Integer.parseInt(p.getComponent(j)));
    }
    assertNull(p.getComponent(-1));
    assertNull(p.getComponent(i + 1));
  }
}
