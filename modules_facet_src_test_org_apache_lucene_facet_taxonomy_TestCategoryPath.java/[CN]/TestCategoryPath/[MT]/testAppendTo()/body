{
  CategoryPath p=new CategoryPath(0,0);
  StringBuilder sb=new StringBuilder();
  p.appendTo(sb,'/');
  assertEquals(0,sb.length());
  p.appendTo(sb,'/',-1);
  assertEquals(0,sb.length());
  p.appendTo(sb,'/',1);
  assertEquals(0,sb.length());
  p.appendTo(sb,'/',-1,1);
  assertEquals(0,sb.length());
}
