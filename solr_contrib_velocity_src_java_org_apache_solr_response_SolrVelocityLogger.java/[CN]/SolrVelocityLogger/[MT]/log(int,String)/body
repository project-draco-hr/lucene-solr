{
switch (level) {
case LogChute.TRACE_ID:
    log.trace(message);
  break;
case LogChute.DEBUG_ID:
log.debug(message);
break;
case LogChute.INFO_ID:
log.info(message);
break;
case LogChute.WARN_ID:
log.warn(message);
break;
case LogChute.ERROR_ID:
log.error(message);
break;
default :
log.warn(message);
break;
}
}
