def scrubCheckout():
    unversionedRex = re.compile('^ ?[\\?ID] *[1-9 ]*[a-zA-Z]* +(.*)')
    for l in os.popen('svn status --no-ignore -v').readlines():
        match = unversionedRex.match(l)
        if match:
            s = match.group(1)
            if os.path.exists(s):
                print ('    delete %s' % s)
                if (os.path.isdir(s) and (not os.path.islink(s))):
                    shutil.rmtree(s)
                else:
                    os.remove(s)
