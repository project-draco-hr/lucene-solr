{
  if (lastElement > ord) {
    spare.offset=offsets[ord];
    spare.length=ord == lastElement - 1 ? currentOffset - spare.offset : offsets[ord + 1] - spare.offset;
    pool.copyFrom(spare);
    return spare;
  }
  throw new IndexOutOfBoundsException("index " + ord + " must be less than the size: "+ lastElement);
}
