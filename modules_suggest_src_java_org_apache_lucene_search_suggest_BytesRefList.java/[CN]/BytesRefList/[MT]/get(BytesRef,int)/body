{
  if (currentElement > pos) {
    bytes.offset=offsets[pos];
    bytes.length=pos == currentElement - 1 ? currentOffset - bytes.offset : offsets[pos + 1] - bytes.offset;
    pool.copyFrom(bytes);
    return bytes;
  }
  throw new IndexOutOfBoundsException("index " + pos + " must be less than the size: "+ currentElement);
}
