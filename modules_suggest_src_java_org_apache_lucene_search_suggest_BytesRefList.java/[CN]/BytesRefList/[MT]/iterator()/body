{
  final int numElements=currentElement;
  return new BytesRefIterator(){
    private final BytesRef spare=new BytesRef();
    private int pos=0;
    @Override public BytesRef next() throws IOException {
      if (pos < numElements) {
        get(spare,pos++);
        return spare;
      }
      return null;
    }
  }
;
}
