{
  Iterator sqi=getSubQueriesIterator();
  r.append(getOperatorName());
  r.append(getBracketOpen());
  if (sqi.hasNext()) {
    r.append(sqi.next().toString());
    while (sqi.hasNext()) {
      r.append(getPrefixSeparator());
      r.append(sqi.next().toString());
    }
  }
  r.append(getBracketClose());
}
