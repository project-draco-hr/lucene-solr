{
  if (args.length != 2) {
    printHelp(args);
    System.exit(0);
  }
  File workingDir=new File(args[0]);
  File configFile=new File(args[1]);
  if (configFile.exists()) {
    try {
      Driver driver=new Driver(workingDir,new InputSource(new FileReader(configFile)));
      driver.run();
    }
 catch (    Exception e) {
      e.printStackTrace(System.err);
    }
  }
}
