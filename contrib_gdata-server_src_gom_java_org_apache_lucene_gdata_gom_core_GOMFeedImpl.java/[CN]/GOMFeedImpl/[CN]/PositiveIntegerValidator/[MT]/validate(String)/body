{
  super.validate(aTextValue);
  try {
    int i=Integer.parseInt(aTextValue);
    if (i < 0)     throw new GDataParseException(String.format(AtomParser.INVALID_ELEMENT_VALUE,this.localName,"positive integer value"));
  }
 catch (  NumberFormatException e) {
    throw new GDataParseException(String.format(AtomParser.INVALID_ELEMENT_VALUE,this.localName,"positive integer value"));
  }
}
