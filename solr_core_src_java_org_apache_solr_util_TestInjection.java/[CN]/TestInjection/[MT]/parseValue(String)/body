{
  Matcher m=ENABLED_PERCENT.matcher(raw);
  if (!m.matches())   throw new RuntimeException("No match, probably bad syntax: " + raw);
  String val=m.group(1);
  String percent="100";
  if (m.groupCount() == 2) {
    percent=m.group(2);
  }
  return new Pair<>(Boolean.parseBoolean(val),Integer.parseInt(percent));
}
