{
  if (key.length() == 0 || automaton == null) {
    return EMPTY_RESULT;
  }
  try {
    if (!onlyMorePopular && rootArcs.length > 1) {
      return lookupSortedAlphabetically(key,num);
    }
 else {
      return lookupSortedByWeight(key,num,true);
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
