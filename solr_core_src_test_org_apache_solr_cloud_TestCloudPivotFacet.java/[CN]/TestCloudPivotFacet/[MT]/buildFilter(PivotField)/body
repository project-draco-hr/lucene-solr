{
  Object value=constraint.getValue();
  if (null == value) {
    return "-" + constraint.getField() + ":[* TO *]";
  }
  String prefix="{!term f=" + constraint.getField() + "}";
  if (value instanceof Date) {
    return prefix + DateFormatUtil.formatExternal((Date)value);
  }
 else {
    return prefix + value;
  }
}
