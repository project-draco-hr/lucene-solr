{
  super.setNextReader(reader,docBase);
  this.reader=reader;
  for (  SearchGroup searchGroup : groupMap.values()) {
    for (int i=0; i < searchGroup.sortGroupComparators.length; i++)     searchGroup.sortGroupComparators[i]=searchGroup.sortGroupComparators[i].setNextReader(reader,docBase);
  }
}
