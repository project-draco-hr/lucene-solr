{
  Reader reader=new StringReader("???????????????????? ??????????????????");
  Map<String,String> args=new HashMap<String,String>();
  args.put("id","Cyrillic-Latin");
  ICUTransformFilterFactory factory=new ICUTransformFilterFactory(args);
