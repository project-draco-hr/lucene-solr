{
  if (!matches)   return;
  if (upto >= payloadToMatch.size()) {
    matches=false;
    return;
  }
  BytesRef payload=postings.getPayload();
  if (payloadToMatch.get(upto) == null) {
    matches=payload == null;
    upto++;
    return;
  }
  if (payload == null) {
    matches=false;
    upto++;
    return;
  }
  matches=payloadToMatch.get(upto).bytesEquals(payload);
  upto++;
}
