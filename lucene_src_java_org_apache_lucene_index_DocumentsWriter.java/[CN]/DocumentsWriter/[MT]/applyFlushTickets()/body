{
synchronized (ticketQueue) {
    while (true) {
      final FlushTicket head=ticketQueue.peek();
      if (head != null && head.canPublish()) {
        try {
          finishFlush(head.segment,head.frozenDeletes);
        }
  finally {
          ticketQueue.poll();
        }
      }
 else {
        break;
      }
    }
  }
}
