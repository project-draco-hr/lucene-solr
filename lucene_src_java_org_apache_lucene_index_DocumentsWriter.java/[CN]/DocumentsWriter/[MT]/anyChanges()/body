{
  if (infoStream != null) {
    message("docWriter: anyChanges? numDocsInRam=" + numDocsInRAM.get() + " deletes="+ anyDeletions()+ " hasTickets:"+ ticketQueue.hasTickets()+ " pendingChangesInFullFlush: "+ pendingChangesInCurrentFullFlush);
  }
  return numDocsInRAM.get() != 0 || anyDeletions() || ticketQueue.hasTickets() || pendingChangesInCurrentFullFlush;
}
