{
  if (!d.isSuite()) {
    throw new IllegalArgumentException("This is a @ClassRule (applies to suites only).");
  }
  return new Statement(){
    @Override public void evaluate() throws Throwable {
      try {
        testClass=d.getTestClass();
        s.evaluate();
      }
  finally {
        testClass=null;
      }
    }
  }
;
}
