{
  super(valueCount,BITS_PER_VALUE);
  short[] blocks=new short[valueCount];
  for (int i=0; i < valueCount; i++) {
    blocks[i]=in.readShort();
  }
  final int mod=valueCount % 4;
  if (mod != 0) {
    final int pad=4 - mod;
    for (int i=0; i < pad; i++) {
      in.readShort();
    }
  }
  this.blocks=blocks;
}
