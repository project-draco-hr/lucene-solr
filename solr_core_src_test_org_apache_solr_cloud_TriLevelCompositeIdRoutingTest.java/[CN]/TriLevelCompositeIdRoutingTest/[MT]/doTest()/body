{
  boolean testFinished=false;
  try {
    handle.clear();
    handle.put("QTime",SKIPVAL);
    handle.put("timestamp",SKIPVAL);
    waitForRecoveriesToFinish(true);
    doTriLevelHashingTest();
    doTriLevelHashingTestWithBitMask();
    testFinished=true;
  }
  finally {
    if (!testFinished) {
      printLayoutOnTearDown=true;
    }
  }
}
