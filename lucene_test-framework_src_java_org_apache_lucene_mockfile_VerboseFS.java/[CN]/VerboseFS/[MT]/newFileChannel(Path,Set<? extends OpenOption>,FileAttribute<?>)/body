{
  Throwable exception=null;
  try {
    return super.newFileChannel(path,options,attrs);
  }
 catch (  Throwable t) {
    exception=t;
  }
 finally {
    if (containsDestructive(options)) {
      sop("newFileChannel" + options + ": "+ path(path),exception);
    }
  }
  throw new AssertionError();
}
