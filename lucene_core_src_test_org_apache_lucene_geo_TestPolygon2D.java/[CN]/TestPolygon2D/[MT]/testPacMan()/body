{
  double[] px={0,10,10,0,-8,-10,-8,0,10,10,0};
  double[] py={0,5,9,10,9,0,-9,-10,-9,-5,0};
  double xMin=2;
  double xMax=11;
  double yMin=-1;
  double yMax=1;
  Polygon2D polygon=Polygon2D.create(new Polygon(py,px));
  assertEquals(Relation.CELL_CROSSES_QUERY,polygon.relate(yMin,yMax,xMin,xMax));
}
