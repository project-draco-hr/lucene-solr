{
  double[] lons=new double[]{0,45,160,180,-45,-175,-180};
  for (  double lon : lons) {
    double[] lonWs=new double[]{0,20,180,200,355,360};
    for (    double lonW : lonWs) {
      testRectangle(lon,lonW,0,0);
      testRectangle(lon,lonW,-10,10);
      testRectangle(lon,lonW,80,10);
      testRectangle(lon,lonW,-90,180);
    }
  }
  testRectIntersect();
}
