{
  final IndexInput input=(IndexInput)datIn.clone();
  boolean success=false;
  try {
    ValuesEnum inst=packed ? new PackedIntsEnumImpl(source,input) : new FixedIntsEnumImpl(source,input);
    success=true;
    return inst;
  }
  finally {
    if (!success) {
      IOUtils.closeSafely(true,input);
    }
  }
}
