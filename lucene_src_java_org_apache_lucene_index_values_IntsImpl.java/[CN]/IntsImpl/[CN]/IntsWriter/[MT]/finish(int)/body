{
  try {
    if (!started) {
      minValue=maxValue=0;
    }
    if ((maxValue - minValue) < (((long)1) << 63) && (maxValue - minValue) >= 0) {
      writePackedInts(docCount);
    }
 else {
      writeFixedInts(docCount);
    }
  }
  finally {
    datOut.close();
    bytesUsed.addAndGet(-(RamUsageEstimator.NUM_BYTES_LONG * docToValue.length));
    docToValue=null;
  }
}
