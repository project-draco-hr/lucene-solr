{
  super(bytesUsed);
  this.dir=dir;
  this.id=id;
switch (valueType) {
case FIXED_INTS_16:
    array=new ShortValues(bytesUsed);
  typeOrd=FIXED_16;
break;
case FIXED_INTS_32:
array=new IntValues(bytesUsed);
typeOrd=FIXED_32;
break;
case FIXED_INTS_64:
array=new LongValues(bytesUsed);
typeOrd=FIXED_64;
break;
case FIXED_INTS_8:
array=new ByteValues(bytesUsed);
typeOrd=FIXED_8;
break;
case VAR_INTS:
array=new LongValues(bytesUsed);
typeOrd=PACKED;
break;
default :
throw new IllegalStateException("unknown type " + valueType);
}
}
