{
  WordDelimiterFilter wdf=new WordDelimiterFilter(new TokenStream(){
    Token t;
    public Token next(){
      if (t != null)       return null;
      t=new Token("(??belkeit",7,17);
      return t;
    }
  }
,1,1,0,0,1,1,0);
  Token t=wdf.next();
  assertNotNull(t);
  assertEquals("??belkeit",t.term());
  assertEquals(8,t.startOffset());
  assertEquals(17,t.endOffset());
}
