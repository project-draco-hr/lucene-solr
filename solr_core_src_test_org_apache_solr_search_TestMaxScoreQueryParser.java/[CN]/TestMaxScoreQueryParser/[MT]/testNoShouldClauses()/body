{
  q=parse("+foo +bar");
  clauses=clauses(q);
  assertEquals(2,clauses.length);
  assertTrue(clauses[0].isRequired());
  assertTrue(clauses[1].isRequired());
  q=parse("+foo -bar");
  clauses=clauses(q);
  assertEquals(2,clauses.length);
  assertTrue(clauses[0].isRequired());
  assertTrue(clauses[1].isProhibited());
}
