{
  if (input.incrementToken()) {
    int len=termAtt.termLength();
    if (marker != NOMARKER) {
      len++;
      termAtt.resizeTermBuffer(len);
      termAtt.termBuffer()[len - 1]=marker;
    }
    reverse(matchVersion,termAtt.termBuffer(),0,len);
    termAtt.setTermLength(len);
    return true;
  }
 else {
    return false;
  }
}
