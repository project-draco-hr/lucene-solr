{
  final String stringToCompare="PointInGeo3DShapeQuery: field=point: Shape: GeoStandardCircle: {planetmodel=PlanetModel.WGS84, center=[lat=0.7";
  assertEquals(stringToCompare,Geo3DPoint.newShapeQuery("point",GeoCircleFactory.makeGeoCircle(PlanetModel.WGS84,toRadians(44.244272),toRadians(7.769736),0.1)).toString().substring(0,stringToCompare.length()));
}
