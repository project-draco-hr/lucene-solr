{
  NGramTokenizer tokenizer=new NGramTokenizer(input,1,1);
  Token token=null;
  do {
    token=tokenizer.next();
    if (token != null) {
      tokens.add(token.toString());
    }
  }
 while (token != null);
  assertEquals(5,tokens.size());
  ArrayList exp=new ArrayList();
  exp.add("(a,0,1)");
  exp.add("(b,1,2)");
  exp.add("(c,2,3)");
  exp.add("(d,3,4)");
  exp.add("(e,4,5)");
  assertEquals(exp,tokens);
}
