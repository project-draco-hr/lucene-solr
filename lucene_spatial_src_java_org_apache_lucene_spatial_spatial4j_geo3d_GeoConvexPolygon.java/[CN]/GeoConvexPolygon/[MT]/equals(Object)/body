{
  if (!(o instanceof GeoConvexPolygon))   return false;
  GeoConvexPolygon other=(GeoConvexPolygon)o;
  if (!super.equals(other))   return false;
  if (other.points.size() != points.size())   return false;
  for (int i=0; i < points.size(); i++) {
    if (!other.points.get(i).equals(points.get(i)))     return false;
  }
  return true;
}
