{
  super(planetModel);
  points=new ArrayList<GeoPoint>();
  isInternalEdges=new BitSet();
  if (startLatitude > Math.PI * 0.5 || startLatitude < -Math.PI * 0.5)   throw new IllegalArgumentException("Latitude out of range");
  if (startLongitude < -Math.PI || startLongitude > Math.PI)   throw new IllegalArgumentException("Longitude out of range");
  final GeoPoint p=new GeoPoint(planetModel,startLatitude,startLongitude);
  points.add(p);
}
