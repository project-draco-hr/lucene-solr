{
  return new StrDocValues(this){
    @Override public String strVal(    int doc){
      return string;
    }
    @Override public boolean bytesVal(    int doc,    BytesRef target){
      target.copy(bytesRef);
      return true;
    }
    @Override public String toString(    int doc){
      return string;
    }
  }
;
}
