{
  List list=new ArrayList();
  String[] ramFiles=secondaryDir.listAll();
  for (int x=0; x < ramFiles.length; x++) {
    list.add(ramFiles[x]);
  }
  String[] fsFiles=primaryDir.listAll();
  for (int x=0; x < fsFiles.length; x++) {
    list.add(fsFiles[x]);
  }
  return (String[])list.toArray(new String[0]);
}
