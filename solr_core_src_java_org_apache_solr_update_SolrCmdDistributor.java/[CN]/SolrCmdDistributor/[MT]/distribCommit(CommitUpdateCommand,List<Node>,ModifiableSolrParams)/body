{
  servers.blockUntilFinished();
  doRetriesIfNeeded();
  UpdateRequest uReq=new UpdateRequest();
  uReq.setParams(params);
  addCommit(uReq,cmd);
  log.debug("Distrib commit to:" + nodes + " params:"+ params);
  for (  Node node : nodes) {
    submit(new Req(cmd.toString(),node,uReq,false));
  }
}
