{
  ZkCoreNodeProps leaderProps;
  try {
    leaderProps=new ZkCoreNodeProps(zkStateReader.getLeaderRetry(collection,shardId));
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    return false;
  }
catch (  Exception e) {
    log.warn(null,e);
    return true;
  }
  this.nodeProps=leaderProps;
  return true;
}
