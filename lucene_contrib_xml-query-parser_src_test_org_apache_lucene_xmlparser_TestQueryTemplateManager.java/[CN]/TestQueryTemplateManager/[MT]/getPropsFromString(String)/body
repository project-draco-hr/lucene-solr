{
  Properties result=new Properties();
  StringTokenizer st=new StringTokenizer(nameValuePairs,"\t=");
  while (st.hasMoreTokens()) {
    String name=st.nextToken().trim();
    if (st.hasMoreTokens()) {
      String value=st.nextToken().trim();
      result.setProperty(name,value);
    }
  }
  return result;
}
