{
  if (doIndent)   indent();
  writer.write('<');
  writer.write(tag);
  if (name != null) {
    writeAttr(NAME,name);
    if (closeTag) {
      writer.write("/>");
    }
 else {
      writer.write(">");
    }
  }
 else {
    if (closeTag) {
      writer.write("/>");
    }
 else {
      writer.write('>');
    }
  }
}
