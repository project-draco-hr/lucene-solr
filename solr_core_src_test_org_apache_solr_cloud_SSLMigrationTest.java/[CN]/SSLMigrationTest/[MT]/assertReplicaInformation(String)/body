{
  List<Replica> replicas=getReplicas();
  assertEquals("Wrong number of replicas found",4,replicas.size());
  for (  Replica replica : replicas) {
    assertTrue("Replica didn't have the proper urlScheme in the ClusterState",StringUtils.startsWith(replica.getStr(ZkStateReader.BASE_URL_PROP),urlScheme));
  }
}
