{
  if (proxStream == null) {
    proxStream=(IndexInput)parent.proxStream.clone();
  }
  skipPayload();
  if (lazySkipPointer != 0) {
    proxStream.seek(lazySkipPointer);
    lazySkipPointer=0;
  }
  if (lazySkipProxCount != 0) {
    skipPositions(lazySkipProxCount);
    lazySkipProxCount=0;
  }
}
