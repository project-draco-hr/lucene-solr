{
  String threadName=t.getName();
  if (threadName.equals(TimerThread.THREAD_NAME)) {
    return true;
  }
  if (threadName.startsWith("facetExecutor-") || threadName.startsWith("metrics-") || threadName.startsWith("cmdDistribExecutor-")|| threadName.startsWith("httpShardExecutor-")) {
    return true;
  }
  if (threadName.startsWith("SyncThread")) {
    return true;
  }
  if (threadName.startsWith("Overseer-") || threadName.startsWith("aliveCheckExecutor-") || threadName.startsWith("concurrentUpdateScheduler-")) {
    return true;
  }
  return false;
}
