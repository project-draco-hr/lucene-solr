{
  currentDocTerms.get(doc,tempBR);
  final String val2=tempBR.length == 0 && docsWithField.get(doc) == false ? null : tempBR.utf8ToString();
  if (bottom == null) {
    if (val2 == null) {
      return 0;
    }
    return -1;
  }
 else   if (val2 == null) {
    return 1;
  }
  return collator.compare(bottom,val2);
}
