{
  if (failureMarker.wasSuccessful()) {
    if (file.exists()) {
      try {
        TestUtil.rm(file);
      }
 catch (      IOException e) {
        throw new IOException("Could not remove temporary location '" + file.getAbsolutePath() + "', created at stack trace:\n"+ creationStack,e);
      }
    }
  }
}
