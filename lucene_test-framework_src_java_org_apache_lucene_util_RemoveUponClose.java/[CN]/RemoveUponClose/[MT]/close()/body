{
  if (failureMarker.wasSuccessful()) {
    if (Files.exists(path)) {
      try {
        IOUtils.rm(path);
      }
 catch (      IOException e) {
        throw new IOException("Could not remove temporary location '" + path.toAbsolutePath() + "', created at stack trace:\n"+ creationStack,e);
      }
    }
  }
}
