{
  ensureOpen();
  byte[] bytes=(byte[])normsCache.get(field);
  if (bytes == null && !hasNorms(field))   bytes=fakeNorms();
  if (bytes != null)   System.arraycopy(bytes,0,result,offset,maxDoc());
  for (int i=0; i < subReaders.length; i++)   subReaders[i].norms(field,result,offset + starts[i]);
}
