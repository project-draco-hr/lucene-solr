{
  SegmentMergeInfo top=(SegmentMergeInfo)queue.top();
  if (top == null) {
    term=null;
    return false;
  }
  term=top.term;
  docFreq=0;
  while (top != null && term.compareTo(top.term) == 0) {
    queue.pop();
    docFreq+=top.termEnum.docFreq();
    if (top.next())     queue.put(top);
 else     top.close();
    top=(SegmentMergeInfo)queue.top();
  }
  return true;
}
