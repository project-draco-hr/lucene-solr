{
switch (random.nextInt(10)) {
case 0:
{
      char c[]=new char[1];
      int ret=input.read(c);
      return ret < 0 ? ret : c[0];
    }
case 1:
{
    char c[]=new char[2];
    int ret=input.read(c,1,1);
    return ret < 0 ? ret : c[1];
  }
case 2:
{
  char c[]=new char[1];
  CharBuffer cb=CharBuffer.wrap(c);
  int ret=input.read(cb);
  return ret < 0 ? ret : c[0];
}
default :
return input.read();
}
}
