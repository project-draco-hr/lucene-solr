{
  final int docStoreOffset=docWriter.getDocStoreOffset();
  final int end=docID + docStoreOffset;
  if (lastDocID < end) {
    final long tvfPosition=tvf.getFilePointer();
    while (lastDocID < end) {
      tvx.writeLong(tvd.getFilePointer());
      tvd.writeVInt(0);
      tvx.writeLong(tvfPosition);
      lastDocID++;
    }
  }
}
