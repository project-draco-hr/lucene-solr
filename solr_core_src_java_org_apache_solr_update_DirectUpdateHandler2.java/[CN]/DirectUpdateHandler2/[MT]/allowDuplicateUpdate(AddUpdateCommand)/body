{
  RefCounted<IndexWriter> iw=solrCoreState.getIndexWriter(core);
  try {
    IndexWriter writer=iw.get();
    if (cmd.isBlock()) {
      writer.addDocuments(cmd);
    }
 else {
      writer.addDocument(cmd.getLuceneDocument());
    }
    if (ulog != null)     ulog.add(cmd);
  }
  finally {
    iw.decref();
  }
}
