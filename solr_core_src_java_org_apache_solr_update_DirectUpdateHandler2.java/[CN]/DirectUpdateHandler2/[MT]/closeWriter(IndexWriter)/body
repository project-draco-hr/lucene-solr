{
  commitLock.lock();
  try {
    if (!commitOnClose) {
      if (writer != null) {
        writer.rollback();
      }
      if (ulog != null)       ulog.close();
      return;
    }
    if (writer != null) {
      writer.close();
    }
    if (ulog != null)     ulog.close();
  }
  finally {
    commitLock.unlock();
  }
}
