{
  PostingsEnum postings1=termsEnum.postings(null,firstFlags);
  PostingsEnum postings2=termsEnum.postings(postings1,secondFlags);
  if (shouldReuse)   assertSame("Expected PostingsEnum " + postings1.getClass().getName() + " to be reused",postings1,postings2);
 else   assertNotSame("Expected PostingsEnum " + postings1.getClass().getName() + " to not be reused",postings1,postings2);
}
