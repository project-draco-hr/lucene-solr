{
  SeedTermsEnum termsEnum;
  if (reuse != null && reuse instanceof SeedTermsEnum) {
    termsEnum=(SeedTermsEnum)reuse;
    if (termsEnum.terms != terms) {
      termsEnum=new SeedTermsEnum(terms,maxAllowed,allowPayloads);
    }
  }
 else {
    termsEnum=new SeedTermsEnum(terms,maxAllowed,allowPayloads);
  }
  termsEnum.reset();
  return termsEnum;
}
