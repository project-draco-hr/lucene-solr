{
  CodecUtil.checkHeader(termsIn,BlockPostingsWriter.TERMS_CODEC,BlockPostingsWriter.VERSION_START,BlockPostingsWriter.VERSION_START);
  final int indexBlockSize=termsIn.readVInt();
  if (indexBlockSize != blockSize) {
    throw new IllegalStateException("index-time blockSize (" + indexBlockSize + ") != read-time blockSize ("+ blockSize+ ")");
  }
}
