{
  if (pos < 0)   return null;
  try {
synchronized (this) {
      fos.flushBuffer();
    }
    ChannelFastInputStream fis=new ChannelFastInputStream(channel,pos);
    LogCodec codec=new LogCodec();
    return codec.readVal(fis);
  }
 catch (  IOException e) {
    throw new SolrException(SolrException.ErrorCode.SERVER_ERROR,e);
  }
}
