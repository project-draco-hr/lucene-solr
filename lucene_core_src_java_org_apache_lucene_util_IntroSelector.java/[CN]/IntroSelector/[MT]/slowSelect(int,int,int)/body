{
  new Sorter(){
    @Override protected void swap(    int i,    int j){
      IntroSelector.this.swap(i,j);
    }
    @Override protected int compare(    int i,    int j){
      return IntroSelector.this.compare(i,j);
    }
    public void sort(    int from,    int to){
      heapSort(from,to);
    }
  }
.sort(from,to);
}
