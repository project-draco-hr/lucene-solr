{
  int num=atLeast(1);
  for (int i=0; i < num; i++) {
    automata.clear();
    terms.clear();
    for (int j=0; j < 5000; j++) {
      String randomString=TestUtil.randomUnicodeString(random());
      terms.add(randomString);
      automata.add(Automata.makeString(randomString));
    }
    assertLexicon();
  }
}
