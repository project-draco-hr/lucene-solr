{
  int version=CodecUtil.checkHeader(input,BlockTreeTermsWriter.TERMS_INDEX_CODEC_NAME,BlockTreeTermsWriter.TERMS_INDEX_VERSION_START,BlockTreeTermsWriter.TERMS_INDEX_VERSION_CURRENT);
  if (version < BlockTreeTermsWriter.TERMS_INDEX_VERSION_APPEND_ONLY) {
    indexDirOffset=input.readLong();
  }
  return version;
}
