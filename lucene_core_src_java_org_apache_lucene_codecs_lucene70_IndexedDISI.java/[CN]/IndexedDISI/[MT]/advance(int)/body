{
  final int targetBlock=target & 0xFFFF0000;
  if (block < targetBlock) {
    advanceBlock(targetBlock);
  }
  if (block == targetBlock) {
    if (method.advanceWithinBlock(this,target)) {
      return doc;
    }
    readBlockHeader();
  }
  boolean found=method.advanceWithinBlock(this,block);
  assert found;
  return doc;
}
