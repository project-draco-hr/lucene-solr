{
  Random rnd2=new Random(randomSeed);
  for (int i=0; i < packedInt.size(); i++) {
    long value=_TestUtil.nextLong(rnd2,0,maxValue);
    packedInt.set(i,value);
    assertEquals(String.format("The set/get of the value at index %d should match for %s",i,packedInt.getClass().getSimpleName()),value,packedInt.get(i));
  }
}
