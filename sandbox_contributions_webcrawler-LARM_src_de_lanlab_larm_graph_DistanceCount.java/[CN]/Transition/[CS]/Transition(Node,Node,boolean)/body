{
  LinkedList l=from.getOutgoing();
  Iterator i=l.iterator();
  while (i.hasNext()) {
    Transition t=(Transition)i.next();
    if (t.getTo() == to) {
      return;
    }
  }
  this.from=from;
  this.to=to;
  from.addOutgoing(this);
  to.addIncoming(this);
  this.distance=Integer.MAX_VALUE;
  this.isFrame=isFrame;
  this.linkRank[0]=this.linkRank[1]=1;
}
