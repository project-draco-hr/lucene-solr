{
  writeCoreProperties(solrHome.toPath(),DEFAULT_TEST_CORENAME);
  JettySolrRunner jetty=new JettySolrRunner(solrHome.getAbsolutePath(),context,0,solrConfigOverride,schemaOverride,true,null,sslConfig);
  jetty.setShards(shardList);
  if (System.getProperty("collection") == null) {
    System.setProperty("collection","collection1");
  }
  jetty.start();
  System.clearProperty("collection");
  return jetty;
}
