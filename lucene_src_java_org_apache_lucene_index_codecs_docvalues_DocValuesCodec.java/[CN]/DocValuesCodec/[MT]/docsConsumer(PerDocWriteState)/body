{
  return new PerDocConsumer(){
    public void close() throws IOException {
    }
    @Override public DocValuesConsumer addValuesField(    FieldInfo field) throws IOException {
      final DocValuesConsumer consumer=Writer.create(field.getDocValues(),docValuesId(state.segmentName,state.codecId,field.number),state.directory,comparator,state.bytesUsed);
      return consumer;
    }
  }
;
}
