{
  Directory dir=state.dir;
  Set<String> files=new HashSet<String>();
  other.files(dir,state.segmentInfo,files);
  for (  String string : files) {
    if (dir.fileExists(string))     return new WrappingFielsdProducer(state.segmentInfo,state.dir,state.fieldInfos,other.fieldsProducer(state));
  }
  return new WrappingFielsdProducer(state.segmentInfo,state.dir,state.fieldInfos,FieldsProducer.EMPTY);
}
