{
  final String znodePath=getZnodeForResource(storedResourceId);
  try {
    return zkClient.exists(znodePath,retryOnConnLoss);
  }
 catch (  Exception e) {
    if (e instanceof IOException) {
      throw (IOException)e;
    }
 else {
      throw new IOException("Failed to read data at " + znodePath,e);
    }
  }
}
