{
  String startDate=getLocalizedDate(2002,1,1);
  String endDate=getLocalizedDate(2002,1,4);
  assertQueryEquals("[ " + escapeDateString(startDate) + " TO "+ escapeDateString(endDate)+ "]",null,"[" + getDate(startDate) + " TO "+ getDate(endDate)+ "]");
  assertQueryEquals("{  " + escapeDateString(startDate) + "    "+ escapeDateString(endDate)+ "   }",null,"{" + getDate(startDate) + " TO "+ getDate(endDate)+ "}");
}
