{
  for (pointer++; pointer < pointerMax; pointer++) {
    if (docs[pointer] >= target) {
      return doc=docs[pointer];
    }
  }
  boolean result=termDocs.skipTo(target);
  if (result) {
    pointerMax=1;
    pointer=0;
    docs[pointer]=doc=termDocs.doc();
    freqs[pointer]=termDocs.freq();
  }
 else {
    doc=NO_MORE_DOCS;
  }
  return doc;
}
