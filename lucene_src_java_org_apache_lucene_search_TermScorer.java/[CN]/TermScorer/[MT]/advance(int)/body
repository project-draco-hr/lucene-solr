{
  for (pointer++; pointer < pointerMax; pointer++) {
    if (docs[pointer] >= target) {
      freq=freqs[pointer];
      return doc=docs[pointer];
    }
  }
  int newDoc=docsEnum.advance(target);
  if (newDoc != NO_MORE_DOCS) {
    doc=newDoc;
    freq=docsEnum.freq();
  }
 else {
    doc=NO_MORE_DOCS;
  }
  return doc;
}
