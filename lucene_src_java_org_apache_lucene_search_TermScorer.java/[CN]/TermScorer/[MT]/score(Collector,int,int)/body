{
  c.setScorer(this);
  while (doc < end) {
    c.collect(doc);
    if (++pointer >= pointerMax) {
      refillBuffer();
      if (pointerMax != 0) {
        pointer=0;
      }
 else {
        doc=NO_MORE_DOCS;
        return false;
      }
    }
    doc=docs[pointer];
    freq=freqs[pointer];
  }
  return true;
}
