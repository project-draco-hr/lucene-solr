{
  int version=CodecUtil.checkHeader(input,TempBlockTermsWriter.TERMS_INDEX_CODEC_NAME,TempBlockTermsWriter.TERMS_INDEX_VERSION_START,TempBlockTermsWriter.TERMS_INDEX_VERSION_CURRENT);
  if (version < TempBlockTermsWriter.TERMS_INDEX_VERSION_APPEND_ONLY) {
    indexDirOffset=input.readLong();
  }
  return version;
}
