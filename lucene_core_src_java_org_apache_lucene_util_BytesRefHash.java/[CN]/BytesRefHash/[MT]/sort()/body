{
  final int[] compact=compact();
  new StringMSBRadixSorter(){
    BytesRef scratch=new BytesRef();
    @Override protected void swap(    int i,    int j){
      int tmp=compact[i];
      compact[i]=compact[j];
      compact[j]=tmp;
    }
    @Override protected BytesRef get(    int i){
      pool.setBytesRef(scratch,bytesStart[compact[i]]);
      return scratch;
    }
  }
.sort(0,count);
  return compact;
}
