{
  if (latitude < minLat || latitude > maxLat || longitude < minLon || longitude > maxLon) {
    return false;
  }
  if (tree.contains(latitude,longitude)) {
    for (    LatLonTree hole : holes) {
      if (hole.contains(latitude,longitude)) {
        return false;
      }
    }
    return true;
  }
  return false;
}
