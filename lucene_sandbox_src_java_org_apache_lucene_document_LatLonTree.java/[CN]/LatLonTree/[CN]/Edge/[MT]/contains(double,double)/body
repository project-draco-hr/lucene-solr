{
  boolean res=false;
  if (latitude <= max) {
    if (lat1 > latitude != lat2 > latitude) {
      if (longitude < (lon1 - lon2) * (latitude - lat2) / (lat1 - lat2) + lon2) {
        res=true;
      }
    }
    if (left != null) {
      res^=left.contains(latitude,longitude);
    }
    if (right != null && latitude >= low) {
      res^=right.contains(latitude,longitude);
    }
  }
  return res;
}
