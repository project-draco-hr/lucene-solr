{
  final StringBuilder sb=new StringBuilder();
  sb.append(getClass().getSimpleName());
  sb.append(':');
  if (!this.field.equals(field)) {
    sb.append(" field=");
    sb.append(this.field);
    sb.append(':');
  }
  return sb.append(" Center: [").append(centerLat).append(',').append(centerLon).append(']').append(" Distance: ").append(radiusMeters).append(" meters").append("]").toString();
}
