{
  BufferedReader vocReader=new BufferedReader(new InputStreamReader(voc,"UTF-8"));
  BufferedReader outputReader=new BufferedReader(new InputStreamReader(out,"UTF-8"));
  String inputWord=null;
  while ((inputWord=vocReader.readLine()) != null) {
    String expectedWord=outputReader.readLine();
    Assert.assertNotNull(expectedWord);
    BaseTokenStreamTestCase.checkOneTerm(a,inputWord,expectedWord);
  }
}
