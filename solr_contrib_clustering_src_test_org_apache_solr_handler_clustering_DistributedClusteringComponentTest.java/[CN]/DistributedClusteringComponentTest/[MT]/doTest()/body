{
  del("*:*");
  int numberOfDocs=0;
  for (  String[] doc : AbstractClusteringTestCase.DOCUMENTS) {
    index(id,Integer.toString(numberOfDocs++),"url",doc[0],"title",doc[1],"snippet",doc[2]);
  }
  commit();
  handle.clear();
  handle.put("responseHeader",SKIP);
  handle.put("response",SKIP);
  query(ClusteringComponent.COMPONENT_NAME,"true",CommonParams.Q,"*:*",CommonParams.SORT,id + " desc",ClusteringParams.USE_SEARCH_RESULTS,"true");
}
