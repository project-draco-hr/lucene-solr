{
  if (pending) {
    pending=false;
    decodeStats();
    return term();
  }
  decoded=false;
  DFS:   while (level > 0) {
    Frame frame=newFrame();
    if (loadExpandFrame(topFrame(),frame) != null) {
      pushFrame(frame);
      if (isAccept(frame)) {
        break;
      }
      continue;
    }
    frame=popFrame();
    while (level > 0) {
      if (loadNextFrame(topFrame(),frame) != null) {
        pushFrame(frame);
        if (isAccept(frame)) {
          break DFS;
        }
        continue DFS;
      }
      frame=popFrame();
    }
    return null;
  }
  decodeStats();
  return term();
}
