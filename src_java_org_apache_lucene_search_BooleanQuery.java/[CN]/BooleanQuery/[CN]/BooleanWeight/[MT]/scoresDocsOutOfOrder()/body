{
  int numProhibited=0;
  for (Iterator cIter=clauses.iterator(); cIter.hasNext(); ) {
    BooleanClause c=(BooleanClause)cIter.next();
    if (c.isRequired()) {
      return false;
    }
 else     if (c.isProhibited()) {
      ++numProhibited;
    }
  }
  if (numProhibited > 32) {
    return false;
  }
  return true;
}
