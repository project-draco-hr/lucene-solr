{
  Analyzer anlzr=(Analyzer)Class.forName(config.get("analyzer",StandardAnalyzer.class.getName())).newInstance();
  List queryList=new ArrayList(20);
  queryList.addAll(Arrays.asList(STANDARD_QUERIES));
  if (!config.get("enwikiQueryMaker.disableSpanQueries",false))   queryList.addAll(Arrays.asList(getPrebuiltQueries(DocMaker.BODY_FIELD)));
  return createQueries(queryList,anlzr);
}
