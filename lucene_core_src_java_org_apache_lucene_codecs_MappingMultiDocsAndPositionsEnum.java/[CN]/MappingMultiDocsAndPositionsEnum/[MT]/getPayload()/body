{
  BytesRef payload=current.getPayload();
  if (mergeState.currentPayloadProcessor[upto] != null && payload != null) {
    payload=BytesRef.deepCopyOf(payload);
    mergeState.currentPayloadProcessor[upto].processPayload(payload);
    if (payload.length == 0) {
      return null;
    }
  }
  return payload;
}
