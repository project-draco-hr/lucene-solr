{
  assertEquals("dogs",UnicodeUtil.nextValidUTF16String("dogs"));
  assertEquals("dogs\uD802\uDC02",UnicodeUtil.nextValidUTF16String("dogs\uD802\uDC02"));
  assertEquals("dogs\uD801\uDC00",UnicodeUtil.nextValidUTF16String("dogs\uD801"));
  assertEquals("dogs\uD801\uDC00",UnicodeUtil.nextValidUTF16String("dogs\uD801b"));
  assertEquals("dogs\uD801\uDC00",UnicodeUtil.nextValidUTF16String("dogs\uD801\uD800"));
  assertEquals("dogs\uD802\uDC00",UnicodeUtil.nextValidUTF16String("dogs\uD801\uE001"));
  assertEquals("dogs\uE000",UnicodeUtil.nextValidUTF16String("dogs\uDBFF\uE001"));
  assertEquals("dogs\uE000",UnicodeUtil.nextValidUTF16String("dogs\uDC00"));
  assertEquals("\uE000",UnicodeUtil.nextValidUTF16String("\uDC00dogs"));
}
