{
  if (this.request.getParameter(START_INDEX_NEXT_PAGE_PARAMETER) == null)   return DEFAULT_START_INDEX;
  int retval=-1;
  try {
    retval=new Integer(this.request.getParameter(START_INDEX_NEXT_PAGE_PARAMETER)).intValue();
  }
 catch (  Exception e) {
    LOG.warn("Start-index could not be parsed - " + e.getMessage(),e);
  }
  return retval < 0 ? DEFAULT_START_INDEX : retval;
}
