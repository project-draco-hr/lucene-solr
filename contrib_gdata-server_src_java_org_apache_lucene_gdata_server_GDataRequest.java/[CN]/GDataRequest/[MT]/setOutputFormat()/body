{
  String formatParameter=this.request.getParameter(RESPONSE_FORMAT_PARAMETER);
  if (formatParameter == null)   return;
  if (formatParameter.equalsIgnoreCase(RESPONSE_FORMAT_PARAMETER_RSS))   this.responseFormat=OutputFormat.RSS;
  if (formatParameter.equalsIgnoreCase(RESPONSE_FORMAT_PARAMETER_HTML))   this.responseFormat=OutputFormat.HTML;
}
