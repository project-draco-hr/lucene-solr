{
  if (this.request.getParameter(ITEMS_PER_PAGE_PARAMETER) == null)   return DEFAULT_ITEMS_PER_PAGE;
  int retval=-1;
  try {
    retval=new Integer(this.request.getParameter(ITEMS_PER_PAGE_PARAMETER)).intValue();
  }
 catch (  Exception e) {
    LOG.warn("Intems per page could not be parsed - " + e.getMessage(),e);
  }
  return retval < 0 ? DEFAULT_ITEMS_PER_PAGE : retval;
}
