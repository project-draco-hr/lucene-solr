{
  String retVal=this.request.getQueryString();
  if (this.request.getParameter(ITEMS_PER_PAGE_PARAMETER) != null)   return retVal;
  String tempString=(retVal == null ? "?" + ITEMS_PER_PAGE_PARAMETER + "="+ DEFAULT_ITEMS_PER_PAGE : "&" + ITEMS_PER_PAGE_PARAMETER + "="+ DEFAULT_ITEMS_PER_PAGE);
  return retVal == null ? tempString : retVal + tempString;
}
