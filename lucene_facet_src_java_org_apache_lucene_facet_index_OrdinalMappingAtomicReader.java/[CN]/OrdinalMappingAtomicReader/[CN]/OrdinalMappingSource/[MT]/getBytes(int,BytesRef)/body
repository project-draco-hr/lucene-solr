{
  ref=delegate.getBytes(docID,ref);
  if (ref == null || ref.length == 0) {
    return ref;
  }
 else {
    decoder.decode(ref,ordinals);
    for (int i=0; i < ordinals.length; i++) {
      ordinals.ints[i]=ordinalMap[ordinals.ints[i]];
    }
    encoder.encode(ordinals,ref);
    return ref;
  }
}
