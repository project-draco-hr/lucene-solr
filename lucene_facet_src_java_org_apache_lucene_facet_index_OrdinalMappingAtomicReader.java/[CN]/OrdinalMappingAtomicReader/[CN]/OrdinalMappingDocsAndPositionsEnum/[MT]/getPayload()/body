{
  BytesRef payload=super.getPayload();
  if (payload == null) {
    return payload;
  }
 else {
    decoder.decode(payload,ordinals);
    for (int i=0; i < ordinals.length; i++) {
      ordinals.ints[i]=ordinalMap[ordinals.ints[i]];
    }
    encoder.encode(ordinals,payloadOut);
    return payloadOut;
  }
}
