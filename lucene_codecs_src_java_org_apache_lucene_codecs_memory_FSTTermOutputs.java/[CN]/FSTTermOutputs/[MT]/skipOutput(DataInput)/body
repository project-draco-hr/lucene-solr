{
  int bits=in.readByte() & 0xff;
  int bit0=bits & 1;
  int bit1=bits & 2;
  int bit2=bits & 4;
  int bytesSize=(bits >>> 3);
  if (bit1 > 0 && bytesSize == 0) {
    bytesSize=in.readVInt();
  }
  if (bit0 > 0) {
    for (int pos=0; pos < longsSize; pos++) {
      in.readVLong();
    }
  }
  if (bit1 > 0) {
    in.skipBytes(bytesSize);
  }
  if (bit2 > 0) {
    int code=in.readVInt();
    if (hasPos && (code & 1) == 0) {
      in.readVLong();
    }
  }
}
