{
  CharArraySet set=new CharArraySet(TEST_VERSION_CURRENT,1,true);
  set.add("Bras??lia");
  CharArraySet set1=new CharArraySet(TEST_VERSION_CURRENT,1,true);
  set1.add("Brasilia");
  BrazilianStemFilter filter=new BrazilianStemFilter(new KeywordMarkerFilter(new LowerCaseTokenizer(TEST_VERSION_CURRENT,new StringReader("Bras??lia Brasilia")),set),set1);
  assertTokenStreamContents(filter,new String[]{"bras??lia","brasilia"});
}
