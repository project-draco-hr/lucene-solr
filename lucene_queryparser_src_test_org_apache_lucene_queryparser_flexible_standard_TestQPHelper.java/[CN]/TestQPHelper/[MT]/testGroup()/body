{
  assertQueryEquals("!(a AND b) OR c",null,"-(+a +b) c");
  assertQueryEquals("!(a AND b) AND c",null,"-(+a +b) +c");
  assertQueryEquals("((a AND b) AND c)",null,"+(+a +b) +c");
  assertQueryEquals("(a AND b) AND c",null,"+(+a +b) +c");
  assertQueryEquals("b !(a AND b)",null,"b -(+a +b)");
  assertQueryEquals("(a AND b)^4 OR c",null,"(+a +b)^4.0 c");
}
