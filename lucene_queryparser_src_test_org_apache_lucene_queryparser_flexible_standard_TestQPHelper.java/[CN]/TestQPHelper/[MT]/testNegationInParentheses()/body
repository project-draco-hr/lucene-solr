{
  assertQueryEquals("(-a)",null,"-a");
  assertQueryEquals("(!a)",null,"-a");
  assertQueryEquals("(NOT a)",null,"-a");
  assertQueryEquals("a (!b)",null,"a (-b)");
  assertQueryEquals("+a +(!b)",null,"+a +(-b)");
  assertQueryEquals("a AND (!b)",null,"+a +(-b)");
  assertQueryEquals("a (NOT b)",null,"a (-b)");
  assertQueryEquals("a AND (NOT b)",null,"+a +(-b)");
}
