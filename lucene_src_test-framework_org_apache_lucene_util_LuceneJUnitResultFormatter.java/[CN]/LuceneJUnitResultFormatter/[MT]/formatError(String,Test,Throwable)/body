{
  if (test != null) {
    endTest(test);
  }
  append(formatTest(test) + type);
  append(StringUtils.LINE_SEP);
  append(error.getMessage());
  append(StringUtils.LINE_SEP);
  String strace=JUnitTestRunner.getFilteredTrace(error);
  append(strace);
  append(StringUtils.LINE_SEP);
  append(StringUtils.LINE_SEP);
}
