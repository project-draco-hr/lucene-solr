{
  FrenchAnalyzer fa=new FrenchAnalyzer();
  assertAnalyzesTo(fa,"",new String[]{});
  assertAnalyzesTo(fa,"chien chat cheval",new String[]{"chien","chat","cheval"});
  assertAnalyzesTo(fa,"chien CHAT CHEVAL",new String[]{"chien","chat","cheval"});
  assertAnalyzesTo(fa,"  chien  ,? + = -  CHAT /: > CHEVAL",new String[]{"chien","chat","cheval"});
  assertAnalyzesTo(fa,"chien++",new String[]{"chien"});
  assertAnalyzesTo(fa,"mot \"entreguillemet\"",new String[]{"mot","entreguilemet"});
  assertAnalyzesTo(fa,"Jean-Fran??ois",new String[]{"jean","francoi"});
  assertAnalyzesTo(fa,"le la chien les aux chat du des ?? cheval",new String[]{"chien","chat","cheval"});
  assertAnalyzesTo(fa,"lances chismes habitable chiste ??l??ments captifs",new String[]{"lanc","chism","habitabl","chist","element","captif"});
  assertAnalyzesTo(fa,"finissions souffrirent rugissante",new String[]{"finision","soufrirent","rugisant"});
  assertAnalyzesTo(fa,"C3PO aujourd'hui oeuf ???????????? anticonstitutionnellement Java++ ",new String[]{"c3po","aujourd'hui","oeuf","??a??ua??","anticonstitutionel","java"});
  assertAnalyzesTo(fa,"33Bis 1940-1945 1940:1945 (---i+++)*",new String[]{"33bi","1940","1945","1940","1945","i"});
}
