{
  FrenchAnalyzer fa=new FrenchAnalyzer(Version.LUCENE_30);
  assertAnalyzesTo(fa,"",new String[]{});
  assertAnalyzesTo(fa,"chien chat cheval",new String[]{"chien","chat","cheval"});
  assertAnalyzesTo(fa,"chien CHAT CHEVAL",new String[]{"chien","chat","cheval"});
  assertAnalyzesTo(fa,"  chien  ,? + = -  CHAT /: > CHEVAL",new String[]{"chien","chat","cheval"});
  assertAnalyzesTo(fa,"chien++",new String[]{"chien"});
  assertAnalyzesTo(fa,"mot \"entreguillemet\"",new String[]{"mot","entreguillemet"});
  assertAnalyzesTo(fa,"Jean-Fran??ois",new String[]{"jean","fran??ois"});
  assertAnalyzesTo(fa,"le la chien les aux chat du des ?? cheval",new String[]{"chien","chat","cheval"});
  assertAnalyzesTo(fa,"lances chismes habitable chiste ??l??ments captifs",new String[]{"lanc","chism","habit","chist","??l??ment","captif"});
  assertAnalyzesTo(fa,"finissions souffrirent rugissante",new String[]{"fin","souffr","rug"});
  assertAnalyzesTo(fa,"C3PO aujourd'hui oeuf ???????????? anticonstitutionnellement Java++ ",new String[]{"c3po","aujourd'hui","oeuf","????????????","anticonstitutionnel","jav"});
  assertAnalyzesTo(fa,"33Bis 1940-1945 1940:1945 (---i+++)*",new String[]{"33bis","1940-1945","1940","1945","i"});
}
