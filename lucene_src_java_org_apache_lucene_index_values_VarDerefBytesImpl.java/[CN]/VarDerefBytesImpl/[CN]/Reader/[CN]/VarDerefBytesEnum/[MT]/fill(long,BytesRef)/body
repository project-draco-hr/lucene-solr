{
  datIn.seek(fp + --address);
  final byte sizeByte=datIn.readByte();
  final int size;
  if ((sizeByte & 128) == 0) {
    size=sizeByte;
  }
 else {
    size=((sizeByte & 0x7f) << 8) | ((datIn.readByte() & 0xff));
  }
  if (ref.bytes.length < size)   ref.grow(size);
  ref.length=size;
  ref.offset=0;
  datIn.readBytes(ref.bytes,0,size);
}
