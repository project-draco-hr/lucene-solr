{
  if (failureMarker.wasSuccessful()) {
    if (file.exists()) {
      if (file.listFiles().length > 0) {
      }
      try {
        TestUtil.rmDir(file);
      }
 catch (      IOException e) {
      }
      if (file.exists()) {
        throw new IOException("Could not remove temporary folder: " + file.getAbsolutePath() + ". Created at stack trace:\n"+ creationStack);
      }
    }
  }
}
