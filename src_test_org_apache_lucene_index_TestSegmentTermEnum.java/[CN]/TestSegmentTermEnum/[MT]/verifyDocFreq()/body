{
  IndexReader reader=IndexReader.open(dir);
  TermEnum enum=null;
  enum=reader.terms();
  enum.next();
  assertEquals("aaa",enum.term().text());
  assertEquals(200,enum.docFreq());
  enum.next();
  assertEquals("bbb",enum.term().text());
  assertEquals(100,enum.docFreq());
  enum.close();
  enum=reader.terms(new Term("content","aaa"));
  assertEquals("aaa",enum.term().text());
  assertEquals(200,enum.docFreq());
  enum.next();
  assertEquals("bbb",enum.term().text());
  assertEquals(100,enum.docFreq());
  enum.close();
}
