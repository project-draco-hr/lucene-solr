{
  if (field == term.field()) {
    String target=term.text();
    int targetlen=target.length();
    int dist=editDistance(text,target,textlen,targetlen);
    distance=1 - ((double)dist / (double)Math.min(textlen,targetlen));
    return (distance > FUZZY_THRESHOLD);
  }
  endEnum=true;
  return false;
}
