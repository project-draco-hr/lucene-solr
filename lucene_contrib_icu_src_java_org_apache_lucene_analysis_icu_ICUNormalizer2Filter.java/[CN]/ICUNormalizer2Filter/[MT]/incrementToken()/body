{
  if (input.incrementToken()) {
    if (normalizer.quickCheck(termAtt) != Normalizer.YES) {
      buffer.setLength(0);
      buffer.append(termAtt.buffer(),0,termAtt.length());
      termAtt.setEmpty();
      normalizer.normalize(buffer,termAtt);
    }
    return true;
  }
 else {
    return false;
  }
}
