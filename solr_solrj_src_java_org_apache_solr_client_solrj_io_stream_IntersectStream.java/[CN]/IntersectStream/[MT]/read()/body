{
  while (true) {
    Tuple a=streamA.read();
    Tuple b=streamB.read();
    if (a.EOF) {
      return a;
    }
    if (b.EOF) {
      return b;
    }
    if (eq.test(a,b)) {
      streamB.pushBack(b);
      return a;
    }
    int aComp=streamA.getStreamSort().compare(a,b);
    if (aComp < 0) {
      streamB.pushBack(b);
    }
 else {
      streamA.pushBack(a);
    }
  }
}
