{
  fieldType.freeze();
  this.fieldType=fieldType;
  if (fieldType.numericType() != FieldType.NumericType.DOUBLE)   throw new IllegalArgumentException("BBoxStrategy only supports doubles at this time.");
  xdlFieldType=new FieldType(StringField.TYPE_NOT_STORED);
  xdlFieldType.setStored(fieldType.stored());
  xdlFieldType.setIndexed(fieldType.indexed());
  xdlFieldType.freeze();
}
