{
  fieldType.freeze();
  this.fieldType=fieldType;
  if (fieldType.numericType() != FieldType.LegacyNumericType.DOUBLE)   throw new IllegalArgumentException("BBoxStrategy only supports doubles at this time.");
  xdlFieldType=new FieldType(StringField.TYPE_NOT_STORED);
  xdlFieldType.setStored(fieldType.stored());
  xdlFieldType.setIndexOptions(fieldType.indexOptions());
  xdlFieldType.freeze();
}
