{
  for (  Clause clause : clauses) {
    if (clause.must == '+')     return true;
    if (clause.must == '-')     return true;
    if (clause.isBareWord()) {
      String s=clause.val;
      if ("OR".equals(s)) {
        return true;
      }
 else       if ("NOT".equals(s)) {
        return true;
      }
 else       if (lowercaseOperators && "or".equals(s)) {
        return true;
      }
    }
  }
  return false;
}
