{
  for (  Clause clause : clauses) {
    if (clause.must == '+')     return false;
    if (clause.must == '-')     return false;
    if (clause.isBareWord()) {
      String s=clause.val;
      if ("OR".equals(s)) {
        return false;
      }
 else       if ("NOT".equals(s)) {
        return false;
      }
 else       if (lowercaseOperators && "or".equals(s)) {
        return false;
      }
    }
  }
  return true;
}
