{
  assertStemsTo("drink","drink");
  assertStemsTo("drinks","drink");
  assertStemsTo("walk");
  assertStemsTo("walks","walk");
  assertStemsTo("prewalk","walk");
  assertStemsTo("prewalks","walk");
  assertStemsTo("test");
  assertStemsTo("pretest");
  assertStemsTo("tests");
  assertStemsTo("pretests");
}
