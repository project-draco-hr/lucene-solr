{
  assertQEx("no error asking for bogus selector","hoss",req("q","*:*","fl","field(val_tds_dv,'hoss')"),SolrException.ErrorCode.BAD_REQUEST);
  assertQEx("no error asking for max on a non docVals field","val_tds",req("q","*:*","fl","field(val_tds,'max')"),SolrException.ErrorCode.BAD_REQUEST);
  assertQEx("no error asking for max on a non docVals field","max",req("q","*:*","fl","field(val_tds,'max')"),SolrException.ErrorCode.BAD_REQUEST);
  assertQEx("no error asking for max on a non docVals field","docValues",req("q","*:*","fl","field(val_tds,'max')"),SolrException.ErrorCode.BAD_REQUEST);
  assertQEx("no error asking for max on a str field","cat_docValues",req("q","*:*","fl","field(cat_docValues,'max')"),SolrException.ErrorCode.BAD_REQUEST);
  assertQEx("no error asking for max on a str field","string",req("q","*:*","fl","field(cat_docValues,'max')"),SolrException.ErrorCode.BAD_REQUEST);
}
