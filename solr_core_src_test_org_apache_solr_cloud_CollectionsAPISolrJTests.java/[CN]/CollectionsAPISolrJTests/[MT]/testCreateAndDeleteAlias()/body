{
  CollectionAdminRequest.CreateAlias createAliasRequest=new CollectionAdminRequest.CreateAlias().setAliasName("solrj_alias").setAliasedCollections(DEFAULT_COLLECTION);
  CollectionAdminResponse response=createAliasRequest.process(cloudClient);
  assertEquals(0,response.getStatus());
  CollectionAdminRequest.DeleteAlias deleteAliasRequest=new CollectionAdminRequest.DeleteAlias().setAliasName("solrj_alias");
  deleteAliasRequest.process(cloudClient);
  assertEquals(0,response.getStatus());
}
