{
  char[] termBuffer=token.termBuffer();
  int termLength=token.termLength();
  int gramSize=minGram;
  while (gramSize <= maxGram) {
    int pos=0;
    while (pos + gramSize <= termLength) {
      ngrams.add(token.clone(termBuffer,pos,gramSize,pos,pos + gramSize));
      pos++;
    }
    gramSize++;
  }
}
