{
  QueryParsing.StrParser sp=new QueryParsing.StrParser(val);
  int start=0;
  sp.eatws();
  if (sp.pos >= sp.end)   addStat(val,val);
  String key=null;
  String funcStr=val;
  if (key == null) {
    key=SolrReturnFields.getFieldName(sp);
    if (key != null && sp.opt(':')) {
      funcStr=val.substring(sp.pos);
    }
 else {
      sp.pos=start;
      key=null;
    }
  }
  if (key == null) {
    key=funcStr;
  }
  addStat(key,funcStr);
}
