{
  for (; ; ) {
    if (doc == NO_MORE_DOCS) {
      return NO_MORE_DOCS;
    }
    advanceHead:     for (; ; ) {
      for (      DocIdSetIterator other : others) {
        if (other.docID() < doc) {
          final int next=other.advance(doc);
          if (next > doc) {
            doc=lead.advance(next);
            break advanceHead;
          }
        }
      }
      if (matches()) {
        return doc;
      }
 else {
        doc=lead.nextDoc();
        break advanceHead;
      }
    }
  }
}
