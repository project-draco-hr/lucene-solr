{
  OutputStream os=fsdir.createFile(file);
  for (int i=0; i < 2000; i++) {
    os.writeByte((byte)i);
  }
  os.close();
  InputStream in=fsdir.openFile(file);
  byte b=in.readByte();
  in.close();
  b=in.readByte();
  in.seek(1099);
  try {
    b=in.readByte();
  }
 catch (  IOException e) {
  }
}
