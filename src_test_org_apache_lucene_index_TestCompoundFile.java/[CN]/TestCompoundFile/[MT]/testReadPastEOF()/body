{
  setUp_2();
  CompoundFileReader cr=new CompoundFileReader(dir,"f.comp");
  InputStream is=cr.openFile("f2");
  is.seek(is.length() - 10);
  byte b[]=new byte[100];
  is.readBytes(b,0,10);
  try {
    byte test=is.readByte();
    fail("Single byte read past end of file");
  }
 catch (  IOException e) {
  }
  is.seek(is.length() - 10);
  try {
    is.readBytes(b,0,50);
    fail("Block read past end of file");
  }
 catch (  IOException e) {
  }
  is.close();
  cr.close();
}
