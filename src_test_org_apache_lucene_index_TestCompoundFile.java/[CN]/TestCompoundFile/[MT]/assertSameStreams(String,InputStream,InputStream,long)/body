{
  if (seekTo < 0) {
    try {
      actual.seek(seekTo);
      fail(msg + ", " + seekTo+ ", negative seek");
    }
 catch (    IOException e) {
    }
  }
 else   if (seekTo > 0 && seekTo >= expected.length()) {
    try {
      actual.seek(seekTo);
      fail(msg + ", " + seekTo+ ", seek past EOF");
    }
 catch (    IOException e) {
    }
  }
 else {
    expected.seek(seekTo);
    actual.seek(seekTo);
    assertSameStreams(msg + ", seek(mid)",expected,actual);
  }
}
