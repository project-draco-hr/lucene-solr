{
  hasAborted=aborting=true;
  try {
    if (infoStream != null) {
      message("now abort");
    }
    try {
      consumer.abort();
    }
 catch (    Throwable t) {
    }
    pendingDeletes.clear();
    deleteSlice=deleteQueue.newSlice();
    doAfterFlush();
  }
  finally {
    aborting=false;
    if (infoStream != null) {
      message("done abort");
    }
  }
}
