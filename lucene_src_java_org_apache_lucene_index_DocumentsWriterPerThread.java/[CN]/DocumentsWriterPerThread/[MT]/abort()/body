{
  hasAborted=aborting=true;
  try {
    infoStream.message("DWPT","now abort");
    try {
      consumer.abort();
    }
 catch (    Throwable t) {
    }
    pendingDeletes.clear();
    deleteSlice=deleteQueue.newSlice();
    doAfterFlush();
  }
  finally {
    aborting=false;
    infoStream.message("DWPT","done abort");
  }
}
