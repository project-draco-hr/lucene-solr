{
  aborting=true;
  try {
    if (infoStream != null) {
      message("docWriter: now abort");
    }
    try {
      consumer.abort();
    }
 catch (    Throwable t) {
    }
    pendingDeletes.clear();
    doAfterFlush();
  }
  finally {
    aborting=false;
    if (infoStream != null) {
      message("docWriter: done abort");
    }
  }
}
