{
  if (target >= maxDoc)   return pos=NO_MORE_DOCS;
  dataIn.seek(fp + (target * precision));
  int intBits;
  while ((intBits=dataIn.readInt()) == INT_DEFAULT) {
    if (++target >= maxDoc)     return pos=NO_MORE_DOCS;
  }
  floatsRef.floats[0]=Float.intBitsToFloat(intBits);
  floatsRef.offset=0;
  return pos=target;
}
