{
  if (target >= maxDoc) {
    return pos=NO_MORE_DOCS;
  }
  dataIn.seek(fp + (target * precision));
  long value;
  while ((value=dataIn.readLong()) == LONG_DEFAULT) {
    if (++target >= maxDoc)     return pos=NO_MORE_DOCS;
  }
  ref.floats[0]=Double.longBitsToDouble(value);
  ref.offset=0;
  return pos=target;
}
