{
  streamB.open();
  while (true) {
    Tuple t=streamB.read();
    if (t.EOF) {
      break;
    }
    HashKey hashKey=new HashKey(t,keys);
    if (hashMap.containsKey(hashKey)) {
      List<Tuple> tuples=hashMap.get(hashKey);
      tuples.add(t);
    }
 else {
      List<Tuple> tuples=new ArrayList();
      tuples.add(t);
      hashMap.put(hashKey,tuples);
    }
  }
  streamB.close();
  streamA.open();
}
