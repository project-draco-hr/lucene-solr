{
  TermVectorsReader reader=null;
  try {
    reader=new TermVectorsReader(dir,seg,fieldInfos);
    reader.get(50,testFields[0]);
    fail();
  }
 catch (  IOException e) {
  }
 finally {
    reader.close();
  }
  try {
    reader=new TermVectorsReader(dir,seg,fieldInfos);
    reader.get(50);
    fail();
  }
 catch (  IOException e) {
  }
 finally {
    reader.close();
  }
  try {
    reader=new TermVectorsReader(dir,seg,fieldInfos);
    TermFreqVector vector=reader.get(0,"f50");
    assertTrue(vector == null);
    reader.close();
  }
 catch (  IOException e) {
    fail();
  }
 finally {
    reader.close();
  }
}
