{
  String s=null;
  if (i > 0 && random.nextInt(3) == 1) {
    for (int iter=0; iter < 10 && s == null; iter++) {
      s=unicodeStrings[random.nextInt(i)];
    }
    if (s == null) {
      s=_TestUtil.randomUnicodeString(random,250);
    }
  }
 else {
    s=_TestUtil.randomUnicodeString(random,250);
  }
  return s;
}
