{
  if (indexHasFreq) {
    for (int i=0; i < num; i++) {
      final int code=docIn.readVInt();
      docBuffer[i]=code >>> 1;
      if ((code & 1) != 0) {
        freqBuffer[i]=1;
      }
 else {
        freqBuffer[i]=docIn.readVInt();
      }
    }
  }
 else {
    for (int i=0; i < num; i++) {
      docBuffer[i]=docIn.readVInt();
    }
  }
}
