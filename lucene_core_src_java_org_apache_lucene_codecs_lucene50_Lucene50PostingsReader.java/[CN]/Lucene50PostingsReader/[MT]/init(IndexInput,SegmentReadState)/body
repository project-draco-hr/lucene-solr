{
  CodecUtil.checkSegmentHeader(termsIn,Lucene50PostingsWriter.TERMS_CODEC,Lucene50PostingsWriter.VERSION_START,Lucene50PostingsWriter.VERSION_CURRENT,state.segmentInfo.getId(),state.segmentSuffix);
  final int indexBlockSize=termsIn.readVInt();
  if (indexBlockSize != BLOCK_SIZE) {
    throw new IllegalStateException("index-time BLOCK_SIZE (" + indexBlockSize + ") != read-time BLOCK_SIZE ("+ BLOCK_SIZE+ ")");
  }
}
