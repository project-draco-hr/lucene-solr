{
  if (docUpto == docFreq) {
    return doc=NO_MORE_DOCS;
  }
  if (docBufferUpto == BLOCK_SIZE) {
    refillDocs();
  }
  accum+=docDeltaBuffer[docBufferUpto];
  freq=freqBuffer[docBufferUpto];
  posPendingCount+=freq;
  docBufferUpto++;
  docUpto++;
  doc=accum;
  position=0;
  lastStartOffset=0;
  return doc;
}
