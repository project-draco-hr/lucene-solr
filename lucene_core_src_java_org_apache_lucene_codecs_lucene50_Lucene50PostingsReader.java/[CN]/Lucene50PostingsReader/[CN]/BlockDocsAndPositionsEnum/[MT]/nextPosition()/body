{
  if (posPendingFP != -1) {
    posIn.seek(posPendingFP);
    posPendingFP=-1;
    posBufferUpto=BLOCK_SIZE;
  }
  if (posPendingCount > freq) {
    skipPositions();
    posPendingCount=freq;
  }
  if (posBufferUpto == BLOCK_SIZE) {
    refillPositions();
    posBufferUpto=0;
  }
  position+=posDeltaBuffer[posBufferUpto++];
  posPendingCount--;
  return position;
}
