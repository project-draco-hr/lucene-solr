{
  Result r=JUnitCore.runClasses(Nested1.class);
  RandomizedTest.assumeTrue("Ignoring nested test, very likely zombie threads present.",r.getIgnoreCount() == 0);
  for (  Failure f : r.getFailures()) {
    System.out.println(f.getTrace());
  }
  Assert.assertEquals("Expected exactly one failure.",1,r.getFailureCount());
  Assert.assertTrue("Expected unreferenced files assertion.",r.getFailures().get(0).getTrace().contains("unreferenced files:"));
}
