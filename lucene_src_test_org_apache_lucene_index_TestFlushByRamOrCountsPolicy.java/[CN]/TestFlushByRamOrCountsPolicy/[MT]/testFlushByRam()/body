{
  int[] numThreads=new int[]{numCPUs + random.nextInt(numCPUs + 1),1};
  for (int i=0; i < numThreads.length; i++) {
    runFlushByRam(numThreads[i],1 + random.nextInt(10) + random.nextDouble(),false);
  }
  for (int i=0; i < numThreads.length; i++) {
    runFlushByRam(numThreads[i],250.d,true);
  }
}
