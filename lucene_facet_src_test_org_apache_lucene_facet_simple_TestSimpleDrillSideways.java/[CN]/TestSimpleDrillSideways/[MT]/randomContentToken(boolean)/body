{
  double d=random().nextDouble();
  if (isQuery) {
    if (d < 0.33) {
      return "a";
    }
 else     if (d < 0.66) {
      return "b";
    }
 else {
      return "c";
    }
  }
 else {
    if (d <= aChance) {
      return "a";
    }
 else     if (d < aChance + bChance) {
      return "b";
    }
 else {
      return "c";
    }
  }
}
