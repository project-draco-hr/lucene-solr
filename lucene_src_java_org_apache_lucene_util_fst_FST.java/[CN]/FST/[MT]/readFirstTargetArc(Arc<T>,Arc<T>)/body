{
  if (follow.isFinal()) {
    arc.label=END_LABEL;
    arc.output=follow.nextFinalOutput;
    if (follow.target <= 0) {
      arc.flags=BIT_LAST_ARC | BIT_FINAL_ARC;
    }
 else {
      arc.flags=BIT_FINAL_ARC;
      arc.nextArc=follow.target;
    }
    return arc;
  }
 else {
    return readFirstRealArc(follow.target,arc,getBytesReader(0));
  }
}
