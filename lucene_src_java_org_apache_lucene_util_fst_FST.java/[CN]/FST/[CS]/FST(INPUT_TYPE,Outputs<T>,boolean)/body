{
  this.inputType=inputType;
  this.outputs=outputs;
  bytes=new byte[128];
  NO_OUTPUT=outputs.getNoOutput();
  if (willPackFST) {
    nodeAddress=new int[8];
    inCounts=new int[8];
  }
 else {
    nodeAddress=null;
    inCounts=null;
  }
  writer=new BytesWriter();
  emptyOutput=null;
  packed=false;
  nodeRefToAddress=null;
}
