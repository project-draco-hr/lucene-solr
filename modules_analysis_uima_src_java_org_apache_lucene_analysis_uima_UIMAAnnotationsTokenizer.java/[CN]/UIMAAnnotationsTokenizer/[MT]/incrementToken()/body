{
  if (iterator == null) {
    try {
      analyzeText(descriptorPath);
    }
 catch (    Exception e) {
      throw new IOException(e);
    }
  }
  if (iterator.hasNext()) {
    clearAttributes();
    AnnotationFS next=iterator.next();
    termAttr.append(next.getCoveredText());
    offsetAttr.setOffset(correctOffset(next.getBegin()),correctOffset(next.getEnd()));
    return true;
  }
 else {
    return false;
  }
}
