{
  SolrQueryRequest req=req("myField","field_b","myTrue","true");
  try {
    assertFuncEquals(req,"not(true)","not($myTrue)");
    assertFuncEquals(req,"not(not(true))","not(not($myTrue))");
    assertFuncEquals(req,"not(field_b)","not($myField)","not(field('field_b'))","not(field($myField))");
    assertFuncEquals(req,"not(exists(field_b))","not(exists($myField))","not(exists(field('field_b')))","not(exists(field($myField)))");
  }
  finally {
    req.close();
  }
}
