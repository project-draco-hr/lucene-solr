{
  SolrQueryRequest req=req("myVar","5","df","text","ff","foo_s","tt","bar_s","scoreavg","avg");
  try {
    assertQueryEquals("join",req,"{!join from=foo_s to=bar_s score=avg}asdf","{!join from=$ff to=$tt score=Avg}asdf","{!join from=$ff to='bar_s' score=$scoreavg}text:asdf");
  }
  finally {
    req.close();
  }
}
