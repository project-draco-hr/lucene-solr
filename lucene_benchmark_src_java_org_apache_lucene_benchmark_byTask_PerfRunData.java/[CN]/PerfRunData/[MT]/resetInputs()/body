{
  contentSource.resetInputs();
  docMaker.resetInputs();
  facetSource.resetInputs();
  for (  final QueryMaker queryMaker : readTaskQueryMaker.values()) {
    try {
      queryMaker.resetInputs();
    }
 catch (    IOException e) {
      throw e;
    }
catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
}
