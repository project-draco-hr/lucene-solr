{
  int size=random.nextInt(maxLength);
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < size; i++) {
switch (random.nextInt(4)) {
case 0:
      sb.append('a');
    break;
case 1:
  sb.append('\u0645');
break;
case 2:
sb.append('\u092a');
break;
case 3:
sb.appendCodePoint(0x29B05);
}
}
return sb.toString();
}
