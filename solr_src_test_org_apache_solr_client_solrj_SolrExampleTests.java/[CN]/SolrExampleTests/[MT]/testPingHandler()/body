{
  SolrServer server=getSolrServer();
  server.deleteByQuery("*:*");
  server.commit();
  assertNumFound("*:*",0);
  server.ping();
  try {
    SolrPing ping=new SolrPing();
    ping.getParams().set("qt","unknown handler!");
    ping.process(server);
    fail("sent unknown query type!");
  }
 catch (  Exception ex) {
  }
}
