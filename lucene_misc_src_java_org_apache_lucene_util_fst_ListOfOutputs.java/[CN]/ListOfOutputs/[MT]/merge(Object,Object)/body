{
  List<T> outputList=new ArrayList<T>();
  if (!(first instanceof List)) {
    outputList.add((T)first);
  }
 else {
    outputList.addAll((List<T>)first);
  }
  if (!(second instanceof List)) {
    outputList.add((T)second);
  }
 else {
    outputList.addAll((List<T>)second);
  }
  return outputList;
}
