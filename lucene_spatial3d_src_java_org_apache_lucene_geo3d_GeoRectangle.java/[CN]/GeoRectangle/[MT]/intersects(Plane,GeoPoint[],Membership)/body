{
  return p.intersects(planetModel,topPlane,notablePoints,topPlanePoints,bounds,bottomPlane,leftPlane,rightPlane) || p.intersects(planetModel,bottomPlane,notablePoints,bottomPlanePoints,bounds,topPlane,leftPlane,rightPlane) || p.intersects(planetModel,leftPlane,notablePoints,leftPlanePoints,bounds,rightPlane,topPlane,bottomPlane)|| p.intersects(planetModel,rightPlane,notablePoints,rightPlanePoints,bounds,leftPlane,topPlane,bottomPlane);
}
