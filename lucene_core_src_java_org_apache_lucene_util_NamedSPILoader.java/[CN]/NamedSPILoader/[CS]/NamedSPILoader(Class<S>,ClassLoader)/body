{
  this.clazz=clazz;
  final ClassLoader clazzClassloader=clazz.getClassLoader();
  if (classloader == null) {
    classloader=clazzClassloader;
  }
  if (clazzClassloader != null && !SPIClassIterator.isParentClassLoader(clazzClassloader,classloader)) {
    reload(clazzClassloader);
  }
  reload(classloader);
}
