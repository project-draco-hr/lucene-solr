{
  if (index < 0) {
    return -1;
  }
  int i=index >> 6;
  if (i >= wlen) {
    i=wlen - 1;
  }
  final int subIndex=index & 0x3f;
  long word=(bits[i] << (63 - subIndex));
  if (word != 0) {
    return (i << 6) + subIndex - Long.numberOfLeadingZeros(word);
  }
  while (--i >= 0) {
    word=bits[i];
    if (word != 0) {
      return (i << 6) + 63 - Long.numberOfLeadingZeros(word);
    }
  }
  return -1;
}
