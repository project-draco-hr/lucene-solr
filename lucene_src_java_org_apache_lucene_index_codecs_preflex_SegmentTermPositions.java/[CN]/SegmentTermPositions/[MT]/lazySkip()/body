{
  if (proxStream == null) {
    proxStream=(IndexInput)proxStreamOrig.clone();
  }
  skipPayload();
  if (lazySkipPointer != -1) {
    proxStream.seek(lazySkipPointer);
    lazySkipPointer=-1;
  }
  if (lazySkipProxCount != 0) {
    skipPositions(lazySkipProxCount);
    lazySkipProxCount=0;
  }
}
