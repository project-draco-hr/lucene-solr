{
  IProjector projector=(IProjector)loader.newInstance(projectorName);
  if (projector != null) {
    plotters=new ArrayList<CartesianTierPlotter>(tileDiff);
    for (int i=start; i <= end; i++) {
      plotters.add(new CartesianTierPlotter(i,projector,""));
    }
  }
 else {
    throw new SolrException(SolrException.ErrorCode.SERVER_ERROR,"Could not instantiate a Projector Instance for: " + projectorName + ". Make sure the "+ PROJECTOR_CLASS+ " attribute is set properly in the schema");
  }
}
