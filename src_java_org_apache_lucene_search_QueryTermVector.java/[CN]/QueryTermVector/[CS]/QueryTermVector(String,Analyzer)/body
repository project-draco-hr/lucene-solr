{
  if (analyzer != null) {
    TokenStream stream=analyzer.tokenStream("",new StringReader(queryString));
    if (stream != null) {
      Token next=null;
      List terms=new ArrayList();
      try {
        while ((next=stream.next()) != null) {
          terms.add(next.termText());
        }
        processTerms((String[])terms.toArray(new String[terms.size()]));
      }
 catch (      IOException e) {
      }
    }
  }
}
