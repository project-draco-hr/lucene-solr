{
  SolrLocator locator=new SolrLocator(createMorphlineContext());
  locator.setSolrHomeDir(testSolrHome + "/collection1");
  Config config=new Compiler().parse(new File(tempDir + "/" + file+ ".conf"),locator.toConfig("SOLR_LOCATOR"));
