{
  if (parentEncapsulation) {
    for (int i=0; i < documents.size(); i++) {
      Document d=(Document)documents.get(i);
      for (Enumeration e=d.fields(); e.hasMoreElements(); ) {
        Field f=(Field)e.nextElement();
        String fieldName=f.name();
        if (!fieldName.equals(DataSource.CONTAINER_IDENTIFIER) && !fieldName.equals(DataSource.OBJECT_CLASS) && !fieldName.equals(DataSource.OBJECT_IDENTIFIER)) {
          doc.add(f);
        }
      }
    }
  }
  writer.addDocument(doc);
  for (int i=0; i < documents.size(); i++) {
    writer.addDocument((Document)documents.get(i));
  }
}
