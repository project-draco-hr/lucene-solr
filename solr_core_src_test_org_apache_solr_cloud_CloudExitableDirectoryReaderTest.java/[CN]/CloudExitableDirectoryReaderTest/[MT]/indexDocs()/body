{
  int counter=1;
  for (; (counter % NUM_DOCS_PER_TYPE) != 0; counter++)   indexDoc(sdoc("id",Integer.toString(counter),"name","a" + counter));
  counter++;
  for (; (counter % NUM_DOCS_PER_TYPE) != 0; counter++)   indexDoc(sdoc("id",Integer.toString(counter),"name","b" + counter));
  counter++;
  for (; counter % NUM_DOCS_PER_TYPE != 0; counter++)   indexDoc(sdoc("id",Integer.toString(counter),"name","dummy term doc" + counter));
  commit();
}
