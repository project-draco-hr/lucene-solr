{
  final SpatialRelation bboxSect=enclosingBox.relate(r,ctx);
  if (bboxSect == SpatialRelation.DISJOINT || bboxSect == SpatialRelation.WITHIN)   return bboxSect;
 else   if (bboxSect == SpatialRelation.CONTAINS && enclosingBox.equals(r))   return SpatialRelation.WITHIN;
  return relateRectanglePhase2(r,bboxSect,ctx);
}
