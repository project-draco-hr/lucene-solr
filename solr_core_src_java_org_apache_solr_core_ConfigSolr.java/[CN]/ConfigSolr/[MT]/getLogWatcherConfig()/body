{
  String loggingClass=getString(CfgProp.SOLR_LOGGING_CLASS,null);
  String loggingWatcherThreshold=getString(CfgProp.SOLR_LOGGING_WATCHER_THRESHOLD,null);
  return new LogWatcherConfig(getBoolean(CfgProp.SOLR_LOGGING_ENABLED,true),loggingClass,loggingWatcherThreshold,getInt(CfgProp.SOLR_LOGGING_WATCHER_SIZE,50));
}
