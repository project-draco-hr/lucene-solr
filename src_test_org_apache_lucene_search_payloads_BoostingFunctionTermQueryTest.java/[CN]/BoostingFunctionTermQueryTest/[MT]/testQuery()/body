{
  BoostingFunctionTermQuery boostingFuncTermQuery=new BoostingFunctionTermQuery(new Term(PayloadHelper.MULTI_FIELD,"seventy"),new MaxPayloadFunction());
  QueryUtils.check(boostingFuncTermQuery);
  SpanTermQuery spanTermQuery=new SpanTermQuery(new Term(PayloadHelper.MULTI_FIELD,"seventy"));
  assertTrue(boostingFuncTermQuery.equals(spanTermQuery) == spanTermQuery.equals(boostingFuncTermQuery));
  BoostingFunctionTermQuery boostingFuncTermQuery2=new BoostingFunctionTermQuery(new Term(PayloadHelper.MULTI_FIELD,"seventy"),new AveragePayloadFunction());
  QueryUtils.checkUnequal(boostingFuncTermQuery,boostingFuncTermQuery2);
}
