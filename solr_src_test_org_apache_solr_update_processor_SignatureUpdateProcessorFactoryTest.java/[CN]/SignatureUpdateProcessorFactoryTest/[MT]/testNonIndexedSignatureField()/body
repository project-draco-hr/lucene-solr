{
  SolrCore core=h.getCore();
  assertEquals("docs found when none are expected at start",0l,core.getSearcher().get().getReader().numDocs());
  processor="stored_sig";
  addDoc(adoc("id","2a","v_t","Hello Dude man!","name","ali babi'"));
  addDoc(adoc("id","2b","v_t","Hello Dude man!","name","ali babi'"));
  addDoc(commit());
  assertEquals("did not find exepcted docs",2l,core.getSearcher().get().getReader().numDocs());
}
