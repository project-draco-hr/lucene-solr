{
  int ret=0;
  if (offset <= ordinal) {
    if (0 != facetRequest.getValueOf(arrays,ordinal % partitionSize)) {
      ret++;
    }
  }
  if (currentDepth >= maxDepth) {
    return ret;
  }
  int yc=youngestChild[ordinal];
  while (yc >= endOffset) {
    yc=olderSibling[yc];
  }
  while (yc > TaxonomyReader.INVALID_ORDINAL) {
    ret+=countOnly(yc,youngestChild,olderSibling,arrays,partitionSize,offset,endOffset,currentDepth + 1,maxDepth);
    yc=olderSibling[yc];
  }
  return ret;
}
