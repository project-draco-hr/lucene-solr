{
  for (  String key : propertyNames) {
    try {
      String value=restore.get(key);
      if (value == null) {
        System.clearProperty(key);
      }
 else {
        System.setProperty(key,value);
      }
    }
 catch (    SecurityException e) {
      errors.add(e);
    }
  }
  restore.clear();
  super.afterAlways(errors);
}
