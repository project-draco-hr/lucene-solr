{
  if (tenum == null)   return null;
  for (; ; ) {
    if (doSeek) {
      doSeek=false;
      final BytesRef t=nextSeekTerm(actualTerm);
      if (t == null || tenum.seek(t,useTermsCache) == SeekStatus.END) {
        return null;
      }
      actualTerm=tenum.term();
    }
 else {
      actualTerm=tenum.next();
      if (actualTerm == null) {
        return null;
      }
    }
switch (accept(actualTerm)) {
case YES_AND_SEEK:
      doSeek=true;
case YES:
    return actualTerm;
case NO_AND_SEEK:
  doSeek=true;
break;
case END:
return null;
}
}
}
