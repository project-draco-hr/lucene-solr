{
  FacetMerger merger=null;
  if (subs != null) {
    merger=subs.get(key);
    if (merger != null)     return merger;
  }
  merger=parent.createFacetMerger(key,prototype);
  if (merger != null) {
    if (subs == null) {
      subs=new HashMap<>();
    }
    subs.put(key,merger);
  }
  return merger;
}
