{
  final String ONE_DIGIT="%1d";
  final String TWO_DIGIT="%02d";
  final Random r=random();
  final int iters=atLeast(r,50);
  for (int i=0; i <= iters; i++) {
    int hour=_TestUtil.nextInt(r,0,23);
    int min=_TestUtil.nextInt(r,0,59);
    String hours=String.format(Locale.US,(r.nextBoolean() ? ONE_DIGIT : TWO_DIGIT),hour);
    String mins=String.format(Locale.US,TWO_DIGIT,min);
    String input="GMT" + (r.nextBoolean() ? "+" : "-") + hours+ (r.nextBoolean() ? "" : ((r.nextBoolean() ? ":" : "") + mins));
    assertSameRules(input,TimeZone.getTimeZone(input),TimeZoneUtils.getTimeZone(input));
  }
}
