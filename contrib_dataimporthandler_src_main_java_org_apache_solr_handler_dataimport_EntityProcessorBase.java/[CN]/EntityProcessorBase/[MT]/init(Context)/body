{
  rowIterator=null;
  rowcache=null;
  this.context=context;
  if (isFirstInit) {
    entityName=context.getEntityAttribute("name");
    String s=context.getEntityAttribute(ON_ERROR);
    if (s != null)     onError=s;
  }
  resolver=(VariableResolverImpl)context.getVariableResolver();
  query=null;
  session=null;
  isFirstInit=false;
}
