{
  try {
    if (rowIterator == null)     return null;
    if (rowIterator.hasNext())     return rowIterator.next();
    rowIterator=null;
    query=null;
    return null;
  }
 catch (  Exception e) {
    LOG.log(Level.SEVERE,"getNext() failed for query '" + query + "'",e);
    rowIterator=null;
    query=null;
    throw new DataImportHandlerException(DataImportHandlerException.WARN,e);
  }
}
