{
  try {
    if (rowIterator == null)     return null;
    if (rowIterator.hasNext())     return rowIterator.next();
    query=null;
    rowIterator=null;
    return null;
  }
 catch (  Exception e) {
    log.error("getNext() failed for query '" + query + "'",e);
    query=null;
    rowIterator=null;
    wrapAndThrow(DataImportHandlerException.WARN,e);
    return null;
  }
}
