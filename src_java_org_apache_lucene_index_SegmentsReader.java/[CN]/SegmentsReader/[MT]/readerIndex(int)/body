{
  int lo=0;
  int hi=readers.length - 1;
  while (hi >= lo) {
    int mid=(lo + hi) >> 1;
    int midValue=starts[mid];
    if (n < midValue)     hi=mid - 1;
 else     if (n > midValue)     lo=mid + 1;
 else {
      while (mid + 1 < readers.length && starts[mid + 1] == midValue) {
        mid++;
      }
      return mid;
    }
  }
  return hi;
}
