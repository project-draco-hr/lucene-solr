{
  bs=new OpenBitSet(maxSize);
  ids=new int[bitsToSet];
  int count=0;
  if (maxSize > 0) {
    for (int i=0; i < bitsToSet; i++) {
      int id=rand.nextInt(maxSize);
      if (!bs.get(id)) {
        bs.fastSet(id);
        ids[count++]=id;
      }
    }
  }
  bds=new BitDocSet(bs,bitsToSet);
  hds=new HashDocSet(ids,0,count);
}
