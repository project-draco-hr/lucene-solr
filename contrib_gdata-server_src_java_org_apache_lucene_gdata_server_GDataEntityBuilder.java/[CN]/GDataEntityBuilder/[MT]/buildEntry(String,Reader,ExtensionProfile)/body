{
  BaseEntry retVal=null;
  try {
    retVal=((BaseFeed)createEntityInstance(feedId)).createEntry();
  }
 catch (  FeedNotFoundException e) {
    throw e;
  }
catch (  Exception e) {
    DataBuilderException ex=new DataBuilderException("Could not build Entry for Entry class ",e);
    ex.setStackTrace(e.getStackTrace());
    throw ex;
  }
  retVal.parseAtom(new ExtensionProfile(),reader);
  return retVal;
}
