{
  assertMatchEquals(lookup.lookup("one",true,2),"one/0.0","oneness/1.0");
  assertMatchEquals(lookup.lookup("one",false,2),"one/0.0","oneness/1.0");
  lookup=new FSTLookup(10,false);
  lookup.build(new TermFreqArrayIterator(evalKeys()));
  assertMatchEquals(lookup.lookup("one",true,2),"oneness/1.0","onerous/1.0");
  assertMatchEquals(lookup.lookup("one",false,2),"one/0.0","oneness/1.0");
}
