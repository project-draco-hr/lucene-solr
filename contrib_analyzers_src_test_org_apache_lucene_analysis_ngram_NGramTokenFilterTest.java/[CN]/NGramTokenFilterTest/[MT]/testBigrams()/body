{
  NGramTokenFilter filter=new NGramTokenFilter(input,2,2);
  Token token=null;
  do {
    token=filter.next();
    if (token != null) {
      tokens.add(token.toString());
    }
  }
 while (token != null);
  assertEquals(4,tokens.size());
  ArrayList exp=new ArrayList();
  exp.add("(ab,0,2)");
  exp.add("(bc,1,3)");
  exp.add("(cd,2,4)");
  exp.add("(de,3,5)");
  assertEquals(exp,tokens);
}
