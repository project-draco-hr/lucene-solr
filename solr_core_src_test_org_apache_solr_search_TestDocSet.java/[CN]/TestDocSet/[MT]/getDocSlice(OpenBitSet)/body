{
  int len=(int)bs.cardinality();
  int[] arr=new int[len + 5];
  arr[0]=10;
  arr[1]=20;
  arr[2]=30;
  arr[arr.length - 1]=1;
  arr[arr.length - 2]=2;
  int offset=3;
  int end=offset + len;
  OpenBitSetIterator iter=new OpenBitSetIterator(bs);
  for (int i=end - 1; i >= offset; i--) {
    arr[i]=iter.nextDoc();
  }
  return new DocSlice(offset,len,arr,null,len * 2,100.0f);
}
