{
  SolrInputDocument d=null;
  d=processAdd("remove-all-blanks",doc(f("id","1111"),f("foo_s","string1",""),f("bar_dt","string2","","string3"),f("yak_t",""),f("foo_d",new Integer(42))));
  assertNotNull(d);
  assertEquals(Arrays.asList("string1"),d.getFieldValues("foo_s"));
  assertEquals(Arrays.asList("string2","string3"),d.getFieldValues("bar_dt"));
  assertFalse("shouldn't be any values for yak_t",d.containsKey("yak_t"));
  assertEquals("processor borked non string value",new Integer(42),d.getFieldValue("foo_d"));
}
