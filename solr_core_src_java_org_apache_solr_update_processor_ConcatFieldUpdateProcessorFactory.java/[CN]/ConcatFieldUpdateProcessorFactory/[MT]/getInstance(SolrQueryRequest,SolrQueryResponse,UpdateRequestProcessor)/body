{
  return new FieldMutatingUpdateProcessor(getSelector(),next){
    @Override protected SolrInputField mutate(    final SolrInputField src){
      if (src.getValueCount() <= 1)       return src;
      SolrInputField result=new SolrInputField(src.getName());
      result.setValue(StringUtils.join(src.getValues(),delimiter),src.getBoost());
      return result;
    }
  }
;
}
