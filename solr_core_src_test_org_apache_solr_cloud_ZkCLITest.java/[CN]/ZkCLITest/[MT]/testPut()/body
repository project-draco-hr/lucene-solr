{
  String data="my data";
  String[] args=new String[]{"-zkhost",zkServer.getZkAddress(),"-cmd","put","/data.txt",data};
  ZkCLI.main(args);
  zkClient.getData("/data.txt",null,null,true);
  assertArrayEquals(zkClient.getData("/data.txt",null,null,true),data.getBytes(StandardCharsets.UTF_8));
  data="my data deux";
  args=new String[]{"-zkhost",zkServer.getZkAddress(),"-cmd","put","/data.txt",data};
  ZkCLI.main(args);
  assertArrayEquals(zkClient.getData("/data.txt",null,null,true),data.getBytes(StandardCharsets.UTF_8));
}
