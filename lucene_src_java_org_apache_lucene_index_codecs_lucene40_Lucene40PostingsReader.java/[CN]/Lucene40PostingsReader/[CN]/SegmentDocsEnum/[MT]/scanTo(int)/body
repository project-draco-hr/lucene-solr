{
  while (ord++ < limit) {
    int code=freqIn.readVInt();
    if (indexOmitsTF) {
      accum+=code;
    }
 else {
      accum+=code >>> 1;
      if ((code & 1) != 0) {
        freq=1;
      }
 else {
        freq=freqIn.readVInt();
      }
    }
    if (accum >= target && (liveDocs == null || liveDocs.get(accum))) {
      return accum;
    }
  }
  return NO_MORE_DOCS;
}
