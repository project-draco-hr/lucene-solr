{
  while (true) {
    if (ord == limit) {
      return doc=NO_MORE_DOCS;
    }
    ord++;
    final int code=freqIn.readVInt();
    if (omitTF) {
      accum+=code;
    }
 else {
      accum+=code >>> 1;
      if ((code & 1) != 0) {
        freq=1;
      }
 else {
        freq=freqIn.readVInt();
      }
    }
    if (liveDocs == null || liveDocs.get(accum)) {
      break;
    }
  }
  return (doc=accum);
}
