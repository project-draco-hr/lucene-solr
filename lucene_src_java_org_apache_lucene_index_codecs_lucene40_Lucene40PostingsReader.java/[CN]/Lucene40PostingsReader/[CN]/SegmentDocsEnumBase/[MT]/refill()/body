{
  final int doc=nextUnreadDoc();
  count=0;
  start=-1;
  if (doc == NO_MORE_DOCS) {
    return NO_MORE_DOCS;
  }
  final int numDocs=Math.min(docs.length,limit - ord);
  ord+=numDocs;
  if (indexOmitsTF) {
    count=fillDocs(numDocs);
  }
 else {
    count=fillDocsAndFreqs(numDocs);
  }
  maxBufferedDocId=count > 0 ? docs[count - 1] : NO_MORE_DOCS;
  return doc;
}
