{
  String errString="returned non ok status:404, message:Not Found";
  ignoreException(Pattern.quote(errString));
  String rawContent=null;
  try {
    rawContent=getFileContentFromZooKeeper(fileName);
  }
 catch (  Exception e) {
    if (-1 != e.getMessage().indexOf(errString))     return;
    throw new SolrException(SolrException.ErrorCode.SERVER_ERROR,"Unexpected error, expected error matching: " + errString,e);
  }
 finally {
    resetExceptionIgnores();
  }
  fail("File '" + fileName + "' was unexpectedly found in ZooKeeper.  Content starts with '"+ rawContent.substring(0,100)+ " [...]'");
}
