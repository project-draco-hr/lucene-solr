{
  nextDocStart=fp;
  docID=-1;
  readPositions=indexOptions.compareTo(IndexOptions.DOCS_AND_FREQS_AND_POSITIONS) >= 0;
  readOffsets=indexOptions.compareTo(IndexOptions.DOCS_AND_FREQS_AND_POSITIONS_AND_OFFSETS) >= 0;
  if (!readOffsets) {
    startOffset=-1;
    endOffset=-1;
  }
  cost=docFreq;
  return this;
}
