{
  CharArraySet set=new CharArraySet(TEST_VERSION_CURRENT,1,true);
  set.add("fischen");
  GermanStemFilter filter=new GermanStemFilter(new KeywordMarkerFilter(new LowerCaseTokenizer(TEST_VERSION_CURRENT,new StringReader("Fischen Trinken")),set));
  assertTokenStreamContents(filter,new String[]{"fischen","trink"});
}
