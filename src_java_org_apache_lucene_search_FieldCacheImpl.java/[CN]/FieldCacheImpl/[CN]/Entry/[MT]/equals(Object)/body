{
  if (o instanceof Entry) {
    Entry other=(Entry)o;
    if (other.field == field && other.type == type) {
      if (other.locale == null ? locale == null : other.locale.equals(locale)) {
        if (other.custom == null) {
          if (custom == null)           return true;
        }
 else         if (other.custom.equals(custom)) {
          return true;
        }
      }
    }
  }
  return false;
}
