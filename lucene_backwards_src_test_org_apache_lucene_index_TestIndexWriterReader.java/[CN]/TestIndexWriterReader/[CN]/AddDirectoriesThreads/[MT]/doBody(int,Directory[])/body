{
switch (j % 4) {
case 0:
    mainWriter.addIndexesNoOptimize(dirs);
  mainWriter.optimize();
break;
case 1:
mainWriter.addIndexesNoOptimize(dirs);
numAddIndexesNoOptimize.incrementAndGet();
break;
case 2:
mainWriter.addIndexes(readers);
break;
case 3:
mainWriter.commit();
}
count.addAndGet(dirs.length * NUM_INIT_DOCS);
}
