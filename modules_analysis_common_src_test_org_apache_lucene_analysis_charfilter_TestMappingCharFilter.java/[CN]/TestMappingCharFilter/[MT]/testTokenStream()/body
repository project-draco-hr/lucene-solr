{
  CharStream cs=new MappingCharFilter(normMap,CharReader.get(new StringReader("h i j k ll cccc bbb aa")));
  TokenStream ts=new MockTokenizer(cs,MockTokenizer.WHITESPACE,false);
  assertTokenStreamContents(ts,new String[]{"i","i","jj","kkk","llll","cc","b","a"},new int[]{0,2,4,6,8,11,16,20},new int[]{1,3,5,7,10,15,19,22});
}
