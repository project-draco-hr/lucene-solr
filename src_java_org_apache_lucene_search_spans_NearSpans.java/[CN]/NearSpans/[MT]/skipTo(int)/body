{
  if (firstTime) {
    initList(false);
    firstTime=false;
  }
  for (SpansCell cell=first; more && cell != null; cell=cell.next) {
    more=cell.skipTo(target);
  }
  if (more) {
    listToQueue();
    if (min().doc() == max.doc()) {
      int matchLength=max.end() - min().start();
      if ((matchLength - totalLength) <= slop) {
        return true;
      }
    }
    return next();
  }
  return false;
}
