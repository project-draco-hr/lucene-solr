{
  if (firstTime) {
    initList(false);
    for (SpansCell cell=first; more && cell != null; cell=cell.next) {
      more=cell.skipTo(target);
    }
    if (more) {
      listToQueue();
    }
    firstTime=false;
  }
 else {
    while (more && min().doc() < target) {
      more=min().skipTo(target);
      if (more)       queue.adjustTop();
    }
  }
  if (more) {
    if (atMatch())     return true;
    return next();
  }
  return false;
}
