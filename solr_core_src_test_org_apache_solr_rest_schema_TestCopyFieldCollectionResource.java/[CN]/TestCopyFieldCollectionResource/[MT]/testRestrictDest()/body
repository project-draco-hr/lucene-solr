{
  assertQ("/schema/copyfields/?indent=on&wt=xml&dest.fl=title,*_s,*_dest_sub_s,dest_sub_no_ast_s","count(/response/arr[@name='copyFields']/lst) = 16","count(/response/arr[@name='copyFields']/lst/str[@name='dest'][.='title']) = 3","count(/response/arr[@name='copyFields']/lst/str[@name='dest'][.='*_s']) = 4","count(/response/arr[@name='copyFields']/lst/str[@name='dest'][.='*_dest_sub_s']) = 4","count(/response/arr[@name='copyFields']/lst/str[@name='dest'][.='dest_sub_no_ast_s']) = 5");
}
