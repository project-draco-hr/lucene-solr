{
  State s=new State();
  if (x.length() == n)   s.setAccept(true);
 else {
    if (zeros)     initials.add(s);
    char c=x.charAt(n);
    s.addTransition(new Transition(c,atLeast(x,n + 1,initials,zeros && c == '0')));
    if (c < '9')     s.addTransition(new Transition((char)(c + 1),'9',anyOfRightLength(x,n + 1)));
  }
  return s;
}
