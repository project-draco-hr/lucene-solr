{
  if (shapeStr != null && shapeStr.length() > 0) {
    try {
      return strategy.getSpatialContext().readShapeFromWkt(shapeStr);
    }
 catch (    Exception e) {
      System.err.println("Shape " + name + " wasn't parseable: "+ e+ "  (skipping it)");
      return null;
    }
  }
  return null;
}
