{
  final BytesRef b=new BytesRef(input);
  final BytesRef result=c.floor(b,b);
  if (expected == null) {
    assertNull(result);
  }
 else {
    assertNotNull(result);
    assertEquals("actual=" + result.utf8ToString() + " vs expected="+ expected+ " (input="+ input+ ")",result,new BytesRef(expected));
  }
}
