{
  for (int i=0; i < buffer.length(); i++) {
switch (buffer.charAt(i)) {
case '?':
case '?':
{
        buffer.setCharAt(i,'a');
        break;
      }
case '?':
case '?':
{
      buffer.setCharAt(i,'e');
      break;
    }
case '?':
case '?':
{
    buffer.setCharAt(i,'u');
    break;
  }
case '?':
case 'i':
{
  buffer.setCharAt(i,'i');
  break;
}
case '?':
case '?':
{
buffer.setCharAt(i,'o');
break;
}
}
}
}
