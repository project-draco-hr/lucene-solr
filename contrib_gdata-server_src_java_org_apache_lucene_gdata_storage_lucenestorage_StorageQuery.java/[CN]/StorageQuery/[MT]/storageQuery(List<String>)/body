{
  BooleanQuery query=new BooleanQuery();
  for (  String id : entryId) {
    TermQuery termQuery=new TermQuery(new Term(StorageEntryWrapper.FIELD_ENTRY_ID,id));
    query.add(new BooleanClause(termQuery,Occur.SHOULD));
  }
  return this.searcher.search(query,new ModifiedEntryFilter(this.buffer.getExculdList()));
}
