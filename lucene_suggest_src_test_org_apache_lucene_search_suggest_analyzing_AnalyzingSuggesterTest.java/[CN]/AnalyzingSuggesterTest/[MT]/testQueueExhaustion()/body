{
  Analyzer a=new MockAnalyzer(random());
  AnalyzingSuggester suggester=new AnalyzingSuggester(a,a,AnalyzingSuggester.EXACT_FIRST,256,-1);
  suggester.build(new TermFreqPayloadArrayIterator(new TermFreqPayload[]{new TermFreqPayload("a",2),new TermFreqPayload("a b c",3),new TermFreqPayload("a c a",1),new TermFreqPayload("a c b",1)}));
  suggester.lookup("a",false,4);
}
