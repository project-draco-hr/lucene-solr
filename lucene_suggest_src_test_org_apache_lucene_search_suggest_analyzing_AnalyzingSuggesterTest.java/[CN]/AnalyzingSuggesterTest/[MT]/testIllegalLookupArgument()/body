{
  Analyzer a=new MockAnalyzer(random());
  Directory tempDir=getDirectory();
  AnalyzingSuggester suggester=new AnalyzingSuggester(tempDir,"suggest",a,a,0,256,-1,true);
  suggester.build(new InputArrayIterator(new Input[]{new Input("?? ?????? ?????????",7)}));
  try {
    suggester.lookup("??\u001E",false,3);
    fail("should throw IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    suggester.lookup("??\u001F",false,3);
    fail("should throw IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
  IOUtils.close(a,tempDir);
}
