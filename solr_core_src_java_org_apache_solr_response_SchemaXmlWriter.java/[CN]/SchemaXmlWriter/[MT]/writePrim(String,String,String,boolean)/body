{
  int contentLen=val == null ? 0 : val.length();
  startTag(tag,name,contentLen == 0);
  if (contentLen == 0)   return;
  if (escape) {
    XML.escapeCharData(val,writer);
  }
 else {
    writer.write(val,0,contentLen);
  }
  writer.write('<');
  writer.write('/');
  writer.write(tag);
  writer.write('>');
}
