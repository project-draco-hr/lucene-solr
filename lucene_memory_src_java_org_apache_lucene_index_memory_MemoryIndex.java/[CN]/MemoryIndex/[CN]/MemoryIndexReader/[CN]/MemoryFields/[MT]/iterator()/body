{
  return new FieldsEnum(){
    int upto=-1;
    @Override public String next(){
      upto++;
      if (upto >= sortedFields.length) {
        return null;
      }
      return sortedFields[upto].getKey();
    }
    @Override public Terms terms(){
      return MemoryFields.this.terms(sortedFields[upto].getKey());
    }
  }
;
}
