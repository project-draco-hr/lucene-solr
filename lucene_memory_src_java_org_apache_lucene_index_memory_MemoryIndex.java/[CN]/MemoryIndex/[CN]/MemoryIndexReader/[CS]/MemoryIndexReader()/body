{
  super();
  boolean hasPointValues=false;
  for (  Info info : fields.values()) {
    info.prepareDocValuesAndPointValues();
    if (info.pointValues != null) {
      hasPointValues=true;
    }
  }
  if (hasPointValues) {
    pointValues=new MemoryIndexPointValues();
  }
 else {
    pointValues=null;
  }
}
