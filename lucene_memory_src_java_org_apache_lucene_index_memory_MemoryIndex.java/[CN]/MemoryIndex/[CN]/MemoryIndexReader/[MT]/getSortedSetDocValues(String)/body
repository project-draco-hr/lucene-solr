{
  Info info=getInfoForExpectedDocValuesType(field,DocValuesType.SORTED_SET);
  if (info != null) {
    return new SortedSetDocValues(){
      int index=0;
      @Override public long nextOrd(){
        if (index >= info.binaryProducer.dvBytesValuesSet.size()) {
          return NO_MORE_ORDS;
        }
        return index++;
      }
      @Override public void setDocument(      int docID){
        index=0;
      }
      @Override public BytesRef lookupOrd(      long ord){
        return info.binaryProducer.getValue((int)ord);
      }
      @Override public long getValueCount(){
        return info.binaryProducer.dvBytesValuesSet.size();
      }
    }
;
  }
 else {
    return null;
  }
}
