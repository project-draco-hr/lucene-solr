{
  return "SolrSuggester [ name=" + name + ", "+ "sourceLocation="+ sourceLocation+ ", "+ "storeDir="+ ((storeDir == null) ? "" : storeDir.getAbsoluteFile())+ ", "+ "lookupImpl="+ lookupImpl+ ", "+ "dictionaryImpl="+ dictionaryImpl+ ", "+ "sizeInBytes="+ ((lookup != null) ? String.valueOf(sizeInBytes()) : "0")+ " ]";
}
