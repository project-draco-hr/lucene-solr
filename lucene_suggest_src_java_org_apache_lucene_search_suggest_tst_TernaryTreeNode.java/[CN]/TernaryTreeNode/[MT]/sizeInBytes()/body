{
  long mem=RamUsageEstimator.shallowSizeOf(this);
  if (loKid != null) {
    mem+=loKid.sizeInBytes();
  }
  if (eqKid != null) {
    mem+=eqKid.sizeInBytes();
  }
  if (hiKid != null) {
    mem+=hiKid.sizeInBytes();
  }
  if (token != null) {
    mem+=RamUsageEstimator.sizeOf(token);
  }
  mem+=RamUsageEstimator.shallowSizeOf(val);
  return mem;
}
