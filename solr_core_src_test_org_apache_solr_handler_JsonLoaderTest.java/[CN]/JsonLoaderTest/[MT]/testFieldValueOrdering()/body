{
  final String pre="{'add':[{'id':'1',";
  final String post="},{'id':'2'}]}";
  checkFieldValueOrdering((pre + "'f':[45,67,89]" + post).replace('\'','"'),1.0F);
  checkFieldValueOrdering((pre + "'f':45,'f':67,'f':89" + post).replace('\'','"'),1.0F);
  checkFieldValueOrdering((pre + "'f':{'boost':4.0,'value':[45,67,89]}" + post).replace('\'','"'),4.0F);
  checkFieldValueOrdering((pre + "'f':{'boost':2.0,'value':[45,67]}," + "'f':{'boost':2.0,'value':89}"+ post).replace('\'','"'),4.0F);
}
