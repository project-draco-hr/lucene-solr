{
  updateJ(json("[{'id':'1','big_decimal_td':100000000000000000000000000001234567890.0987654321}]"),params("commit","true"));
  assertJQ(req("q","id:1","fl","big_decimal_td"),"/response/docs/[0]=={'big_decimal_td':[1.0E38]}");
  updateJ(json("[{'id':'2','big_decimal_tf':100000000000000000000000000001234567890.0987654321}]"),params("commit","true"));
  assertJQ(req("q","id:2","fl","big_decimal_tf"),"/response/docs/[0]=={'big_decimal_tf':[1.0E38]}");
}
