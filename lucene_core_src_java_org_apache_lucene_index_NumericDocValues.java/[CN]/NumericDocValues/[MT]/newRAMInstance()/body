{
  final int maxDoc=size();
  final long minValue=minValue();
  final long maxValue=maxValue();
  final long[] values=new long[maxDoc];
  for (int docID=0; docID < maxDoc; docID++) {
    values[docID]=get(docID);
  }
  return new NumericDocValues(){
    @Override public long get(    int docID){
      return values[docID];
    }
    @Override public int size(){
      return maxDoc;
    }
    @Override public long minValue(){
      return minValue;
    }
    @Override public long maxValue(){
      return maxValue;
    }
    @Override public NumericDocValues newRAMInstance(){
      return this;
    }
  }
;
}
