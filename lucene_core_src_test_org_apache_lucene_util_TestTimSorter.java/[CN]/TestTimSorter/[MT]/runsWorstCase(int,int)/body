{
  List<Integer> runs=new LinkedList<>();
  int runningTotal=0, Y=minRun + 4, X=minRun;
  while ((long)runningTotal + Y + X <= length) {
    runningTotal+=X + Y;
    generateWrongElem(X,minRun,runs);
    runs.add(0,Y);
    X=Y + runs.get(1) + 1;
    Y+=X + 1;
  }
  if ((long)runningTotal + X <= length) {
    runningTotal+=X;
    generateWrongElem(X,minRun,runs);
  }
  runs.add(length - runningTotal);
  return runs;
}
