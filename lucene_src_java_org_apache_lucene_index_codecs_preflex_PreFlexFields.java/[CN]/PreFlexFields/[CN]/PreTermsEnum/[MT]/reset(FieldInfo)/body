{
  this.fieldInfo=fieldInfo;
  protoTerm=new Term(fieldInfo.name);
  if (termEnum == null) {
    termEnum=getTermsDict().terms(protoTerm);
    seekTermEnum=getTermsDict().terms(protoTerm);
  }
 else {
    getTermsDict().seekEnum(termEnum,protoTerm);
  }
  skipNext=true;
  surrogateSeekUpto=0;
  newSuffixStart=0;
  surrogatesDance();
}
