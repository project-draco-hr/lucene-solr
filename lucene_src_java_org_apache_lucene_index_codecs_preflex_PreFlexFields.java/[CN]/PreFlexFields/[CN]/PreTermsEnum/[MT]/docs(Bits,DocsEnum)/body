{
  PreDocsEnum docsEnum;
  if (reuse == null || !(reuse instanceof PreDocsEnum)) {
    docsEnum=new PreDocsEnum();
  }
 else {
    docsEnum=(PreDocsEnum)reuse;
    if (docsEnum.getFreqStream() != freqStream) {
      docsEnum=new PreDocsEnum();
    }
  }
  return docsEnum.reset(termEnum,skipDocs);
}
