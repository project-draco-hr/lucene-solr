{
  Term t1=randomTerm();
  Term t2=randomTerm();
  Term t3=randomTerm();
  PhraseQuery q1=new PhraseQuery();
  q1.add(t1);
  q1.add(t2);
  q1.add(t3);
  PhraseQuery q2=new PhraseQuery();
  q2.add(t1);
  q2.add(t2);
  q2.add(t3);
  for (int i=0; i < 10; i++) {
    q1.setSlop(i);
    q2.setSlop(i + 1);
    assertSubsetOf(q1,q2);
  }
}
