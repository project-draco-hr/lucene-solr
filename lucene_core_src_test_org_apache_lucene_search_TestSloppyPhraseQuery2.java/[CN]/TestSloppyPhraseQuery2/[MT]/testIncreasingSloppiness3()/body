{
  Term t1=randomTerm();
  Term t2=randomTerm();
  Term t3=randomTerm();
  for (int i=0; i < 10; i++) {
    PhraseQuery q1=new PhraseQuery(i,t1.field(),t1.bytes(),t2.bytes(),t3.bytes());
    PhraseQuery q2=new PhraseQuery(i + 1,t1.field(),t1.bytes(),t2.bytes(),t3.bytes());
    assertSubsetOf(q1,q2);
    assertSubsetOf(q1,q2);
  }
}
