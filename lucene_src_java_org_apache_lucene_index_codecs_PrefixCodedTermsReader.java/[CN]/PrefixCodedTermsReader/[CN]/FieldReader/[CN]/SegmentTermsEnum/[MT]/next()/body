{
  if (seekPending) {
    seekPending=false;
    in.seek(state.filePointer);
    indexEnum.seek(bytesReader.term);
    didIndexNext=false;
  }
  if (!bytesReader.read()) {
    positioned=false;
    return null;
  }
  final byte b=in.readByte();
  isIndexTerm=(b & 0x80) != 0;
  if ((b & 0x40) == 0) {
    state.docFreq=b & 0x3F;
  }
 else {
    state.docFreq=(in.readVInt() << 6) | (b & 0x3F);
  }
  postingsReader.readTerm(in,fieldInfo,state,isIndexTerm);
  if (doOrd) {
    state.ord++;
  }
  positioned=true;
  return bytesReader.term;
}
