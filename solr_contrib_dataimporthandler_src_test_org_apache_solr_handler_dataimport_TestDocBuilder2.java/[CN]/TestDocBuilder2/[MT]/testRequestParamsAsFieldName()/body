{
  List rows=new ArrayList();
  rows.add(createMap("mypk","101","text","ApacheSolr"));
  MockDataSource.setIterator("select * from x",rows.iterator());
  LocalSolrQueryRequest request=lrf.makeRequest("command","full-import","debug","on","clean","true","commit","true","mypk","id","text","desc","dataConfig",dataConfigWithTemplatizedFieldNames);
  h.query("/dataimport",request);
  assertQ(req("id:101"),"//*[@numFound='1']");
}
