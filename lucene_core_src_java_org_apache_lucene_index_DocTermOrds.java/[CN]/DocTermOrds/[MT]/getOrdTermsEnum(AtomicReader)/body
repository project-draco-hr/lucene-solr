{
  if (termInstances == 0) {
    return null;
  }
  if (indexedTermsArray == null) {
    final InvertedFields fields=reader.fields();
    if (fields == null) {
      return null;
    }
    final Terms terms=fields.terms(field);
    if (terms == null) {
      return null;
    }
 else {
      return terms.iterator(null);
    }
  }
 else {
    return new OrdWrappedTermsEnum(reader);
  }
}
