{
  int n=rand.nextInt(maxSetSize);
  OpenBitSet obs=new OpenBitSet(maxDoc);
  int[] a=new int[n];
  for (int i=0; i < n; i++) {
    for (; ; ) {
      int idx=rand.nextInt(maxDoc);
      if (obs.getAndSet(idx))       continue;
      a[i]=idx;
      break;
    }
  }
  return loadfactor != 0 ? new HashDocSet(a,0,n,1 / loadfactor) : new HashDocSet(a,0,n);
}
