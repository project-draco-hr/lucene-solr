{
  if (ref.decRef() == 0) {
    if (tis != null) {
      tis.close();
      tis=null;
    }
    if (tisNoIndex != null) {
      tisNoIndex.close();
    }
    if (freqStream != null) {
      freqStream.close();
    }
    if (proxStream != null) {
      proxStream.close();
    }
    if (termVectorsReaderOrig != null) {
      termVectorsReaderOrig.close();
    }
    if (fieldsReaderOrig != null) {
      fieldsReaderOrig.close();
    }
    if (cfsReader != null) {
      cfsReader.close();
    }
    if (storeCFSReader != null) {
      storeCFSReader.close();
    }
  }
}
