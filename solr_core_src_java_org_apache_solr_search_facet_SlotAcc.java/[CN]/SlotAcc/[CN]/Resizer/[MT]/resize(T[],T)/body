{
  T[] values=(T[])Array.newInstance(old.getClass().getComponentType(),getNewSize());
  if (defaultValue != null) {
    Arrays.fill(values,0,values.length,defaultValue);
  }
  for (int i=0; i < old.length; i++) {
    T val=old[i];
    if (val != defaultValue) {
      int newSlot=getNewSlot(i);
      if (newSlot >= 0) {
        values[newSlot]=val;
      }
    }
  }
  return values;
}
