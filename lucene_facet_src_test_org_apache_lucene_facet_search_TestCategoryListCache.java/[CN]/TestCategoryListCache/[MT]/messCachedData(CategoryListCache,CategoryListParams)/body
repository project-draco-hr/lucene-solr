{
  final CategoryListData cld=clCache.get(clp);
  CategoryListData badCld=new CategoryListData(){
    @Override public CategoryListIterator iterator(    int partition) throws IOException {
      final CategoryListIterator it=cld.iterator(partition);
      return new CategoryListIterator(){
        @Override public void getOrdinals(        int docID,        IntsRef ints) throws IOException {
          it.getOrdinals(docID,ints);
          for (int i=0; i < ints.length; i++) {
            if (ints.ints[i] > 1) {
              ints.ints[i]--;
            }
 else {
              ints.ints[i]++;
            }
          }
        }
        @Override public boolean setNextReader(        AtomicReaderContext context) throws IOException {
          return it.setNextReader(context);
        }
      }
;
    }
  }
;
  clCache.register(clp,badCld);
}
