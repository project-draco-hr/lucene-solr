{
  int docCount=state.segmentInfo.getDocCount();
  if (!initialized()) {
    return null;
  }
  consumer.finish(docCount);
  if (normsConsumer != null) {
    simpleNormsWriter.finish(docCount);
    simpleNormsWriter.flush(state,normsConsumer);
  }
 else {
    simpleNormsWriter.reset();
  }
  return initType;
}
