{
  this.include=Objects.requireNonNull(include);
  this.exclude=Objects.requireNonNull(exclude);
  this.pre=(pre >= 0) ? pre : 0;
  this.post=(post >= 0) ? post : 0;
  if (include.getField() != null && exclude.getField() != null && !include.getField().equals(exclude.getField()))   throw new IllegalArgumentException("Clauses must have same field.");
}
