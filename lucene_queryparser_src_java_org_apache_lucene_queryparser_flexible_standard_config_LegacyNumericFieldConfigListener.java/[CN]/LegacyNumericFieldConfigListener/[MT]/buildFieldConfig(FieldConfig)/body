{
  Map<String,LegacyNumericConfig> numericConfigMap=config.get(ConfigurationKeys.LEGACY_NUMERIC_CONFIG_MAP);
  if (numericConfigMap != null) {
    LegacyNumericConfig numericConfig=numericConfigMap.get(fieldConfig.getField());
    if (numericConfig != null) {
      fieldConfig.set(ConfigurationKeys.LEGACY_NUMERIC_CONFIG,numericConfig);
    }
  }
}
