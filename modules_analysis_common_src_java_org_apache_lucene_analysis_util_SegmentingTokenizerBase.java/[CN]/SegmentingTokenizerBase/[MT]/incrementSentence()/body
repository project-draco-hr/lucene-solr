{
  if (length == 0)   return false;
  while (true) {
    int start=iterator.current();
    if (start == BreakIterator.DONE)     return false;
    int end=iterator.next();
    if (end == BreakIterator.DONE)     return false;
    setNextSentence(start,end);
    if (incrementWord()) {
      return true;
    }
  }
}
