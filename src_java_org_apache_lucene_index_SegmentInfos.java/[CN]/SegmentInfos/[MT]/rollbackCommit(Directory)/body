{
  if (pendingOutput != null) {
    try {
      pendingOutput.close();
    }
 catch (    Throwable t) {
    }
    try {
      final String segmentFileName=IndexFileNames.fileNameFromGeneration(IndexFileNames.SEGMENTS,"",generation);
      dir.deleteFile(segmentFileName);
    }
 catch (    Throwable t) {
    }
    pendingOutput=null;
  }
}
