{
  if (history == null) {
    return null;
  }
  SolrDocumentList docs=new SolrDocumentList();
  Iterator<E> iter=history.iterator();
  while (iter.hasNext()) {
    E e=iter.next();
    long ts=getTimestamp(e);
    if (ts == since) {
      if (found != null) {
        found.set(true);
      }
    }
    if (ts > since) {
      docs.add(toSolrDocument(e));
    }
  }
  docs.setNumFound(docs.size());
  return docs;
}
