{
switch (script) {
case UScript.JAPANESE:
    return (BreakIterator)cjkBreakIterator.clone();
case UScript.MYANMAR:
  if (myanmarAsWords) {
    return (BreakIterator)defaultBreakIterator.clone();
  }
 else {
    return (BreakIterator)myanmarSyllableIterator.clone();
  }
default :
return (BreakIterator)defaultBreakIterator.clone();
}
}
