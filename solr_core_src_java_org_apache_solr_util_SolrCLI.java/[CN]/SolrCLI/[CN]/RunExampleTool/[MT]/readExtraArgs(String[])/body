{
  String extraArgs="";
  if (extraArgsArr != null && extraArgsArr.length > 0) {
    StringBuilder sb=new StringBuilder();
    int app=0;
    for (int e=0; e < extraArgsArr.length; e++) {
      String arg=extraArgsArr[e];
      if ("e".equals(arg) || "example".equals(arg)) {
        e++;
        continue;
      }
      if (app > 0)       sb.append(" ");
      sb.append(arg);
      ++app;
    }
    extraArgs=sb.toString().trim();
  }
  return extraArgs;
}
