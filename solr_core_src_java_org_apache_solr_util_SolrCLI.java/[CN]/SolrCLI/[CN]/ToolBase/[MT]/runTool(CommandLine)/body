{
  verbose=cli.hasOption("verbose");
  int toolExitStatus=0;
  try {
    runImpl(cli);
  }
 catch (  Exception exc) {
    String excMsg=exc.getMessage();
    if (excMsg != null) {
      System.err.println("\nERROR: " + excMsg + "\n");
      toolExitStatus=1;
    }
 else {
      throw exc;
    }
  }
  return toolExitStatus;
}
