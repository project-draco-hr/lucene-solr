{
  List<String> list=null;
  Object obj=atPath(jsonPath,json);
  if (obj != null) {
    if (obj instanceof List) {
      list=(List<String>)obj;
    }
 else {
      throw new IllegalStateException("Expected a List at path " + jsonPath + " but found "+ obj+ " instead! "+ json);
    }
  }
  return list;
}
