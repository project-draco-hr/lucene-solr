{
  if (ccid < 0 || ccid > WordDictionary.GB2312_CHAR_NUM)   return "";
  int cc1=ccid / 94 + 161;
  int cc2=ccid % 94 + 161;
  byte[] buffer=new byte[2];
  buffer[0]=(byte)cc1;
  buffer[1]=(byte)cc2;
  try {
    String cchar=new String(buffer,"GB2312");
    return cchar;
  }
 catch (  UnsupportedEncodingException e) {
    return "";
  }
}
