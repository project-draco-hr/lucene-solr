{
  if (!basicsAndFieldFacetsComputed) {
    super.compute();
    for (    Map<String,StatsCollector[]> f : fieldFacetCollectors.values()) {
      for (      StatsCollector[] arr : f.values()) {
        for (        StatsCollector b : arr) {
          b.compute();
        }
      }
    }
    basicsAndFieldFacetsComputed=true;
  }
}
