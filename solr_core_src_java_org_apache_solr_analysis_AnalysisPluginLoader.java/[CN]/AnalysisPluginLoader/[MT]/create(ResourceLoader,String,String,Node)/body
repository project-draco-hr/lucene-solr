{
  S instance=null;
  Matcher m=legacyPattern.matcher(className);
  if (m.matches()) {
    try {
      instance=createSPI(m.group(4));
    }
 catch (    IllegalArgumentException ex) {
    }
  }
  if (instance != null) {
    className=instance.getClass().getName();
  }
  return super.create(loader,name,className,node);
}
