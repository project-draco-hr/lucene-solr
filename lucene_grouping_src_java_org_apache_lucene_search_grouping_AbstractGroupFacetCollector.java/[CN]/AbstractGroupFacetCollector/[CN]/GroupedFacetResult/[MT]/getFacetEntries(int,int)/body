{
  List<FacetEntry> entries=new LinkedList<FacetEntry>();
  int skipped=0;
  int included=0;
  for (  FacetEntry facetEntry : facetEntries) {
    if (skipped < offset) {
      skipped++;
      continue;
    }
    if (included++ >= limit) {
      break;
    }
    entries.add(facetEntry);
  }
  return entries;
}
