{
  feed.setGenerator(generator);
  feed.setItemsPerPage(request.getItemsPerPage());
  feed.setStartIndex(request.getStartIndex());
  feed.setId(request.getContextPath());
  List<Link> links=feed.getLinks();
  links.add(buildLink(Link.Rel.SELF,Link.Type.ATOM,request.getSelfId()));
  links.add(buildLink(Link.Rel.NEXT,XMLMIME,request.getNextId()));
  String prevLink=request.getPreviousId();
  if (prevLink != null)   links.add(buildLink(Link.Rel.PREVIOUS,XMLMIME,prevLink));
}
