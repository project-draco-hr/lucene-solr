{
  int n=(short)buffer.charAt(offset++);
  if (ncomponents != n) {
    return false;
  }
  if (ncomponents == 0) {
    return true;
  }
  for (int i=0; i < ncomponents; i++) {
    if (ends[i] != (short)buffer.charAt(offset++)) {
      return false;
    }
  }
  int usedchars=ends[ncomponents - 1];
  for (int i=0; i < usedchars; i++) {
    if (chars[i] != buffer.charAt(offset++)) {
      return false;
    }
  }
  return true;
}
