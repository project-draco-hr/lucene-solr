{
  if (prefixLen < 0 || prefixLen > ncomponents) {
    prefixLen=ncomponents;
  }
  if (prefixLen == 0) {
    return "";
  }
  StringBuilder sb=new StringBuilder(ends[prefixLen - 1] + (prefixLen - 1));
  try {
    this.appendTo(sb,delimiter,prefixLen);
  }
 catch (  IOException e) {
  }
  return sb.toString();
}
