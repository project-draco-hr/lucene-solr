{
  int INDEX=(int)Math.pow(2,30) + 1;
  int BITS=2;
  Packed32 p32=new Packed32(INDEX,BITS);
  p32.set(INDEX - 1,1);
  assertEquals("The value at position " + (INDEX - 1) + " should be correct for Packed32",1,p32.get(INDEX - 1));
  p32=null;
  Packed64 p64=new Packed64(INDEX,BITS);
  p64.set(INDEX - 1,1);
  assertEquals("The value at position " + (INDEX - 1) + " should be correct for Packed64",1,p64.get(INDEX - 1));
}
