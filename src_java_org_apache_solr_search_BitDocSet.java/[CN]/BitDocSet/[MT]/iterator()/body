{
  return new DocIterator(){
    private final BitSetIterator iter=new BitSetIterator(bits);
    private int pos=iter.next();
    public boolean hasNext(){
      return pos >= 0;
    }
    public Integer next(){
      return nextDoc();
    }
    public void remove(){
      bits.clear(pos);
    }
    public int nextDoc(){
      int old=pos;
      pos=iter.next();
      return old;
    }
    public float score(){
      return 0.0f;
    }
  }
;
}
