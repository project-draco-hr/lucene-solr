{
  return new DocIterator(){
    int pos=bits.nextSetBit(0);
    public boolean hasNext(){
      return pos >= 0;
    }
    public Integer next(){
      return nextDoc();
    }
    public void remove(){
      bits.clear(pos);
    }
    public int nextDoc(){
      int old=pos;
      pos=bits.nextSetBit(old + 1);
      return old;
    }
    public float score(){
      return 0.0f;
    }
  }
;
}
