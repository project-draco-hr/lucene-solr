{
  String str="abc defgh ijklmn opqrstu vwxy z";
  Token[] out_tokens={newToken("abc",0,3,CJKTokenizer.SINGLE_TOKEN_TYPE),newToken("defgh",4,9,CJKTokenizer.SINGLE_TOKEN_TYPE),newToken("ijklmn",10,16,CJKTokenizer.SINGLE_TOKEN_TYPE),newToken("opqrstu",17,24,CJKTokenizer.SINGLE_TOKEN_TYPE),newToken("vwxy",25,29,CJKTokenizer.SINGLE_TOKEN_TYPE),newToken("z",30,31,CJKTokenizer.SINGLE_TOKEN_TYPE)};
  checkCJKToken(str,out_tokens);
}
