{
  if (doc > values.docID()) {
    values.advance(doc);
  }
  BytesRef ref;
  if (doc == values.docID()) {
    ref=values.binaryValue();
  }
 else {
    ref=null;
  }
  this.fieldType.indexedToReadable(ref,charsRefBuilder);
  CharsRef charsRef=charsRefBuilder.get();
  return charsRef.hashCode();
}
