{
  if (id == null && indexedId != null) {
    IndexSchema schema=req.getSchema();
    SchemaField sf=schema.getUniqueKeyField();
    if (sf != null) {
      CharsRefBuilder ref=new CharsRefBuilder();
      sf.getType().indexedToReadable(indexedId,ref);
      id=ref.toString();
    }
  }
  return id;
}
