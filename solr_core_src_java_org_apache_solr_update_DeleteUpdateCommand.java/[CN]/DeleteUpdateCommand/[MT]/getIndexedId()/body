{
  if (indexedId == null) {
    IndexSchema schema=req.getSchema();
    SchemaField sf=schema.getUniqueKeyField();
    if (sf != null && id != null) {
      BytesRefBuilder b=new BytesRefBuilder();
      sf.getType().readableToIndexed(id,b);
      indexedId=b.get();
    }
  }
  return indexedId;
}
