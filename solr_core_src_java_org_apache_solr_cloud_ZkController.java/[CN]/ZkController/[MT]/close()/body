{
  try {
    overseer.close();
  }
 catch (  Throwable t) {
    log.error("Error closing overseer",t);
  }
  try {
    zkClient.close();
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    log.warn("",e);
    throw new ZooKeeperException(SolrException.ErrorCode.SERVER_ERROR,"",e);
  }
}
