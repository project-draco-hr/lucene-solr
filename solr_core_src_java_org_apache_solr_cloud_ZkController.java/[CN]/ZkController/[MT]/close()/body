{
  this.isClosed=true;
  if (cmdDistribExecutor != null) {
    try {
      ExecutorUtil.shutdownNowAndAwaitTermination(cmdDistribExecutor);
    }
 catch (    Throwable e) {
      SolrException.log(log,e);
    }
  }
  for (  ElectionContext context : electionContexts.values()) {
    try {
      context.close();
    }
 catch (    Throwable t) {
      log.error("Error closing overseer",t);
    }
  }
  try {
    overseer.close();
  }
 catch (  Throwable t) {
    log.error("Error closing overseer",t);
  }
  zkClient.close();
}
