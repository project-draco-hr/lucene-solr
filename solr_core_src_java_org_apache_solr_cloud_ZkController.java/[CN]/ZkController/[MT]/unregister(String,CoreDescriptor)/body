{
  final String zkNodeName=getCoreNodeName(cd);
  ElectionContext context=electionContexts.remove(zkNodeName);
  if (context != null) {
    context.cancelElection();
  }
  ZkNodeProps m=new ZkNodeProps(Overseer.QUEUE_OPERATION,"deletecore",ZkStateReader.CORE_NAME_PROP,coreName,ZkStateReader.NODE_NAME_PROP,getNodeName(),ZkStateReader.COLLECTION_PROP,cd.getCloudDescriptor().getCollectionName());
  overseerJobQueue.offer(ZkStateReader.toJSON(m));
}
