{
  initCalled=true;
  primary.onInit(wrapCommits(commits));
  for (  IndexCommit commit : commits) {
    if (refCounts.containsKey(commit.getGeneration())) {
      indexCommits.put(commit.getGeneration(),commit);
    }
  }
  if (!commits.isEmpty()) {
    lastCommit=commits.get(commits.size() - 1);
  }
}
