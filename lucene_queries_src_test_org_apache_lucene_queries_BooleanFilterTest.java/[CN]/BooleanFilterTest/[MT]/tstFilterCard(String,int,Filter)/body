{
  final DocIdSet docIdSet=filt.getDocIdSet(reader.getContext(),reader.getLiveDocs());
  int actual=0;
  if (docIdSet != null) {
    DocIdSetIterator disi=docIdSet.iterator();
    while (disi.nextDoc() != DocIdSetIterator.NO_MORE_DOCS) {
      actual++;
    }
  }
  assertEquals(mes,expected,actual);
}
