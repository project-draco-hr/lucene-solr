{
  final SparseFixedBitSet set=new SparseFixedBitSet(length);
  List<Integer> buffer=new ArrayList<>();
  for (int doc=bs.nextSetBit(0); doc != -1; doc=bs.nextSetBit(doc + 1)) {
    buffer.add(doc);
    if (buffer.size() >= 100000) {
      Collections.shuffle(buffer);
      for (      int i : buffer) {
        set.set(i);
      }
      buffer.clear();
    }
  }
  Collections.shuffle(buffer);
  for (  int i : buffer) {
    set.set(i);
  }
  return set;
}
