{
  String[] args=new String[]{"--input-list","file:///tmp","--morphline-file",MORPHLINE_FILE,"--output-dir","file:/tmp/foo","--solr-home-dir",MINIMR_INSTANCE_DIR.getPath(),"--shards","1","--shard-url","http://localhost:8983/solr/collection1"};
  assertNull(parser.parseArgs(args,conf,opts));
  assertEmptySystemErrAndEmptySystemOut();
}
