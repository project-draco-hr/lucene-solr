{
  assertEquals("",stripOp(""));
  assertEquals("foo",stripOp("foo"));
  assertEquals("foo -bar",stripOp("foo -bar"));
  assertEquals("foo +bar",stripOp("foo +bar"));
  assertEquals("foo + bar",stripOp("foo + bar"));
  assertEquals("foo+ bar",stripOp("foo+ bar"));
  assertEquals("foo+ bar",stripOp("foo+ bar"));
  assertEquals("foo+",stripOp("foo+"));
  assertEquals("foo bar",stripOp("foo bar -"));
  assertEquals("foo bar ",stripOp("foo bar - + ++"));
  assertEquals("foo bar",stripOp("foo --bar"));
  assertEquals("foo bar ",stripOp("foo -------------------------------------------------------------------------------------------------------------------------bar --"));
  assertEquals("foo bar ",stripOp("foo --bar -----------------------------------------------------------------------------------------------------------------------"));
}
