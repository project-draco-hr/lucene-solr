{
  assertEquals("",pe(""));
  assertEquals("foo",pe("foo"));
  assertEquals("foo\\:bar",pe("foo:bar"));
  assertEquals("+foo\\:bar",pe("+foo:bar"));
  assertEquals("foo \\! bar",pe("foo ! bar"));
  assertEquals("foo\\?",pe("foo?"));
  assertEquals("foo \"bar\"",pe("foo \"bar\""));
  assertEquals("foo\\! \"bar\"",pe("foo! \"bar\""));
}
