{
  Type v=mapping.get(field);
  if (v != null) {
switch (v) {
case INTEGER:
      return FieldCache.DEFAULT.getNumerics(in,field,FieldCache.NUMERIC_UTILS_INT_PARSER,true);
case FLOAT:
    return FieldCache.DEFAULT.getNumerics(in,field,FieldCache.NUMERIC_UTILS_FLOAT_PARSER,true);
case LONG:
  return FieldCache.DEFAULT.getNumerics(in,field,FieldCache.NUMERIC_UTILS_LONG_PARSER,true);
case DOUBLE:
return FieldCache.DEFAULT.getNumerics(in,field,FieldCache.NUMERIC_UTILS_DOUBLE_PARSER,true);
default :
throw new IllegalStateException("Type mismatch: " + field + " was indexed as "+ v);
}
}
return super.getNumericDocValues(field);
}
