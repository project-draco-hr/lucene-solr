{
  CategoryPath catPath=fresNode.getLabel(taxonomyReader);
  Term drillDownTerm=DrillDown.term(searchParams,catPath);
  Bits deletedDocs=MultiFields.getDeletedDocs(indexReader);
  int updatedCount=countIntersection(MultiFields.getTermDocsEnum(indexReader,deletedDocs,drillDownTerm.field(),drillDownTerm.bytes()),docIds.iterator());
  fresNode.setValue(updatedCount);
}
