{
switch (minimum) {
case NO:
    return tvSettings[_TestUtil.nextInt(random,0,tvSettings.length - 1)];
case YES:
  return tvSettings[_TestUtil.nextInt(random,1,tvSettings.length - 1)];
case WITH_OFFSETS:
return random.nextBoolean() ? TermVector.WITH_OFFSETS : TermVector.WITH_POSITIONS_OFFSETS;
case WITH_POSITIONS:
return random.nextBoolean() ? TermVector.WITH_POSITIONS : TermVector.WITH_POSITIONS_OFFSETS;
default :
return TermVector.WITH_POSITIONS_OFFSETS;
}
}
