{
  if (size == 0)   return null;
  if (enum.term() != null && ((enum.prev != null && term.compareTo(enum.prev) > 0) || term.compareTo(enum.term()) >= 0)) {
    int enumOffset=(enum.position / TermInfosWriter.INDEX_INTERVAL) + 1;
    if (indexTerms.length == enumOffset || term.compareTo(indexTerms[enumOffset]) < 0)     return scanEnum(term);
  }
  seekEnum(getIndexOffset(term));
  return scanEnum(term);
}
