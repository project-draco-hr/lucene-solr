{
  if (size == 0)   return null;
  SegmentTermEnum enumerator=getEnum();
  if (enumerator != null && enumerator.term() != null && position >= enumerator.position && position < (enumerator.position + enumerator.indexInterval))   return scanEnum(position);
  seekEnum(position / enumerator.indexInterval);
  return scanEnum(position);
}
