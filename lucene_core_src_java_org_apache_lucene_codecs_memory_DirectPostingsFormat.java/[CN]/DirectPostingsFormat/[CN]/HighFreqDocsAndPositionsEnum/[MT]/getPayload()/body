{
  if (payloads == null) {
    return null;
  }
 else {
    final byte[] payloadBytes=payloads[upto][posUpto / (hasOffsets ? 3 : 1)];
    if (payloadBytes == null) {
      return null;
    }
    payload.bytes=payloadBytes;
    payload.length=payloadBytes.length;
    payload.offset=0;
    return payload;
  }
}
