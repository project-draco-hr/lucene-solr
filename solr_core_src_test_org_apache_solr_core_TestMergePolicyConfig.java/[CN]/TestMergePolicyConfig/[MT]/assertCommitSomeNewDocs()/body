{
  for (int i=0; i < 5; i++) {
    int val=docIdCounter.getAndIncrement();
    assertU(adoc("id","" + val,"a_s",val + "_" + val+ "_"+ val+ "_"+ val,"b_s",val + "_" + val+ "_"+ val+ "_"+ val,"c_s",val + "_" + val+ "_"+ val+ "_"+ val,"d_s",val + "_" + val+ "_"+ val+ "_"+ val,"e_s",val + "_" + val+ "_"+ val+ "_"+ val,"f_s",val + "_" + val+ "_"+ val+ "_"+ val));
  }
  assertU(commit());
}
