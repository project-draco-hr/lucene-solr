{
  StringBuffer buf=new StringBuffer();
  buf.append("SerialChainFilter(");
  for (int i=0; i < chain.length; i++) {
switch (actionType[i]) {
case (SERIALAND):
      buf.append("SERIALAND");
    break;
case (SERIALOR):
  buf.append("SERIALOR");
break;
case (AND):
buf.append("AND");
break;
case (OR):
buf.append("OR");
break;
default :
buf.append(actionType[i]);
}
buf.append(" " + chain[i].toString() + " ");
}
return buf.toString().trim() + ")";
}
