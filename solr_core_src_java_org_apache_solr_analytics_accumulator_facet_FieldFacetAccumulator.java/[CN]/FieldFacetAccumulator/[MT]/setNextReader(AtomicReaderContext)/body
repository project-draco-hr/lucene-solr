{
  if (multiValued) {
    setValues=context.reader().getSortedSetDocValues(name);
  }
 else {
    if (numField) {
      numValues=context.reader().getNumericDocValues(name);
      numValuesBits=context.reader().getDocsWithField(name);
    }
 else {
      sortValues=context.reader().getSortedDocValues(name);
    }
  }
}
