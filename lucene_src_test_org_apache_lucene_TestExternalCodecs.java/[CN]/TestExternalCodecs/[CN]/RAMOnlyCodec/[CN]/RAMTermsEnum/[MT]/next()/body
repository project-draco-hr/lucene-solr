{
  if (it == null) {
    if (current == null) {
      it=ramField.termToDocs.keySet().iterator();
    }
 else {
      it=ramField.termToDocs.tailMap(current).keySet().iterator();
    }
  }
  if (it.hasNext()) {
    current=it.next();
    return new BytesRef(current);
  }
 else {
    return null;
  }
}
