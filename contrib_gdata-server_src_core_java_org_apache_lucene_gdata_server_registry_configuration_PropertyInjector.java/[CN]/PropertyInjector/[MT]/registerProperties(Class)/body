{
  if (clazz == null)   return;
  Method[] methodes=clazz.getMethods();
  for (int i=0; i < methodes.length; i++) {
    if (methodes[i].getName().startsWith(PropertyInjector.SETTER_PREFIX)) {
      String methodName=methodes[i].getName();
      String fieldName=getFieldName(methodName);
      if (methodes[i].getAnnotation(Requiered.class) != null)       this.requieredProperties.put(fieldName,methodes[i]);
 else       this.optionalProperties.put(fieldName,methodes[i]);
    }
  }
  registerProperties(clazz.getSuperclass());
}
