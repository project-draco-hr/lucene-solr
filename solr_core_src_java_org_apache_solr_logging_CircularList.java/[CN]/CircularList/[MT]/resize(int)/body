{
  if (newsize == this.size)   return;
  T[] vals=(T[])new Object[newsize];
  int i=0;
  if (newsize > size) {
    for (i=0; i < size; i++) {
      vals[i]=data[convert(i)];
    }
  }
 else {
    int off=size - newsize;
    for (i=0; i < newsize; i++) {
      vals[i]=data[convert(i + off)];
    }
  }
  data=vals;
  head=0;
  tail=i;
}
