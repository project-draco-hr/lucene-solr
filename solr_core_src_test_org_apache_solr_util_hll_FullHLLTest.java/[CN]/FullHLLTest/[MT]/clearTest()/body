{
  final int regwidth=5;
  final int log2m=4;
  final int m=1 << log2m;
  final HLL hll=new HLL(log2m,regwidth,128,256,HLLType.FULL);
  final BitVector bitVector=hll.probabilisticStorage;
  for (int i=0; i < m; i++)   bitVector.setRegister(i,i);
  hll.clear();
  for (int i=0; i < m; i++) {
    assertEquals(bitVector.getRegister(i),0L);
  }
}
