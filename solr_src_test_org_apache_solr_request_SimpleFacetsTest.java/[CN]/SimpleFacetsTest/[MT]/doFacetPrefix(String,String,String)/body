  assertQ("test facet.prefix middle, exact match first term, unsorted",req(params,"q","id:[* TO *]","indent",indent,"facet","true","facet.field",lf,"facet.mincount","0","facet.offset","0","facet.limit","100","facet.sort","index","facet.prefix","B"),"*[count(//lst[@name='facet_fields']/lst/int)=3]",pre + "/int[1][@name='B'][.='1']",pre + "/int[2][@name='BB'][.='2']",pre + "/int[3][@name='BBB'][.='3']");
  assertQ("test facet.prefix middle, exact match first term, unsorted",req(params,"q","id:[* TO *]","indent",indent,"facet","true","facet.field",lf,"facet.mincount","0","facet.offset","0","facet.limit","100","facet.sort","index","facet.prefix","B"),"*[count(//lst[@name='facet_fields']/lst/int)=3]",pre + "/int[1][@name='B'][.='1']",pre + "/int[2][@name='BB'][.='2']",pre + "/int[3][@name='BBB'][.='3']");
