{
  PhraseQuery expected=new PhraseQuery();
  expected.add(new Term("field","star"));
  expected.add(new Term("field","wars"));
  assertEquals(expected,parse("\"star wars\""));
  assertEquals(expected,parse("\"star wars\\ \""));
  assertEquals(expected,parse("\"\" | \"star wars\""));
  assertEquals(expected,parse("          \"star wars\"        \"\"\\"));
}
