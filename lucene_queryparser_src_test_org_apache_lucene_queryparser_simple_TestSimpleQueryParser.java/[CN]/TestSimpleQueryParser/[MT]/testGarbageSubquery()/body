{
  Query expected=new TermQuery(new Term("field","star"));
  assertEquals(expected,parse("(star)"));
  assertEquals(expected,parse("(star))"));
  assertEquals(expected,parse("((star)"));
  assertEquals(expected,parse("     -()(star)        \n\n\r     "));
  assertEquals(expected,parse("| + - ( + - |      star    \n      ) \n"));
}
