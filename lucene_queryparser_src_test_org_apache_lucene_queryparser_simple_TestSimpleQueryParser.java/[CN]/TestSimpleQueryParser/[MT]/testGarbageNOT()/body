{
  BooleanQuery expected=new BooleanQuery();
  expected.add(new TermQuery(new Term("field","star")),Occur.MUST_NOT);
  expected.add(new MatchAllDocsQuery(),Occur.SHOULD);
  assertEquals(expected,parse("-star"));
  assertEquals(expected,parse("---star"));
  assertEquals(expected,parse("- -star -"));
}
