{
  BooleanQuery expected=new BooleanQuery();
  expected.add(new TermQuery(new Term("field","star")),Occur.MUST);
  expected.add(new TermQuery(new Term("field","wars")),Occur.MUST);
  assertEquals(expected,parse("star wars"));
  assertEquals(expected,parse("star+wars"));
  assertEquals(expected,parse("     star     wars   "));
  assertEquals(expected,parse("     star +    wars   "));
  assertEquals(expected,parse("  |     star + + |   wars   "));
  assertEquals(expected,parse("  |     star + + |   wars   \\"));
}
