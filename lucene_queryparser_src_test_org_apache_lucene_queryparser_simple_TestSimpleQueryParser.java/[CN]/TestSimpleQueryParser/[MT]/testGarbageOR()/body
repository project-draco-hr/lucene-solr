{
  BooleanQuery expected=new BooleanQuery();
  expected.add(new TermQuery(new Term("field","star")),Occur.SHOULD);
  expected.add(new TermQuery(new Term("field","wars")),Occur.SHOULD);
  assertEquals(expected,parse("star|wars"));
  assertEquals(expected,parse("     star |    wars   "));
  assertEquals(expected,parse("  |     star | + |   wars   "));
  assertEquals(expected,parse("  +     star | + +   wars   \\"));
}
