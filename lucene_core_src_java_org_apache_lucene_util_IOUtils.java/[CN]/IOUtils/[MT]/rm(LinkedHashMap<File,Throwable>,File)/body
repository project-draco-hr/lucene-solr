{
  if (locations != null) {
    for (    File location : locations) {
      if (location != null && location.exists()) {
        if (location.isDirectory()) {
          rm(unremoved,location.listFiles());
        }
        try {
          Files.delete(location.toPath());
        }
 catch (        Throwable cause) {
          unremoved.put(location,cause);
        }
      }
    }
  }
  return unremoved;
}
