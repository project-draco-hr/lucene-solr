{
  Throwable th=null;
  for (  Closeable object : objects) {
    try {
      if (object != null) {
        object.close();
      }
    }
 catch (    Throwable t) {
      addSuppressed((priorException == null) ? th : priorException,t);
      if (th == null) {
        th=t;
      }
    }
  }
  if (priorException != null) {
    throw priorException;
  }
 else {
    reThrow(th);
  }
}
