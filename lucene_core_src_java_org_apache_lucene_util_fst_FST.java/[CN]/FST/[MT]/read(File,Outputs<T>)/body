{
  InputStream is=new BufferedInputStream(new FileInputStream(file));
  boolean success=false;
  try {
    FST<T> fst=new FST<T>(new InputStreamDataInput(is),outputs);
    success=true;
    return fst;
  }
  finally {
    if (success) {
      IOUtils.close(is);
    }
 else {
      IOUtils.closeWhileHandlingException(is);
    }
  }
}
