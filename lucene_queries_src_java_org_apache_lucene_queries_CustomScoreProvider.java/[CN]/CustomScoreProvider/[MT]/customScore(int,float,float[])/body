{
  if (valSrcScores.length == 1) {
    return customScore(doc,subQueryScore,valSrcScores[0]);
  }
  if (valSrcScores.length == 0) {
    return customScore(doc,subQueryScore,1);
  }
  float score=subQueryScore;
  for (  float valSrcScore : valSrcScores) {
    score*=valSrcScore;
  }
  return score;
}
