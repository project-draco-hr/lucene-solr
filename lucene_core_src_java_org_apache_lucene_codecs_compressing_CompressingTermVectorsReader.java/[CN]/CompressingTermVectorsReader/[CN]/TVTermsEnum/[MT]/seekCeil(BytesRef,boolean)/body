{
  if (ord < numTerms && ord >= 0) {
    final int cmp=term().compareTo(text);
    if (cmp == 0) {
      return SeekStatus.FOUND;
    }
 else     if (cmp > 0) {
      reset();
    }
  }
  while (true) {
    final BytesRef term=next();
    if (term == null) {
      return SeekStatus.END;
    }
    final int cmp=term.compareTo(text);
    if (cmp > 0) {
      return SeekStatus.NOT_FOUND;
    }
 else     if (cmp == 0) {
      return SeekStatus.FOUND;
    }
  }
}
