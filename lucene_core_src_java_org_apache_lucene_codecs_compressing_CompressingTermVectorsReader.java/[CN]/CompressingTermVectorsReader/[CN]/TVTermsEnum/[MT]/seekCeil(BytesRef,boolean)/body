{
  if (ord < numTerms && ord >= 0) {
    final int cmp=term().compareTo(text);
    if (cmp == 0) {
      return SeekStatus.FOUND;
    }
 else     if (cmp > 0) {
      reset();
    }
  }
  do {
    next();
  }
 while (ord < numTerms - 1 && term().compareTo(text) < 0);
  return term().equals(text) ? SeekStatus.FOUND : SeekStatus.END;
}
