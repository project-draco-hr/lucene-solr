{
  if (type == null) {
    return 0;
  }
 else   if (type == DocValuesType.NUMERIC) {
    return 1;
  }
 else   if (type == DocValuesType.BINARY) {
    return 2;
  }
 else   if (type == DocValuesType.SORTED) {
    return 3;
  }
 else   if (type == DocValuesType.SORTED_SET) {
    return 4;
  }
 else {
    throw new AssertionError();
  }
}
