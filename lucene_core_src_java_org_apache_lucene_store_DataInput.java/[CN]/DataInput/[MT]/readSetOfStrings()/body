{
  int count=readVInt();
  if (count == 0) {
    return Collections.emptySet();
  }
 else   if (count == 1) {
    return Collections.singleton(readString());
  }
 else {
    Set<String> set=count > 10 ? new HashSet<>() : new TreeSet<>();
    for (int i=0; i < count; i++) {
      set.add(readString());
    }
    return Collections.unmodifiableSet(set);
  }
}
