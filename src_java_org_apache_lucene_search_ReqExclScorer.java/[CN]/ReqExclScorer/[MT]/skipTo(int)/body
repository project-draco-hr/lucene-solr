{
  if (firstTime) {
    firstTime=false;
    if (!exclScorer.skipTo(target)) {
      exclScorer=null;
    }
  }
  if (reqScorer == null) {
    return false;
  }
  if (exclScorer == null) {
    return reqScorer.skipTo(target);
  }
  if (!reqScorer.skipTo(target)) {
    reqScorer=null;
    return false;
  }
  return toNonExcluded();
}
