{
  if (firstTime) {
    firstTime=false;
    if (!exclDisi.skipTo(target)) {
      exclDisi=null;
    }
  }
  if (reqScorer == null) {
    return false;
  }
  if (exclDisi == null) {
    return reqScorer.skipTo(target);
  }
  if (!reqScorer.skipTo(target)) {
    reqScorer=null;
    return false;
  }
  return toNonExcluded();
}
