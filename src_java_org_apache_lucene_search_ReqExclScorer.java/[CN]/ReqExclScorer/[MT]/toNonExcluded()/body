{
  int exclDoc=exclScorer.doc();
  do {
    int reqDoc=reqScorer.doc();
    if (reqDoc < exclDoc) {
      return true;
    }
 else     if (reqDoc > exclDoc) {
      if (!exclScorer.skipTo(reqDoc)) {
        exclScorer=null;
        return true;
      }
      exclDoc=exclScorer.doc();
      if (exclDoc > reqDoc) {
        return true;
      }
    }
  }
 while (reqScorer.next());
  reqScorer=null;
  return false;
}
