{
  if (firstTime) {
    if (!exclScorer.next()) {
      exclScorer=null;
    }
    firstTime=false;
  }
  if (reqScorer == null) {
    return false;
  }
  if (!reqScorer.next()) {
    reqScorer=null;
    return false;
  }
  if (exclScorer == null) {
    return true;
  }
  return toNonExcluded();
}
