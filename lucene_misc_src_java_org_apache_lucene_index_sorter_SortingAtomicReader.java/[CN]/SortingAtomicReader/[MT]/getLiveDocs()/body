{
  final Bits inLiveDocs=in.getLiveDocs();
  if (inLiveDocs == null) {
    return null;
  }
  return new Bits(){
    @Override public boolean get(    int index){
      return inLiveDocs.get(docMap.newToOld(index));
    }
    @Override public int length(){
      return inLiveDocs.length();
    }
  }
;
}
