{
  super.getBounds(bounds);
  bounds.addHorizontalPlane(planetModel,bottomLat,bottomPlane,leftPlane,rightPlane).addVerticalPlane(planetModel,leftLon,leftPlane,bottomPlane,rightPlane).addVerticalPlane(planetModel,rightLon,rightPlane,bottomPlane,leftPlane).addPoint(LLHC).addPoint(LRHC).addPoint(planetModel.NORTH_POLE);
}
