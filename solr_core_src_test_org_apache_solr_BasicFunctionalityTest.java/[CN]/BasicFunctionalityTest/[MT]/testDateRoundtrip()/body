{
  assertU(adoc("id","99","bday","99-01-01T12:34:56.789Z"));
  assertU(commit());
  assertQ("year should be canonicallized to 4 digits",req("q","id:99"),"//date[@name='bday'][.='0099-01-01T12:34:56.789Z']");
  assertU(adoc("id","99","bday","1999-01-01T12:34:56.900Z"));
  assertU(commit());
  assertQ("millis should be canonicallized to no trailing zeros",req("q","id:99"),"//date[@name='bday'][.='1999-01-01T12:34:56.9Z']");
}
