{
  Analyzer analyzer=new ReusableAnalyzerBase(){
    @Override protected TokenStreamComponents createComponents(    String fieldName,    Reader reader){
      Tokenizer t=new KeywordTokenizer(reader);
      return new TokenStreamComponents(t,new GermanStemFilter(new LowerCaseFilter(TEST_VERSION_CURRENT,t)));
    }
  }
;
  InputStream vocOut=getClass().getResourceAsStream("data.txt");
  assertVocabulary(analyzer,vocOut);
  vocOut.close();
}
