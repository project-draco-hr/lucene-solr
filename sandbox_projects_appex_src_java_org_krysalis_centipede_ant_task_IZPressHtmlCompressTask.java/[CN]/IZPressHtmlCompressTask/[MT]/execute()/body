{
  try {
    FileInputStream in=new FileInputStream(src);
    FileOutputStream out=new FileOutputStream(dest);
    compressor=new Compressor(in,out,conf);
    compressor.compress();
    out.flush();
    in.close();
    out.close();
  }
 catch (  IOException ioe) {
    throw new BuildException(ioe);
  }
}
