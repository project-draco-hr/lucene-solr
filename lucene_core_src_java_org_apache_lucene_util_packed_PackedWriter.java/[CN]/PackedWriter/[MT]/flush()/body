{
  final int nvalues=nextValues.position();
  nextValues.rewind();
  nextBlocks.clear();
  bulkOperation.encode(nextValues,nextBlocks,iterations);
  final int blocks=format.nblocks(bitsPerValue,nvalues);
  nextBlocks.rewind();
  for (int i=0; i < blocks; ++i) {
    out.writeLong(nextBlocks.get());
  }
  nextValues.clear();
}
