{
  advance(doc);
  boolean matched=this.doc == doc;
  this.doc=doc;
  float sc=matched ? score() : 0;
  double dist=dist(latVals.doubleVal(doc),lonVals.doubleVal(doc));
  String description=SpatialDistanceQuery.this.toString();
  if (matched) {
    return Explanation.match(sc,description + " product of:",Explanation.match((float)dist,"hsin(" + latVals.doubleVal(doc) + ","+ lonVals.doubleVal(doc)),Explanation.match(getBoost(),"boost"),Explanation.match(weight.queryNorm,"queryNorm"));
  }
 else {
    return Explanation.noMatch("No match");
  }
}
