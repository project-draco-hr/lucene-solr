{
  if (base.isMatch() == false) {
    return base;
  }
  double dist=dist(latVals.doubleVal(doc),lonVals.doubleVal(doc));
  String description=SpatialDistanceQuery.this.toString();
  return Explanation.match((float)(base.getValue() * dist),description + " product of:",base,Explanation.match((float)dist,"hsin(" + latVals.doubleVal(doc) + ","+ lonVals.doubleVal(doc)));
}
