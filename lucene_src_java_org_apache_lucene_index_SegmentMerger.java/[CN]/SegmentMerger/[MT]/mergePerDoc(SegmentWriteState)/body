{
  final PerDocConsumer docsConsumer=codec.docValuesFormat().docsConsumer(new PerDocWriteState(segmentWriteState));
  if (docsConsumer == null) {
    return;
  }
  boolean success=false;
  try {
    docsConsumer.merge(mergeState);
    success=true;
  }
  finally {
    if (success) {
      IOUtils.close(docsConsumer);
    }
 else {
      IOUtils.closeWhileHandlingException(docsConsumer);
    }
  }
}
