{
  int pos=0;
  for (Iterator iter=a.iterator(); iter.hasNext(); ) {
    Token tok=(Token)iter.next();
    pos+=tok.getPositionIncrement();
    if (!tokAt(b,new String(tok.termBuffer(),0,tok.termLength()),pos,checkOff ? tok.startOffset() : -1,checkOff ? tok.endOffset() : -1)) {
      fail(a + "!=" + b);
    }
  }
}
