{
  int type;
  if (typeString.equals("float")) {
    type=SortField.FLOAT;
  }
 else   if (typeString.equals("int")) {
    type=SortField.INT;
  }
 else   if (typeString.equals("string")) {
    type=SortField.STRING;
  }
 else   if (typeString.equals("string_val")) {
    type=SortField.STRING_VAL;
  }
 else {
    type=SortField.AUTO;
  }
  return type;
}
