{
  int sz=map.size();
  if (!excludeOuter) {
    startTag("lst",name,sz <= 0);
    incLevel();
  }
  for (  Map.Entry entry : (Set<Map.Entry>)map.entrySet()) {
    Object k=entry.getKey();
    Object v=entry.getValue();
    writeVal(null == k ? null : k.toString(),v);
  }
  if (!excludeOuter) {
    decLevel();
    if (sz > 0) {
      if (doIndent)       indent();
      writer.write("</lst>");
    }
  }
}
