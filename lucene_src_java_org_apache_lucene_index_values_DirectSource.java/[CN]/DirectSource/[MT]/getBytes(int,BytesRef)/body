{
  try {
    final int sizeToRead=position(docID);
    ref.grow(sizeToRead);
    data.readBytes(ref.bytes,0,sizeToRead);
    ref.length=sizeToRead;
    ref.offset=0;
    return ref;
  }
 catch (  IOException ex) {
    throw new IllegalStateException("failed to get value for docID: " + docID,ex);
  }
}
