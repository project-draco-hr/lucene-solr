{
  if (bufferedState != null) {
    restoreState(bufferedState);
    posIncAtt.setPositionIncrement(0);
    termAtt.setEmpty().append("hte");
    bufferedState=null;
    return true;
  }
 else   if (input.incrementToken()) {
    if (termAtt.toString().equals("the")) {
      bufferedState=captureState();
    }
    return true;
  }
 else {
    return false;
  }
}
