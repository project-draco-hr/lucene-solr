{
  if (indexArray == 0) {
    if (word != 0) {
      word>>>=8;
      wordShift+=8;
    }
    while (word == 0) {
      if (++i >= words) {
        curDocId=-1;
        return false;
      }
      word=arr[i];
      wordShift=-1;
    }
    shift();
  }
  int bitIndex=(indexArray & 0x0f) + wordShift;
  indexArray>>>=4;
  curDocId=(i << 6) + bitIndex;
  return true;
}
