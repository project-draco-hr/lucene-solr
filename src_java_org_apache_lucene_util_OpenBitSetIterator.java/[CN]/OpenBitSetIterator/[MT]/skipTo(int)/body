{
  indexArray=0;
  i=target >> 6;
  if (i >= words) {
    word=0;
    curDocId=-1;
    return false;
  }
  wordShift=target & 0x3f;
  word=arr[i] >>> wordShift;
  if (word != 0) {
    wordShift--;
  }
 else {
    while (word == 0) {
      if (++i >= words) {
        curDocId=-1;
        return false;
      }
      word=arr[i];
    }
    wordShift=-1;
  }
  shift();
  int bitIndex=(indexArray & 0x0f) + wordShift;
  indexArray>>>=4;
  curDocId=(i << 6) + bitIndex;
  return true;
}
