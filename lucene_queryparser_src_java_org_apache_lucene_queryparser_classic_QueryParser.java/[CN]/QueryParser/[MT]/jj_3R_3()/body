{
  if (jj_scan_token(TERM))   return true;
  jj_lookingAhead=true;
  jj_semLA=getToken(1).kind == TERM && allowedPostMultiTerm(getToken(2).kind);
  jj_lookingAhead=false;
  if (!jj_semLA || jj_3R_6())   return true;
  Token xsp;
  if (jj_3R_7())   return true;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_7()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
