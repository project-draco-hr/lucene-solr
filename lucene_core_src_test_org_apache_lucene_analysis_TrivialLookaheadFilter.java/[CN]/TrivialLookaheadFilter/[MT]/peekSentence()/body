{
  List<String> facts=new ArrayList<>();
  boolean haveSentence=false;
  do {
    if (peekToken()) {
      String term=new String(termAtt.buffer(),0,termAtt.length());
      facts.add(term + "-huh?");
      if (".".equals(term)) {
        haveSentence=true;
      }
    }
 else {
      haveSentence=true;
    }
  }
 while (!haveSentence);
  for (int x=0; x < facts.size(); x++) {
    positions.get(outputPos + x).setFact(facts.get(x));
  }
}
