{
  if (attributes != null) {
    for (    Node n : attributes) {
      if (n.multiValued)       putText(values,null,n.fieldName,true);
    }
  }
  if (hasText && multiValued)   putText(values,null,fieldName,true);
  if (childNodes != null) {
    for (    Node childNode : childNodes)     childNode.putNulls(values);
  }
}
