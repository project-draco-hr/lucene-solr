{
  long start=System.currentTimeMillis();
  while (true) {
    BytesRef item=null;
    try {
      item=reader.next();
    }
 catch (    Throwable t) {
      verifyChecksum(t,reader);
    }
    if (item == null) {
      break;
    }
    buffer.append(item);
    if (bufferBytesUsed.get() > ramBufferSize.bytes) {
      break;
    }
  }
  sortInfo.readTime+=(System.currentTimeMillis() - start);
  return buffer.size();
}
