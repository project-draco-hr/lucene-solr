{
  return new CodecProvider(){
    @Override public Codec getWriter(    SegmentWriteState state){
      return c;
    }
    @Override public Codec lookup(    String name){
      if (name.equals(c.name)) {
        return c;
      }
 else {
        return CodecProvider.getDefault().lookup(name);
      }
    }
  }
;
}
