{
  while (upto < input.length()) {
    char ch=input.charAt(upto);
    if (isJSONWhitespace(ch)) {
      upto++;
      continue;
    }
    if (ch != expected) {
      throw newParseException("expected '" + expected + "' but got '"+ ch+ "'");
    }
    upto++;
    return;
  }
  throw newParseException("expected '" + expected + "' but got EOF");
}
