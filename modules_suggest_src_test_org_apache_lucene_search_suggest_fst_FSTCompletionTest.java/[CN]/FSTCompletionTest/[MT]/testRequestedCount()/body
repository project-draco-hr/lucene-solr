{
  assertMatchEquals(completion.lookup(_TestUtil.stringToCharSequence("one",random),2),"one/0.0","oneness/1.0");
  assertMatchEquals(completion.lookup(_TestUtil.stringToCharSequence("four",random),2),"four/0.0","fourblah/1.0");
  assertMatchEquals(completion.lookup(_TestUtil.stringToCharSequence("four",random),4),"four/0.0","fourblah/1.0","fourteen/1.0","fourier/0.0");
  assertMatchEquals(completionAlphabetical.lookup(_TestUtil.stringToCharSequence("one",random),2),"one/0.0","oneness/1.0");
  FSTCompletion noPromotion=new FSTCompletion(completion.getFST(),true,false);
  assertMatchEquals(noPromotion.lookup(_TestUtil.stringToCharSequence("one",random),2),"oneness/1.0","onerous/1.0");
  assertMatchEquals(completionAlphabetical.lookup(_TestUtil.stringToCharSequence("one",random),2),"one/0.0","oneness/1.0");
}
