 else {
    String filler="";
    for (    QueryNode child : getChildren()) {
      sb.append(filler).append(child.toQueryString(escapeSyntaxParser));
      filler=" ";
    }
  }
  if (isDefaultField(this.field)) {
    return "( " + sb.toString() + " ) "+ withinSTR;
  }
 else {
    return this.field + ":(( " + sb.toString()+ " ) "+ withinSTR+ ")";
  }
