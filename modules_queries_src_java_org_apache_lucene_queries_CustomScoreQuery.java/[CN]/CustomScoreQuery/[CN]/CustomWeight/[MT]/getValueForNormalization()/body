{
  float sum=subQueryWeight.getValueForNormalization();
  for (int i=0; i < valSrcWeights.length; i++) {
    if (qStrict) {
      valSrcWeights[i].getValueForNormalization();
    }
 else {
      sum+=valSrcWeights[i].getValueForNormalization();
    }
  }
  sum*=getBoost() * getBoost();
  return sum;
}
