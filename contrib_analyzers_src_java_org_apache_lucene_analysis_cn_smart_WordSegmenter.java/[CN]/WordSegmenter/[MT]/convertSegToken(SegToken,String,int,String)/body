{
  Token result;
switch (st.wordType) {
case WordType.STRING:
case WordType.NUMBER:
case WordType.FULLWIDTH_NUMBER:
case WordType.FULLWIDTH_STRING:
    st.charArray=sentence.substring(st.startOffset,st.endOffset).toCharArray();
  break;
default :
break;
}
st=tokenFilter.filter(st);
result=new Token(st.charArray,0,st.charArray.length,st.startOffset + sentenceStartOffset,st.endOffset + sentenceStartOffset);
return result;
}
