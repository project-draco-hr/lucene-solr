{
  int nDocs=TestUtil.nextInt(random(),1,100);
  masterClient.deleteByQuery("*:*");
  for (int i=0; i < nDocs; i++) {
    SolrInputDocument doc=new SolrInputDocument();
    doc.addField("id",i);
    doc.addField("name","name = " + i);
    masterClient.add(doc);
  }
  masterClient.commit();
  return nDocs;
}
