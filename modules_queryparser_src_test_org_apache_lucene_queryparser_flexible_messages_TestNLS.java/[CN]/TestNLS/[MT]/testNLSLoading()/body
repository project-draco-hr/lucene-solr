{
  String message=NLS.getLocalizedMessage(MessagesTestBundle.Q0004E_INVALID_SYNTAX_ESCAPE_UNICODE_TRUNCATION,Locale.ENGLISH);
  if (!Locale.getDefault().getLanguage().equals("ja"))   assertEquals("Truncated unicode escape sequence.",message);
  message=NLS.getLocalizedMessage(MessagesTestBundle.Q0001E_INVALID_SYNTAX,Locale.ENGLISH,"XXX");
  if (!Locale.getDefault().getLanguage().equals("ja"))   assertEquals("Syntax Error: XXX",message);
}
