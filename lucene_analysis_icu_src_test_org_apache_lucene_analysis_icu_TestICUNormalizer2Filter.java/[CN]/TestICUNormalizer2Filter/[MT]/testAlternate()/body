{
  Analyzer a=new Analyzer(){
    @Override public TokenStreamComponents createComponents(    String fieldName){
      Tokenizer tokenizer=new MockTokenizer(MockTokenizer.WHITESPACE,false);
      return new TokenStreamComponents(tokenizer,new ICUNormalizer2Filter(tokenizer,Normalizer2.getInstance(null,"nfc",Normalizer2.Mode.DECOMPOSE)));
    }
  }
;
  assertAnalyzesTo(a,"\u00E9",new String[]{"\u0065\u0301"});
  a.close();
}
