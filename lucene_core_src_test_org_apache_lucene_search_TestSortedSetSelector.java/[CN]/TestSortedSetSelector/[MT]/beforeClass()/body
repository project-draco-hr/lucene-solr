{
  savedCodec=Codec.getDefault();
  int victim=random().nextInt(3);
switch (victim) {
case 0:
    Codec.setDefault(TestUtil.alwaysDocValuesFormat(new DirectDocValuesFormat()));
case 1:
  Codec.setDefault(TestUtil.alwaysDocValuesFormat(new DiskDocValuesFormat()));
default :
Codec.setDefault(TestUtil.alwaysDocValuesFormat(new Lucene45DocValuesFormat()));
}
}
