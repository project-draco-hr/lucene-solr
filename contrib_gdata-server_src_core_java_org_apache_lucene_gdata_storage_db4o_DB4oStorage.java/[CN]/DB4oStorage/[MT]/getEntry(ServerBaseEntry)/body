{
  if (entry == null)   throw new StorageException("can not retrieve entry -- is null");
  if (entry.getId() == null)   throw new StorageException("can not retrieve entry -- id is null");
  if (LOG.isInfoEnabled())   LOG.info("Retrieving entry for entryID: " + entry.getId());
  DB4oEntry retval=getInternalEntry(entry.getId());
  this.container.activate(retval.getEntry(),RENDER_ACTIVATION_DEPTH);
  return clearDynamicElements(retval.getEntry());
}
