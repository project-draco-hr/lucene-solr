{
  if (!indexLoaded) {
    if (indexDivisor < 0) {
      this.indexDivisor=-indexDivisor;
    }
 else {
      this.indexDivisor=indexDivisor;
    }
    this.totalIndexInterval=indexInterval * this.indexDivisor;
    Iterator<FieldIndexData> it=fields.values().iterator();
    while (it.hasNext()) {
      it.next().loadTermsIndex();
    }
    indexLoaded=true;
    in.close();
    termBytesReader=termBytes.freeze(true);
  }
}
