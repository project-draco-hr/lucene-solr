{
  if (position + 1 > length)   throw new IOException("Reading past end of file");
  int blockPos=(int)(position++ & DbIndexOutput.BLOCK_MASK);
  byte b=block.getData()[blockPos];
  if (blockPos + 1 == DbIndexOutput.BLOCK_LEN) {
    block.seek(position);
    block.get(directory);
  }
  return b;
}
