{
  SepTermState other=(SepTermState)super.clone();
  other.docIndex=(IntIndexInput.Index)docIndex.clone();
  if (freqIndex != null) {
    other.freqIndex=(IntIndexInput.Index)freqIndex.clone();
  }
  if (posIndex != null) {
    other.posIndex=(IntIndexInput.Index)posIndex.clone();
  }
  return other;
}
