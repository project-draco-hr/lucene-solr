{
  try {
    if (freqIn != null)     freqIn.close();
  }
  finally {
    try {
      if (docIn != null)       docIn.close();
    }
  finally {
      try {
        if (skipIn != null)         skipIn.close();
      }
  finally {
        try {
          if (posIn != null) {
            posIn.close();
          }
        }
  finally {
          if (payloadIn != null) {
            payloadIn.close();
          }
        }
      }
    }
  }
}
