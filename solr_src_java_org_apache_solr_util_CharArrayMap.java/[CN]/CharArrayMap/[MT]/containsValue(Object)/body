{
  if (value == null) {
    for (int i=0; i < keys.length; i++) {
      if (keys[i] != null && values[i] == null)       return true;
    }
    return false;
  }
  for (int i=0; i < values.length; i++) {
    Object val=values[i];
    if (val != null && value.equals(val))     return true;
  }
  return false;
}
