{
  if (len != text2.length)   return false;
  if (ignoreCase) {
    for (int i=0; i < len; i++) {
      if (Character.toLowerCase(text1[off + i]) != text2[i])       return false;
    }
  }
 else {
    for (int i=0; i < len; i++) {
      if (text1[off + i] != text2[i])       return false;
    }
  }
  return true;
}
