{
  final StringBuilder sb=new StringBuilder();
  sb.append(getClass().getSimpleName());
  sb.append(':');
  if (this.field.equals(field) == false) {
    sb.append("field=");
    sb.append(this.field);
    sb.append(':');
  }
  return sb.append('[').append(Arrays.toString(lowerPoint)).append(" TO ").append(Arrays.toString(upperPoint)).append(']').toString();
}
