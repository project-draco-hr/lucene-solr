{
  final StringBuilder sb=new StringBuilder();
  if (this.field.equals(field) == false) {
    sb.append(this.field);
    sb.append(':');
  }
  for (int i=0; i < numDims; i++) {
    if (i > 0) {
      sb.append(',');
    }
    sb.append('[');
    sb.append(toString(i,lowerPoint[i]));
    sb.append(" TO ");
    sb.append(toString(i,upperPoint[i]));
    sb.append(']');
  }
  return sb.toString();
}
