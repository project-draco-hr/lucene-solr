{
  int val=defaultVal;
  String paramS=params.get(paramName);
  if (paramS != null) {
    String trimmed=paramS.trim();
    if (trimmed.length() > 0) {
      try {
        val=Integer.parseInt(trimmed);
      }
 catch (      NumberFormatException nfe) {
        log.warn("Invalid value " + paramS + " passed for parameter "+ paramName+ "; expected integer!");
      }
    }
  }
  return val;
}
