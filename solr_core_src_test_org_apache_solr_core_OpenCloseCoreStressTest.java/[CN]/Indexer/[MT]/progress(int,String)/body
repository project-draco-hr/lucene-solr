{
  if (nextTime - System.currentTimeMillis() <= 0) {
    verbose(String.format(Locale.ROOT," s indexed: [run %,8d] [cycle %,8d] [last minute %,8d] Last core updated: %s. Seconds left in cycle %,4d",myId,docsThisCycle.get(),myId - lastCount,core,stopTime - (System.currentTimeMillis() / 1000)));
    lastCount=myId;
    nextTime+=(System.currentTimeMillis() / 1000) * 60;
  }
}
