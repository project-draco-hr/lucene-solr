{
  assertU(adoc("id","43","title","w11","description",""));
  assertU(commit());
  loadFeature("description-length3",FieldLengthFeature.class.getCanonicalName(),"{\"field\":\"description\"}");
  loadModel("description-model3",LinearModel.class.getCanonicalName(),new String[]{"description-length3"},"{\"weights\":{\"description-length3\":1.0}}");
  final SolrQuery query=new SolrQuery();
  query.setQuery("title:w11");
  query.add("fl","*, score");
  query.add("rows","4");
  query.add("rq","{!ltr model=description-model3 reRankDocs=8}");
  assertJQ("/query" + query.toQueryString(),"/response/docs/[0]/score==0.0");
}
