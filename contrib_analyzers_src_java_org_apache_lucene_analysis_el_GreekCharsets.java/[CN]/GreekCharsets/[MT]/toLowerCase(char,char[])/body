    }
    if (letter == '\u038C') {
      return '\u03BF';
    }
    if (letter == '\u038F') {
      return '\u03C9';
    }
  }
 else   if (charset == ISO) {
    if (letter >= 0xe1 && letter <= 0xf9) {
      if (letter == 0xf2) {
        return 0xf3;
      }
 else {
        return letter;
      }
    }
    if (letter == 0xdc) {
      return 0xe1;
