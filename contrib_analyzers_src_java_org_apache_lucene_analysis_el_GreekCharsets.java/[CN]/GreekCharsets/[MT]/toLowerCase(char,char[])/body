    if (letter >= 0xc1 && letter <= 0xd9) {
      return (char)(letter + 32);
    }
    if (letter == 0xb6) {
      return 0xe1;
    }
    if (letter == 0xb8) {
      return 0xe5;
    }
    if (letter == 0xb9) {
      return 0xe7;
    }
    if (letter == 0xba || letter == 0xda) {
      return 0xe9;
    }
    if (letter == 0xbe || letter == 0xdb) {
      return 0xf5;
    }
    if (letter == 0xbc) {
      return 0xef;
    }
    if (letter == 0xbf) {
      return 0xf9;
    }
  }
 else   if (charset == CP1253) {
    if (letter >= 0xe1 && letter <= 0xf9) {
      if (letter == 0xf2) {
        return 0xf3;
      }
 else {
        return letter;
      }
    }
    if (letter == 0xdc) {
