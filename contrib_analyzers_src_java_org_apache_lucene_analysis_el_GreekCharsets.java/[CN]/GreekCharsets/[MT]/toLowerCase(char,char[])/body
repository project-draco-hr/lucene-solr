{
  if (charset == UnicodeGreek) {
    if (letter >= '\u03B1' && letter <= '\u03C9') {
      if (letter == '\u03C2') {
        return '\u03C3';
      }
 else {
        return letter;
      }
    }
    if (letter == '\u03AC') {
      return '\u03B1';
    }
    if (letter == '\u03AD') {
      return '\u03B5';
    }
    if (letter == '\u03AE') {
      return '\u03B7';
