{
  datIn=input;
  boolean success=false;
  try {
    CodecUtil.checkHeader(datIn,Lucene41NumericDocValuesConsumer.CODEC_NAME,Lucene41NumericDocValuesConsumer.VERSION_START,Lucene41NumericDocValuesConsumer.VERSION_START);
    minValue=datIn.readLong();
    success=true;
  }
  finally {
    if (!success) {
      IOUtils.closeWhileHandlingException(datIn);
    }
  }
}
