{
  boolean success=false;
  final Source source;
  IndexInput input=null;
  try {
    input=datIn.clone();
    source=new PackedIntsSource(input,false,minValue);
    success=true;
    return source;
  }
  finally {
    if (!success) {
      IOUtils.closeWhileHandlingException(input,datIn);
    }
  }
}
