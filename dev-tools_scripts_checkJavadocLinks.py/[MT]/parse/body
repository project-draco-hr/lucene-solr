def parse(baseURL, html):
    global failures
    parser = FindHyperlinks(baseURL)
    try:
        parser.feed(html)
        parser.close()
    except HTMLParseError:
        parser.printFile()
        print '  WARNING: failed to parse:'
        traceback.print_exc()
        failures = True
        return ([], [])
    return (parser.links, parser.anchors)
