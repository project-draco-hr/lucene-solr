{
  CategoryPath p1=new CategoryPath(0,0);
  CategoryPath p2=new CategoryPath(1000,300);
  assertEquals(p1.hashCode(),p2.hashCode());
  p2.add("hi");
  assertEquals(false,p1.hashCode() == p2.hashCode());
  p1.clear();
  p1.add("hello");
  p1.add("world");
  p2.clear();
  p2.add("hello");
  p2.add("world");
  assertEquals(p1.hashCode(),p2.hashCode());
  p1.clear();
  p1.add("averylongcategoryname");
  p1.clear();
  p1.add("hi");
  p2.clear();
  p2.add("hi");
  assertEquals(p1.hashCode(),p2.hashCode());
  p1.clear();
  p1.add("hi");
  p2.clear();
  p2.add("hello");
  assertEquals(false,p1.hashCode() == p2.hashCode());
  p1.clear();
  p1.add("hi");
  p2.clear();
  p2.add("ho");
  assertEquals(false,p1.hashCode() == p2.hashCode());
}
