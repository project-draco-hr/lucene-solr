{
  assertJQ(req("q",joinPrefix + " from=dept_id_s to=dept_s fromIndex=fromCore}cat:dev","fl","id","debugQuery",random().nextBoolean() ? "true" : "false"),"/response=={'numFound':3,'start':0,'docs':[{'id':'1'},{'id':'4'},{'id':'5'}]}");
  assertJQ(req("q",joinPrefix + " from=dept_id_s to=dept_s fromIndex=fromCore}cat:dev","fl","id","fq","name:john","debugQuery",random().nextBoolean() ? "true" : "false"),"/response=={'numFound':1,'start':0,'docs':[{'id':'1'}]}");
}
