{
  for (int i=0; i < numTop; i++) {
    top[i].current=top[i].fields.next();
    if (top[i].current != null) {
      queue.add(top[i]);
    }
 else {
    }
  }
  numTop=0;
  if (queue.size() > 0) {
    while (true) {
      top[numTop++]=queue.pop();
      if (queue.size() == 0 || (queue.top()).current != top[0].current) {
        break;
      }
    }
    currentField=top[0].current;
  }
 else {
    currentField=null;
  }
  return currentField;
}
