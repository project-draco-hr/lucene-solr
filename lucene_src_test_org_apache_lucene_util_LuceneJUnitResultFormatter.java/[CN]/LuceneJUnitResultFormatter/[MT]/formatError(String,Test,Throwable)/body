{
  if (test != null) {
    endTest(test);
  }
  sb.append(formatTest(test) + type);
  sb.append(StringUtils.LINE_SEP);
  sb.append(error.getMessage());
  sb.append(StringUtils.LINE_SEP);
  String strace=JUnitTestRunner.getFilteredTrace(error);
  sb.append(strace);
  sb.append(StringUtils.LINE_SEP);
  sb.append(StringUtils.LINE_SEP);
}
