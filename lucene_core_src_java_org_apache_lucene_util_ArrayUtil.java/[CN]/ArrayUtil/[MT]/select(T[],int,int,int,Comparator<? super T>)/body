{
  new IntroSelector(){
    T pivot;
    @Override protected void swap(    int i,    int j){
      ArrayUtil.swap(arr,i,j);
    }
    @Override protected void setPivot(    int i){
      pivot=arr[i];
    }
    @Override protected int comparePivot(    int j){
      return comparator.compare(pivot,arr[j]);
    }
  }
.select(from,to,k);
}
