{
  if (hasNextCalled) {
    return actualTerm != null;
  }
  hasNextCalled=true;
  if (termsEnum == null) {
    return false;
  }
  while (true) {
    try {
      actualTerm=termsEnum.next();
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
    if (actualTerm == null) {
      return false;
    }
    try {
      if (isFrequent(termsEnum.docFreq())) {
        return true;
      }
    }
 catch (    IOException ioe) {
      throw new RuntimeException(ioe);
    }
  }
}
