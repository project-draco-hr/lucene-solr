{
  super.setUp();
  RAMDirectory directory=new RAMDirectory();
  IndexWriter writer=new IndexWriter(directory,new MockAnalyzer(),true,IndexWriter.MaxFieldLength.LIMITED);
  Document doc=new Document();
  doc.add(new Field(FN,"the quick brown fox jumps over the lazy ??? dog 493432 49344",Field.Store.NO,Field.Index.ANALYZED));
  writer.addDocument(doc);
  writer.optimize();
  writer.close();
  searcher=new IndexSearcher(directory,true);
}
