{
  delQuery=getVariableResolver().replaceTokens(delQuery);
  if (requestParameters.clean) {
    if (delQuery == null && !completeCleanDone.get()) {
      writer.doDeleteAll();
      completeCleanDone.set(true);
    }
 else     if (delQuery != null) {
      writer.deleteByQuery(delQuery);
    }
  }
}
