{
  if (entity.processor != null)   return entity.processor;
  EntityProcessor entityProcessor=null;
  if (entity.proc == null) {
    entityProcessor=new SqlEntityProcessor();
  }
 else {
    try {
      entityProcessor=(EntityProcessor)loadClass(entity.proc,dataImporter.getCore()).newInstance();
    }
 catch (    Exception e) {
      wrapAndThrow(SEVERE,e,"Unable to load EntityProcessor implementation for entity:" + entity.name);
    }
  }
  return entity.processor=new EntityProcessorWrapper(entityProcessor,this);
}
