{
  for (  Method method : TolerantUpdateProcessor.class.getMethods()) {
    if (method.getDeclaringClass().equals(Object.class)) {
      continue;
    }
    assertEquals("base class(es) has changed, TolerantUpdateProcessor needs updated to ensure it " + "overrides all solr update lifcycle methods with exception tracking: " + method.toString(),TolerantUpdateProcessor.class,method.getDeclaringClass());
  }
}
