{
  if (!(o instanceof GeoPath))   return false;
  GeoPath p=(GeoPath)o;
  if (points.size() != p.points.size())   return false;
  if (cutoffAngle != p.cutoffAngle)   return false;
  for (int i=0; i < points.size(); i++) {
    SegmentEndpoint point=points.get(i);
    SegmentEndpoint point2=p.points.get(i);
    if (!point.equals(point2))     return false;
  }
  return true;
}
