{
  for (  final SegmentEndpoint pathPoint : points) {
    if (pathPoint.intersects(plane,notablePoints,bounds)) {
      return true;
    }
  }
  for (  final PathSegment pathSegment : segments) {
    if (pathSegment.intersects(plane,notablePoints,bounds)) {
      return true;
    }
  }
  return false;
}
