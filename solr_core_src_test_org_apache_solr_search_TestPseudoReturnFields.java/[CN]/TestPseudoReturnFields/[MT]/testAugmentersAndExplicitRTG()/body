{
  for (  String id : Arrays.asList("42","99")) {
    for (    SolrParams p : Arrays.asList(params("fl","id,[explain],x_alias:[value v=10 t=int]"),params("fl","id","fl","[explain],x_alias:[value v=10 t=int]"),params("fl","id","fl","[explain]","fl","x_alias:[value v=10 t=int]"))) {
      assertQ(id + ": " + p,req(p,"qt","/get","id",id,"wt","xml"),"count(//doc)=1","//doc/str[@name='id']","//doc/int[@name='x_alias'][.=10]","//doc[count(*)=2]");
    }
  }
}
