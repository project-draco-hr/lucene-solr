{
  for (  String id : Arrays.asList("42","99")) {
    for (    SolrParams p : Arrays.asList(params("fl","log(val_i),val_i"),params("fl","log(val_i)","fl","val_i"))) {
      assertQ(id + " " + p,req(p,"qt","/get","wt","xml","id",id),"count(//doc)=1","//doc/double[@name='log(val_i)'][.='0.0']","//doc/int[@name='val_i'][.='1']","//doc[count(*)=2]");
    }
  }
}
