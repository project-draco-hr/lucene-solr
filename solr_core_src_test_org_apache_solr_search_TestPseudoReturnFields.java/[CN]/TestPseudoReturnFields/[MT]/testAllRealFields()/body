{
  for (  String fl : ALL_REAL_FIELDS) {
    assertQ("fl=" + fl + " ... all real fields",req("q","*:*","rows","1","fl",fl),"//result[@numFound='5']","//result/doc/str[@name='id']","//result/doc/int[@name='val_i']","//result/doc/str[@name='ssto']","//result/doc/str[@name='subject']","//result/doc[count(*)=4]");
  }
}
