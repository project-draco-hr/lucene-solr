{
  assertQ("fl=[docid],id",req("q","*:*","rows","1","fl","[docid],id"),"//result[@numFound='5']","//result/doc/int[@name='[docid]']","//result/doc/str[@name='id']","//result/doc[count(*)=2]");
  assertQ("fl=[docid],[explain],id",req("q","*:*","rows","1","fl","[docid],[explain],id"),"//result[@numFound='5']","//result/doc/int[@name='[docid]']","//result/doc/str[@name='[explain]']","//result/doc/str[@name='id']","//result/doc[count(*)=3]");
  assertQ("fl=[docid]&fl=[explain]&fl=id",req("q","*:*","rows","1","fl","[docid]","fl","[explain]","fl","id"),"//result[@numFound='5']","//result/doc/int[@name='[docid]']","//result/doc/str[@name='[explain]']","//result/doc/str[@name='id']","//result/doc[count(*)=3]");
}
