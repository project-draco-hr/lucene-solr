{
  for (  SolrParams p : Arrays.asList(params("fl","id,[docid],[explain],x_alias:[value v=10 t=int]"),params("fl","id","fl","[docid],[explain],x_alias:[value v=10 t=int]"),params("fl","id","fl","[docid]","fl","[explain]","fl","x_alias:[value v=10 t=int]"))) {
    assertQ(p.toString(),req(p,"q","*:*","rows","1"),"//result[@numFound='5']","//result/doc/str[@name='id']","//result/doc/int[@name='[docid]']","//result/doc/str[@name='[explain]']","//result/doc/int[@name='x_alias'][.=10]","//result/doc[count(*)=4]");
  }
}
