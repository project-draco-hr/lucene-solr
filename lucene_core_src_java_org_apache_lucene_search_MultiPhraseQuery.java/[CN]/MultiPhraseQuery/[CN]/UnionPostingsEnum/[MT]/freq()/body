{
  int doc=docID();
  if (doc != posQueueDoc) {
    posQueue.clear();
    for (    PostingsEnum sub : subs) {
      if (sub.docID() == doc) {
        int freq=sub.freq();
        for (int i=0; i < freq; i++) {
          posQueue.add(sub.nextPosition());
        }
      }
    }
    posQueue.sort();
    posQueueDoc=doc;
  }
  return posQueue.size();
}
