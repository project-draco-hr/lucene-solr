{
  assertJQ(req("json.nl","map","qt",rh,SpellCheckComponent.COMPONENT_NAME,"true","q","lowerfilt:lazy lowerfilt:brown",SpellingParams.SPELLCHECK_EXTENDED_RESULTS,"true"),"/spellcheck/suggestions=={'correctlySpelled':true}");
  assertJQ(req("json.nl","map","qt",rh,SpellCheckComponent.COMPONENT_NAME,"true","q","lakkle",SpellingParams.SPELLCHECK_EXTENDED_RESULTS,"true"),"/spellcheck/suggestions/correctlySpelled==false");
}
