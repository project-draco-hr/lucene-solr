{
  assertJQ(req("json.nl","map","qt",rh,SpellCheckComponent.COMPONENT_NAME,"true",SpellingParams.SPELLCHECK_BUILD,"true","q","documemt",SpellingParams.SPELLCHECK_COLLATE,"true"),"/spellcheck/collations/collation=='document'");
  assertJQ(req("json.nl","map","qt",rh,SpellCheckComponent.COMPONENT_NAME,"true","q","documemt lowerfilt:broen^4",SpellingParams.SPELLCHECK_COLLATE,"true"),"/spellcheck/collations/collation=='document lowerfilt:brown^4'");
  assertJQ(req("json.nl","map","qt",rh,SpellCheckComponent.COMPONENT_NAME,"true","q","documemtsss broens",SpellingParams.SPELLCHECK_COLLATE,"true"),"/spellcheck/collations/collation=='document brown'");
  assertJQ(req("json.nl","map","qt",rh,SpellCheckComponent.COMPONENT_NAME,"true","q","pixma",SpellingParams.SPELLCHECK_COLLATE,"true"),"/spellcheck/collations/collation=='pixmaa'");
}
