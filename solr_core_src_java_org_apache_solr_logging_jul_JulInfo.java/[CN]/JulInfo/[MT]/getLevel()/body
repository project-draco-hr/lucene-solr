{
  if (logger == null) {
    return null;
  }
  Level level=logger.getLevel();
  if (level != null) {
    return level.getName();
  }
  for (  Level l : LEVELS) {
    if (l == null) {
      continue;
    }
    if (logger.isLoggable(l)) {
      return l.getName();
    }
  }
  return Level.OFF.getName();
}
