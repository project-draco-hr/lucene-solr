{
  Automaton a=new RegExp("#*").toAutomaton(1000);
  assertTrue(a.toString().length() > 0);
  a=new RegExp("#+").toAutomaton(1000);
  assertTrue(a.toString().length() > 0);
  a=new RegExp("#{2,10}").toAutomaton(1000);
  assertTrue(a.toString().length() > 0);
  a=new RegExp("#?").toAutomaton(1000);
  assertTrue(a.toString().length() > 0);
}
