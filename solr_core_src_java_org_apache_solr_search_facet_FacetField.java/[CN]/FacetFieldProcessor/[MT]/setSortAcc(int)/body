{
  if (indexOrderAcc == null) {
    indexOrderAcc=new SortSlotAcc(fcontext);
  }
  String sortKey=freq.sortVariable;
  sortAcc=accMap.get(sortKey);
  if (sortAcc == null) {
    if ("count".equals(sortKey)) {
      sortAcc=countAcc;
    }
 else     if ("index".equals(sortKey)) {
      sortAcc=indexOrderAcc;
    }
  }
}
