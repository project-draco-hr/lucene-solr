{
  if (method == null || method.length() == 0)   return DEFAULT_METHOD;
switch (method) {
case "dv":
    return DV;
case "uif":
  return UIF;
case "dvhash":
return DVHASH;
case "enum":
return ENUM;
case "stream":
return STREAM;
case "smart":
return SMART;
default :
throw new SolrException(SolrException.ErrorCode.BAD_REQUEST,"Unknown FacetField method " + method);
}
}
