{
  int doc;
  while ((doc=disi.nextDoc()) != DocIdSetIterator.NO_MORE_DOCS) {
    multiDv.setDocument(doc);
    int segOrd=(int)multiDv.nextOrd();
    collect(doc,segOrd,toGlobal);
    if (segOrd < 0)     continue;
    for (; ; ) {
      segOrd=(int)multiDv.nextOrd();
      if (segOrd < 0)       break;
      collect(doc,segOrd,toGlobal);
    }
  }
}
