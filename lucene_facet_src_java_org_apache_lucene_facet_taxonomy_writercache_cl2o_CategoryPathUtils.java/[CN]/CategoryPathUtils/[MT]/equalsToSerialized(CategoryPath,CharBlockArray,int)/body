{
  int n=charBlockArray.charAt(offset++);
  if (cp.length != n) {
    return false;
  }
  if (cp.length == 0) {
    return true;
  }
  for (int i=0; i < cp.length; i++) {
    int len=charBlockArray.charAt(offset++);
    if (len != cp.components[i].length()) {
      return false;
    }
    if (!cp.components[i].equals(charBlockArray.subSequence(offset,offset + len))) {
      return false;
    }
    offset+=len;
  }
  return true;
}
