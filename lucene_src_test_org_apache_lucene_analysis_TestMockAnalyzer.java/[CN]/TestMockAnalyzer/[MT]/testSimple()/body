{
  Analyzer a=new MockAnalyzer(MockTokenizer.SIMPLE,true);
  assertAnalyzesTo(a,"a-bc123 defg+hijklmn567opqrstuv78wxy_z ",new String[]{"a","bc","defg","hijklmn","opqrstuv","wxy","z"});
  assertAnalyzesToReuse(a,"aba4cadaba-Shazam",new String[]{"aba","cadaba","shazam"});
  assertAnalyzesToReuse(a,"break+on/Letters",new String[]{"break","on","letters"});
}
