{
  Thread a=getRunnerThread(this.count);
  Thread b=getRunnerThread((this.count+=10));
  b.start();
  a.start();
  a.join();
  try {
    for (int i=1; i < this.count; i++) {
      ReferenceCounter<StorageQuery> innerQuery=this.controller.getStorageQuery();
      BaseEntry e=innerQuery.get().singleEntryQuery("" + i,feedId,this.configurator);
      assertNotNull(e);
      assertEquals("get entry for id" + i,"" + i,e.getId());
    }
  }
  finally {
    b.join();
  }
  ReferenceCounter<StorageQuery> query=this.controller.getStorageQuery();
  this.count+=10;
  for (int i=1; i < this.count; i++) {
    BaseEntry e=query.get().singleEntryQuery("" + i,feedId,this.configurator);
    assertEquals("get entry for id" + i,"" + i,e.getId());
  }
  BaseEntry e=query.get().singleEntryQuery("" + this.count,feedId,this.configurator);
  assertNull("not entry for ID",e);
  query.decrementRef();
}
