{
  SolrCore core=SolrCore.getSolrCore();
  SolrRequestHandler h1=core.getRequestHandler("/update/csv");
  assertNotNull(h1);
  SolrRequestHandler h2=core.getRequestHandler("/update/csv/");
  assertNotNull(h2);
  assertEquals(h1,h2);
  assertNull(core.getRequestHandler("/update/csv/asdgadsgas"));
}
