def packed64singleblock(bpv, f):
    values = (64 / bpv)
    f.write(('\n  PACKED_SINGLE_BLOCK_%d {\n\n' % bpv))
    f.write('    public int blockCount() {\n')
    f.write('      return 1;\n')
    f.write('     }\n\n')
    f.write('    public int valueCount() {\n')
    f.write(('      return %d;\n' % values))
    f.write('    }\n\n')
    p64sb_decode(bpv, 64)
    p64sb_encode(bpv, 64)
    f.write('  }')
