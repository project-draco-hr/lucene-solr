{
  int num=3 * RANDOM_MULTIPLIER;
  for (int i=0; i < num; i++) {
    automata.clear();
    terms.clear();
    for (int j=0; j < 5000; j++) {
      String randomString=_TestUtil.randomUnicodeString(random);
      terms.add(randomString);
      automata.add(BasicAutomata.makeString(randomString));
    }
    assertLexicon();
  }
}
