{
  if (other == this) {
    return true;
  }
  if (other instanceof TermAttribute) {
    initTermBuffer();
    TermAttributeImpl o=((TermAttributeImpl)other);
    o.initTermBuffer();
    if (termLength != o.termLength)     return false;
    for (int i=0; i < termLength; i++) {
      if (termBuffer[i] != o.termBuffer[i]) {
        return false;
      }
    }
    return true;
  }
  return false;
}
