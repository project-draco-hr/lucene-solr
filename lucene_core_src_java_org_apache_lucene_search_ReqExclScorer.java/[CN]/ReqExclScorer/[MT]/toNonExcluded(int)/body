{
  int exclDoc=exclApproximation.docID();
  for (; ; doc=reqApproximation.nextDoc()) {
    if (doc == NO_MORE_DOCS) {
      return NO_MORE_DOCS;
    }
    if (exclDoc < doc) {
      exclDoc=exclApproximation.advance(doc);
    }
    if (matches(doc,exclDoc,reqTwoPhaseIterator,exclTwoPhaseIterator)) {
      return doc;
    }
  }
}
