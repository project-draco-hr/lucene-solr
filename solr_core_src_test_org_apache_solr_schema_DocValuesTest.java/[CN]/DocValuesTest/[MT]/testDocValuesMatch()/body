{
  assertU(adoc("id","1","floatdv","2","intdv","3","doubledv","3.1","longdv","5","datedv","1995-12-31T23:59:59.999Z","stringdv","b"));
  assertU(adoc("id","2","floatdv","-5","intdv","4","doubledv","-4.3","longdv","2","datedv","1997-12-31T23:59:59.999Z","stringdv","a"));
  assertU(adoc("id","3","floatdv","3","intdv","1","doubledv","2.1","longdv","1","datedv","1996-12-31T23:59:59.999Z","stringdv","c"));
  assertU(adoc("id","4","floatdv","3","intdv","-1","doubledv","1.5","longdv","1","datedv","1996-12-31T23:59:59.999Z","stringdv","car"));
  assertU(commit());
  assertQ(req("q","stringdv:car","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=4]");
  assertQ(req("q","stringdv:[b TO d]","sort","id asc"),"//*[@numFound='3']","//result/doc[1]/int[@name='id'][.=1]","//result/doc[2]/int[@name='id'][.=3]","//result/doc[3]/int[@name='id'][.=4]");
  assertQ(req("q","stringdv:c*","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=3]","//result/doc[2]/int[@name='id'][.=4]");
  assertQ(req("q","stringdv:c?r","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=4]");
  assertQ(req("q","stringdv:/c[a-b]r/","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=4]");
  assertQ(req("q","floatdv:3","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=3]","//result/doc[2]/int[@name='id'][.=4]");
  assertQ(req("q","floatdv:[2 TO 3]","sort","id asc"),"//*[@numFound='3']","//result/doc[1]/int[@name='id'][.=1]","//result/doc[2]/int[@name='id'][.=3]","//result/doc[3]/int[@name='id'][.=4]");
  assertQ(req("q","floatdv:\"-5\"","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=2]");
  assertQ(req("q","floatdv:[-6 TO -4]","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=2]");
  assertQ(req("q","floatdv:[-6 TO 2.1]","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=1]","//result/doc[2]/int[@name='id'][.=2]");
  assertQ(req("q","intdv:1","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=3]");
  assertQ(req("q","intdv:[3 TO 4]","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=1]","//result/doc[2]/int[@name='id'][.=2]");
  assertQ(req("q","intdv:\"-1\"","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=4]");
  assertQ(req("q","intdv:[-1 TO 1]","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=3]","//result/doc[2]/int[@name='id'][.=4]");
  assertQ(req("q","longdv:1","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=3]","//result/doc[2]/int[@name='id'][.=4]");
  assertQ(req("q","longdv:[1 TO 2]","sort","id asc"),"//*[@numFound='3']","//result/doc[1]/int[@name='id'][.=2]","//result/doc[2]/int[@name='id'][.=3]","//result/doc[3]/int[@name='id'][.=4]");
  assertQ(req("q","doubledv:3.1","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=1]");
  assertQ(req("q","doubledv:[2 TO 3.3]","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=1]","//result/doc[2]/int[@name='id'][.=3]");
  assertQ(req("q","doubledv:\"-4.3\"","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=2]");
  assertQ(req("q","doubledv:[-6 TO -4]","sort","id asc"),"//*[@numFound='1']","//result/doc[1]/int[@name='id'][.=2]");
  assertQ(req("q","doubledv:[-6 TO 2.0]","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=2]","//result/doc[2]/int[@name='id'][.=4]");
}
