{
  final Map<String,Object> params=new HashMap<String,Object>();
  params.put("min","5.0f");
  params.put("max","10.0f");
  final Normalizer n=implTestMinMax(params,5.0f,10.0f);
  float value=8;
  assertEquals((value - 5f) / (10f - 5f),n.normalize(value),0.0001);
  value=100;
  assertEquals((value - 5f) / (10f - 5f),n.normalize(value),0.0001);
  value=150;
  assertEquals((value - 5f) / (10f - 5f),n.normalize(value),0.0001);
  value=-1;
  assertEquals((value - 5f) / (10f - 5f),n.normalize(value),0.0001);
  value=5;
  assertEquals((value - 5f) / (10f - 5f),n.normalize(value),0.0001);
}
