{
  final int n=100;
  Object dummy=new Object();
  Cache cache=new SimpleLRUCache(n);
  for (int i=0; i < n; i++) {
    cache.put(new Integer(i),dummy);
  }
  for (int i=0; i < n; i+=2) {
    assertNotNull(cache.get(new Integer(i)));
  }
  for (int i=n; i < n + (n / 2); i++) {
    cache.put(new Integer(i),dummy);
  }
  for (int i=0; i < n; i+=4) {
    assertNotNull(cache.get(new Integer(i)));
  }
  for (int i=n; i < n + (n * 3 / 4); i++) {
    cache.put(new Integer(i),dummy);
  }
  for (int i=0; i < n; i+=4) {
    assertNotNull(cache.get(new Integer(i)));
  }
}
