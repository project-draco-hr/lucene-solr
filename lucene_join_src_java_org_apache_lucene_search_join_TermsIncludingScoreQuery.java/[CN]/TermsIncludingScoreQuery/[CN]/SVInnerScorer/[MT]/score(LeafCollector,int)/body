{
  FakeScorer fakeScorer=new FakeScorer();
  collector.setScorer(fakeScorer);
  if (doc == -1) {
    doc=nextDocOutOfOrder();
  }
  while (doc < max) {
    fakeScorer.doc=doc;
    fakeScorer.score=scores[ords[scoreUpto]];
    collector.collect(doc);
    doc=nextDocOutOfOrder();
  }
  return doc != DocsEnum.NO_MORE_DOCS;
}
