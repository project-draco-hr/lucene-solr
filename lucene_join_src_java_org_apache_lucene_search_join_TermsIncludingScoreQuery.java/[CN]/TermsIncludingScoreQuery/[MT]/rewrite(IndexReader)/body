{
  final Query originalQueryRewrite=originalQuery.rewrite(reader);
  if (originalQueryRewrite != originalQuery) {
    Query rewritten=new TermsIncludingScoreQuery(field,multipleValuesPerDocument,terms,scores,ords,originalQueryRewrite,originalQuery);
    rewritten.setBoost(getBoost());
    return rewritten;
  }
 else {
    return this;
  }
}
