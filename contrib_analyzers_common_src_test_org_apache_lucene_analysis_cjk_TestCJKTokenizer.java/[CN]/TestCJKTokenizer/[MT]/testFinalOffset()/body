{
  checkCJKToken("??????",new TestToken[]{newToken("??????",0,2,CJKTokenizer.DOUBLE_TOKEN_TYPE)});
  checkCJKToken("??????   ",new TestToken[]{newToken("??????",0,2,CJKTokenizer.DOUBLE_TOKEN_TYPE)});
  checkCJKToken("test",new TestToken[]{newToken("test",0,4,CJKTokenizer.SINGLE_TOKEN_TYPE)});
  checkCJKToken("test   ",new TestToken[]{newToken("test",0,4,CJKTokenizer.SINGLE_TOKEN_TYPE)});
  checkCJKToken("??????test",new TestToken[]{newToken("??????",0,2,CJKTokenizer.DOUBLE_TOKEN_TYPE),newToken("test",2,6,CJKTokenizer.SINGLE_TOKEN_TYPE)});
  checkCJKToken("test??????    ",new TestToken[]{newToken("test",0,4,CJKTokenizer.SINGLE_TOKEN_TYPE),newToken("??????",4,6,CJKTokenizer.DOUBLE_TOKEN_TYPE)});
}
