{
  if (docID < pending.size()) {
    throw new IllegalArgumentException("DocValuesField \"" + fieldInfo.name + "\" appears more than once in this document (only one value is allowed per field)");
  }
  mergeValue(value);
  for (int i=pending.size(); i < docID; ++i) {
    pending.add(MISSING);
    mergeValue(0);
  }
  pending.add(value);
  updateBytesUsed();
}
