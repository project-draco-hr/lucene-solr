{
  final DocumentsWriterPerThread.DocWriter doc=consumer.finishDocument();
  final DocumentsWriterPerThread.DocWriter doc2;
  if (nextTermsHash != null) {
    doc2=nextTermsHash.consumer.finishDocument();
  }
 else {
    doc2=null;
  }
  if (doc == null) {
    return doc2;
  }
 else {
    doc.setNext(doc2);
    return doc;
  }
}
