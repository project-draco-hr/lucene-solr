{
  if (!trackAllocations)   return false;
  boolean any;
  final int numToFree;
  if (postingsFreeCount >= postingsFreeChunk)   numToFree=postingsFreeChunk;
 else   numToFree=postingsFreeCount;
  any=numToFree > 0;
  if (any) {
    Arrays.fill(postingsFreeList,postingsFreeCount - numToFree,postingsFreeCount,null);
    postingsFreeCount-=numToFree;
    postingsAllocCount-=numToFree;
    docWriter.bytesAllocated(-numToFree * bytesPerPosting);
    any=true;
  }
  if (nextTermsHash != null)   any|=nextTermsHash.freeRAM();
  return any;
}
