{
  try {
    ordsReader.get(docID,ordinals);
    for (int i=0; i < ordinals.length; i++) {
      ordinals.ints[i]=ordinalMap[ordinals.ints[i]];
    }
    return encode(ordinals);
  }
 catch (  IOException e) {
    throw new RuntimeException("error reading category ordinals for doc " + docID,e);
  }
}
