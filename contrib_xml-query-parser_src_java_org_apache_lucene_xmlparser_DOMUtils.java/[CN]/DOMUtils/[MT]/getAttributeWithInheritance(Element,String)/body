{
  String result=element.getAttribute(attributeName);
  if ((result == null) || ("".equals(result))) {
    Node n=element.getParentNode();
    if ((n == element) || (n == null)) {
      return null;
    }
    Element parent=(Element)n;
    return getAttributeWithInheritance(parent,attributeName);
  }
  return result;
}
