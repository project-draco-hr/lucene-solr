{
  CharacterRunAutomaton keepWords=new CharacterRunAutomaton(BasicOperations.complementLight(BasicOperations.unionLight(Arrays.asList(BasicAutomata.makeStringLight("foo"),BasicAutomata.makeStringLight("bar")))));
  Analyzer a=new MockAnalyzer(random(),MockTokenizer.SIMPLE,true,keepWords);
  assertAnalyzesTo(a,"quick foo brown bar bar fox foo",new String[]{"foo","bar","bar","foo"},new int[]{2,2,1,2});
}
