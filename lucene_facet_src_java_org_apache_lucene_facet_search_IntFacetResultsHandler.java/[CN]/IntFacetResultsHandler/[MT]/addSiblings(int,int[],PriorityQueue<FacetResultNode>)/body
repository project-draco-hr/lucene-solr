{
  FacetResultNode top=pq.top();
  int numResults=0;
  while (ordinal != TaxonomyReader.INVALID_ORDINAL) {
    int value=values[ordinal];
    if (value > top.value) {
      top.value=value;
      top.ordinal=ordinal;
      top=pq.updateTop();
      ++numResults;
    }
    ordinal=siblings[ordinal];
  }
  return numResults;
}
