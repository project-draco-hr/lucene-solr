{
  String endpoint="/schema/analysis/stopwords/german";
  assertJQ(endpoint + "/sch??n","/error/code==404");
  assertJPut(endpoint,JSONUtil.toJSON(Arrays.asList("sch??n")),"/responseHeader/status==0");
  assertJQ(endpoint + "/sch??n","/sch??n=='sch??n'");
  assertJDelete(endpoint + "/sch??n","/responseHeader/status==0");
  assertJQ(endpoint + "/sch??n","/error/code==404");
}
