{
  this.si=si;
  this.liveDocs=liveDocs;
  this.numDocs=numDocs;
  this.core=sr.core;
  core.incRef();
  this.segDocValues=sr.segDocValues;
  boolean success=false;
  try {
    fieldInfos=initFieldInfos();
    docValuesProducer=initDocValuesProducer();
    success=true;
  }
  finally {
    if (!success) {
      doClose();
    }
  }
}
