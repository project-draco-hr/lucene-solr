{
  try {
    core.decRef();
  }
  finally {
    Throwable t=null;
    for (    RefCount<DocValuesProducer> dvp : genDVProducers.values()) {
      try {
        dvp.decRef();
      }
 catch (      Throwable th) {
        if (t != null) {
          t=th;
        }
      }
    }
    genDVProducers.clear();
    dvProducers.clear();
    docValuesLocal.close();
    docsWithFieldLocal.close();
    IOUtils.reThrow(t);
  }
}
