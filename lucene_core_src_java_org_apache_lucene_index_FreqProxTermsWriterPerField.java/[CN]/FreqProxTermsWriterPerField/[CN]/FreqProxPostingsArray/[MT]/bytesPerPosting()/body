{
  int bytes=ParallelPostingsArray.BYTES_PER_POSTING + 2 * RamUsageEstimator.NUM_BYTES_INT;
  if (lastPositions != null) {
    bytes+=RamUsageEstimator.NUM_BYTES_INT;
  }
  if (lastOffsets != null) {
    bytes+=RamUsageEstimator.NUM_BYTES_INT;
  }
  if (termFreqs != null) {
    bytes+=RamUsageEstimator.NUM_BYTES_INT;
  }
  return bytes;
}
