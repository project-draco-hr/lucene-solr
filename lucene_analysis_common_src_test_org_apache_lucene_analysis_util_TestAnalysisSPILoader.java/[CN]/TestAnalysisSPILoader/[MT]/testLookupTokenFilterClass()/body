{
  assertEquals(LowerCaseFilterFactory.class,TokenFilterFactory.lookupClass("Lowercase"));
  assertEquals(LowerCaseFilterFactory.class,TokenFilterFactory.lookupClass("LOWERCASE"));
  assertEquals(LowerCaseFilterFactory.class,TokenFilterFactory.lookupClass("lowercase"));
  assertEquals(RemoveDuplicatesTokenFilterFactory.class,TokenFilterFactory.lookupClass("RemoveDuplicates"));
  assertEquals(RemoveDuplicatesTokenFilterFactory.class,TokenFilterFactory.lookupClass("REMOVEDUPLICATES"));
  assertEquals(RemoveDuplicatesTokenFilterFactory.class,TokenFilterFactory.lookupClass("removeduplicates"));
}
