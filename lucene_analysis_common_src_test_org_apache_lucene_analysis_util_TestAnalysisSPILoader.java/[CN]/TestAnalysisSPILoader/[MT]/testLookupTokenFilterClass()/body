{
  assertSame(LowerCaseFilterFactory.class,TokenFilterFactory.lookupClass("Lowercase"));
  assertSame(LowerCaseFilterFactory.class,TokenFilterFactory.lookupClass("LOWERCASE"));
  assertSame(LowerCaseFilterFactory.class,TokenFilterFactory.lookupClass("lowercase"));
  assertSame(RemoveDuplicatesTokenFilterFactory.class,TokenFilterFactory.lookupClass("RemoveDuplicates"));
  assertSame(RemoveDuplicatesTokenFilterFactory.class,TokenFilterFactory.lookupClass("REMOVEDUPLICATES"));
  assertSame(RemoveDuplicatesTokenFilterFactory.class,TokenFilterFactory.lookupClass("removeduplicates"));
}
