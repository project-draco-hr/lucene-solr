{
  try {
    TokenizerFactory.forName("sdfsdfsdfdsfsdfsdf",new HashMap<String,String>());
    fail();
  }
 catch (  IllegalArgumentException expected) {
  }
  try {
    TokenizerFactory.forName("!(**#$U*#$*",new HashMap<String,String>());
    fail();
  }
 catch (  IllegalArgumentException expected) {
  }
}
