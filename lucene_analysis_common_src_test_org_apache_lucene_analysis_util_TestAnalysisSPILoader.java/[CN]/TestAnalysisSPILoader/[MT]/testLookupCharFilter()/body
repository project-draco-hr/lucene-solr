{
  assertSame(HTMLStripCharFilterFactory.class,CharFilterFactory.forName("HTMLStrip",versionArgOnly()).getClass());
  assertSame(HTMLStripCharFilterFactory.class,CharFilterFactory.forName("HTMLSTRIP",versionArgOnly()).getClass());
  assertSame(HTMLStripCharFilterFactory.class,CharFilterFactory.forName("htmlstrip",versionArgOnly()).getClass());
}
