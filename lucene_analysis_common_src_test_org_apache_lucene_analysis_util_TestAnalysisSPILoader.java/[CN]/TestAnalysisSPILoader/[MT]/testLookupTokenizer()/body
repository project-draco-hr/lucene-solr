{
  assertSame(WhitespaceTokenizerFactory.class,TokenizerFactory.forName("Whitespace",versionArgOnly()).getClass());
  assertSame(WhitespaceTokenizerFactory.class,TokenizerFactory.forName("WHITESPACE",versionArgOnly()).getClass());
  assertSame(WhitespaceTokenizerFactory.class,TokenizerFactory.forName("whitespace",versionArgOnly()).getClass());
}
