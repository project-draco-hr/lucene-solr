{
  PrefixAndSuffixAwareTokenFilter ts=new PrefixAndSuffixAwareTokenFilter(new SingleTokenTokenStream(createToken("^",0,0)),new WhitespaceTokenizer(new StringReader("hello world")),new SingleTokenTokenStream(createToken("$",0,0)));
  Token token=new Token();
  assertNext(ts,token,"^",0,0);
  assertNext(ts,token,"hello",0,5);
  assertNext(ts,token,"world",6,11);
  assertNext(ts,token,"$",11,11);
  assertNull(ts.next(token));
}
