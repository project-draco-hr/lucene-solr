{
  Object v=getMapVal(key);
  if (v == null) {
    return def;
  }
 else {
    if (v instanceof List) {
      ArrayList<String> l=new ArrayList<>();
      for (      Object o : (List)v) {
        l.add(String.valueOf(o));
      }
      if (l.isEmpty())       return def;
      return l;
    }
 else {
      return singletonList(String.valueOf(v));
    }
  }
}
