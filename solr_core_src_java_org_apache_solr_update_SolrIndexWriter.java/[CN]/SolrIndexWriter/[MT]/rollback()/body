{
  Directory dir=getDirectory();
  try {
    while (true) {
      try {
        super.rollback();
      }
 catch (      ThreadInterruptedException e) {
        continue;
      }
      break;
    }
  }
  finally {
    isClosed=true;
    directoryFactory.release(dir);
    numCloses.incrementAndGet();
  }
}
