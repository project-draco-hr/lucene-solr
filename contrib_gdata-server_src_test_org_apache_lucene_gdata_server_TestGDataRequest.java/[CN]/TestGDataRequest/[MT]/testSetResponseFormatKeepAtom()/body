{
  this.control.expectAndDefaultReturn(this.request.getParameter("alt"),"fooBar");
  this.control.expectAndDefaultReturn(this.request.getPathInfo(),"/feed");
  this.control.replay();
  this.feedRequest.initializeRequest();
  assertEquals("ResponseFromat Atom",this.feedRequest.getRequestedResponseFormat(),OutputFormat.ATOM);
  this.control.reset();
}
