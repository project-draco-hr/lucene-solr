{
  this.control.expectAndReturn(this.request.getParameter("max-results"),null);
  this.control.replay();
  assertEquals("default value 25",25,this.feedRequest.getItemsPerPage());
  this.control.verify();
  this.control.reset();
  this.control.expectAndReturn(this.request.getParameter("max-results"),"24",2);
  this.control.replay();
  assertEquals("24 results",24,this.feedRequest.getItemsPerPage());
  this.control.verify();
  this.control.reset();
  this.control.expectAndReturn(this.request.getParameter("max-results"),"-1",2);
  this.control.replay();
  assertEquals("25 results",25,this.feedRequest.getItemsPerPage());
  this.control.verify();
  this.control.reset();
  this.control.expectAndReturn(this.request.getParameter("max-results"),"helloworld",2);
  this.control.replay();
  assertEquals("25 results",25,this.feedRequest.getItemsPerPage());
  this.control.verify();
  this.control.reset();
}
