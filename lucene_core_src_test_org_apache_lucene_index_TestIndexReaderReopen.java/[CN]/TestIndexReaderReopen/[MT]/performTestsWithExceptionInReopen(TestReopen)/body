{
  DirectoryReader index1=test.openReader();
  DirectoryReader index2=test.openReader();
  TestIndexReader.assertIndexEquals(index1,index2);
  try {
    refreshReader(index1,test,0,true);
    fail("Expected exception not thrown.");
  }
 catch (  Exception e) {
  }
  TestIndexReader.assertIndexEquals(index1,index2);
  index1.close();
  index2.close();
}
