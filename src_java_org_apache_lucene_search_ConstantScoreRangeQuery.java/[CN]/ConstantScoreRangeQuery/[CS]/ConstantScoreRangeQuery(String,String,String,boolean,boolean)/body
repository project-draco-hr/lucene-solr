{
  if (lowerVal == null) {
    includeLower=true;
  }
 else   if (includeLower && lowerVal.equals("")) {
    lowerVal=null;
  }
  if (upperVal == null) {
    includeUpper=true;
  }
  this.fieldName=fieldName.intern();
  this.lowerVal=lowerVal;
  this.upperVal=upperVal;
  this.includeLower=includeLower;
  this.includeUpper=includeUpper;
}
