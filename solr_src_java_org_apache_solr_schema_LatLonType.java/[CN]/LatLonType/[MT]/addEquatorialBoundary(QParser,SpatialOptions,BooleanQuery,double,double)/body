{
  SchemaField subSF;
  Query range;
  BooleanQuery tmpQ=new BooleanQuery();
  subSF=subField(options.field,LAT);
  range=subSF.getType().getRangeQuery(parser,subSF,String.valueOf(lowerLeft),"0",true,true);
  tmpQ.add(range,BooleanClause.Occur.SHOULD);
  range=subSF.getType().getRangeQuery(parser,subSF,"0",String.valueOf(upperRight),true,true);
  tmpQ.add(range,BooleanClause.Occur.SHOULD);
  result.add(tmpQ,BooleanClause.Occur.MUST);
}
