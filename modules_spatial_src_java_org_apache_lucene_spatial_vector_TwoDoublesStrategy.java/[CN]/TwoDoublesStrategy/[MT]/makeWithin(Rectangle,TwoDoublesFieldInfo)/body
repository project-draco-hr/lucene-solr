{
  Query qX=NumericRangeQuery.newDoubleRange(fieldInfo.getFieldNameX(),finfo.precisionStep,bbox.getMinX(),bbox.getMaxX(),true,true);
  Query qY=NumericRangeQuery.newDoubleRange(fieldInfo.getFieldNameY(),finfo.precisionStep,bbox.getMinY(),bbox.getMaxY(),true,true);
  BooleanQuery bq=new BooleanQuery();
  bq.add(qX,BooleanClause.Occur.MUST);
  bq.add(qY,BooleanClause.Occur.MUST);
  return bq;
}
