{
  if (null == in || "".equals(in.trim())) {
    return new HashMap<String,Float>();
  }
  String[] bb=in.trim().split("\\s+");
  Map<String,Float> out=new HashMap<String,Float>(7);
  for (  String s : bb) {
    String[] bbb=s.split("\\^");
    out.put(bbb[0],1 == bbb.length ? null : Float.valueOf(bbb[1]));
  }
  return out;
}
