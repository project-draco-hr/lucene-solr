{
  if (input.incrementToken()) {
    final String term=termAtt.term();
    if (!keywordAttr.isKeyword() && (exclusions == null || !exclusions.contains(term))) {
      final String s=stemmer.stem(term);
      if ((s != null) && !s.equals(term))       termAtt.setTermBuffer(s);
    }
    return true;
  }
 else {
    return false;
  }
}
