{
  super.init(args);
  String escapedTagsArg=args.get("escapedTags");
  if (null != escapedTagsArg) {
    Matcher matcher=TAG_NAME_PATTERN.matcher(escapedTagsArg);
    while (matcher.find()) {
      if (null == escapedTags) {
        escapedTags=new HashSet<String>();
      }
      escapedTags.add(matcher.group(0));
    }
  }
}
