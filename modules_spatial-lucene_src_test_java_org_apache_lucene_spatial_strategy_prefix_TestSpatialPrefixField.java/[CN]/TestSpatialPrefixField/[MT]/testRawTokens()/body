{
  List<String> docA=Arrays.asList("AAAAAA*","AAAAAB+");
  List<String> docB=Arrays.asList("A*","BB*");
  checkQuery("AAAAA","docA","docB");
  checkQuery("AAAAA*","docA","docB");
  checkQuery("AAAAA+","docA","docB");
  checkQuery("AA*","docB","docA");
  checkQuery("AAAAAAAAAAAA*","docA","docB");
  checkQuery("BC");
  checkQuery("XX");
  checkQuery("B","docB");
  checkQuery("BBBB","docB");
  checkQuery("B*","docB");
  checkQuery("BBBB*","docB");
}
