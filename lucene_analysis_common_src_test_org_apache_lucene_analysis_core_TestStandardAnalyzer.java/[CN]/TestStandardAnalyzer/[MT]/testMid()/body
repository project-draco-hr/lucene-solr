{
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"A:B",new String[]{"A:B"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"A::B",new String[]{"A","B"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"1.2",new String[]{"1.2"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"A.B",new String[]{"A.B"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"1..2",new String[]{"1","2"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"A..B",new String[]{"A","B"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"1,2",new String[]{"1,2"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"1,,2",new String[]{"1","2"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"A.:B",new String[]{"A","B"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"A:.B",new String[]{"A","B"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"1,.2",new String[]{"1","2"});
  BaseTokenStreamTestCase.assertAnalyzesTo(a,"1.,2",new String[]{"1","2"});
}
