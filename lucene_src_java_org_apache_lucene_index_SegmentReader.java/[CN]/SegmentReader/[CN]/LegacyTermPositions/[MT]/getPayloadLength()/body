{
  if (!any || postingsEnum == null) {
    return 0;
  }
 else {
    if (pendingPayload == null) {
      if (!postingsEnum.hasPayload()) {
        return 0;
      }
      pendingPayload=postingsEnum.getPayload();
    }
    if (pendingPayload == null) {
      return 0;
    }
    return pendingPayload.length;
  }
}
