{
  if (done) {
    return false;
  }
  while (true) {
    if (terms == null) {
      currentField=fields.next();
      if (currentField == null) {
        done=true;
        return false;
      }
      terms=fields.terms();
    }
    currentTerm=terms.next();
    if (currentTerm != null) {
      return true;
    }
 else {
      terms=null;
    }
  }
}
