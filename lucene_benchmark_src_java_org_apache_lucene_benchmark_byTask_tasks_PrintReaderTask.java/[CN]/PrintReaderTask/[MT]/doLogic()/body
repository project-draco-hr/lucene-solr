{
  Directory dir=getRunData().getDirectory();
  IndexReader r=null;
  if (userData == null)   r=IndexReader.open(dir);
 else   r=IndexReader.open(OpenReaderTask.findIndexCommit(dir,userData));
  System.out.println("--> numDocs:" + r.numDocs() + " dels:"+ r.numDeletedDocs());
  r.close();
  return 1;
}
