{
  StringBuffer buffer=new StringBuffer();
  buffer.append("Hit<");
  buffer.append(hits.toString());
  buffer.append(" [");
  buffer.append(hitNumber);
  buffer.append("] ");
  if (resolved) {
    buffer.append("resolved");
  }
 else {
    buffer.append("unresolved");
  }
  buffer.append(">");
  return buffer.toString();
}
