{
  assertEquals("incompatible change in compressionMode property","compressionMode",SchemaCodecFactory.COMPRESSION_MODE);
  doTestCompressionMode("BEST_SPEED","BEST_SPEED");
  doTestCompressionMode("BEST_COMPRESSION","BEST_COMPRESSION");
  doTestCompressionMode("best_speed","BEST_SPEED");
  doTestCompressionMode("best_compression","BEST_COMPRESSION");
}
