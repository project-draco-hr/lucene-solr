{
  RefCounted<SolrIndexSearcher> holder=new RefCounted<SolrIndexSearcher>(newSearcher){
    public void close(){
      try {
        resource.close();
      }
 catch (      IOException e) {
        log.severe("Error closing searcher:" + SolrException.toStr(e));
      }
    }
  }
;
  holder.incref();
  return holder;
}
