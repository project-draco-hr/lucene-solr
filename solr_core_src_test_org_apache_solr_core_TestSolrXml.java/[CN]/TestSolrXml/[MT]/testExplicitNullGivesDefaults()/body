{
  String solrXml="<solr>" + "<solrcloud>" + "<str name=\"host\">host</str>"+ "<int name=\"hostPort\">8983</int>"+ "<str name=\"hostContext\">solr</str>"+ "<null name=\"leaderVoteWait\"/>"+ "</solrcloud></solr>";
  NodeConfig cfg=SolrXmlConfig.fromString(loader,solrXml);
  assertEquals("leaderVoteWait",180000,cfg.getCloudConfig().getLeaderVoteWait());
}
