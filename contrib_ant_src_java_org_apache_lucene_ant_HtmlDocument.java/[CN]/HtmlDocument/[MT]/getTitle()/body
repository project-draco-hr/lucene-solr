{
  if (rawDoc == null) {
    return null;
  }
  String title="";
  NodeList nl=rawDoc.getElementsByTagName("title");
  if (nl.getLength() > 0) {
    Element titleElement=((Element)nl.item(0));
    Text text=(Text)titleElement.getFirstChild();
    if (text != null) {
      title=text.getData();
    }
  }
  return title;
}
