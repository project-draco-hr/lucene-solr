{
  DuplicateFilter df=new DuplicateFilter(KEY_FIELD);
  df.setKeepMode(DuplicateFilter.KeepMode.KM_USE_LAST_OCCURRENCE);
  ScoreDoc[] hits=searcher.search(tq,df,1000).scoreDocs;
  assertTrue("Filtered searching should have found some matches",hits.length > 0);
  for (  ScoreDoc hit : hits) {
    Document d=searcher.doc(hit.doc);
    String url=d.get(KEY_FIELD);
    DocsEnum td=_TestUtil.docs(random(),reader,KEY_FIELD,new BytesRef(url),MultiFields.getLiveDocs(reader),null,0);
    int lastDoc=0;
    while (td.nextDoc() != DocIdSetIterator.NO_MORE_DOCS) {
      lastDoc=td.docID();
    }
    assertEquals("Duplicate urls should return last doc",lastDoc,hit.doc);
  }
}
