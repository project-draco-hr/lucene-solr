{
switch (bits & TYPE_MASK) {
case BYTE_ARR:
case STRING:
    final int length=in.readVInt();
  in.skipBytes(length);
break;
case NUMERIC_INT:
case NUMERIC_FLOAT:
in.readInt();
break;
case NUMERIC_LONG:
case NUMERIC_DOUBLE:
in.readLong();
break;
default :
throw new AssertionError("Unknown type flag: " + Integer.toHexString(bits));
}
}
