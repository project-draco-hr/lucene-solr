{
  this.docBase=-1;
  bytes=new BytesRef();
  spare=new BytesRef();
  numStoredFields=new int[1];
  lengths=new int[1];
  IndexInput in=CompressingStoredFieldsReader.this.fieldsStream;
  in.seek(0);
  fieldsStream=new BufferedChecksumIndexInput(in);
  fieldsStream.seek(indexReader.getStartPointer(startDocId));
}
