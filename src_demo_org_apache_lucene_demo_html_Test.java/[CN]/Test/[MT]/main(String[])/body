{
  if ("-dir".equals(argv[0])) {
    String[] files=new File(argv[1]).list();
    java.util.Arrays.sort(files);
    for (int i=0; i < files.length; i++) {
      System.err.println(files[i]);
      File file=new File(argv[1],files[i]);
      parse(file);
    }
  }
 else   parse(new File(argv[0]));
}
