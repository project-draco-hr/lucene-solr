{
  ArrayList<FixedBitSet> bb=new ArrayList<>(rpp.length);
  for (  PhrasePositions pp : rpp) {
    FixedBitSet b=new FixedBitSet(tord.size());
    Integer ord;
    for (    Term t : pp.terms) {
      if ((ord=tord.get(t)) != null) {
        b.set(ord);
      }
    }
    bb.add(b);
  }
  return bb;
}
