{
  ArrayList<OpenBitSet> bb=new ArrayList<OpenBitSet>(rpp.length);
  for (  PhrasePositions pp : rpp) {
    OpenBitSet b=new OpenBitSet(tord.size());
    Integer ord;
    for (    Term t : pp.terms) {
      if ((ord=tord.get(t)) != null) {
        b.set(ord);
      }
    }
    bb.add(b);
  }
  return bb;
}
