{
  sloppyFreq=0.0f;
  if (!advanceMin(target)) {
    return NO_MORE_DOCS;
  }
  boolean restart=false;
  while (sloppyFreq == 0.0f) {
    while (min.doc < max.doc || restart) {
      restart=false;
      if (!advanceMin(max.doc)) {
        return NO_MORE_DOCS;
      }
    }
    sloppyFreq=phraseFreq();
    restart=true;
  }
  return max.doc;
}
