{
  HashMap<Term,Integer> tg=new HashMap<Term,Integer>();
  Term[] t=tord.keySet().toArray(new Term[0]);
  for (int i=0; i < bb.size(); i++) {
    DocIdSetIterator bits=bb.get(i).iterator();
    int ord;
    while ((ord=bits.nextDoc()) != NO_MORE_DOCS) {
      tg.put(t[ord],i);
    }
  }
  return tg;
}
