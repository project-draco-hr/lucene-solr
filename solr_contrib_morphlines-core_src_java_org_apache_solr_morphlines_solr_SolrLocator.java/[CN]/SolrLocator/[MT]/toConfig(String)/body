{
  String json="";
  if (key != null) {
    json=toJson(key) + " : ";
  }
  json+="{" + " collection : " + toJson(collectionName) + ", "+ " zkHost : "+ toJson(zkHost)+ ", "+ " solrUrl : "+ toJson(solrUrl)+ ", "+ " solrHomeDir : "+ toJson(solrHomeDir)+ ", "+ " batchSize : "+ toJson(batchSize)+ " "+ "}";
  return ConfigFactory.parseString(json);
}
