{
  NamedList<Object> nl=control.getResponse();
  @SuppressWarnings("unchecked") NamedList<Object> sc=(NamedList<Object>)nl.get("suggest");
  @SuppressWarnings("unchecked") NamedList<Object> sug=(NamedList<Object>)sc.get("suggestions");
  if (sug.size() == 0) {
    Assert.fail("Control data did not return any suggestions.");
  }
}
