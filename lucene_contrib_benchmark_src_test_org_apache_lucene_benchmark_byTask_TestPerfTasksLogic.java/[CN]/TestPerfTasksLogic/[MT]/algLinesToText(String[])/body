{
  String indent="  ";
  StringBuffer sb=new StringBuffer();
  for (int i=0; i < propLines.length; i++) {
    sb.append(indent).append(propLines[i]).append(NEW_LINE);
  }
  for (int i=0; i < algLines.length; i++) {
    sb.append(indent).append(algLines[i]).append(NEW_LINE);
  }
  return sb.toString();
}
