{
  final BytesRefBuilder spare=new BytesRefBuilder();
  final int size=size();
  final int[] indices=comp == null ? null : sort(comp);
  return new BytesRefIterator(){
    int pos=0;
    @Override public BytesRef next(){
      if (pos < size) {
        return get(spare,indices == null ? pos++ : indices[pos++]);
      }
      return null;
    }
  }
;
}
