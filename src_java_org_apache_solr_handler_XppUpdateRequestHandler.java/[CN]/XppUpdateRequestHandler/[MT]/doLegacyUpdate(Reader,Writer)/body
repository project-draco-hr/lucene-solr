{
  try {
    NamedList ignored=this.update(input);
    output.write("<result status=\"0\"></result>");
  }
 catch (  Exception ex) {
    try {
      XML.writeXML(output,"result",SolrException.toStr(ex),"status","1");
    }
 catch (    Exception ee) {
      log.severe("Error writing to output stream: " + ee);
    }
  }
}
