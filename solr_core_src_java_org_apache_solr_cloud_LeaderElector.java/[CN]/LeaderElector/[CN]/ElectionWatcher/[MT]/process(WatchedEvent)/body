{
  if (EventType.None.equals(event.getType())) {
    return;
  }
  if (canceled) {
    log.info("This watcher is not active anymore {}",leaderSeqPath);
    return;
  }
  try {
    checkIfIamLeader(seq,context,true);
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    log.warn("",e);
  }
catch (  IOException e) {
    log.warn("",e);
  }
catch (  Exception e) {
    log.warn("",e);
  }
}
