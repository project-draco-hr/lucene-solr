{
  IOException ioe=null;
  for (int i=0; i < subReaders.length; i++) {
    try {
      subReaders[i].decRef();
    }
 catch (    IOException e) {
      if (ioe == null)       ioe=e;
    }
  }
  FieldCache.DEFAULT.purge(this);
  if (writer != null) {
    writer.deleteUnusedFiles();
  }
  if (ioe != null)   throw ioe;
}
