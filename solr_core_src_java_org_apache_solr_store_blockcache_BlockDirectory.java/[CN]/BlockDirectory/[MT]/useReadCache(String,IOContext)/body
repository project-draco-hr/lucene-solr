{
  if (!blockCacheReadEnabled) {
    return false;
  }
  if (blockCacheFileTypes != null && !isCachableFile(name)) {
    return false;
  }
switch (context.context) {
case MERGE:
{
      return cacheMerges;
    }
case READ:
{
    if (context.readOnce) {
      return cacheReadOnce;
    }
 else {
      return true;
    }
  }
default :
{
  return true;
}
}
}
