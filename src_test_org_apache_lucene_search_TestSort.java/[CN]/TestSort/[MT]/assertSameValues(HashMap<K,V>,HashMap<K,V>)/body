{
  int n=m1.size();
  int m=m2.size();
  assertEquals(n,m);
  Iterator<K> iter=m1.keySet().iterator();
  while (iter.hasNext()) {
    K key=iter.next();
    V o1=m1.get(key);
    V o2=m2.get(key);
    if (o1 instanceof Float) {
      assertEquals(((Float)o1).floatValue(),((Float)o2).floatValue(),1e-6);
    }
 else {
      assertEquals(m1.get(key),m2.get(key));
    }
  }
}
