{
  assertAnalyzesTo(analyzer,"??????",new String[]{"??????"},new int[]{0},new int[]{2},new String[]{"<DOUBLE>"},new int[]{1});
  assertAnalyzesTo(analyzer,"??????   ",new String[]{"??????"},new int[]{0},new int[]{2},new String[]{"<DOUBLE>"},new int[]{1});
  assertAnalyzesTo(analyzer,"test",new String[]{"test"},new int[]{0},new int[]{4},new String[]{"<ALPHANUM>"},new int[]{1});
  assertAnalyzesTo(analyzer,"test   ",new String[]{"test"},new int[]{0},new int[]{4},new String[]{"<ALPHANUM>"},new int[]{1});
  assertAnalyzesTo(analyzer,"??????test",new String[]{"??????","test"},new int[]{0,2},new int[]{2,6},new String[]{"<DOUBLE>","<ALPHANUM>"},new int[]{1,1});
  assertAnalyzesTo(analyzer,"test??????    ",new String[]{"test","??????"},new int[]{0,4},new int[]{4,6},new String[]{"<ALPHANUM>","<DOUBLE>"},new int[]{1,1});
}
