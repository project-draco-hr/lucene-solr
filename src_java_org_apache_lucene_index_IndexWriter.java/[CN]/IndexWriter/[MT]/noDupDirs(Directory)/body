{
  HashSet dups=new HashSet();
  for (int i=0; i < dirs.length; i++) {
    if (dups.contains(dirs[i]))     throw new IllegalArgumentException("Directory " + dirs[i] + " appears more than once");
    if (dirs[i] == directory)     throw new IllegalArgumentException("Cannot add directory to itself");
    dups.add(dirs[i]);
  }
}
