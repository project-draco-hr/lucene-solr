{
  Vector result=new Vector();
  if (!directory.fileExists(IndexFileNames.DELETABLE))   return result;
  IndexInput input=directory.openInput(IndexFileNames.DELETABLE);
  try {
    for (int i=input.readInt(); i > 0; i--)     result.addElement(input.readString());
  }
  finally {
    input.close();
  }
  return result;
}
