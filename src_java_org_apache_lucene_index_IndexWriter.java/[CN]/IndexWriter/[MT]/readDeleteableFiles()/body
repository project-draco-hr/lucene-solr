{
  Vector result=new Vector();
  if (!directory.fileExists(Constants.INDEX_DELETABLE_FILENAME))   return result;
  IndexInput input=directory.openInput(Constants.INDEX_DELETABLE_FILENAME);
  try {
    for (int i=input.readInt(); i > 0; i--)     result.addElement(input.readString());
  }
  finally {
    input.close();
  }
  return result;
}
