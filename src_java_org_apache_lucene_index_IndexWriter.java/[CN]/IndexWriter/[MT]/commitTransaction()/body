{
  autoCommit=localAutoCommit;
  boolean success=false;
  try {
    checkpoint();
    success=true;
  }
  finally {
    if (!success) {
      rollbackTransaction();
    }
  }
  if (!autoCommit)   deleter.decRef(localRollbackSegmentInfos);
  localRollbackSegmentInfos=null;
  deleter.checkpoint(segmentInfos,autoCommit);
}
