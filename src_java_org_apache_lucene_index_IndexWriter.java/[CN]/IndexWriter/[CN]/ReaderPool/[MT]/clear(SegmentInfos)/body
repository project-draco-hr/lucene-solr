{
  if (infos == null) {
    for (    Map.Entry<SegmentInfo,SegmentReader> ent : readerMap.entrySet()) {
      ent.getValue().hasChanges=false;
    }
  }
 else {
    for (    final SegmentInfo info : infos) {
      if (readerMap.containsKey(info)) {
        readerMap.get(info).hasChanges=false;
      }
    }
  }
}
