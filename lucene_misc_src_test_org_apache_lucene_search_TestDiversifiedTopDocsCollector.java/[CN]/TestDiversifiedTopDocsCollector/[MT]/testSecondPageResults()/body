{
  int numberOfTracksPerCompilation=10;
  int numberOfCompilations=2;
  int requiredMaxHitsPerArtist=1;
  DiversifiedTopDocsCollector tdc=doDiversifiedSearch(numberOfTracksPerCompilation * numberOfCompilations,requiredMaxHitsPerArtist);
  ScoreDoc[] volume2=tdc.topDocs(numberOfTracksPerCompilation,numberOfTracksPerCompilation).scoreDocs;
  assertEquals(numberOfTracksPerCompilation,volume2.length);
  assertTrue(getMaxNumRecordsPerArtist(volume2) <= requiredMaxHitsPerArtist);
}
