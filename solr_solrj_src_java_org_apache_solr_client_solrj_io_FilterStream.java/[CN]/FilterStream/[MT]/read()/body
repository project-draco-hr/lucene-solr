{
  a=streamA.read();
  if (b == null) {
    b=streamB.read();
  }
  while (true) {
    if (a.EOF) {
      return a;
    }
    if (b.EOF) {
      return b;
    }
    int i=comp.compare(a,b);
    if (i == 0) {
      return a;
    }
 else     if (i < 0) {
      a=streamA.read();
    }
 else {
      b=streamB.read();
    }
  }
}
