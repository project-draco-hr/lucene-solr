{
  final IndexInput freqIn=this.freqIn;
  final int docs[]=this.docs;
  final int freqs[]=this.freqs;
  int docAc=accum;
  for (int i=0; i < size; i++) {
    final int code=freqIn.readVInt();
    docAc+=code >>> 1;
    freqs[i]=readFreq(freqIn,code);
    docs[i]=docAc;
  }
  accum=docAc;
  return size;
}
