{
  while (true) {
    if (ord == limit) {
      return doc=NO_MORE_DOCS;
    }
    ord++;
    final int code=freqIn.readVInt();
    accum+=code >>> 1;
    if ((code & 1) != 0) {
      freq=1;
    }
 else {
      freq=freqIn.readVInt();
    }
    posPendingCount+=freq;
    if (liveDocs == null || liveDocs.get(accum)) {
      break;
    }
  }
  position=0;
  return (doc=accum);
}
