{
  Explanation exp=null;
  try {
    exp=s.explain(q,doc);
  }
 catch (  IOException e) {
    throw new RuntimeException("exception in hitcollector of [[" + d + "]] for #"+ doc,e);
  }
  TestCase.assertNotNull("Explanation of [[" + d + "]] for #"+ doc+ " is null",exp);
  TestCase.assertEquals("Score of [[" + d + "]] for #"+ doc+ " does not match explanation: "+ exp.toString(),score,exp.getValue(),SCORE_TOLERANCE_DELTA);
}
