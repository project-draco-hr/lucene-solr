{
  IOException keep=null;
  if (tvx != null)   try {
    tvx.close();
  }
 catch (  IOException e) {
    keep=e;
  }
  if (tvd != null)   try {
    tvd.close();
  }
 catch (  IOException e) {
    if (keep == null)     keep=e;
  }
  if (tvf != null)   try {
    tvf.close();
  }
 catch (  IOException e) {
    if (keep == null)     keep=e;
  }
  if (keep != null)   throw (IOException)keep.fillInStackTrace();
}
