{
  return ImmutableList.of(new CoreDescriptor(cc,coreName,cc.getCoreRootDirectory().resolve(coreName),CoreDescriptor.CORE_DATADIR,dataDir,CoreDescriptor.CORE_CONFIG,solrConfig,CoreDescriptor.CORE_SCHEMA,schema,CoreDescriptor.CORE_COLLECTION,System.getProperty("collection","collection1"),CoreDescriptor.CORE_SHARD,System.getProperty("shard","shard1")));
