{
switch (key) {
case SOLR_HOST:
    return System.getProperty("host");
case SOLR_HOSTPORT:
  return System.getProperty("hostPort","");
case SOLR_HOSTCONTEXT:
return System.getProperty("hostContext","");
case SOLR_DISTRIBUPDATESOTIMEOUT:
return "30000";
case SOLR_ZKCLIENTTIMEOUT:
return System.getProperty("zkClientTimeout","30000");
case SOLR_DISTRIBUPDATECONNTIMEOUT:
return "30000";
case SOLR_SHARESCHEMA:
return System.getProperty("shareSchema","false");
}
return null;
}
