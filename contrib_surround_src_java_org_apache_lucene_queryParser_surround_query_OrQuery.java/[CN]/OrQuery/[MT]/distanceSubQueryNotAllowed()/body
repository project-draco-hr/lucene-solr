{
  Iterator sqi=getSubQueriesIterator();
  while (sqi.hasNext()) {
    SrndQuery leq=(SrndQuery)sqi.next();
    if (leq instanceof DistanceSubQuery) {
      String m=((DistanceSubQuery)leq).distanceSubQueryNotAllowed();
      if (m != null) {
        return m;
      }
    }
 else {
      return "subquery not allowed: " + leq.toString();
    }
  }
  return null;
}
