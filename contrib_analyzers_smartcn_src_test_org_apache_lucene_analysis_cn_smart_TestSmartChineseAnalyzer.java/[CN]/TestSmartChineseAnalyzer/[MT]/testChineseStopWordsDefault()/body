{
  Analyzer ca=new SmartChineseAnalyzer(Version.LUCENE_CURRENT);
  String sentence="??????????????????????????????";
  String result[]={"???","??????","???","??????","???","??????"};
  assertAnalyzesTo(ca,sentence,result);
  ca=new SmartChineseAnalyzer(Version.LUCENE_CURRENT,SmartChineseAnalyzer.getDefaultStopSet());
  assertAnalyzesTo(ca,sentence,result);
}
