{
  int index=0;
  for (  Metric metric : _metrics) {
    if (metric.getIdentifier().startsWith("count(")) {
      if (id.startsWith("count(")) {
        return "count";
      }
    }
 else {
      if (id.equals(_metrics[index].getIdentifier())) {
        return "facet_" + index;
      }
      ++index;
    }
  }
  return "index";
}
