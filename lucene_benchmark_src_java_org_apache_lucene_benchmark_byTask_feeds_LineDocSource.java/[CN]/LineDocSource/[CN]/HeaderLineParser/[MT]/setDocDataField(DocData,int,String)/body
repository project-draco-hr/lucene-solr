{
switch (posToF[position]) {
case NAME:
    docData.setName(text);
  break;
case TITLE:
docData.setTitle(text);
break;
case DATE:
docData.setDate(text);
break;
case BODY:
docData.setBody(text);
break;
case PROP:
Properties p=docData.getProps();
if (p == null) {
p=new Properties();
docData.setProps(p);
}
p.setProperty(header[position],text);
break;
}
}
