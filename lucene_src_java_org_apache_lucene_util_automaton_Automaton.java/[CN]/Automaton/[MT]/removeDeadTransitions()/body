{
  clearHashCode();
  if (isSingleton())   return;
  Set<State> states=getStates();
  Set<State> live=getLiveStates(states);
  for (  State s : states) {
    Set<Transition> st=s.transitions;
    s.resetTransitions();
    for (    Transition t : st)     if (live.contains(t.to))     s.transitions.add(t);
  }
  reduce();
}
