{
  State s=new State();
  s.transitions.add(new Transition(Character.MIN_VALUE,Character.MAX_VALUE,s));
  for (  State p : getStates()) {
    int maxi=Character.MIN_VALUE;
    for (    Transition t : p.getSortedTransitions(false)) {
      if (t.min > maxi)       p.transitions.add(new Transition((char)maxi,(char)(t.min - 1),s));
      if (t.max + 1 > maxi)       maxi=t.max + 1;
    }
    if (maxi <= Character.MAX_VALUE)     p.transitions.add(new Transition((char)maxi,Character.MAX_VALUE,s));
  }
}
