{
  super.setUp();
  mDirectory=newDirectory();
  final RandomIndexWriter writer=new RandomIndexWriter(random,mDirectory,newIndexWriterConfig(TEST_VERSION_CURRENT,new MockAnalyzer(MockTokenizer.SIMPLE,true,MockTokenFilter.ENGLISH_STOPSET,true)).setMergePolicy(newInOrderLogMergePolicy()));
  addDocument(writer,"1","I think it should work.");
  addDocument(writer,"2","I think it should work.");
  addDocument(writer,"3","I think it should work.");
  addDocument(writer,"4","I think it should work.");
  reader=writer.getReader();
  writer.close();
  searcher=newSearcher(reader);
}
