{
  Node node=current;
  for (; ; ) {
    if (node == root) {
      err.println("Stray end tag ignored: " + name + " line "+ tok.line+ " column "+ tok.column);
      return;
    }
 else     if (name.equals(((Element)node).getTagName())) {
      current=node.getParentNode();
      return;
    }
 else {
      node=node.getParentNode();
    }
  }
}
