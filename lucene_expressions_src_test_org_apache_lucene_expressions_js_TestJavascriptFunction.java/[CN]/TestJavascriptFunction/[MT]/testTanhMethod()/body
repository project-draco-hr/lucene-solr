{
  assertEvaluatesTo("tanh(0)",0);
  assertEvaluatesTo("tanh(-1)",-0.76159415595);
  assertEvaluatesTo("tanh(1)",0.76159415595);
  assertEvaluatesTo("tanh(-0.5)",-0.46211715726);
  assertEvaluatesTo("tanh(0.5)",0.46211715726);
  assertEvaluatesTo("tanh(-12.3456789)",-0.99999999996);
  assertEvaluatesTo("tanh(12.3456789)",0.99999999996);
}
