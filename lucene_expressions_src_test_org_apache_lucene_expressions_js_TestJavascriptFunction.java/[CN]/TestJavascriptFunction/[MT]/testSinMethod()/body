{
  assertEvaluatesTo("sin(0)",0);
  assertEvaluatesTo("sin(" + Math.PI / 2 + ")",1);
  assertEvaluatesTo("sin(" + -Math.PI / 2 + ")",-1);
  assertEvaluatesTo("sin(" + Math.PI / 4 + ")",0.7071068);
  assertEvaluatesTo("sin(" + -Math.PI / 4 + ")",-0.7071068);
  assertEvaluatesTo("sin(" + Math.PI * 2 / 3 + ")",0.8660254);
  assertEvaluatesTo("sin(" + -Math.PI * 2 / 3 + ")",-0.8660254);
  assertEvaluatesTo("sin(" + Math.PI / 6 + ")",0.5);
  assertEvaluatesTo("sin(" + -Math.PI / 6 + ")",-0.5);
}
