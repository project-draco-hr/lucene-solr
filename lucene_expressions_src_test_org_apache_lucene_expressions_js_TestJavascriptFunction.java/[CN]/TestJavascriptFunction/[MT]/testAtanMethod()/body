{
  assertEvaluatesTo("atan(-1.732050808)",-Math.PI / 3);
  assertEvaluatesTo("atan(-1)",-Math.PI / 4);
  assertEvaluatesTo("atan(-0.577350269)",-Math.PI / 6);
  assertEvaluatesTo("atan(0)",0);
  assertEvaluatesTo("atan(0.577350269)",Math.PI / 6);
  assertEvaluatesTo("atan(1)",Math.PI / 4);
  assertEvaluatesTo("atan(1.732050808)",Math.PI / 3);
}
