{
  assertEvaluatesTo("atan2(+0,+0)",+0.0);
  assertEvaluatesTo("atan2(+0,-0)",+Math.PI);
  assertEvaluatesTo("atan2(-0,+0)",-0.0);
  assertEvaluatesTo("atan2(-0,-0)",-Math.PI);
  assertEvaluatesTo("atan2(2,2)",Math.PI / 4);
  assertEvaluatesTo("atan2(-2,2)",-Math.PI / 4);
  assertEvaluatesTo("atan2(2,-2)",Math.PI * 3 / 4);
  assertEvaluatesTo("atan2(-2,-2)",-Math.PI * 3 / 4);
}
