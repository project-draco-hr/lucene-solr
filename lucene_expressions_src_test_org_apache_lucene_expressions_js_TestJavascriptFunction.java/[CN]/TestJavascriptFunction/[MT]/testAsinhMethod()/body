{
  assertEvaluatesTo("asinh(-1234567.89)",-14.719378760740035);
  assertEvaluatesTo("asinh(-2.5)",-1.6472311463710958);
  assertEvaluatesTo("asinh(-1)",-0.8813735870195429);
  assertEvaluatesTo("asinh(0)",0);
  assertEvaluatesTo("asinh(1)",0.8813735870195429);
  assertEvaluatesTo("asinh(2.5)",1.6472311463710958);
  assertEvaluatesTo("asinh(1234567.89)",14.719378760740035);
}
