{
  assertEvaluatesTo("sinh(0)",0);
  assertEvaluatesTo("sinh(-1)",-1.1752011936438014);
  assertEvaluatesTo("sinh(1)",1.1752011936438014);
  assertEvaluatesTo("sinh(-0.5)",-0.52109530549);
  assertEvaluatesTo("sinh(0.5)",0.52109530549);
  assertEvaluatesTo("sinh(-12.3456789)",-114982.09728236674);
  assertEvaluatesTo("sinh(12.3456789)",114982.09728236674);
}
