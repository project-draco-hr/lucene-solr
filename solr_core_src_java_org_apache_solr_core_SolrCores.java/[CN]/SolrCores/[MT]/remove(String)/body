{
synchronized (modifyLock) {
    SolrCore tmp=cores.remove(name);
    SolrCore ret=null;
    ret=(ret == null) ? tmp : ret;
    tmp=transientCores.remove(name);
    ret=(ret == null) ? tmp : ret;
    tmp=createdCores.remove(name);
    ret=(ret == null) ? tmp : ret;
    dynamicDescriptors.remove(name);
    return ret;
  }
}
