{
  Random random=random();
  checkRandomData(random,new Analyzer(){
    @Override protected TokenStreamComponents createComponents(    String fieldName,    Reader reader){
      Tokenizer tokenizer=new JapaneseTokenizer(reader,readDict(),false,Mode.SEARCH);
      TokenStream graph=new MockGraphTokenFilter(random(),tokenizer);
      return new TokenStreamComponents(tokenizer,graph);
    }
  }
,100 * RANDOM_MULTIPLIER,8192);
}
