{
  final StringBuilder buffer=new StringBuilder("ParallelReader(");
  final Iterator<AtomicIndexReader> iter=readers.iterator();
  if (iter.hasNext()) {
    buffer.append(iter.next());
  }
  while (iter.hasNext()) {
    buffer.append(", ").append(iter.next());
  }
  buffer.append(')');
  return buffer.toString();
}
