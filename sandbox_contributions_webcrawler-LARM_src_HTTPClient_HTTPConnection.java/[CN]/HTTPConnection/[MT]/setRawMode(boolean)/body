{
  String[] modules={"HTTPClient.CookieModule","HTTPClient.RedirectionModule","HTTPClient.AuthorizationModule","HTTPClient.DefaultModule","HTTPClient.TransferEncodingModule","HTTPClient.ContentMD5Module","HTTPClient.ContentEncodingModule"};
  for (int idx=0; idx < modules.length; idx++) {
    try {
      if (raw) {
        removeModule(Class.forName(modules[idx]));
      }
 else {
        addModule(Class.forName(modules[idx]),-1);
      }
    }
 catch (    ClassNotFoundException cnfe) {
    }
  }
}
