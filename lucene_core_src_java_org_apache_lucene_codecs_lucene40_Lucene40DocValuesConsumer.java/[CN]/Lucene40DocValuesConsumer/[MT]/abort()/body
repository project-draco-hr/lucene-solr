{
  try {
    close();
  }
 catch (  Throwable t) {
  }
 finally {
    IOUtils.deleteFilesIgnoringExceptions(mainDirectory,IndexFileNames.segmentFileName(segmentName,segmentSuffix,IndexFileNames.COMPOUND_FILE_EXTENSION),IndexFileNames.segmentFileName(segmentName,segmentSuffix,IndexFileNames.COMPOUND_FILE_ENTRIES_EXTENSION));
  }
}
