{
  String key=new StringBuilder(keyPrefix != null ? keyPrefix : "").append(aePath).append(runtimeParameters != null ? runtimeParameters.toString() : "").toString();
  if (providerCache.get(key) == null) {
    AEProvider aeProvider;
    if (runtimeParameters != null)     aeProvider=new OverridingParamsAEProvider(aePath,runtimeParameters);
 else     aeProvider=new BasicAEProvider(aePath);
    providerCache.put(key,aeProvider);
  }
  return providerCache.get(key);
}
