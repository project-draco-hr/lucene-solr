{
  final Analyzer a=new KuromojiAnalyzer(TEST_VERSION_CURRENT,TestKuromojiTokenizer.readDict(),Mode.SEARCH,KuromojiAnalyzer.getDefaultStopSet(),KuromojiAnalyzer.getDefaultStopTags());
  assertTokenStreamContents(a.tokenStream("foo",new StringReader("abcd")),new String[]{"a","b","cd"},new int[]{0,1,2},new int[]{1,2,4},new Integer(4));
}
