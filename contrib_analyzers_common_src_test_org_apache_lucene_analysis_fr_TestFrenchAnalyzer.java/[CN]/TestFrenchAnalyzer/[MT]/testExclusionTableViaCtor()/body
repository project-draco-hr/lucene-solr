{
  CharArraySet set=new CharArraySet(Version.LUCENE_CURRENT,1,true);
  set.add("habitable");
  FrenchAnalyzer fa=new FrenchAnalyzer(Version.LUCENE_CURRENT,CharArraySet.EMPTY_SET,set);
  assertAnalyzesToReuse(fa,"habitable chiste",new String[]{"habitable","chist"});
  fa=new FrenchAnalyzer(Version.LUCENE_CURRENT,CharArraySet.EMPTY_SET,set);
  assertAnalyzesTo(fa,"habitable chiste",new String[]{"habitable","chist"});
}
