{
  Directory directory=new RAMDirectory();
  IndexWriter writer=getWriter(directory);
  for (int i=0; i < 10; i++) {
    Document doc=new Document();
    doc.add(new StringField("foo",String.valueOf(i),Store.YES));
    writer.addDocument(doc);
  }
  writer.commit();
  writer.shutdown();
  return directory;
}
