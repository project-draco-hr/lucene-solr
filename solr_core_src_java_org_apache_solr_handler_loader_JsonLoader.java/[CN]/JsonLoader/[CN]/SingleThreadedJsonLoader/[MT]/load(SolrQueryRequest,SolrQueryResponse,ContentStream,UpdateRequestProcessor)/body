{
  Reader reader=null;
  try {
    reader=stream.getReader();
    if (log.isTraceEnabled()) {
      String body=IOUtils.toString(reader);
      log.trace("body",body);
      reader=new StringReader(body);
    }
    this.processUpdate(reader);
  }
  finally {
    IOUtils.closeQuietly(reader);
  }
}
