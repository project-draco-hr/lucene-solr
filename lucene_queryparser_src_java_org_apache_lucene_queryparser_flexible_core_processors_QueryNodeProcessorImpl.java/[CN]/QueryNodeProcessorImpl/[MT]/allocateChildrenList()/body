{
  ChildrenList list=null;
  for (  ChildrenList auxList : this.childrenListPool) {
    if (!auxList.beingUsed) {
      list=auxList;
      list.clear();
      break;
    }
  }
  if (list == null) {
    list=new ChildrenList();
    this.childrenListPool.add(list);
  }
  list.beingUsed=true;
  return list;
}
