{
  bodyField=new Field(BasicDocMaker.BODY_FIELD,"",storeVal,Field.Index.TOKENIZED,termVecVal);
  titleField=new Field(BasicDocMaker.TITLE_FIELD,"",storeVal,Field.Index.TOKENIZED,termVecVal);
  dateField=new Field(BasicDocMaker.DATE_FIELD,"",storeVal,Field.Index.TOKENIZED,termVecVal);
  idField=new Field(BasicDocMaker.ID_FIELD,"",Field.Store.YES,Field.Index.NO_NORMS);
  doc=new Document();
  doc.add(bodyField);
  doc.add(titleField);
  doc.add(dateField);
  doc.add(idField);
}
