{
  bodyField=new Field(BasicDocMaker.BODY_FIELD,"",storeVal,Field.Index.TOKENIZED,termVecVal);
  titleField=new Field(BasicDocMaker.TITLE_FIELD,"",storeVal,Field.Index.TOKENIZED,termVecVal);
  dateField=new Field(BasicDocMaker.TITLE_FIELD,"",storeVal,Field.Index.TOKENIZED,termVecVal);
  doc=new Document();
  doc.add(bodyField);
  doc.add(titleField);
  doc.add(dateField);
}
