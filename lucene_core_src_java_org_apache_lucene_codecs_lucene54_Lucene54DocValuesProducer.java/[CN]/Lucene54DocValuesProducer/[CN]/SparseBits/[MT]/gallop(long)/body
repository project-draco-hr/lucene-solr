{
  index++;
  this.docId=nextDocId;
  long hiIndex=index + 1;
  while (true) {
    if (hiIndex >= docIDsLength) {
      hiIndex=docIDsLength;
      nextDocId=maxDoc;
      break;
    }
    final long hiDocId=docIds.get(hiIndex);
    if (hiDocId > docId) {
      nextDocId=hiDocId;
      break;
    }
    final long delta=hiIndex - index;
    index=hiIndex;
    this.docId=hiDocId;
    hiIndex+=delta << 1;
  }
  return hiIndex;
}
