{
  for (DisiWrapper<Scorer> s=lead; s != null; s=s.next) {
    final DisiWrapper<Scorer> evicted=insertTailWithOverFlow(s);
    if (evicted != null) {
      if (evicted.doc == doc) {
        evicted.doc=evicted.iterator.nextDoc();
      }
 else {
        evicted.doc=evicted.iterator.advance(doc + 1);
      }
      head.add(evicted);
    }
  }
  setDocAndFreq();
  return doNext();
}
