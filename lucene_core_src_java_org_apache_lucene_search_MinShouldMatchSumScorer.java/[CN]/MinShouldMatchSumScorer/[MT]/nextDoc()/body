{
  for (ScorerWrapper s=lead; s != null; s=s.next) {
    final ScorerWrapper evicted=insertTailWithOverFlow(s);
    if (evicted != null) {
      if (evicted.doc == doc) {
        evicted.doc=evicted.scorer.nextDoc();
      }
 else {
        evicted.doc=evicted.scorer.advance(doc + 1);
      }
      head.add(evicted);
    }
  }
  setDocAndFreq();
  return doNext();
}
