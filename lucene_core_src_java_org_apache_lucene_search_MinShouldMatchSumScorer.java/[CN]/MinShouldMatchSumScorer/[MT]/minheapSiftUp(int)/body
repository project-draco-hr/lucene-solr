{
  Scorer scorer=subScorers[i];
  final int doc=scorer.docID();
  while (i > 0) {
    int parent=(i - 1) >> 1;
    Scorer pscorer=subScorers[parent];
    int pdoc=pscorer.docID();
    if (pdoc > doc) {
      subScorers[i]=subScorers[parent];
      i=parent;
    }
 else {
      break;
    }
  }
  subScorers[i]=scorer;
}
