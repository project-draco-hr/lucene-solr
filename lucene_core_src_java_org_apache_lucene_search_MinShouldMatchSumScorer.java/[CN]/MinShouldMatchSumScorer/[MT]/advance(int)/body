{
  if (numScorers < mm)   return doc=NO_MORE_DOCS;
  while (subScorers[0].docID() < target) {
    if (subScorers[0].advance(target) != NO_MORE_DOCS) {
      minheapAdjust(0);
    }
 else {
      minheapRemoveRoot();
      numScorers--;
      if (numScorers < mm) {
        return doc=NO_MORE_DOCS;
      }
    }
  }
  evaluateSmallestDocInHeap();
  if (nrMatchers >= mm) {
    return doc;
  }
 else {
    return nextDoc();
  }
}
