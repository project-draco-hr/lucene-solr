{
  long numTerms=0;
  FieldsEnum it=iterator();
  while (true) {
    String field=it.next();
    if (field == null) {
      break;
    }
    Terms terms=terms(field);
    if (terms != null) {
      final long termCount=terms.size();
      if (termCount == -1) {
        return -1;
      }
      numTerms+=termCount;
    }
  }
  return numTerms;
}
