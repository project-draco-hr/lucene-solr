{
  int to=tokenPair.to;
  if (!isToExist(to)) {
    ArrayList<SegTokenPair> newlist=new ArrayList<SegTokenPair>();
    newlist.add(tokenPair);
    tokenPairListTable.put(to,newlist);
  }
 else {
    List<SegTokenPair> tokenPairList=tokenPairListTable.get(to);
    tokenPairList.add(tokenPair);
  }
}
