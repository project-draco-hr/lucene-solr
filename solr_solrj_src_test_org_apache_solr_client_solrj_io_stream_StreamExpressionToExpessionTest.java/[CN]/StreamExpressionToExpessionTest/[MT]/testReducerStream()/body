{
  ReducerStream stream;
  String expressionString;
  stream=new ReducerStream(StreamExpressionParser.parse("group(" + "search(collection1, q=*:*, fl=\"id,a_s,a_i,a_f\", sort=\"a_s desc, a_f asc\")," + "by=\"a_s desc\")"),factory);
  expressionString=stream.toExpression(factory).toString();
  assertTrue(expressionString.contains("group(search(collection1"));
  assertTrue(expressionString.contains("by=\"a_s desc\""));
}
