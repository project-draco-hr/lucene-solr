{
{
    SegmentReader reader=new SegmentReader(info,indexDivisor,newIOContext(random()));
    assertTrue(reader != null);
    DocsEnum termDocs=_TestUtil.docs(random(),reader,"textField2",new BytesRef("bad"),reader.getLiveDocs(),null,0);
    assertNull(termDocs);
    reader.close();
  }
{
    SegmentReader reader=new SegmentReader(info,indexDivisor,newIOContext(random()));
    assertTrue(reader != null);
    DocsEnum termDocs=_TestUtil.docs(random(),reader,"junk",new BytesRef("bad"),reader.getLiveDocs(),null,0);
    assertNull(termDocs);
    reader.close();
  }
}
