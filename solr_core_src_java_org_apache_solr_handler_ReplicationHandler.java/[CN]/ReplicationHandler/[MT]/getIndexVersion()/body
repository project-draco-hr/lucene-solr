{
  CommitVersionInfo v=null;
  RefCounted<SolrIndexSearcher> searcher=core.getSearcher();
  try {
    v=CommitVersionInfo.build(searcher.get().getIndexReader().getIndexCommit());
  }
 catch (  IOException e) {
    LOG.warn("Unable to get index commit: ",e);
  }
 finally {
    searcher.decref();
  }
  return v;
}
