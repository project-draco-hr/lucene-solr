{
  if (handlerCache.containsKey(extension))   return (FileContentHandler)((FileContentHandler)handlerCache.get(extension)).clone();
 else   if (handlerCache.containsKey(DEFAULT_HANDLER_KEY))   return (FileContentHandler)((FileContentHandler)handlerCache.get(DEFAULT_HANDLER_KEY)).clone();
 else   return NullHandler.getInstance();
}
