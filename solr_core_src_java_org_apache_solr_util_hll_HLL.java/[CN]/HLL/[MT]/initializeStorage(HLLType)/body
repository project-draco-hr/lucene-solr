{
  this.type=type;
switch (type) {
case EMPTY:
    break;
case EXPLICIT:
  this.explicitStorage=new LongHashSet();
break;
case SPARSE:
this.sparseProbabilisticStorage=new IntByteHashMap();
break;
case FULL:
this.probabilisticStorage=new BitVector(regwidth,m);
break;
default :
throw new RuntimeException("Unsupported HLL type " + type);
}
}
