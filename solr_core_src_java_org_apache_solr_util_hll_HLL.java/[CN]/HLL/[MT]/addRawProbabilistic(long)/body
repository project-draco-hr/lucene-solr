{
  final long substreamValue=(rawValue >>> log2m);
  final byte p_w;
  if (substreamValue == 0L) {
    p_w=0;
  }
 else {
    p_w=(byte)(1 + BitUtil.leastSignificantBit(substreamValue | pwMaxMask));
  }
  if (p_w == 0) {
    return;
  }
  final int j=(int)(rawValue & mBitsMask);
  probabilisticStorage.setMaxRegister(j,p_w);
}
