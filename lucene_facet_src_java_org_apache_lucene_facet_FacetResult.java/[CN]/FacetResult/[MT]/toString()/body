{
  StringBuilder sb=new StringBuilder();
  sb.append("dim=");
  sb.append(dim);
  sb.append(" path=");
  sb.append(Arrays.toString(path));
  sb.append(" value=");
  sb.append(value);
  sb.append(" childCount=");
  sb.append(childCount);
  sb.append('\n');
  for (  LabelAndValue labelValue : labelValues) {
    sb.append("  " + labelValue + "\n");
  }
  return sb.toString();
}
