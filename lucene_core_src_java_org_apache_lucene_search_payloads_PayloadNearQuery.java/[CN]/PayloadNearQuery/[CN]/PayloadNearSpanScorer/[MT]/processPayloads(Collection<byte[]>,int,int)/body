{
  for (  final byte[] thePayload : payLoads) {
    scratch.bytes=thePayload;
    scratch.offset=0;
    scratch.length=thePayload.length;
    payloadScore=function.currentScore(doc,fieldName,start,end,payloadsSeen,payloadScore,docScorer.computePayloadFactor(doc,spans.startPosition(),spans.endPosition(),scratch));
    ++payloadsSeen;
  }
}
