{
  long v=((encoded[offset] & 0xFFL) << 56) | ((encoded[offset + 1] & 0xFFL) << 48) | ((encoded[offset + 2] & 0xFFL) << 40)| ((encoded[offset + 3] & 0xFFL) << 32)| ((encoded[offset + 4] & 0xFFL) << 24)| ((encoded[offset + 5] & 0xFFL) << 16)| ((encoded[offset + 6] & 0xFFL) << 8)| (encoded[offset + 7] & 0xFFL);
