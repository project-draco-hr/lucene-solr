{
  BytesRef payload=postings.getPayload();
  if (payload == null)   return;
  float payloadFactor=docScorer.computePayloadFactor(docID(),spans.startPosition(),spans.endPosition(),payload);
  payloadScore=function.currentScore(docID(),getField(),spans.startPosition(),spans.endPosition(),payloadsSeen,payloadScore,payloadFactor);
  payloadsSeen++;
}
