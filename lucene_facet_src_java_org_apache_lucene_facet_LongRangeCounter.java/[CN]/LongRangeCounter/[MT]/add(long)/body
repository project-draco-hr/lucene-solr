{
  int lo=0;
  int hi=boundaries.length - 1;
  while (true) {
    int mid=(lo + hi) >>> 1;
    if (v <= boundaries[mid]) {
      if (mid == 0) {
        leafCounts[0]++;
        return;
      }
 else {
        hi=mid - 1;
      }
    }
 else     if (v > boundaries[mid + 1]) {
      lo=mid + 1;
    }
 else {
      leafCounts[mid + 1]++;
      return;
    }
  }
}
