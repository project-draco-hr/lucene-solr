{
  if (value == null)   throw new NullPointerException("value is null");
  int i=getIndex(key);
  if (i >= 0) {
    String old=elms[i];
    elms[i]=value;
    return old;
  }
  int len=keys.length;
  if (len == n) {
    String[] k, e;
    k=new String[len * 2];
    e=new String[len * 2];
    System.arraycopy(keys,0,k,0,len);
    System.arraycopy(elms,0,e,0,len);
    keys=k;
    elms=e;
  }
  keys[n]=key;
  elms[n]=value;
  n++;
  return null;
}
