{
  StringBuffer sb=new StringBuffer();
  boolean f=true;
  sb.append("{ ");
  for (Enumeration e=getAttributeNames(); e.hasMoreElements(); ) {
    if (f) {
      f=false;
    }
 else {
      sb.append(", ");
    }
    String key=(String)e.nextElement();
    sb.append("" + key + '='+ getValue(key));
  }
  sb.append(" }");
  return sb.toString();
}
