{
  super(applyAble);
  if (this.applyAble.getServiceConfig() == null)   throw new NotIndexableException("ServiceConfig is not set");
  try {
    this.document=buildDomDocument();
  }
 catch (  ParserConfigurationException e) {
    throw new NotIndexableException("Can not create document builder",e);
  }
catch (  IOException e) {
    throw new NotIndexableException("IO Exception occurred while building indexable",e);
  }
catch (  SAXException e) {
    throw new NotIndexableException("Can not parse entry",e);
  }
  this.xPath=XPathFactory.newInstance().newXPath();
}
