{
  try {
    Class<?> clazz=Class.forName("sun.misc.Unsafe");
    Field field=clazz.getDeclaredField("theUnsafe");
    field.setAccessible(true);
    Object o=field.get(null);
    Method m=clazz.getMethod("putAddress",long.class,long.class);
    m.invoke(o,0L,0L);
  }
 catch (  Exception e) {
