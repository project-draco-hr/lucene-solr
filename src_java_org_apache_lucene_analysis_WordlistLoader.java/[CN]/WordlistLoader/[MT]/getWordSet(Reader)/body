{
  HashSet result=new HashSet();
  BufferedReader br=null;
  try {
    if (reader instanceof BufferedReader) {
      br=(BufferedReader)reader;
    }
 else {
      br=new BufferedReader(reader);
    }
    String word=null;
    while ((word=br.readLine()) != null) {
      result.add(word.trim());
    }
  }
  finally {
    if (br != null)     br.close();
  }
  return result;
}
