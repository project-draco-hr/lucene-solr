{
  if (dir.isDirectory()) {
    File[] dirContents=dir.listFiles();
    for (int i=0; i < dirContents.length; i++) {
      indexTarDirectory(dirContents[i],dataMapList);
    }
  }
 else   if (dir.isFile()) {
    Map dataMap=new HashMap();
    dataMap.put("filePath",dir.toString());
    dataMapList.add(dataMap);
  }
}
