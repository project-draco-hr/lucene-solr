{
  SolrUIMAConfigurationReader uimaConfigurationReader=new SolrUIMAConfigurationReader(solrCore.getSolrConfig());
  solrUIMAConfiguration=uimaConfigurationReader.readSolrUIMAConfiguration();
  aeProvider=AEProviderFactory.getInstance().getAEProvider(solrCore.getName(),solrUIMAConfiguration.getAePath(),solrUIMAConfiguration.getRuntimeParameters());
}
