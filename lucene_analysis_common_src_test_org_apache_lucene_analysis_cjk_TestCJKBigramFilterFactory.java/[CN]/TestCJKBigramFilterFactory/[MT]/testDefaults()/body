  TokenStream stream=factory.create(new StandardTokenizer(TEST_VERSION_CURRENT,reader));
  assertTokenStreamContents(stream,new String[]{"??????","??????","??????","??????","??????","??????","??????","??????","??????","??????","??????"});
}
