{
  List<GOMAttribute> xmlNamespaceAttributes=getXmlNamespaceAttributes();
  xmlNamespaceAttributes.add(GOMUtils.getAttributeByContentTypeDefaultNs(this.contentType));
  if (this.contentType == ContentType.XHTML) {
    aStreamWriter.writeStartElement(this.localName,xmlNamespaceAttributes);
    aStreamWriter.writeContentUnescaped(this.textValue);
    aStreamWriter.writeEndElement();
  }
 else {
    aStreamWriter.writeSimpleXMLElement(this.localName,xmlNamespaceAttributes,this.textValue);
  }
}
