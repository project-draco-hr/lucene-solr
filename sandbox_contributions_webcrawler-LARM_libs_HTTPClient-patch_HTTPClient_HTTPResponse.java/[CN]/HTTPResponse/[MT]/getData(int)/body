{
  if (!initialized) {
    handleResponse();
  }
  if (Data == null) {
    try {
      readResponseData(inp_stream,max);
    }
 catch (    InterruptedIOException ie) {
      throw ie;
    }
catch (    IOException ioe) {
      Log.write(Log.RESP,"HResp: (\"" + method + " "+ OriginalURI.getPathAndQuery()+ "\")");
      Log.write(Log.RESP,"       ",ioe);
      try {
        inp_stream.close();
      }
 catch (      Exception e) {
      }
      throw ioe;
    }
    inp_stream.close();
  }
  return Data;
}
