{
  StringBuilder b=new StringBuilder();
  b.append("MultiDimensionalValues(");
  for (int i=0; i < subs.size(); i++) {
    if (i > 0) {
      b.append(", ");
    }
    b.append("docBase=");
    b.append(docBases.get(i));
    b.append(" sub=" + subs.get(i));
  }
  b.append(')');
  return b.toString();
}
