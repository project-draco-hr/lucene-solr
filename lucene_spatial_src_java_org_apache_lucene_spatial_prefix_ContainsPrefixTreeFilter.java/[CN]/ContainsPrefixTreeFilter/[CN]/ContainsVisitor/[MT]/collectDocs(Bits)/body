{
  SmallDocSet set=null;
  docsEnum=termsEnum.docs(acceptContains,docsEnum,DocsEnum.FLAG_NONE);
  int docid;
  while ((docid=docsEnum.nextDoc()) != DocIdSetIterator.NO_MORE_DOCS) {
    if (set == null) {
      int size=termsEnum.docFreq();
      if (size <= 0)       size=16;
      set=new SmallDocSet(size);
    }
    set.set(docid);
  }
  return set;
}
