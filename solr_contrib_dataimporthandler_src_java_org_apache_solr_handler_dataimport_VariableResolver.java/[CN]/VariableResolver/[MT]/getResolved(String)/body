{
  Resolved r=cache.get(template);
  if (r == null) {
    r=new Resolved();
    Matcher m=PLACEHOLDER_PATTERN.matcher(template);
    while (m.find()) {
      String variable=m.group(1);
      r.startIndexes.add(m.start(0));
      r.endOffsets.add(m.end(0));
      r.variables.add(variable);
    }
    cache.put(template,r);
  }
  return r;
}
