{
  seqName=super.getName();
  if (repetitions == REPEAT_EXHAUST) {
    seqName+="_Exhaust";
  }
 else   if (repetitions > 1) {
    seqName+="_" + repetitions;
  }
  if (rate > 0) {
    seqName+="_" + rate + (perMin ? "/min" : "/sec");
  }
  if (parallel && seqName.toLowerCase().indexOf("par") < 0) {
    seqName+="_Par";
  }
}
