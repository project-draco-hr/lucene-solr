{
  Document doc=new Document();
  doc.add(new NumericDocValuesField("id",id));
  if (VERBOSE) {
    System.out.println("TEST: addDoc id:" + id + " terms="+ terms);
  }
  for (  String s2 : terms) {
    doc.add(newStringField("f",s2,Field.Store.NO));
    termToID.put(new BytesRef(s2),id);
  }
  w.addDocument(doc);
  terms.clear();
}
