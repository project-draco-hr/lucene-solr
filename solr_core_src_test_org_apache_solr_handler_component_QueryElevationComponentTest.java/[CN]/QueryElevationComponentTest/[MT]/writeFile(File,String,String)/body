{
  PrintWriter out=new PrintWriter(new FileOutputStream(file));
  out.println("<?xml version=\"1.0\" encoding=\"UTF-8\" ?>");
  out.println("<elevate>");
  out.println("<query text=\"" + query + "\">");
  for (  String id : ids) {
    out.println(" <doc id=\"" + id + "\"/>");
  }
  out.println("</query>");
  out.println("</elevate>");
  out.flush();
  out.close();
  log.info("OUT:" + file.getAbsolutePath());
}
