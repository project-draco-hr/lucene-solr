{
  List<String> filtered=new ArrayList<>();
  for (  String file : files) {
    if (IndexFileNames.CODEC_FILE_PATTERN.matcher(file).matches() || file.startsWith(IndexFileNames.SEGMENTS) || file.startsWith(IndexFileNames.PENDING_SEGMENTS)) {
      filtered.add(file);
    }
  }
  IndexFileDeleter.inflateGens(sis,filtered,stream);
}
