{
  ensureOpen();
  if (dir.verboseClone) {
    new Exception("clone: " + this).printStackTrace(System.out);
  }
  dir.inputCloneCount.incrementAndGet();
  IndexInput iiclone=delegate.clone();
  MockIndexInputWrapper clone=new MockIndexInputWrapper(dir,name,iiclone);
  clone.isClone=true;
  return clone;
}
