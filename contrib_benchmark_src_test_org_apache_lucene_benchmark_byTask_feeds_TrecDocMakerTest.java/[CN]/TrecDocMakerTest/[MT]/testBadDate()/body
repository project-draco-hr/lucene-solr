{
  String docs="<DOC>\r\n" + "<DOCNO>TEST-000</DOCNO>\r\n" + "<DOCHDR>\r\n"+ "http://lucene.apache.org.trecdocmaker.test\r\n"+ "HTTP/1.1 200 OK\r\n"+ "Date: Bad Date\r\n"+ "Server: Apache/1.3.27 (Unix)\r\n"+ "Last-Modified: Sun, 11 Jan 2009 08:00:00 GMT\r\n"+ "Content-Length: 614\r\n"+ "Connection: close\r\n"+ "Content-Type: text/html\r\n"+ "</DOCHDR>\r\n"+ "<html>\r\n"+ "\r\n"+ "<head>\r\n"+ "<title>\r\n"+ "TEST-000 title\r\n"+ "</title>\r\n"+ "</head>\r\n"+ "\r\n"+ "<body>\r\n"+ "TEST-000 text\r\n"+ "\r\n"+ "</body>\r\n"+ "\r\n"+ "</DOC>";
  StringableTrecDocMaker stdm=new StringableTrecDocMaker(docs,false);
  stdm.setHTMLParser(new DemoHTMLParser());
  DocData dd=stdm.getNextDocData();
  assertDocData(dd,"TEST-000_0","TEST-000 title","TEST-000 text",null);
  assertNoMoreDataException(stdm);
}
