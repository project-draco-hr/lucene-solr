{
  if (currentIndexVersion != indexVersionCache) {
    indexVersionCache=currentIndexVersion;
    try {
      etagCache="\"" + new String(Base64.encodeBase64((Long.toHexString(Long.reverse(indexVersionCache)) + etagSeed).getBytes("US-ASCII")),"US-ASCII") + "\"";
    }
 catch (    UnsupportedEncodingException e) {
      throw new RuntimeException(e);
    }
  }
  return etagCache;
}
