{
  List<String> names=op.getStrs("");
  if (names == null || names.isEmpty()) {
    op.addError("Invalid command");
    return null;
  }
  Map<String,Object> p=getMapValue(latestConf,"permissions");
  for (  String s : names) {
    if (p.remove(s) == null) {
      op.addError("Unknown permission : " + s);
      return null;
    }
  }
  return latestConf;
}
