{
  byte b=bytes.bytes[bytes.offset++];
  if (b >= 0)   return b;
  int value=b & 0x7F;
  b=bytes.bytes[bytes.offset++];
  value=(value << 7) | b & 0x7F;
  if (b >= 0)   return value;
  b=bytes.bytes[bytes.offset++];
  value=(value << 7) | b & 0x7F;
  if (b >= 0)   return value;
  b=bytes.bytes[bytes.offset++];
  value=(value << 7) | b & 0x7F;
  if (b >= 0)   return value;
  b=bytes.bytes[bytes.offset++];
  return (value << 7) | b & 0x7F;
}
