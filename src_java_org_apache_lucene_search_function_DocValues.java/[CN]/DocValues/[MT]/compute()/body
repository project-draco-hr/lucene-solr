{
  if (computed) {
    return;
  }
  minVal=Float.MAX_VALUE;
  maxVal=0;
  float sum=0;
  int n=0;
  while (true) {
    float val;
    try {
      val=floatVal(n);
    }
 catch (    ArrayIndexOutOfBoundsException e) {
      break;
    }
    sum+=val;
    minVal=Math.min(minVal,val);
    maxVal=Math.max(maxVal,val);
  }
  avgVal=sum / n;
  computed=true;
}
