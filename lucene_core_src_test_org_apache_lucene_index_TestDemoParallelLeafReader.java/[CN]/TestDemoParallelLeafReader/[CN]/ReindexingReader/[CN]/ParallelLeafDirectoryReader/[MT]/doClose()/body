{
  Throwable firstExc=null;
  for (  final LeafReader r : getSequentialSubReaders()) {
    if (r instanceof ParallelLeafReader) {
      try {
        r.decRef();
      }
 catch (      Throwable t) {
        if (firstExc == null) {
          firstExc=t;
        }
      }
    }
  }
  try {
    in.doClose();
  }
 catch (  Throwable t) {
    if (firstExc == null) {
      firstExc=t;
    }
  }
  IOUtils.reThrow(firstExc);
}
