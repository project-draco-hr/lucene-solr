{
  SpatialRelation yIntersect=relate_yRange(rect.getMinY(),rect.getMaxY(),ctx);
  if (yIntersect == SpatialRelation.DISJOINT)   return SpatialRelation.DISJOINT;
  SpatialRelation xIntersect=relate_xRange(rect.getMinX(),rect.getMaxX(),ctx);
  if (xIntersect == SpatialRelation.DISJOINT)   return SpatialRelation.DISJOINT;
  if (xIntersect == yIntersect)   return xIntersect;
  if (getMinX() == rect.getMinX() && getMaxX() == rect.getMaxX())   return yIntersect;
  if (getMinY() == rect.getMinY() && getMaxY() == rect.getMaxY())   return xIntersect;
  return SpatialRelation.INTERSECTS;
}
