{
  long ord=values.lookupTerm(text);
  if (ord >= 0) {
    currentOrd=ord;
    term.offset=0;
    term.bytes=new byte[text.length];
    term.copyBytes(text);
    return SeekStatus.FOUND;
  }
 else {
    currentOrd=-ord - 1;
    if (currentOrd == values.getValueCount()) {
      return SeekStatus.END;
    }
 else {
      values.lookupOrd(currentOrd,term);
      return SeekStatus.NOT_FOUND;
    }
  }
}
