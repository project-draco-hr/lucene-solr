{
  super(dir,id,codecNameIdx,codecNameDat,codecVersion,bytesUsed,context,type);
  hash=new BytesRefHash(new ByteBlockPool(allocator),BytesRefHash.DEFAULT_CAPACITY,new TrackingDirectBytesStartArray(BytesRefHash.DEFAULT_CAPACITY,bytesUsed));
