{
  try {
    if (null != resultSet) {
      resultSet.close();
    }
    if (null != statement && !statement.isClosed()) {
      statement.close();
    }
    if (null != connection && !connection.isClosed()) {
      connection.close();
    }
  }
 catch (  SQLException e) {
    throw new IOException("Failed to properly close JDBCStream",e);
  }
}
