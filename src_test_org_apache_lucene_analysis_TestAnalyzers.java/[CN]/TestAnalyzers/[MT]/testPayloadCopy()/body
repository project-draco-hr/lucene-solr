{
  String s="how now brown cow";
  TokenStream ts;
  ts=new WhitespaceTokenizer(Version.LUCENE_CURRENT,new StringReader(s));
  ts=new PayloadSetter(ts);
  verifyPayload(ts);
  ts=new WhitespaceTokenizer(Version.LUCENE_CURRENT,new StringReader(s));
  ts=new PayloadSetter(ts);
  verifyPayload(ts);
}
