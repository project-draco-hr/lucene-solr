{
  if (target >= maxDoc) {
    ref.length=0;
    ref.offset=0;
    return pos=NO_MORE_DOCS;
  }
  final long addr=addresses.get(target);
  if (addr == totBytes) {
    ref.length=0;
    ref.offset=0;
    return pos=target;
  }
  datIn.seek(fp + addr);
  final int size=(int)(target == maxDoc - 1 ? totBytes - addr : addresses.get(target + 1) - addr);
  if (ref.bytes.length < size)   ref.grow(size);
  ref.length=size;
  datIn.readBytes(ref.bytes,0,size);
  return pos=target;
}
