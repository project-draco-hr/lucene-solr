{
  if (target >= maxDoc) {
    return pos=NO_MORE_DOCS;
  }
  final long addr=pos + 1 == target ? nextAddress : addresses.advance(target);
  if (addr == totBytes) {
    bytesRef.length=0;
    bytesRef.offset=0;
    return pos=target;
  }
  datIn.seek(fp + addr);
  final int size=(int)(target == maxDoc - 1 ? totBytes - addr : (nextAddress=addresses.next()) - addr);
  if (bytesRef.bytes.length < size) {
    bytesRef.grow(size);
  }
  bytesRef.length=size;
  datIn.readBytes(bytesRef.bytes,0,size);
  return pos=target;
}
