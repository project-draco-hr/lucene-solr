{
  try {
    ((DB4oController)this.c).visiteInitialize();
    this.s=this.c.getStorage();
  }
 catch (  StorageException e1) {
    e1.printStackTrace();
  }
  try {
    if (this.op == StorageOperation.DELETE)     this.s.deleteEntry(e);
    if (this.op == StorageOperation.UPDATE)     this.s.updateEntry(e);
    if (expConf)     fail=true;
  }
 catch (  ModificationConflictException ex) {
    if (!expConf)     fail=true;
    ex.printStackTrace();
  }
catch (  StorageException ex) {
    ex.printStackTrace();
    fail=true;
  }
 finally {
    ((DB4oController)this.c).visiteDestroy();
  }
}
