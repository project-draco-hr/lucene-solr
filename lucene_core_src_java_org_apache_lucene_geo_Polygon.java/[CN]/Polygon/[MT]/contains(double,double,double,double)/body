{
  boolean contains=crosses(minLat,maxLat,minLon,maxLon) == false && contains(minLat,minLon) && contains(minLat,maxLon) && contains(maxLat,maxLon) && contains(maxLat,minLon);
  if (contains) {
    for (    Polygon hole : holes) {
      if (hole.crosses(minLat,maxLat,minLon,maxLon) || hole.contains(minLat,maxLat,minLon,maxLon)) {
        return false;
      }
    }
    return true;
  }
 else {
    return false;
  }
}
