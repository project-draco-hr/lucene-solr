{
  assertUnpositioned(iter);
  leapFrog(iter,new LeapFrogCallBack(){
    int previous=-1;
    @Override public void onMatch(    int doc){
      clear(previous + 1,doc);
      previous=doc;
    }
    @Override public void finish(){
      if (previous + 1 < length()) {
        clear(previous + 1,length());
      }
    }
  }
);
}
