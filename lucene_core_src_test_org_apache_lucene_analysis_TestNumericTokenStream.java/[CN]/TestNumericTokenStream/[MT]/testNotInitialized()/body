{
  final NumericTokenStream stream=new NumericTokenStream();
  try {
    stream.reset();
    fail("reset() should not succeed.");
  }
 catch (  IllegalStateException e) {
  }
  try {
    stream.incrementToken();
    fail("incrementToken() should not succeed.");
  }
 catch (  IllegalStateException e) {
  }
  stream.close();
}
