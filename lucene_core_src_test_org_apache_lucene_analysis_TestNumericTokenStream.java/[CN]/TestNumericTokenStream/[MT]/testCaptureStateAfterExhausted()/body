{
  try (LegacyNumericTokenStream stream=new LegacyNumericTokenStream()){
    stream.setIntValue(ivalue);
    stream.reset();
    while (stream.incrementToken())     ;
    stream.captureState();
    stream.end();
    stream.captureState();
    stream.setLongValue(lvalue);
    stream.reset();
    while (stream.incrementToken())     ;
    stream.captureState();
    stream.end();
    stream.captureState();
  }
   try (LegacyNumericTokenStream stream=new LegacyNumericTokenStream(Integer.MAX_VALUE)){
    stream.setIntValue(ivalue);
    stream.reset();
    while (stream.incrementToken())     ;
    stream.captureState();
    stream.end();
    stream.captureState();
    stream.setLongValue(lvalue);
    stream.reset();
    while (stream.incrementToken())     ;
    stream.captureState();
    stream.end();
    stream.captureState();
  }
 }
