{
  if (nullDoc > -1) {
    this.collapsedSet.set(nullDoc);
  }
  if (this.boostOrds != null) {
    for (int i=0; i < this.boostOrds.length; i++) {
      ords[boostOrds[i]]=-1;
    }
  }
  for (int i=0; i < ords.length; i++) {
    int doc=ords[i];
    if (doc > -1) {
      collapsedSet.set(doc);
    }
  }
  return collapsedSet;
}
