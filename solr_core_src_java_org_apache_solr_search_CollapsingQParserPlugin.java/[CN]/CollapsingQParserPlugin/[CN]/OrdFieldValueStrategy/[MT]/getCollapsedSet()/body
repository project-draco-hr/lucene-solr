{
  if (nullDoc > -1) {
    this.collapsedSet.set(nullDoc);
  }
  if (this.boostOrds != null) {
    int s=boostOrds.size();
    for (int i=0; i < s; i++) {
      int ord=boostOrds.get(i);
      if (ord > -1) {
        ords[ord]=-1;
      }
      collapsedSet.set(boostDocs.get(i));
    }
    mergeBoost.reset();
  }
  for (int i=0; i < ords.length; i++) {
    int doc=ords[i];
    if (doc > -1) {
      collapsedSet.set(doc);
    }
  }
  return collapsedSet;
}
