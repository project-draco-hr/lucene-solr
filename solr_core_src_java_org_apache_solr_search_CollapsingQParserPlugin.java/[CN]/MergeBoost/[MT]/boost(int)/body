{
  if (index == Integer.MIN_VALUE) {
    return false;
  }
 else {
    while (true) {
      if (index >= boostDocs.length) {
        index=Integer.MIN_VALUE;
        return false;
      }
 else {
        int comp=boostDocs[index];
        if (comp == globalDoc) {
          ++index;
          return true;
        }
 else         if (comp < globalDoc) {
          ++index;
        }
 else {
          return false;
        }
      }
    }
  }
}
