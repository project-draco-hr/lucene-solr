{
  Polygon polygon=new Polygon(new double[]{0,-50,-50,50,50,-50,0,-5,-5,5,5,-5,0,-10,-10,10,10,-10,0},new double[]{0,-50,50,50,-50,-50,0,-5,5,5,-5,-5,0,-10,10,10,-10,-10,0});
  assertTrue(polygon.contains(-2,2));
  assertFalse(polygon.contains(-6,6));
  assertTrue(polygon.contains(-25,25));
  assertFalse(polygon.contains(-51,51));
}
