{
  String[] john=new String[]{"q","name_s:john","indent","true","fl","name_s_dv," + "[subquery]","rows","" + (2 * peopleMultiplier),"depts.q","+{!term f=dept_id_s v=$row.dept_ss_dv}^=0 _val_:id_i","depts.fl","id","depts.fl","score","depts.indent","true","depts.rows","" + deptMultiplier,"depts.start","0"};
  assertQEx("no prefix, no subquery",req(john),ErrorCode.BAD_REQUEST);
  assertQEx("no prefix, no subsubquery",req("q","name_s:john","indent","true","fl","name_s_dv," + "depts:[subquery]","rows","" + (2 * peopleMultiplier),"depts.q","+{!term f=dept_id_s v=$row.dept_ss_dv}^=0 _val_:id_i","depts.fl","id","depts.fl","score","depts.fl","[subquery]","depts.indent","true","depts.rows","" + deptMultiplier,"depts.start","0"),ErrorCode.BAD_REQUEST);
}
