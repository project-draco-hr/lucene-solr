{
  final HashMap<Integer,Long> clonedNormGen;
  if (normGen != null) {
    clonedNormGen=new HashMap<Integer,Long>();
    for (    Entry<Integer,Long> entry : normGen.entrySet()) {
      clonedNormGen.put(entry.getKey(),entry.getValue());
    }
  }
 else {
    clonedNormGen=null;
  }
  SegmentInfo newInfo=new SegmentInfo(dir,version,name,docCount,docStoreOffset,docStoreSegment,docStoreIsCompoundFile,clonedNormGen,isCompoundFile,codec,new HashMap<String,String>(diagnostics));
  final Set<String> clonedFiles;
  if (setFiles != null) {
    clonedFiles=new HashSet<String>(setFiles);
  }
 else {
    clonedFiles=null;
  }
  newInfo.setFiles(clonedFiles);
  return newInfo;
}
