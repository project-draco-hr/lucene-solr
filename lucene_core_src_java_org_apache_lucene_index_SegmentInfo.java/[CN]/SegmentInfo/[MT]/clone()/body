{
  final HashMap<Integer,Long> clonedNormGen;
  if (normGen != null) {
    clonedNormGen=new HashMap<Integer,Long>();
    for (    Entry<Integer,Long> entry : normGen.entrySet()) {
      clonedNormGen.put(entry.getKey(),entry.getValue());
    }
  }
 else {
    clonedNormGen=null;
  }
  return new SegmentInfo(dir,version,name,docCount,delGen,docStoreOffset,docStoreSegment,docStoreIsCompoundFile,clonedNormGen,isCompoundFile,delCount,hasProx,codec,new HashMap<String,String>(diagnostics),hasVectors);
}
