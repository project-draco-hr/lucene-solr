{
  final HashMap<Integer,Long> clonedNormGen;
  if (normGen != null) {
    clonedNormGen=new HashMap<Integer,Long>();
    for (    Entry<Integer,Long> entry : normGen.entrySet()) {
      clonedNormGen.put(entry.getKey(),entry.getValue());
    }
  }
 else {
    clonedNormGen=null;
  }
  SegmentInfo newInfo=new SegmentInfo(dir,version,name,docCount,docStoreOffset,docStoreSegment,docStoreIsCompoundFile,clonedNormGen,isCompoundFile,delCount,codec,new HashMap<String,String>(diagnostics));
  newInfo.setDelGen(delGen);
  return newInfo;
}
