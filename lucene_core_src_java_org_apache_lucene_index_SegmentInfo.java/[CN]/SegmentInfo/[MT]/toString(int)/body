{
  StringBuilder s=new StringBuilder();
  s.append(name).append('(').append(version == null ? "?" : version).append(')').append(':');
  char cfs=getUseCompoundFile() ? 'c' : 'C';
  s.append(cfs);
  s.append(docCount);
  if (delCount != 0) {
    s.append('/').append(delCount);
  }
  return s.toString();
}
