{
  postingUpto++;
  if (postingUpto == field.numPostings)   return false;
  p=postings[postingUpto];
  docID=0;
  text=field.threadState.charPool.buffers[p.textStart >> CHAR_BLOCK_SHIFT];
  textOffset=p.textStart & CHAR_BLOCK_MASK;
  if (p.freqUpto > p.freqStart)   freq.init(field.threadState.postingsPool,p.freqStart,p.freqUpto);
 else   freq.bufferOffset=freq.upto=freq.endIndex=0;
  prox.init(field.threadState.postingsPool,p.proxStart,p.proxUpto);
  boolean result=nextDoc();
  assert result;
  return true;
}
