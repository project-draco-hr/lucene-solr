{
  final int size=freeCharBlocks.size();
  final char[] c;
  if (0 == size) {
    numBytesAlloc+=CHAR_BLOCK_SIZE * CHAR_NUM_BYTE;
    balanceRAM();
    c=new char[BYTE_BLOCK_SIZE];
  }
 else   c=(char[])freeCharBlocks.remove(size - 1);
  numBytesUsed+=CHAR_BLOCK_SIZE * CHAR_NUM_BYTE;
  return c;
}
