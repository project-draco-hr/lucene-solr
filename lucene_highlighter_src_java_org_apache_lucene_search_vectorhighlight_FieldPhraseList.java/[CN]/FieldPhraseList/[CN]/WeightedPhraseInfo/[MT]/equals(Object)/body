{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  WeightedPhraseInfo other=(WeightedPhraseInfo)obj;
  if (getStartOffset() != other.getStartOffset()) {
    return false;
  }
  if (getEndOffset() != other.getEndOffset()) {
    return false;
  }
  if (getBoost() != other.getBoost()) {
    return false;
  }
  return true;
}
