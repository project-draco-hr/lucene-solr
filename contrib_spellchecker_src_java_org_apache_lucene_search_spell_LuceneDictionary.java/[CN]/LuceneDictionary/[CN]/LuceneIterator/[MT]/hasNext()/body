{
  has_next_called=true;
  try {
    if (!enum.next()) {
      actualTerm=null;
      return false;
    }
    actualTerm=enum.term();
    String fieldt=actualTerm.field();
    if (fieldt != field) {
      actualTerm=null;
      return false;
    }
    return true;
  }
 catch (  IOException ex) {
    ex.printStackTrace();
    return false;
  }
}
