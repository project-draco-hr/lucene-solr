{
  hasNextCalled=true;
  try {
    if (!termEnum.next()) {
      actualTerm=null;
      return false;
    }
    actualTerm=termEnum.term();
    String currentField=actualTerm.field();
    if (currentField != field) {
      actualTerm=null;
      return false;
    }
    return true;
  }
 catch (  IOException e) {
    e.printStackTrace();
    return false;
  }
}
