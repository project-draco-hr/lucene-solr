{
  if (random().nextBoolean()) {
    long l=1;
    if (random().nextBoolean()) {
      l*=-1;
    }
    for (    long i : PRIMES) {
      final int m=random().nextInt(3);
      for (int j=0; j < m; ++j) {
        l*=i;
      }
    }
    return l;
  }
 else   if (random().nextBoolean()) {
    return random().nextLong();
  }
 else {
    return RandomPicks.randomFrom(random(),Arrays.asList(Long.MIN_VALUE,Long.MAX_VALUE,0L,-1L,1L));
  }
}
