{
  return new DocIdSetIterator(){
    int bytePos=0;
    int lastInt=0;
    private void advance(){
      byte b=bytes[bytePos++];
      lastInt+=b & VB1;
      for (int s=BIT_SHIFT; (b & ~VB1) != 0; s+=BIT_SHIFT) {
        b=bytes[bytePos++];
        lastInt+=(b & VB1) << s;
      }
    }
    public int doc(){
      return lastInt;
    }
    public boolean next(){
      if (bytePos >= lastBytePos) {
        return false;
      }
 else {
        advance();
        return true;
      }
    }
    public boolean skipTo(    int docNr){
      while (bytePos < lastBytePos) {
        advance();
        if (lastInt >= docNr) {
          return true;
        }
      }
      return false;
    }
  }
;
}
