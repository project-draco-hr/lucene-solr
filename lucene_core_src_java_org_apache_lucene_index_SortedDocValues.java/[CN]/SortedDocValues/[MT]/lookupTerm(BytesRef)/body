{
  int low=0;
  int high=getValueCount() - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    final BytesRef term=lookupOrd(mid);
    int cmp=term.compareTo(key);
    if (cmp < 0) {
      low=mid + 1;
    }
 else     if (cmp > 0) {
      high=mid - 1;
    }
 else {
      return mid;
    }
  }
  return -(low + 1);
}
