{
  int low=0;
  int high=getValueCount() - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    lookupOrd(mid,spare);
    int cmp=spare.compareTo(key);
    if (cmp < 0) {
      low=mid + 1;
    }
 else     if (cmp > 0) {
      high=mid - 1;
    }
 else {
      spare.bytes=BytesRef.EMPTY_BYTES;
      return mid;
    }
  }
  spare.bytes=BytesRef.EMPTY_BYTES;
  return -(low + 1);
}
