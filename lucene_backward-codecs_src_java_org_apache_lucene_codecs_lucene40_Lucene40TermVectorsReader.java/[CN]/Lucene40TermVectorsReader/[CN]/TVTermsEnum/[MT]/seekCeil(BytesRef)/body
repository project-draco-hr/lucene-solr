{
  if (nextTerm != 0) {
    final int cmp=text.compareTo(term.get());
    if (cmp < 0) {
      nextTerm=0;
      tvf.seek(tvfFP);
    }
 else     if (cmp == 0) {
      return SeekStatus.FOUND;
    }
  }
  while (next() != null) {
    final int cmp=text.compareTo(term.get());
    if (cmp < 0) {
      return SeekStatus.NOT_FOUND;
    }
 else     if (cmp == 0) {
      return SeekStatus.FOUND;
    }
  }
  return SeekStatus.END;
}
