{
  SolrCache<String,UnInvertedField> cache=searcher.getFieldValueCache();
  if (cache == null) {
    return new UnInvertedField(field,searcher);
  }
  UnInvertedField uif=cache.get(field);
  if (uif == null) {
synchronized (cache) {
      uif=cache.get(field);
      if (uif == null) {
        uif=new UnInvertedField(field,searcher);
        cache.put(field,uif);
      }
    }
  }
  return uif;
}
