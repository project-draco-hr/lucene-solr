{
  final long indexSize=indexedTermsArray == null ? 0 : (8 + 8 + 8+ 8+ (indexedTermsArray.length << 3)+ sizeOfIndexedStrings);
  return "{field=" + field + ",memSize="+ memSize()+ ",tindexSize="+ indexSize+ ",time="+ total_time+ ",phase1="+ phase1_time+ ",nTerms="+ numTermsInField+ ",bigTerms="+ bigTerms.size()+ ",termInstances="+ termInstances+ ",uses="+ use.get()+ "}";
}
