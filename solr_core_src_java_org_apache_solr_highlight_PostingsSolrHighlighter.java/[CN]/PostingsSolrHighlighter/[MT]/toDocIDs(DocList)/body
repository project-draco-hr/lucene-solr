{
  int[] docIDs=new int[docs.size()];
  DocIterator iterator=docs.iterator();
  for (int i=0; i < docIDs.length; i++) {
    if (!iterator.hasNext()) {
      throw new AssertionError();
    }
    docIDs[i]=iterator.nextDoc();
  }
  if (iterator.hasNext()) {
    throw new AssertionError();
  }
  return docIDs;
}
