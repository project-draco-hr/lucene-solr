{
  DirectTermsEnum termsEnum;
  if (reuse != null && reuse instanceof DirectTermsEnum) {
    termsEnum=(DirectTermsEnum)reuse;
    if (!termsEnum.canReuse(terms)) {
      termsEnum=new DirectTermsEnum();
    }
  }
 else {
    termsEnum=new DirectTermsEnum();
  }
  termsEnum.reset();
  return termsEnum;
}
