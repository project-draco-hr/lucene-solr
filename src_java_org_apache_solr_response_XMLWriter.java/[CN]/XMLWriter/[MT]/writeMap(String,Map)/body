{
  Map map=val;
  int sz=map.size();
  startTag("lst",name,sz <= 0);
  incLevel();
  for (  Map.Entry entry : (Set<Map.Entry>)map.entrySet()) {
    String k=(String)entry.getKey();
    Object v=entry.getValue();
    writeVal(k,v);
  }
  decLevel();
  if (sz > 0) {
    if (doIndent)     indent();
    writer.write("</lst>");
  }
}
