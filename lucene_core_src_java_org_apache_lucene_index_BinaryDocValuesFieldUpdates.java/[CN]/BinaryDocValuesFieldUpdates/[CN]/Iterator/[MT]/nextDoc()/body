{
  if (idx >= size) {
    offset=-1;
    return doc=DocIdSetIterator.NO_MORE_DOCS;
  }
  doc=(int)docs.get(idx);
  ++idx;
  while (idx < size && docs.get(idx) == doc) {
    ++idx;
  }
  long prevIdx=idx - 1;
  if (!docsWithField.get((int)prevIdx)) {
    offset=-1;
  }
 else {
    offset=(int)offsets.get(prevIdx);
    length=(int)lengths.get(prevIdx);
  }
  return doc;
}
