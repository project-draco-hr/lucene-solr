{
  Tuple a=streamA.read();
  Tuple b=streamB.read();
  if (a.EOF && b.EOF) {
    return a;
  }
  if (a.EOF) {
    streamA.pushBack(a);
    return b;
  }
  if (b.EOF) {
    streamB.pushBack(b);
    return a;
  }
  int c=comp.compare(a,b);
  if (c < 0) {
    streamB.pushBack(b);
    return a;
  }
 else {
    streamA.pushBack(a);
    return b;
  }
}
