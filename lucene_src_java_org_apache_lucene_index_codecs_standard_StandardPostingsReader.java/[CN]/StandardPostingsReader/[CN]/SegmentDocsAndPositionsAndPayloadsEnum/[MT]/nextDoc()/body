{
  while (true) {
    if (ord == limit) {
      return doc=NO_MORE_DOCS;
    }
    ord++;
    final int code=freqIn.readVInt();
    doc+=code >>> 1;
    if ((code & 1) != 0) {
      freq=1;
    }
 else {
      freq=freqIn.readVInt();
    }
    posPendingCount+=freq;
    if (liveDocs == null || liveDocs.get(doc)) {
      break;
    }
  }
  position=0;
  return doc;
}
