{
  String[] s=new String[variables.size()];
  for (int i=0; i < variables.size(); i++) {
    Object val=resolver.resolve(variables.get(i));
    s[i]=val == null ? "" : getObjectAsString(val);
  }
  StringBuffer sb=new StringBuffer();
  for (int i=0; i < pcs.size(); i++) {
    sb.append(pcs.get(i));
    if (i < s.length) {
      sb.append(s[i]);
    }
  }
  return sb.toString();
}
