{
  if (this.summary == null && this.content != null) {
    if (this.content.getAtomMediaType() == AtomMediaType.BINARY || "".equals(this.content.getSrc())) {
      throw new GDataParseException(String.format(MISSING_ELEMENT_CHILD,this.localName,GOMSummary.LOCALNAME));
    }
  }
  if (this.id == null)   throw new GDataParseException(String.format(MISSING_ELEMENT_CHILD,this.localName,GOMId.LOCALNAME));
  if (this.title == null)   throw new GDataParseException(String.format(MISSING_ELEMENT_CHILD,this.localName,GOMTitle.LOCALNAME));
  if (this.updated == null)   throw new GDataParseException(String.format(MISSING_ELEMENT_CHILD,this.localName,GOMUpdated.LOCALNAME));
  List<GOMLink> alternateLinks=new LinkedList<GOMLink>();
  for (  GOMLink link : this.links) {
    if (link.getRel() == null || link.getRel().equalsIgnoreCase("alternate"))     alternateLinks.add(link);
  }
  if (alternateLinks.size() > 1) {
    for (    GOMLink link : alternateLinks) {
      for (      GOMLink link2 : alternateLinks) {
        if (AtomParserUtils.compareAlternateLinks(link,link2))         throw new GDataParseException(String.format(DUPLICATE_ELEMENT,"link with rel=\"alternate\" and same href and type attributes"));
      }
    }
  }
 else   if (this.content == null && alternateLinks.size() == 0) {
    throw new GDataParseException("Element Entry must contain a element link with attribute alternate if no content element is set");
  }
}
