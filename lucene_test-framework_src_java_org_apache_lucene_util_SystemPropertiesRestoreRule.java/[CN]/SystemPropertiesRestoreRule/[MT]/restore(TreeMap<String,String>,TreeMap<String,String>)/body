{
  after.keySet().removeAll(before.keySet());
  for (  String key : after.keySet()) {
    System.clearProperty(key);
  }
  for (  Map.Entry<String,String> e : before.entrySet()) {
    if (e.getValue() == null) {
      System.clearProperty(e.getKey());
    }
 else {
      System.setProperty(e.getKey(),e.getValue());
    }
  }
}
