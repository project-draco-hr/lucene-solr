{
  Token t=input.next();
  if (t == null)   return null;
  POSToken pt=new POSToken();
  pt.reinit(t);
  if (pt.termLength() > 0) {
    if (Character.isUpperCase(pt.termBuffer()[0])) {
      pt.setPartOfSpeech(POSToken.PROPERNOUN);
    }
 else {
      pt.setPartOfSpeech(POSToken.NO_NOUN);
    }
  }
  return pt;
}
