{
  StringBuilder sb=new StringBuilder();
  if (changedKeys != null) {
    sb.append("changes: ");
    for (    int i : changedKeys) {
      sb.append(i).append(" ");
    }
  }
  if (deletedKeys != null) {
    sb.append("deletes: ");
    for (    int i : deletedKeys) {
      sb.append(i).append(" ");
    }
  }
  if (addedKeys != null) {
    sb.append("adds: ");
    for (    int i : addedKeys) {
      sb.append(i).append(" ");
    }
  }
  return sb.toString();
}
