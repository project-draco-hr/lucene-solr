{
  Connection conn=null;
  Statement s=null;
  ResultSet rs=null;
  try {
    conn=newConnection();
    s=conn.createStatement();
    rs=s.executeQuery(query);
    List<String> results=new ArrayList<>();
    while (rs.next()) {
      results.add(rs.getString(1));
    }
    return results.toArray(new String[results.size()]);
  }
 catch (  SQLException e) {
    throw e;
  }
 finally {
    try {
      rs.close();
    }
 catch (    Exception ex) {
    }
    try {
      s.close();
    }
 catch (    Exception ex) {
    }
    try {
      conn.close();
    }
 catch (    Exception ex) {
    }
  }
}
