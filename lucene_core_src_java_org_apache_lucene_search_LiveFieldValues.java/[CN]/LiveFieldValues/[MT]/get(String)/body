{
  T value=current.get(id);
  if (value == missingValue) {
    return null;
  }
 else   if (value != null) {
    return value;
  }
 else {
    value=old.get(id);
    if (value == missingValue) {
      return null;
    }
 else     if (value != null) {
      return value;
    }
 else {
      S s=mgr.acquire();
      try {
        return lookupFromSearcher(s,id);
      }
  finally {
        mgr.release(s);
      }
    }
  }
}
