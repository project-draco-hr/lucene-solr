{
  int size=slices.length;
  int lo=0;
  int hi=size - 1;
  while (hi >= lo) {
    int mid=(lo + hi) >>> 1;
    int midValue=slices[mid].start;
    if (n < midValue)     hi=mid - 1;
 else     if (n > midValue)     lo=mid + 1;
 else {
      while (mid + 1 < size && slices[mid + 1].start == midValue) {
        mid++;
      }
      return mid;
    }
  }
  return hi;
}
