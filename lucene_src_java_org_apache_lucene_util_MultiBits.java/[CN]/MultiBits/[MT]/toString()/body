{
  StringBuilder b=new StringBuilder();
  b.append(subs.length + " subs: ");
  for (int i=0; i < subs.length; i++) {
    if (i != 0) {
      b.append("; ");
    }
    if (subs[i] == null) {
      b.append("s=" + starts[i] + " l=null");
    }
 else {
      b.append("s=" + starts[i] + " l="+ subs[i].length()+ " b="+ subs[i]);
    }
  }
  b.append(" end=" + starts[subs.length]);
  return b.toString();
}
