{
  assertQ("/schema/copyfields/?indent=on&wt=xml&dest.fl=title,*_s,*_dest_sub_s,dest_sub_no_ast_s","count(/response/arr[@name='copyfields']/lst) = 13","count(/response/arr[@name='copyfields']/lst/str[@name='dest'][.='title']) = 3","count(/response/arr[@name='copyfields']/lst/str[@name='dest'][.='*_s']) = 3","count(/response/arr[@name='copyfields']/lst/str[@name='dest'][.='*_dest_sub_s']) = 3","count(/response/arr[@name='copyfields']/lst/str[@name='dest'][.='dest_sub_no_ast_s']) = 4");
}
