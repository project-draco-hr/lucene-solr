{
  if (termInstances == 0) {
    return null;
  }
  if (indexedTermsArray == null) {
    final Terms terms=MultiFields.getTerms(reader,field);
    if (terms != null) {
      return terms.iterator();
    }
 else {
      return null;
    }
  }
 else {
    return new OrdWrappedTermsEnum(reader);
  }
}
