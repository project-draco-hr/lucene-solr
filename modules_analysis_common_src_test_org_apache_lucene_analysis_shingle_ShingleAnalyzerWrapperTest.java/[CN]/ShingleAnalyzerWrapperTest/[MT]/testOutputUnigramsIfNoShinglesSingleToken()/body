{
  ShingleAnalyzerWrapper analyzer=new ShingleAnalyzerWrapper(new MockAnalyzer(random,MockTokenizer.WHITESPACE,false),ShingleFilter.DEFAULT_MIN_SHINGLE_SIZE,ShingleFilter.DEFAULT_MAX_SHINGLE_SIZE,"",false,true);
  assertAnalyzesToReuse(analyzer,"please",new String[]{"please"},new int[]{0},new int[]{6},new int[]{1});
}
