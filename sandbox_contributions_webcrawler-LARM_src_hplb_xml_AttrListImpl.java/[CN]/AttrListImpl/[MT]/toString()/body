{
  StringBuffer sb=new StringBuffer();
  boolean f=true;
  int n=getLength();
  sb.append("{ ");
  for (int i=0; i < n; i++) {
    if (f) {
      f=false;
    }
 else {
      sb.append(", ");
    }
    Attribute attr=item(i);
    sb.append(attr.getName() + '=' + attr);
  }
  sb.append(" }");
  return sb.toString();
}
