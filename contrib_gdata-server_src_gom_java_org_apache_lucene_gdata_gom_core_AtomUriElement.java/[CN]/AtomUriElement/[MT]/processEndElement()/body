{
  if (this.textValue == null)   throw new GDataParseException(String.format(MISSING_ELEMENT_VALUE,this.localName,"atomUri"));
  try {
    AtomParserUtils.getAbsolutAtomURI(this.xmlBase,this.textValue);
  }
 catch (  URISyntaxException e) {
    throw new GDataParseException(String.format(INVALID_ELEMENT_VALUE,this.localName,"absolute uri"),e);
  }
}
