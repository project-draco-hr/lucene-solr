{
  if (this == o) {
    return true;
  }
  if (!(o instanceof SolrInputField)) {
    return false;
  }
  SolrInputField sif=(SolrInputField)o;
  if (!name.equals(sif.name)) {
    return false;
  }
  if (!value.equals(sif.value)) {
    return false;
  }
  if (Float.compare(sif.boost,boost) != 0) {
    return false;
  }
  return true;
}
