{
  DataOutputStream dos=new DataOutputStream(new BufferedOutputStream(new FileOutputStream(outputFile)));
  try {
    dos.writeInt(tfc.totalCounts.length);
    for (    int[] counts : tfc.totalCounts) {
      if (counts == null) {
        dos.writeInt(-1);
      }
 else {
        dos.writeInt(counts.length);
        for (        int i : counts) {
          dos.writeInt(i);
        }
      }
    }
  }
  finally {
    dos.close();
  }
}
