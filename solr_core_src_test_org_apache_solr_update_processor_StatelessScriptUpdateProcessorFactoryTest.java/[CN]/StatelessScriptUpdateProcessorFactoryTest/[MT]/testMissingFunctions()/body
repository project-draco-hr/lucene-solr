{
  final String chain="missing-functions";
  try {
    SolrInputDocument d=processAdd(chain,doc(f("id","5"),f("name"," foo "),f("subject","bar")));
  }
 catch (  SolrException e) {
    assertTrue("Exception doesn't contain expected error: " + e.getMessage(),0 < e.getMessage().indexOf("processAdd"));
    return;
  }
  fail("Did not get exception from script");
}
