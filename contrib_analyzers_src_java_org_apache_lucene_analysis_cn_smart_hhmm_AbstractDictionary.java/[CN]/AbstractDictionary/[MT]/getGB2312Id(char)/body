{
  try {
    byte[] buffer=Character.toString(ch).getBytes("GB2312");
    if (buffer.length != 2) {
      return -1;
    }
    int b0=(int)(buffer[0] & 0x0FF) - 161;
    int b1=(int)(buffer[1] & 0x0FF) - 161;
    return (short)(b0 * 94 + b1);
  }
 catch (  UnsupportedEncodingException e) {
    e.printStackTrace();
  }
  return -1;
}
