{
  if (pointInRect(aX1,aY1,rMinX,rMinY,rMaxX,rMaxY) ? !pointInRect(aX2,aY2,rMinX,rMinY,rMaxX,rMaxY) : pointInRect(aX2,aY2,rMinX,rMinY,rMaxX,rMaxY)) {
    return true;
  }
  return lineCrossesLine(aX1,aY1,aX2,aY2,rMinX,rMinY,rMaxX,rMaxY) || lineCrossesLine(aX1,aY1,aX2,aY2,rMaxX,rMinY,rMinX,rMaxY);
}
