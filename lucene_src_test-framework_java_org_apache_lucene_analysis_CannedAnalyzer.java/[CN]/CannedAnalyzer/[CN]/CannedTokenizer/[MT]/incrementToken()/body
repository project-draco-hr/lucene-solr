{
  if (upto < tokens.length) {
    final Token token=tokens[upto++];
    clearAttributes();
    termAtt.setEmpty();
    termAtt.append(token.toString());
    posIncrAtt.setPositionIncrement(token.getPositionIncrement());
    offsetAtt.setOffset(token.startOffset(),token.endOffset());
    return true;
  }
 else {
    return false;
  }
}
