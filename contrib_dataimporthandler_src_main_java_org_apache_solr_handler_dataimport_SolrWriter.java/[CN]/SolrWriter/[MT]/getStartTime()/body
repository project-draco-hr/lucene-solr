{
  Properties props=readIndexerProperties();
  String result=props.getProperty(SolrWriter.LAST_INDEX_KEY);
  try {
    if (result != null)     return DataImporter.DATE_TIME_FORMAT.get().parse(result);
  }
 catch (  ParseException e) {
    throw new DataImportHandlerException(DataImportHandlerException.WARN,"Unable to read last indexed time from: " + SolrWriter.IMPORTER_PROPERTIES,e);
  }
  return null;
}
