{
  ByteArrayOutputStream baos=new ByteArrayOutputStream(1024);
  byte[] buf=new byte[1024];
  int sz=0;
  try {
    while ((sz=in.read(buf)) != -1) {
      baos.write(buf,0,sz);
    }
  }
  finally {
    try {
      in.close();
    }
 catch (    Exception e) {
    }
  }
  return new String(baos.toByteArray(),"UTF-8");
}
