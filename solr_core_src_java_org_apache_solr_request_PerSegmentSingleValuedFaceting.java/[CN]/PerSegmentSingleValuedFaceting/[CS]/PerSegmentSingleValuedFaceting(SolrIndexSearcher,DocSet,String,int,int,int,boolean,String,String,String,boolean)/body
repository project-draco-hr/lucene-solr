{
  this.searcher=searcher;
  this.docs=docs;
  this.fieldName=fieldName;
  this.offset=offset;
  this.limit=limit;
  this.mincount=mincount;
  this.missing=missing;
  this.sort=sort;
  this.prefix=prefix;
  this.containsBR=contains != null ? new BytesRef(contains) : null;
  this.ignoreCase=ignoreCase;
}
