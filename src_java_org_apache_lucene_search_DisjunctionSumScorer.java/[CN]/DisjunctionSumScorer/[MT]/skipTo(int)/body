{
  if (scorerDocQueue.size() < minimumNrMatchers) {
    return false;
  }
  if (target <= currentDoc) {
    return true;
  }
  do {
    if (scorerDocQueue.topDoc() >= target) {
      return advanceAfterCurrent();
    }
 else     if (!scorerDocQueue.topSkipToAndAdjustElsePop(target)) {
      if (scorerDocQueue.size() < minimumNrMatchers) {
        return false;
      }
    }
  }
 while (true);
}
