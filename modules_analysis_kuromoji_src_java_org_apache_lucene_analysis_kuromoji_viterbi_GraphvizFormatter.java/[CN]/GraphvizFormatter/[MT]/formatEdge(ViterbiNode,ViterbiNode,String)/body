{
  StringBuilder sb=new StringBuilder();
  sb.append(getNodeId(from));
  sb.append(" -> ");
  sb.append(getNodeId(to));
  sb.append(" [ ");
  sb.append("label=\"");
  sb.append(getCost(from,to));
  sb.append("\"");
  sb.append(" ");
  sb.append(attributes);
  sb.append(" ");
  sb.append(" ]");
  sb.append("\n");
  return sb.toString();
}
