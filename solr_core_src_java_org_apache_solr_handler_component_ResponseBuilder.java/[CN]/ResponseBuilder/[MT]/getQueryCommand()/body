{
  QueryCommand cmd=new QueryCommand();
  cmd.setQuery(wrap(getQuery())).setFilterList(getFilters()).setSort(getSortSpec().getSort()).setOffset(getSortSpec().getOffset()).setLen(getSortSpec().getCount()).setFlags(getFieldFlags()).setNeedDocSet(isNeedDocSet()).setCursorMark(getCursorMark());
  return cmd;
}
