{
  lock=new CountDownLatch(1);
  try {
    for (; ; ) {
      Runner runner;
synchronized (runners) {
        runner=runners.peek();
      }
      if ((runner == null && queue.isEmpty()) || scheduler.isTerminated())       break;
      if (runner != null) {
        runner.runnerLock.lock();
        runner.runnerLock.unlock();
      }
    }
  }
  finally {
    lock.countDown();
    lock=null;
  }
}
