{
  StringBuilder buffer=new StringBuilder();
  if (!term.field().equals(field)) {
    buffer.append(term.field());
    buffer.append(":");
  }
  buffer.append(getClass().getSimpleName());
  buffer.append(" {");
  buffer.append('\n');
  buffer.append(automaton.toString());
  buffer.append("}");
  return buffer.toString();
}
