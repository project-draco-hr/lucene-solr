{
  String[] output=new String[]{"the","quick","brown","LuceneFox","jumps"};
  assertTokenStreamContents(new LowerCaseFilterMock(new PatternKeywordMarkerFilter(whitespaceMockTokenizer("The quIck browN LuceneFox Jumps"),Pattern.compile("[a-zA-Z]+[fF]ox"))),output);
  output=new String[]{"the","quick","brown","lucenefox","jumps"};
  assertTokenStreamContents(new LowerCaseFilterMock(new PatternKeywordMarkerFilter(whitespaceMockTokenizer("The quIck browN LuceneFox Jumps"),Pattern.compile("[a-zA-Z]+[f]ox"))),output);
}
