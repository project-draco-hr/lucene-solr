{
  IntHashSet set0=new IntHashSet();
  assertEquals(0,set0.size());
  assertTrue(set0.isEmpty());
  for (int i=0; i < 1000; ++i) {
    set0.add(1);
    set0.add(-382);
  }
  assertEquals(2,set0.size());
  assertFalse(set0.isEmpty());
  set0.remove(-382);
  set0.remove(1);
  assertEquals(0,set0.size());
  assertTrue(set0.isEmpty());
}
