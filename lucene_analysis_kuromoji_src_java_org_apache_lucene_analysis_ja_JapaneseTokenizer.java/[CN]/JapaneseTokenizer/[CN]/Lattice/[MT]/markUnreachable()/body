{
  for (int index=1; index < rootSize - 1; ++index) {
    if (rRoot[index] < 0) {
      for (int node=lRoot[index]; 0 <= node; node=nodeLeftChain[node]) {
        if (VERBOSE) {
          System.out.printf("DEBUG: markUnreachable: node=%d\n",node);
        }
        nodeMark[node]=-1;
      }
    }
  }
}
