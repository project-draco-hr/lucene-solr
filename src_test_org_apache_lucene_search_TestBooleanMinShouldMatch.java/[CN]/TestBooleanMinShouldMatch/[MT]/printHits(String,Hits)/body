{
  System.err.println("------- " + test + " -------");
  DecimalFormat f=new DecimalFormat("0.000000");
  for (int i=0; i < h.length(); i++) {
    Document d=h.doc(i);
    float score=h.score(i);
    System.err.println("#" + i + ": "+ f.format(score)+ " - "+ d.get("id")+ " - "+ d.get("data"));
  }
}
