{
  super(args);
  String dictionaryName=get(args,DICTIONARY_SCHEMA_ATTRIBUTE);
  if (dictionaryName != null && !dictionaryName.isEmpty()) {
    throw new IllegalArgumentException("The " + DICTIONARY_SCHEMA_ATTRIBUTE + " attribute is no "+ "longer supported (Morfologik now offers one unified Polish dictionary): "+ dictionaryName+ ". Perhaps you wanted to use 'dictionary-resource' attribute instead?");
  }
  dictionaryResource=get(args,DICTIONARY_RESOURCE_ATTRIBUTE,DEFAULT_DICTIONARY_RESOURCE);
  if (!args.isEmpty()) {
    throw new IllegalArgumentException("Unknown parameters: " + args);
  }
}
