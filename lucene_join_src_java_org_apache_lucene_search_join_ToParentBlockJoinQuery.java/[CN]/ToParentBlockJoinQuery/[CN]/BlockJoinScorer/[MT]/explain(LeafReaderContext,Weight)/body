{
  int start=context.docBase + prevParentDoc + 1;
  int end=context.docBase + parentDoc - 1;
  Explanation bestChild=null;
  int matches=0;
  for (int childDoc=start; childDoc <= end; childDoc++) {
    Explanation child=childWeight.explain(context,childDoc - context.docBase);
    if (child.isMatch()) {
      matches++;
      if (bestChild == null || child.getValue() > bestChild.getValue()) {
        bestChild=child;
      }
    }
  }
  return Explanation.match(score(),String.format(Locale.ROOT,"Score based on %d child docs in range from %d to %d, best match:",matches,start,end),bestChild);
}
