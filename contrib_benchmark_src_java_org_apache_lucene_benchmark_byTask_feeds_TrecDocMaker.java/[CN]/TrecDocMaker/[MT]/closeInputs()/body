{
  if (zis != null) {
    try {
      zis.close();
    }
 catch (    IOException e) {
      System.out.println("closeInputs(): Ingnoring error: " + e);
      e.printStackTrace();
    }
    zis=null;
  }
  if (reader != null) {
    try {
      reader.close();
    }
 catch (    IOException e) {
      System.out.println("closeInputs(): Ingnoring error: " + e);
      e.printStackTrace();
    }
    reader=null;
  }
}
