{
  DateFormat df[]=(DateFormat[])dateFormat.get();
  if (df == null) {
    df=new SimpleDateFormat[DATE_FORMATS.length];
    for (int i=0; i < df.length; i++) {
      df[i]=new SimpleDateFormat(DATE_FORMATS[i],Locale.US);
      df[i].setLenient(true);
    }
    dateFormat.set(df);
  }
  return df[n];
}
