{
  Date date=null;
  for (int i=0; i < dateFormat.length; i++) {
    try {
      date=dateFormat[i].parse(dateStr.trim());
      return date;
    }
 catch (    ParseException e) {
    }
  }
  System.out.println("ignoring date parse exception (assigning 'now') for: " + dateStr);
  date=new Date();
  return date;
}
