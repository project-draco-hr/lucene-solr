{
  for (int i=0; i < ints.length; i++) {
    if ((i > 0) && (ints[i - 1] == ints[i])) {
      return;
    }
  }
  DocIdSetIterator m=vintList.iterator();
  for (int i=0; i < ints.length; i++) {
    assertTrue("No end of Matcher at: " + i,m.next());
    assertEquals(ints[i],m.doc());
  }
  assertTrue("End of Matcher",(!m.next()));
}
