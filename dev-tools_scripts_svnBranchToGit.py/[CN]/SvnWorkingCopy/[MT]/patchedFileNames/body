def patchedFileNames(self, patchFileName):
    indexPrefix = 'Index: '
    regExp = ('^' + indexPrefix)
    patchedFileNamesLines = self.check_output(('grep', regExp, patchFileName))
    indexPrefixLength = len(indexPrefix)
    return [line[indexPrefixLength:] for line in patchedFileNamesLines.split('\n') if (len(line) > 0)]
