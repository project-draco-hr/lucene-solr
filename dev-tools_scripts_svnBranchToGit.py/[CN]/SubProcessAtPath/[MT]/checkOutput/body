def checkOutput(self, *args, **kwArgs):
    assert (type(*args) != types.StringType)
    self.chDirToPath()
    if self.verbose:
        print 'check_output args:', ' '.join(*args)
    result = subprocess.check_output(*args, **kwArgs)
    if self.verbose:
        print 'check_output result:', result
    return result
