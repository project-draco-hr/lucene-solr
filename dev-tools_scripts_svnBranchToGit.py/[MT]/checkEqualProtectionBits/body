def checkEqualProtectionBits(fn1, fn2):
    stat1 = os.stat(fn1)
    stat2 = os.stat(fn2)
    if (stat1.st_mode != stat2.st_mode):
        print(('Protection bits %s of %s' % (octal(stat1.st_mode), fn1)))
        print(('Protection bits %s of %s' % (octal(stat2.st_mode), fn2)))
        return False
    return True
