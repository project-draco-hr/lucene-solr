{
  IndexWriter writer=new IndexWriter(dir,newIndexWriterConfig(TEST_VERSION_CURRENT,new MockAnalyzer()).setOpenMode(OpenMode.CREATE).setMaxBufferedDocs(2).setMergePolicy(newInOrderLogMergePolicy(2)));
  for (int i=start; i < (start + numDocs); i++) {
    Document temp=new Document();
    temp.add(newField("count",("" + i),Field.Store.YES,Field.Index.NOT_ANALYZED));
    writer.addDocument(temp);
  }
  writer.close();
}
