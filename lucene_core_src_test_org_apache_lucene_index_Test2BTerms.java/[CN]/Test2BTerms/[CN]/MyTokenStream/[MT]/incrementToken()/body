{
  clearAttributes();
  if (tokenCount >= tokensPerDoc) {
    return false;
  }
  int shift=32;
  for (int i=0; i < 5; i++) {
    bytes.bytes[i]=(byte)((termCounter >> shift) & 0xFF);
    shift-=8;
  }
  termCounter++;
  tokenCount++;
  if (--nextSave == 0) {
    savedTerms.add(BytesRef.deepCopyOf(bytes));
    System.out.println("TEST: save term=" + bytes);
    nextSave=TestUtil.nextInt(random,500000,1000000);
  }
  return true;
}
