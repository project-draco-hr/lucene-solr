{
  do {
    if (lead.doc == DocsEnum.NO_MORE_DOCS) {
      return NO_MORE_DOCS;
    }
    advanceHead:     do {
      for (int i=1; i < docsAndFreqs.length; i++) {
        if (docsAndFreqs[i].doc < doc) {
          docsAndFreqs[i].doc=docsAndFreqs[i].docs.advance(doc);
        }
        if (docsAndFreqs[i].doc > doc) {
          break advanceHead;
        }
      }
      return doc;
    }
 while (true);
    doc=lead.doc=lead.docs.nextDoc();
  }
 while (true);
}
