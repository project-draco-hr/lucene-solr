{
  if ((index.getDeletedDocuments() != null && index.getDeletedDocuments().get(docNum)) || (uncommittedDeletedDocuments != null && uncommittedDeletedDocuments.get(docNum))) {
    return;
  }
  if (uncommittedDeletedDocuments == null) {
    uncommittedDeletedDocuments=new BitVector(maxDoc());
  }
  uncommittedDeletedDocuments.set(docNum);
}
