{
  if (isOpen) {
    boolean success=false;
    try {
      super.close();
      success=true;
    }
  finally {
      isOpen=false;
      if (!success) {
        try {
          file.close();
          parent.onIndexOutputClosed(this);
        }
 catch (        Throwable t) {
        }
      }
 else       file.close();
    }
  }
}
