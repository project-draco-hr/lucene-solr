{
  SimpleCJKAnalyzer analyzer=new SimpleCJKAnalyzer();
  BooleanQuery expected=new BooleanQuery();
  expected.add(new TermQuery(new Term("field","???")),BooleanClause.Occur.SHOULD);
  expected.add(new TermQuery(new Term("field","???")),BooleanClause.Occur.SHOULD);
