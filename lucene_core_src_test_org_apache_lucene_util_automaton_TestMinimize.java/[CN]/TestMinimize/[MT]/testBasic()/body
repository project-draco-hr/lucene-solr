{
  int num=atLeast(200);
  for (int i=0; i < num; i++) {
    Automaton a=AutomatonTestUtil.randomAutomaton(random());
    Automaton la=Operations.determinize(Operations.removeDeadStates(a),DEFAULT_MAX_DETERMINIZED_STATES);
    Automaton lb=MinimizationOperations.minimize(a,DEFAULT_MAX_DETERMINIZED_STATES);
    assertTrue(Operations.sameLanguage(la,lb));
  }
}
