{
  System.err.println("------- " + test + " -------");
  DecimalFormat f=new DecimalFormat("0.000000000",DecimalFormatSymbols.getInstance(Locale.ROOT));
  for (int i=0; i < h.length; i++) {
    Document d=searcher.doc(h[i].doc);
