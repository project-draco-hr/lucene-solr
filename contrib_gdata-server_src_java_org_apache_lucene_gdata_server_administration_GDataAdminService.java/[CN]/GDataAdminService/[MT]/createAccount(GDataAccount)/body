{
  if (account == null)   throw new ServiceException("Can not save null account");
  try {
    this.storage.storeAccount(account);
  }
 catch (  StorageException e) {
    if (LOG.isInfoEnabled())     LOG.info("Can not save account -- " + e.getMessage(),e);
    throw new ServiceException("Can not save account",e);
  }
}
