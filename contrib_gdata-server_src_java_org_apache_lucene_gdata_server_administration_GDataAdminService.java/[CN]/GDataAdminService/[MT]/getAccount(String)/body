{
  if (accountName == null)   throw new ServiceException("Can not get null account",GDataResponse.BAD_REQUEST);
  try {
    return this.storage.getAccount(accountName);
  }
 catch (  StorageException e) {
    if (LOG.isInfoEnabled())     LOG.info("Can not get account -- " + e.getMessage(),e);
    throw new ServiceException("Can not get account",e,GDataResponse.BAD_REQUEST);
  }
}
