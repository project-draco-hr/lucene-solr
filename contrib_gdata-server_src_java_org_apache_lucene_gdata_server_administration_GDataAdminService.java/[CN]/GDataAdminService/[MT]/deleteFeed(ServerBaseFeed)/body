{
  if (feed == null)   throw new ServiceException("Can not delete null feed");
  if (feed.getId() == null)   throw new ServiceException("Feed ID is null can not delete feed");
  try {
    this.storage.deleteFeed(feed.getId());
  }
 catch (  StorageException e) {
    if (LOG.isInfoEnabled())     LOG.info("Can not delete feed -- " + e.getMessage(),e);
    throw new ServiceException("Can not delete feed",e);
  }
}
