{
  if (feed == null)   throw new ServiceException("Can not delete null feed",GDataResponse.BAD_REQUEST);
  if (feed.getId() == null)   throw new ServiceException("Feed ID is null can not delete feed",GDataResponse.BAD_REQUEST);
  try {
    this.storage.deleteFeed(feed.getId());
  }
 catch (  StorageException e) {
    if (LOG.isInfoEnabled())     LOG.info("Can not delete feed -- " + e.getMessage(),e);
    throw new ServiceException("Can not delete feed",e,GDataResponse.SERVER_ERROR);
  }
}
