{
  CharArraySet set=new CharArraySet(Version.LUCENE_31,1,true);
  set.add("??????????????");
  MockTokenizer tokenStream=new MockTokenizer(new StringReader("?????????????????? ??????????????"),MockTokenizer.WHITESPACE,false);
  BulgarianStemFilter filter=new BulgarianStemFilter(new KeywordMarkerFilter(tokenStream,set));
  assertTokenStreamContents(filter,new String[]{"??????????","??????????????"});
}
