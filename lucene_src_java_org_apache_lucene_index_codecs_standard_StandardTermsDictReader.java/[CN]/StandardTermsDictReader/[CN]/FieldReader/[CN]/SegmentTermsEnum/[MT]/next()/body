{
  if (seekPending) {
    seekPending=false;
    in.seek(state.filePointer);
  }
  if (state.ord >= numTerms - 1) {
    return null;
  }
  bytesReader.read();
  state.docFreq=in.readVInt();
  postingsReader.readTerm(in,fieldInfo,state,indexReader.isIndexTerm(1 + state.ord,state.docFreq,false));
  state.ord++;
  return bytesReader.term;
}
