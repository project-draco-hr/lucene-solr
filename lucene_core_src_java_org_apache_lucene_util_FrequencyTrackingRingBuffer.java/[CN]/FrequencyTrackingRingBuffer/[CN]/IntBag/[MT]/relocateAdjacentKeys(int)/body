{
  for (int slot=(freeSlot + 1) & mask; ; slot=(slot + 1) & mask) {
    final int freq=freqs[slot];
    if (freq == 0) {
      break;
    }
    final int key=keys[slot];
    final int expectedSlot=key & mask;
    if (between(expectedSlot,slot,freeSlot)) {
      keys[freeSlot]=key;
      freqs[freeSlot]=freq;
      freqs[slot]=0;
      freeSlot=slot;
    }
  }
}
