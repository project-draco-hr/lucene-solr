{
  if (data == null || data.trim().length() == 0) {
    return null;
  }
  Map<String,TermStats> map=new HashMap<>();
  String[] entries=data.split("!");
  for (  String es : entries) {
    TermStats termStats=termStatsFromString(es,null);
    if (termStats != null) {
      map.put(termStats.term,termStats);
    }
  }
  return map;
}
