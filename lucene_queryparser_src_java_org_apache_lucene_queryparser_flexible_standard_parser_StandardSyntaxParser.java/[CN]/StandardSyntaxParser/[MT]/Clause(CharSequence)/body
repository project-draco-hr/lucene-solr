}
switch ((jj_ntk == -1) ? jj_ntk() : jj_ntk) {
case QUOTED:
case TERM:
case REGEXPTERM:
case RANGEIN_START:
case RANGEEX_START:
case NUMBER:
q=Term(field);
break;
case LPAREN:
jj_consume_token(LPAREN);
q=Query(field);
jj_consume_token(RPAREN);
switch ((jj_ntk == -1) ? jj_ntk() : jj_ntk) {
case CARAT:
jj_consume_token(CARAT);
boost=jj_consume_token(NUMBER);
break;
default :
jj_la1[10]=jj_gen;
;
}
group=true;
break;
default :
jj_la1[11]=jj_gen;
jj_consume_token(-1);
throw new ParseException();
}
break;
default :
jj_la1[12]=jj_gen;
jj_consume_token(-1);
throw new ParseException();
}
}
if (boost != null) {
float f=(float)1.0;
try {
f=Float.valueOf(boost.image).floatValue();
if (q != null) {
q=new BoostQueryNode(q,f);
}
}
 catch (Exception ignored) {
}
}
if (group) {
q=new GroupQueryNode(q);
}
{
if (true) return q;
}
throw new Error("Missing return statement in function");
}
