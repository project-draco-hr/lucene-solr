case OP_COLON:
case OP_EQUAL:
switch ((jj_ntk == -1) ? jj_ntk() : jj_ntk) {
case OP_COLON:
        jj_consume_token(OP_COLON);
      break;
case OP_EQUAL:
    jj_consume_token(OP_EQUAL);
  break;
default :
jj_la1[5]=jj_gen;
