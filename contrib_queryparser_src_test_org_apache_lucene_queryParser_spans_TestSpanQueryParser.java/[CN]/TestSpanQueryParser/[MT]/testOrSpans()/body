{
  assertEquals(getSpanQuery("term1 term2").toString(),"spanOr([term1, term2])");
  assertEquals(getSpanQuery("term1 OR term2").toString(),"spanOr([term1, term2])");
  assertTrue(getSpanQuery("term1 term2") instanceof SpanOrQuery);
  assertTrue(getSpanQuery("term1 term2") instanceof SpanOrQuery);
}
