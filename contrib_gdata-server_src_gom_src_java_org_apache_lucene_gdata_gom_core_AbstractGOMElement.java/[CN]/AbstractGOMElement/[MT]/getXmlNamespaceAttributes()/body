{
  List<GOMAttribute> retVal=new LinkedList<GOMAttribute>();
  if (this.xmlBase != null)   retVal.add(new GOMAttributeImpl(GOMNamespace.XML_NS_URI,GOMNamespace.XML_NS_PREFIX,"base",this.xmlBase));
  if (this.xmlLang != null)   retVal.add(new GOMAttributeImpl(GOMNamespace.XML_NS_URI,GOMNamespace.XML_NS_PREFIX,"lang",this.xmlLang));
  return retVal;
}
