{
  if (done) {
    return null;
  }
  line=in.readLine();
  if (line != null) {
    String[] fields=line.split("\t");
    if (fields.length > 1) {
      curFreq=Float.parseFloat(fields[1]);
      spare.copyChars(fields[0]);
    }
 else {
      spare.copyChars(line);
      curFreq=1;
    }
    return spare;
  }
 else {
    done=true;
    IOUtils.close(in);
    return null;
  }
}
