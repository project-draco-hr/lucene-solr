{
  super(tenum);
  this.maxShift=query.maxShift;
  this.relationImpl=query.cellComparator;
  this.shift=maxShift;
  final long mask=(1L << shift) - 1;
  this.start=query.minEncoded & ~mask;
  this.end=start | mask;
  this.currentCellBRB=new BytesRefBuilder();
  this.range=new Range(-1,shift,true);
}
