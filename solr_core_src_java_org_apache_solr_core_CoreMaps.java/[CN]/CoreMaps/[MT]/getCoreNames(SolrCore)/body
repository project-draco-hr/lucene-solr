{
  List<String> lst=new ArrayList<String>();
synchronized (locker) {
    for (    Map.Entry<String,SolrCore> entry : cores.entrySet()) {
      if (core == entry.getValue()) {
        lst.add(entry.getKey());
      }
    }
    for (    Map.Entry<String,SolrCore> entry : transientCores.entrySet()) {
      if (core == entry.getValue()) {
        lst.add(entry.getKey());
      }
    }
  }
  return lst;
}
