{
  Hits result=searcher.search(query,sort);
  StringBuffer buff=new StringBuffer(10);
  int n=result.length();
  for (int i=0; i < n; ++i) {
    Document doc=result.doc(i);
    String[] v=doc.getValues("tracer");
    for (int j=0; j < v.length; ++j) {
      buff.append(v[j]);
    }
  }
  assertEquals(expectedResult,buff.toString());
}
