{
  final GeoCompositePolygon rval=new GeoCompositePolygon();
  if (buildPolygonShape(rval,planetModel,pointList,new BitSet(),convexPointIndex,getLegalIndex(convexPointIndex + 1,pointList.size()),new SidedPlane(pointList.get(getLegalIndex(convexPointIndex - 1,pointList.size())),pointList.get(convexPointIndex),pointList.get(getLegalIndex(convexPointIndex + 1,pointList.size()))),holes,null) == false) {
    return null;
  }
  return rval;
}
