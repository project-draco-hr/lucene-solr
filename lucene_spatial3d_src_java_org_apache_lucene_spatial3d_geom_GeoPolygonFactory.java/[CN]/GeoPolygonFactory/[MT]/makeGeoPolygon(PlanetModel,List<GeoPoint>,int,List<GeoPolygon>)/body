{
  return buildPolygonShape(new GeoCompositePolygon(),planetModel,pointList,new BitSet(),convexPointIndex,getLegalIndex(convexPointIndex + 1,pointList.size()),new SidedPlane(pointList.get(getLegalIndex(convexPointIndex - 1,pointList.size())),pointList.get(convexPointIndex),pointList.get(getLegalIndex(convexPointIndex + 1,pointList.size()))),holes,null);
}
