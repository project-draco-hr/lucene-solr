{
  File f=makeFile(CONTENTS);
  HttpRequestBase m=getUpdateMethod("GET",CommonParams.STREAM_FILE,f.getCanonicalPath(),CommonParams.STREAM_CONTENTTYPE,"text/csv");
  HttpResponse response=getClient().execute(m);
  assertEquals(200,response.getStatusLine().getStatusCode());
  checkVetoHeaders(response,true);
  Files.delete(f.toPath());
}
