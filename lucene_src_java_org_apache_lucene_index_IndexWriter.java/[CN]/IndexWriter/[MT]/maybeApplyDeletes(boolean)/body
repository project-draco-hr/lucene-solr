{
  if (applyAllDeletes) {
    if (infoStream != null) {
      message("apply all deletes during flush");
    }
    applyAllDeletes();
  }
 else   if (infoStream != null) {
    message("don't apply deletes now delTermCount=" + bufferedDeletesStream.numTerms() + " bytesUsed="+ bufferedDeletesStream.bytesUsed());
  }
}
