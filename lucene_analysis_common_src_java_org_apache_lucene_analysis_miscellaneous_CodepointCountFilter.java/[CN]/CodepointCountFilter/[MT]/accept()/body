{
  final int max32=termAtt.length();
  final int min32=max32 >> 1;
  if (min32 >= min && max32 <= max) {
    return true;
  }
 else   if (min32 > max || max32 < min) {
    return false;
  }
 else {
    int len=Character.codePointCount(termAtt.buffer(),0,termAtt.length());
    return (len >= min && len <= max);
  }
}
