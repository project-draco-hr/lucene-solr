{
  super(args);
  String cfgLanguage=args.remove("language");
  if (cfgLanguage == null) {
    language="English";
  }
 else {
    language=cfgLanguage;
  }
  wordFiles=args.remove(PROTECTED_TOKENS);
  if (!args.isEmpty()) {
    throw new IllegalArgumentException("Unknown parameters: " + args);
  }
}
