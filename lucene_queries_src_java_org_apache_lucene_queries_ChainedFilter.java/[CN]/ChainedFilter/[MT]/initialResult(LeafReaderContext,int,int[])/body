{
  LeafReader reader=context.reader();
  FixedBitSet result=new FixedBitSet(reader.maxDoc());
  if (logic == AND) {
    result.or(getDISI(chain[index[0]],context));
    ++index[0];
  }
 else   if (logic == ANDNOT) {
    result.or(getDISI(chain[index[0]],context));
    result.flip(0,reader.maxDoc());
    ++index[0];
  }
  return result;
}
