{
  String startDate=getLocalizedDate(2002,1,1,false);
  String endDate=getLocalizedDate(2002,1,4,false);
  Calendar endDateExpected=Calendar.getInstance();
  endDateExpected.set(2002,1,4,23,59,59);
  endDateExpected.set(Calendar.MILLISECOND,999);
  assertQueryEquals("[ " + startDate + " TO "+ endDate+ "]",null,"[" + getLegacyDate(startDate) + " TO "+ DateField.dateToString(endDateExpected.getTime())+ "]");
  assertQueryEquals("{  " + startDate + "    "+ endDate+ "   }",null,"{" + getLegacyDate(startDate) + " TO "+ getLegacyDate(endDate)+ "}");
}
