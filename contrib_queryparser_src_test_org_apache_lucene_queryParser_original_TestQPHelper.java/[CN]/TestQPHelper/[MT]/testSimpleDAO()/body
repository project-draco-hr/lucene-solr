{
  assertQueryEqualsDOA("term term term",null,"+term +term +term");
  assertQueryEqualsDOA("term +term term",null,"+term +term +term");
  assertQueryEqualsDOA("term term +term",null,"+term +term +term");
  assertQueryEqualsDOA("term +term +term",null,"+term +term +term");
  assertQueryEqualsDOA("-term term term",null,"-term +term +term");
}
