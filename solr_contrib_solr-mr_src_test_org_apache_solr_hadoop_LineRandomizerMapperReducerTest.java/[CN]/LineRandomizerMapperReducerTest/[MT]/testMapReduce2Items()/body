{
  mapReduceDriver.withAll(Arrays.asList(new Pair<LongWritable,Text>(new LongWritable(0),new Text("hello")),new Pair<LongWritable,Text>(new LongWritable(1),new Text("world"))));
  mapReduceDriver.withAllOutput(Arrays.asList(new Pair<Text,NullWritable>(new Text("world"),NullWritable.get()),new Pair<Text,NullWritable>(new Text("hello"),NullWritable.get())));
  mapReduceDriver.runTest();
}
