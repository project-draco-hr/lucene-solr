{
  if (globalFieldNumbers != null) {
    globalFieldNumbers.setIfNotSet(fieldNumber,name);
  }
  final FieldInfo fi=new FieldInfo(name,isIndexed,fieldNumber,storeTermVector,storePositionWithTermVector,storeOffsetWithTermVector,omitNorms,storePayloads,indexOptions,docValuesType);
  putInternal(fi);
  return fi;
}
