{
  StringBuilder sb=new StringBuilder();
  if (baseUrl == null)   return null;
  if (coreName == null)   return null;
  sb.append(baseUrl);
  if (!baseUrl.endsWith("/"))   sb.append("/");
  sb.append(coreName == null ? "" : coreName);
  if (!(sb.substring(sb.length() - 1).equals("/")))   sb.append("/");
  return sb.toString();
}
