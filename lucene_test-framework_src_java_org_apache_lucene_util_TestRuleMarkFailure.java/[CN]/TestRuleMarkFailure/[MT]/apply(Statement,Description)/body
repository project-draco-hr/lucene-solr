{
  return new Statement(){
    @Override public void evaluate() throws Throwable {
      failures=false;
      try {
        s.evaluate();
      }
 catch (      Throwable t) {
        for (        Throwable t2 : expandFromMultiple(t)) {
          if (!(t2 instanceof AssumptionViolatedException)) {
            markFailed();
            break;
          }
        }
        throw t;
      }
    }
  }
;
}
