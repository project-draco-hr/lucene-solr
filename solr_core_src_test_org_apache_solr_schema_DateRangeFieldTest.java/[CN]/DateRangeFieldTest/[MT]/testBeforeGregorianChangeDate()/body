{
  assertU(delQ("*:*"));
  assertU(adoc("id","0","dateRange","1500-01-01T00:00:00Z"));
  assertU(adoc("id","1","dateRange","-1500-01-01T00:00:00Z"));
  assertU(adoc("id","2","dateRange","1400-01-01T00:00:00Z/YEAR"));
  assertU(adoc("id","3","dateRange","1300"));
  assertU(commit());
  assertQ(req("q","id:0","fl","dateRange"),"//result/doc/arr[@name='dateRange']/str[.='1500-01-01T00:00:00Z']");
  assertQ(req("q","id:1","fl","dateRange"),"//result/doc/arr[@name='dateRange']/str[.='-1500-01-01T00:00:00Z']");
  assertQ(req("q","id:2","fl","dateRange"),"//result/doc/arr[@name='dateRange']/str[.='1400-01-01T00:00:00Z']");
  assertQ(req("q","id:3","fl","dateRange"),"//result/doc/arr[@name='dateRange']/str[.='1300']");
  assertQ(req("q","dateRange:[1450-01-01T00:00:00Z TO 1499-12-31T23:59:59Z]"),xpathMatches());
  assertQ(req("q","dateRange:[1500-01-01T00:00:00Z TO 1500-01-01T00:00:00Z]"),xpathMatches(0));
  assertQ(req("q","dateRange:[1500-01-01T00:00:01Z TO 1550-01-01T00:00:00Z]"),xpathMatches());
  assertQ(req("q","dateRange:[-1500-01-01T00:00:00Z TO -1500-01-01T00:00:00Z]"),xpathMatches(1));
  assertQ(req("q","dateRange:[1299 TO 1299-12-31T23:59:59Z]"),xpathMatches());
  assertQ(req("q","dateRange:[1299 TO 1300-01-01T00:00:00Z]"),xpathMatches(3));
  assertQ(req("q","dateRange:1301"),xpathMatches());
  assertQ(req("q","dateRange:[1300-12-31T23:59:59Z TO 1301]"),xpathMatches(3));
}
