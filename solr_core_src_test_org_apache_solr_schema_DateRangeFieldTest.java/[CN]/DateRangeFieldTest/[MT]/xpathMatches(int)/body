{
  String[] tests=new String[docIds != null ? docIds.length + 1 : 1];
  tests[0]="*[count(//doc)=" + (tests.length - 1) + "]";
  if (docIds != null && docIds.length > 0) {
    int i=1;
    for (    int docId : docIds) {
      tests[i++]="//result/doc/int[@name='id'][.='" + docId + "']";
    }
  }
  return tests;
}
