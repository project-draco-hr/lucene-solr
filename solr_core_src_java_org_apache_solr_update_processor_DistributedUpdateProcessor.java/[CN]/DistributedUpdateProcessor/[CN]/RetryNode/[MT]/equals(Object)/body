{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  RetryNode other=(RetryNode)obj;
  if (url == null) {
    if (other.url != null)     return false;
  }
 else   if (!url.equals(other.url))   return false;
  return true;
}
