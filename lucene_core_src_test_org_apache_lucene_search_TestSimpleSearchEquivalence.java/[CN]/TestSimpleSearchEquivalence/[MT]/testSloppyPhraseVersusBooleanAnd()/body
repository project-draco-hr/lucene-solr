{
  Term t1=randomTerm();
  Term t2=null;
  do {
    t2=randomTerm();
  }
 while (t1.equals(t2));
  PhraseQuery q1=new PhraseQuery(Integer.MAX_VALUE,t1.field(),t1.bytes(),t2.bytes());
  BooleanQuery q2=new BooleanQuery();
  q2.add(new TermQuery(t1),Occur.MUST);
  q2.add(new TermQuery(t2),Occur.MUST);
  assertSameSet(q1,q2);
}
