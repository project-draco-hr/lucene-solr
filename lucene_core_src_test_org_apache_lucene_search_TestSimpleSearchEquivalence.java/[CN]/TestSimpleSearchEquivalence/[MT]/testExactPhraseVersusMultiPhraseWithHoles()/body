{
  Term t1=randomTerm();
  Term t2=randomTerm();
  PhraseQuery.Builder builder=new PhraseQuery.Builder();
  builder.add(t1,0);
  builder.add(t2,2);
  PhraseQuery q1=builder.build();
  Term t3=randomTerm();
  MultiPhraseQuery q2=new MultiPhraseQuery();
  q2.add(t1);
  q2.add(new Term[]{t2,t3},2);
  assertSubsetOf(q1,q2);
}
