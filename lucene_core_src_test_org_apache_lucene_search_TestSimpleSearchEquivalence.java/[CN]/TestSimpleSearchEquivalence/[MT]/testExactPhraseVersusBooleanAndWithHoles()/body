{
  Term t1=randomTerm();
  Term t2=randomTerm();
  PhraseQuery.Builder builder=new PhraseQuery.Builder();
  builder.add(t1,0);
  builder.add(t2,2);
  PhraseQuery q1=builder.build();
  BooleanQuery q2=new BooleanQuery();
  q2.add(new TermQuery(t1),Occur.MUST);
  q2.add(new TermQuery(t2),Occur.MUST);
  assertSubsetOf(q1,q2);
}
