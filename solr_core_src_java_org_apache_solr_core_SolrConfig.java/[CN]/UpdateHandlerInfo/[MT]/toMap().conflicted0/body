{
  LinkedHashMap result=new LinkedHashMap();
  result.put("indexWriter",makeMap("closeWaitsForMerges",indexWriterCloseWaitsForMerges));
  result.put("openSearcher",openSearcher);
  result.put("commitWithin",makeMap("softCommit",commitWithinSoftCommit));
  result.put("autoCommit",makeMap("maxDocs",autoCommmitMaxDocs,"maxTime",autoCommmitMaxTime));
  result.put("autoSoftCommit",makeMap("maxDocs",autoSoftCommmitMaxDocs,"maxTime",autoSoftCommmitMaxTime));
  return result;
}
