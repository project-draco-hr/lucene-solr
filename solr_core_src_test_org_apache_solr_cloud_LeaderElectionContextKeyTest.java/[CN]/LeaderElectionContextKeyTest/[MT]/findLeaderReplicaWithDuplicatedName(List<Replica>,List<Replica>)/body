{
  for (  Replica replica1 : replicas1) {
    if (!replica1.containsKey("leader"))     continue;
    for (    Replica replica2 : replicas2) {
      if (replica1.getName().equals(replica2.getName()) && replica1.get("base_url").equals(replica2.get("base_url")) && replica2.containsKey("leader")) {
        return replica1;
      }
    }
  }
  return null;
}
