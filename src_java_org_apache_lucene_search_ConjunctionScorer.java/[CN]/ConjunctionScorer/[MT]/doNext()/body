{
  int first=0;
  Scorer lastScorer=scorers[scorers.length - 1];
  Scorer firstScorer;
  while (more && (firstScorer=scorers[first]).doc() < (lastDoc=lastScorer.doc())) {
    more=firstScorer.skipTo(lastDoc);
    lastScorer=firstScorer;
    first=(first == (scorers.length - 1)) ? 0 : first + 1;
  }
  return more;
}
