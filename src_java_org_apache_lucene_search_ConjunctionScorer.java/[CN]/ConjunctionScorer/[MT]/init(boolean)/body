{
  coord=getSimilarity().coord(length,length);
  more=length > 0;
  if (initScorers) {
    for (int i=0, pos=first; i < length; i++) {
      if (!more)       break;
      more=scorers[pos].next();
      pos=(pos == length - 1) ? 0 : pos + 1;
    }
    if (more)     sortScorers();
  }
  firstTime=false;
}
