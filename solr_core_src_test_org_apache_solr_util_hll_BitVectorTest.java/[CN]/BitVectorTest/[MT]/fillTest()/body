{
  final BitVector vector=new BitVector(5,128);
  for (int i=0; i < 128; i++) {
    vector.setRegister(i,i);
  }
  vector.fill(0L);
  for (int i=0; i < 128; i++) {
    assertEquals(vector.getRegister(i),0);
  }
  vector.fill(17L);
  for (int i=0; i < 128; i++) {
    assertEquals(vector.getRegister(i),17);
  }
}
