{
  assertFalse(QueryUtils.isNegative(q));
  assertTrue(QueryUtils.getAbs(q) == q);
  List<BooleanClause> clauses=(q instanceof BooleanQuery) ? ((BooleanQuery)q).clauses() : null;
  if (clauses != null) {
    if (clauses.size() != 0) {
      assertTrue(QueryUtils.makeQueryable(q) == q);
    }
  }
 else {
    assertTrue(QueryUtils.makeQueryable(q) == q);
  }
}
