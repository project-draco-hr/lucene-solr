{
  if (wrappedMergePolicyArgs == null) {
    return getDefaultWrappedMergePolicy();
  }
  final MergePolicyFactory mpf=resourceLoader.newInstance(wrappedMergePolicyClassName,MergePolicyFactory.class,NO_SUB_PACKAGES,new Class[]{SolrResourceLoader.class,MergePolicyFactoryArgs.class,IndexSchema.class},new Object[]{resourceLoader,wrappedMergePolicyArgs,schema});
  return mpf.getMergePolicy();
}
