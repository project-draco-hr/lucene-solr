{
  thisCell=null;
  if (nextCell != null)   return true;
  while (baseIter.hasNext()) {
    nextCell=baseIter.next();
    if (shapeFilter == null) {
      return true;
    }
 else {
      SpatialRelation rel=nextCell.getShape().relate(shapeFilter);
      if (rel.intersects()) {
        nextCell.setShapeRel(rel);
        if (rel == SpatialRelation.WITHIN)         nextCell.setLeaf();
        return true;
      }
    }
  }
  return false;
}
