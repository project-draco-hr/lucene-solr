{
  if (retryOnConnLoss) {
    return zkCmdExecutor.retryOperation(new ZkOperation(){
      @Override public Stat execute() throws KeeperException, InterruptedException {
        return keeper.setData(path,data,version);
      }
    }
);
  }
 else {
    return keeper.setData(path,data,version);
  }
}
