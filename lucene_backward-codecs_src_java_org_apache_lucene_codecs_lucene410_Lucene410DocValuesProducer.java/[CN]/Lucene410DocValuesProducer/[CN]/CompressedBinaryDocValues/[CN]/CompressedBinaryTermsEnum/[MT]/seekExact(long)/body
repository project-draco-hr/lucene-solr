{
  long block=ord >>> INTERVAL_SHIFT;
  if (block != currentOrd >>> INTERVAL_SHIFT) {
    input.seek(addresses.get(block));
    readHeader();
  }
  currentOrd=ord;
  int offset=(int)(ord & INTERVAL_MASK);
  if (offset == 0) {
    readFirstTerm();
  }
 else {
    input.seek(currentBlockStart + offsets[offset - 1]);
    readTerm(offset);
  }
}
