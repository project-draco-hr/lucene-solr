{
  boolean isMember=false;
  for (  final SidedPlane edge : edges) {
    if (edge.isWithin(x,y,z)) {
      isMember=true;
      break;
    }
  }
  if (isMember == false) {
    return false;
  }
  if (holes != null) {
    for (    final GeoPolygon polygon : holes) {
      if (polygon.isWithin(x,y,z)) {
        return false;
      }
    }
  }
  return true;
}
