{
  aborted=true;
  try {
    if (infoStream.isEnabled("DWPT")) {
      infoStream.message("DWPT","now abort");
    }
    try {
      consumer.abort();
    }
 catch (    Throwable t) {
    }
    pendingUpdates.clear();
  }
  finally {
    if (infoStream.isEnabled("DWPT")) {
      infoStream.message("DWPT","done abort");
    }
  }
}
