{
  SolrInputField f1=doc1.getField(fieldName);
  SolrInputField f2=doc2.getField(fieldName);
  if (f1 == f2) {
    return 0;
  }
 else   if (f1 == null) {
    return -1;
  }
 else   if (f2 == null) {
    return 1;
  }
  Object v1=f1.getFirstValue();
  Object v2=f2.getFirstValue();
  return child.compare(v1,v2);
}
