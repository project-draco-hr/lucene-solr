{
  StringBuffer sb=new StringBuffer();
  sb.append('{');
  Iterator<K> keyIterator=keyIterator();
  while (keyIterator.hasNext()) {
    K key=keyIterator.next();
    sb.append(key);
    sb.append('=');
    sb.append(get(key));
    if (keyIterator.hasNext()) {
      sb.append(',');
      sb.append(' ');
    }
  }
  sb.append('}');
  return sb.toString();
}
