{
  final Collection<PerDocValues> values=codecs.values();
  IOException err=null;
  for (  PerDocValues perDocValues : values) {
    try {
      if (perDocValues != null) {
        perDocValues.close();
      }
    }
 catch (    IOException ioe) {
      if (err == null) {
        err=ioe;
      }
    }
  }
  if (err != null) {
    throw err;
  }
}
