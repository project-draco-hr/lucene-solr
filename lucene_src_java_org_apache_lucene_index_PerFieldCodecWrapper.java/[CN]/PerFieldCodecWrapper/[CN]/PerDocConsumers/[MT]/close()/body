{
  IOException err=null;
  for (int i=0; i < consumers.length; i++) {
    try {
      final PerDocConsumer next=consumers[i];
      if (next != null) {
        next.close();
      }
    }
 catch (    IOException ioe) {
      if (err == null) {
        err=ioe;
      }
    }
  }
  if (err != null) {
    throw err;
  }
}
