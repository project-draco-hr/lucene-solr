{
  Iterator<PerDocConsumer> it=consumers.iterator();
  IOException err=null;
  while (it.hasNext()) {
    try {
      PerDocConsumer next=it.next();
      if (next != null) {
        next.close();
      }
    }
 catch (    IOException ioe) {
      if (err == null) {
        err=ioe;
      }
    }
  }
  if (err != null) {
    throw err;
  }
}
