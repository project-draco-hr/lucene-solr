{
  StringBuilder sb=new StringBuilder();
  sb.append(name).append('(');
  boolean firstTime=true;
  for (  DocValues vals : valsArr) {
    if (firstTime) {
      firstTime=false;
    }
 else {
      sb.append(',');
    }
    sb.append(vals.toString(doc));
  }
  sb.append(')');
  return sb.toString();
}
