{
  Map attrs=createMap(LineEntityProcessor.URL,"dummy.lis",LineEntityProcessor.ACCEPT_LINE_REGEX,"xml",LineEntityProcessor.SKIP_LINE_REGEX,null);
  Context c=getContext(null,new VariableResolverImpl(),getDataSource(filecontents),Context.FULL_DUMP,Collections.EMPTY_LIST,attrs);
  LineEntityProcessor ep=new LineEntityProcessor();
  ep.init(c);
  List<String> fList=new ArrayList<String>();
  while (true) {
    Map<String,Object> f=ep.nextRow();
    if (f == null)     break;
    fList.add((String)f.get("rawLine"));
  }
  assertEquals(5,fList.size());
}
