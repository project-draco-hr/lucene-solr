{
  ZkNodeProps m=new ZkNodeProps(Overseer.QUEUE_OPERATION,Overseer.OverseerAction.DELETECORE.toLower(),ZkStateReader.CORE_NAME_PROP,core,ZkStateReader.NODE_NAME_PROP,replica.getStr(ZkStateReader.NODE_NAME_PROP),ZkStateReader.COLLECTION_PROP,collectionName,ZkStateReader.CORE_NODE_NAME_PROP,replicaName);
  Overseer.getInQueue(zkStateReader.getZkClient()).offer(ZkStateReader.toJSON(m));
}
