{
  String collectionName=message.containsKey(COLLECTION_PROP) ? message.getStr(COLLECTION_PROP) : message.getStr("name");
  if (collectionName == null)   return true;
  if (CLUSTERSTATUS.isEqual(message.getStr(Overseer.QUEUE_OPERATION)))   return true;
  if (collectionWip.contains(collectionName))   return false;
  if (runningZKTasks.contains(id))   return false;
  return true;
}
