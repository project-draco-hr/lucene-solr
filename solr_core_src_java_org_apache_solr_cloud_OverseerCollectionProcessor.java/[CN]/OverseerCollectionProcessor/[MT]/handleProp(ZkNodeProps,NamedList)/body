{
  String name=message.getStr("name");
  String val=message.getStr("val");
  Map m=zkStateReader.getClusterProps();
  if (val == null)   m.remove(name);
 else   m.put(name,val);
  if (zkStateReader.getZkClient().exists(ZkStateReader.CLUSTER_PROPS,true))   zkStateReader.getZkClient().setData(ZkStateReader.CLUSTER_PROPS,ZkStateReader.toJSON(m),true);
 else   zkStateReader.getZkClient().create(ZkStateReader.CLUSTER_PROPS,ZkStateReader.toJSON(m),CreateMode.PERSISTENT,true);
}
