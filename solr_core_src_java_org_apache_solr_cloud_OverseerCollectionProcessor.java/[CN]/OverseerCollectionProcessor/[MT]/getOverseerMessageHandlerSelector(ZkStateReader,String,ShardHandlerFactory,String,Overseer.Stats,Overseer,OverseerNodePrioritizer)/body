{
  final OverseerCollectionMessageHandler messageHandler=new OverseerCollectionMessageHandler(zkStateReader,myId,shardHandlerFactory,adminPath,stats,overseer,overseerNodePrioritizer);
  return new OverseerMessageHandlerSelector(){
    @Override public OverseerMessageHandler selectOverseerMessageHandler(    ZkNodeProps message){
      return messageHandler;
    }
  }
;
}
