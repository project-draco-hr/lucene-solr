{
  elapsed=System.currentTimeMillis() - start;
  long totMem=Runtime.getRuntime().totalMemory();
  if (totMem > maxTotMem) {
    maxTotMem=totMem;
  }
  long usedMem=totMem - Runtime.getRuntime().freeMemory();
  if (usedMem > maxUsedMem) {
    maxUsedMem=usedMem;
  }
  this.numParallelTasks=numParallelTasks;
  this.count=count;
}
