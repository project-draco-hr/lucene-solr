{
  NamedList<T> list=null;
  T value=null;
  for (  String key : args) {
    if (list == null) {
      list=this;
    }
 else {
      if (NamedList.class.isInstance(value)) {
        list=(NamedList<T>)value;
      }
 else {
        value=null;
        break;
      }
    }
    value=list.get(key);
  }
  return value;
}
