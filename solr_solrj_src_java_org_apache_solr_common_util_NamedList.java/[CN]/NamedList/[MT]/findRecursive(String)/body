{
  NamedList<?> currentList=null;
  Object value=null;
  for (int i=0; i < args.length; i++) {
    String key=args[i];
    if (currentList == null) {
      currentList=this;
    }
 else {
      if (value instanceof NamedList) {
        currentList=(NamedList<?>)value;
      }
 else {
        value=null;
        break;
      }
    }
    value=currentList.get(key,0);
  }
  return value;
}
