{
  for (  String test : tests) {
    String err=null;
    if (test == null) {
      if (input != null) {
        err="expected null";
      }
    }
 else     if (input == null) {
      err="got null";
    }
 else {
      err=JSONTestUtil.matchObj(input,test,delta);
    }
    if (err != null) {
      throw new RuntimeException("JSON failed validation. error=" + err + "\n expected ="+ test+ "\n got = "+ input);
    }
  }
}
