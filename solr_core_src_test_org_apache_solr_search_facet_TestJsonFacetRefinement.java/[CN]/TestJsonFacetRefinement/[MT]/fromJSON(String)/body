{
  JSONParser parser=new JSONParser(json);
  ObjectBuilder ob=new ObjectBuilder(parser){
    @Override public Object newObject() throws IOException {
      return new SimpleOrderedMap();
    }
    @Override public void addKeyVal(    Object map,    Object key,    Object val) throws IOException {
      ((SimpleOrderedMap)map).add(key.toString(),val);
    }
  }
;
  return ob.getObject();
}
