{
  for (  String test : tests) {
    if (test == null) {
      assertNull(json);
      continue;
    }
    if (test.length() == 0)     continue;
    String err=JSONTestUtil.match(json,test,delta);
    if (err != null) {
      throw new RuntimeException("JSON failed validation. error=" + err + "\n expected ="+ test+ "\n got = "+ json);
    }
  }
}
