{
  tupleStream.open();
  tupleStream.writeStreamOpen(writer);
  boolean isFirst=true;
  while (true) {
    Tuple tuple=tupleStream.read();
    if (!isFirst) {
      writer.write(",");
    }
    writer.write("\n");
    writeMap(null,tuple.fields,false,true);
    isFirst=false;
    if (tuple.EOF) {
      break;
    }
  }
  tupleStream.writeStreamClose(writer);
  tupleStream.close();
}
