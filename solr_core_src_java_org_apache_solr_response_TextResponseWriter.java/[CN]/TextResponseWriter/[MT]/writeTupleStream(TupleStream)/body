{
  tupleStream.open();
  writeStartDocumentList("response",-1,-1,-1,null);
  boolean isFirst=true;
  while (true) {
    Tuple tuple=tupleStream.read();
    if (!isFirst) {
      writer.write(",");
    }
    writeMap(null,tuple.fields,false,true);
    isFirst=false;
    if (tuple.EOF) {
      break;
    }
  }
  writeEndDocumentList();
  tupleStream.close();
}
