{
  String rulesStr="rack:*,replica:<2";
  List<Rule> rules=parse(Arrays.asList(rulesStr));
  Map shardVsReplicaCount=makeMap("shard1",3,"shard2",3);
  Map nodeVsTags=makeMap("node1:80",makeMap("rack","178"),"node2:80",makeMap("rack","179"),"node3:80",makeMap("rack","180"),"node4:80",makeMap("rack","181"),"node5:80",makeMap("rack","182"));
  MockSnitch.nodeVsTags=nodeVsTags;
  Map<Position,String> mapping=new ReplicaAssigner(rules,shardVsReplicaCount,Collections.singletonList(MockSnitch.class.getName()),new HashMap(),new ArrayList<>(MockSnitch.nodeVsTags.keySet()),null,null).getNodeMappings0();
  assertNull(mapping);
  rulesStr="rack:*,replica:<2~";
  rules=parse(Arrays.asList(rulesStr));
  mapping=new ReplicaAssigner(rules,shardVsReplicaCount,Collections.singletonList(MockSnitch.class.getName()),new HashMap(),new ArrayList<>(MockSnitch.nodeVsTags.keySet()),null,null).getNodeMappings();
  assertNotNull(mapping);
  rulesStr="rack:*,shard:*,replica:<2";
  rules=parse(Arrays.asList(rulesStr));
  mapping=new ReplicaAssigner(rules,shardVsReplicaCount,Collections.singletonList(MockSnitch.class.getName()),new HashMap(),new ArrayList<>(MockSnitch.nodeVsTags.keySet()),null,null).getNodeMappings();
  assertNotNull(mapping);
}
