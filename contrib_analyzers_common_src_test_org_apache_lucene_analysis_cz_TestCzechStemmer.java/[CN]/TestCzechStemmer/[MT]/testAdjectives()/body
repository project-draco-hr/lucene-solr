{
  CzechAnalyzer cz=new CzechAnalyzer(Version.LUCENE_CURRENT);
  assertAnalyzesTo(cz,"mlad??",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??ho",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??ch",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??mu",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??m",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??m",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??mi",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mlad??",new String[]{"mlad"});
  assertAnalyzesTo(cz,"mladou",new String[]{"mlad"});
  assertAnalyzesTo(cz,"jarn??",new String[]{"jarn"});
  assertAnalyzesTo(cz,"jarn??ho",new String[]{"jarn"});
  assertAnalyzesTo(cz,"jarn??ch",new String[]{"jarn"});
  assertAnalyzesTo(cz,"jarn??mu",new String[]{"jarn"});
  assertAnalyzesTo(cz,"jarn??m",new String[]{"jarn"});
  assertAnalyzesTo(cz,"jarn??mi",new String[]{"jarn"});
}
