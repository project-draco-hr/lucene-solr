{
  if (start < 0) {
    start=0;
  }
  if (end < 0 || end > ncomponents) {
    end=ncomponents;
  }
  if (end <= start) {
    return;
  }
  for (int i=(start == 0 ? 0 : ends[start - 1]); i < ends[start]; i++) {
    out.append(chars[i]);
  }
  for (int j=start + 1; j < end; j++) {
    out.append(delimiter);
    for (int i=ends[j - 1]; i < ends[j]; i++) {
      out.append(chars[i]);
    }
  }
}
