{
  if (start < 0) {
    start=0;
  }
  if (end < 0 || end > ncomponents) {
    end=ncomponents;
  }
  if (end <= start) {
    return "";
  }
  int startchar=(start == 0) ? 0 : ends[start - 1];
  StringBuilder sb=new StringBuilder(ends[end - 1] - startchar + (end - start) - 1);
  try {
    this.appendTo(sb,delimiter,start,end);
  }
 catch (  IOException e) {
  }
  return sb.toString();
}
