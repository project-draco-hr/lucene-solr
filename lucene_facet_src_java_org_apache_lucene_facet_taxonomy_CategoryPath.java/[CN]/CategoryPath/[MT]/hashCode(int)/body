{
  if (prefixLen < 0 || prefixLen > ncomponents) {
    prefixLen=ncomponents;
  }
  if (prefixLen == 0) {
    return 0;
  }
  int hash=prefixLen;
  for (int i=0; i < prefixLen; i++) {
    hash=hash * 31 + ends[i];
  }
  int len=ends[prefixLen - 1];
  for (int i=0; i < len; i++) {
    hash=hash * 31 + chars[i];
  }
  return hash;
}
