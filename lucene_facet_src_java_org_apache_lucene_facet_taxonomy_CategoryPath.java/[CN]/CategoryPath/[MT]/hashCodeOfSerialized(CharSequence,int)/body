{
  int ncomponents=(short)buffer.charAt(offset++);
  if (ncomponents == 0) {
    return 0;
  }
  int hash=ncomponents;
  for (int i=0; i < ncomponents; i++) {
    hash=hash * 31 + buffer.charAt(offset++);
  }
  int len=buffer.charAt(offset - 1);
  for (int i=0; i < len; i++) {
    hash=hash * 31 + buffer.charAt(offset++);
  }
  return hash;
}
