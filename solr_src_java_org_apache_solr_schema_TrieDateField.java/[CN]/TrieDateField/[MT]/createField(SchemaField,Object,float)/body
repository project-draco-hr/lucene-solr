{
  boolean indexed=field.indexed();
  boolean stored=field.stored();
  if (!indexed && !stored) {
    if (log.isTraceEnabled())     log.trace("Ignoring unindexed/unstored field: " + field);
    return null;
  }
  int ps=precisionStep;
  byte[] arr=null;
  TokenStream ts=null;
  long time=(value instanceof Date) ? ((Date)value).getTime() : super.parseMath(null,value.toString()).getTime();
  if (stored)   arr=TrieFieldHelper.toArr(time);
  if (indexed)   ts=new NumericTokenStream(ps).setLongValue(time);
  Field f;
  if (stored) {
    f=new Field(field.getName(),arr);
    if (indexed)     f.setTokenStream(ts);
  }
 else {
    f=new Field(field.getName(),ts);
  }
  f.setOmitNorms(field.omitNorms());
  f.setOmitTermFreqAndPositions(field.omitTf());
  f.setBoost(boost);
  return f;
}
