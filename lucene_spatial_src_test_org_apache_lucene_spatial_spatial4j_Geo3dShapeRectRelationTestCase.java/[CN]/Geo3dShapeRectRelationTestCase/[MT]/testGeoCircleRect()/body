{
  new Geo3dRectIntersectionTestHelper(ctx){
    @Override protected Geo3dShape generateRandomShape(    Point nearP){
      final int circleRadius=180 - random().nextInt(180);
      final Point point=nearP;
      final GeoShape shape=new GeoStandardCircle(planetModel,point.getY() * DEGREES_TO_RADIANS,point.getX() * DEGREES_TO_RADIANS,circleRadius * DEGREES_TO_RADIANS);
      return new Geo3dShape(planetModel,shape,ctx);
    }
    @Override protected Point randomPointInEmptyShape(    Geo3dShape shape){
      GeoPoint geoPoint=((GeoStandardCircle)shape.shape).getCenter();
      return geoPointToSpatial4jPoint(geoPoint);
    }
  }
.testRelateWithRectangle();
}
