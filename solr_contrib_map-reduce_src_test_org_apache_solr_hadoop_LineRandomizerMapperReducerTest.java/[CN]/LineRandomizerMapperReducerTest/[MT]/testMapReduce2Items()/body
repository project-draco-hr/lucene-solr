{
  mapReduceDriver.withAll(Arrays.asList(new Pair<>(new LongWritable(0),new Text("hello")),new Pair<>(new LongWritable(1),new Text("world"))));
  mapReduceDriver.withAllOutput(Arrays.asList(new Pair<>(new Text("world"),NullWritable.get()),new Pair<>(new Text("hello"),NullWritable.get())));
  mapReduceDriver.runTest();
}
