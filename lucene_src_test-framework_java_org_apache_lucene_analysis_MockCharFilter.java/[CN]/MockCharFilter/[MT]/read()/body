{
  if (bufferedCh >= 0) {
    int ch=bufferedCh;
    bufferedCh=-1;
    currentOffset++;
    addOffCorrectMap(currentOffset,delta - 1);
    delta--;
    return ch;
  }
  int ch=in.read();
  if (ch < 0)   return ch;
  currentOffset++;
  if ((ch % 10) != remainder || Character.isHighSurrogate((char)ch) || Character.isLowSurrogate((char)ch)) {
    return ch;
  }
  bufferedCh=ch;
  return ch;
}
