{
  try {
    TermVectorsReader reader=new TermVectorsReader(dir,seg,fieldInfos);
    assertTrue(reader != null);
    TermFreqVector vector=reader.get(50,testFields[0]);
    assertTrue(false);
  }
 catch (  Exception e) {
    assertTrue(true);
  }
  try {
    TermVectorsReader reader=new TermVectorsReader(dir,seg,fieldInfos);
    assertTrue(reader != null);
    TermFreqVector vector=reader.get(0,"f50");
    assertTrue(vector == null);
  }
 catch (  Exception e) {
    assertTrue(false);
  }
}
