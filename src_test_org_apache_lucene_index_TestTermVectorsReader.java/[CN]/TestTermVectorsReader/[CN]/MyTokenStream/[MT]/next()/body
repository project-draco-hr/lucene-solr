{
  if (tokenUpto >= tokens.length)   return null;
 else {
    final Token t=new Token();
    final TestToken testToken=tokens[tokenUpto++];
    t.setTermText(testToken.text);
    if (tokenUpto > 1)     t.setPositionIncrement(testToken.pos - tokens[tokenUpto - 2].pos);
 else     t.setPositionIncrement(testToken.pos + 1);
    t.setStartOffset(testToken.startOffset);
    t.setEndOffset(testToken.endOffset);
    return t;
  }
}
