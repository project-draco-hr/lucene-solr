{
  Arrays.sort(currentValues,0,currentUpto);
  int lastValue=-1;
  int count=0;
  for (int i=0; i < currentUpto; i++) {
    int v=currentValues[i];
    if (v != lastValue) {
      pending.add(v);
      count++;
    }
    lastValue=v;
  }
  pendingCounts.add(count);
  currentUpto=0;
  currentDoc++;
}
