{
  Arrays.sort(currentValues,0,currentUpto);
  int lastValue=-1;
  int count=0;
  for (int i=0; i < currentUpto; i++) {
    int termID=currentValues[i];
    if (termID != lastValue) {
      pending.add(termID);
      count++;
    }
    lastValue=termID;
  }
  pendingCounts.add(count);
  maxCount=Math.max(maxCount,count);
  currentUpto=0;
  currentDoc++;
}
