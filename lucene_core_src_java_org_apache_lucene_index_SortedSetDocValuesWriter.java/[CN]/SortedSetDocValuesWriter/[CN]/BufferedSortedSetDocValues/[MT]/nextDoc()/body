{
  while (ordUpto < ordCount) {
    ordsIter.next();
    ordUpto++;
  }
  while (true) {
    docID++;
    if (docID == ordCounts.size()) {
      docID=NO_MORE_DOCS;
      break;
    }
    ordCount=(int)ordCountsIter.next();
    if (ordCount > 0) {
      for (int i=0; i < ordCount; i++) {
        currentDoc[i]=ordMap[Math.toIntExact(ordsIter.next())];
      }
      Arrays.sort(currentDoc,0,ordCount);
      ordUpto=0;
      break;
    }
  }
  return docID;
}
