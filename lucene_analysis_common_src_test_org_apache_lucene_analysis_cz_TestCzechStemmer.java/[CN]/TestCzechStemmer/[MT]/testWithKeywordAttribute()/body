{
  CharArraySet set=new CharArraySet(TEST_VERSION_CURRENT,1,true);
  set.add("hole");
  CzechStemFilter filter=new CzechStemFilter(new KeywordMarkerFilter(new MockTokenizer(new StringReader("hole desek"),MockTokenizer.WHITESPACE,false),set));
  assertTokenStreamContents(filter,new String[]{"hole","desk"});
}
