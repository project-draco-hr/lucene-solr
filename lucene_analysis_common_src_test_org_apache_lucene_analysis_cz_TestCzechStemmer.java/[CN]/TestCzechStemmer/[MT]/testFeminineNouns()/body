{
  CzechAnalyzer cz=new CzechAnalyzer();
  assertAnalyzesTo(cz,"kost",new String[]{"kost"});
  assertAnalyzesTo(cz,"kosti",new String[]{"kost"});
  assertAnalyzesTo(cz,"kost??",new String[]{"kost"});
  assertAnalyzesTo(cz,"kostem",new String[]{"kost"});
  assertAnalyzesTo(cz,"kostech",new String[]{"kost"});
  assertAnalyzesTo(cz,"kostmi",new String[]{"kost"});
  assertAnalyzesTo(cz,"p??se??",new String[]{"p??s??"});
  assertAnalyzesTo(cz,"p??sn??",new String[]{"p??sn"});
  assertAnalyzesTo(cz,"p??sni",new String[]{"p??sn"});
  assertAnalyzesTo(cz,"p??sn??mi",new String[]{"p??sn"});
  assertAnalyzesTo(cz,"p??sn??ch",new String[]{"p??sn"});
  assertAnalyzesTo(cz,"p??sn??m",new String[]{"p??sn"});
  assertAnalyzesTo(cz,"r????e",new String[]{"r??h"});
  assertAnalyzesTo(cz,"r??????",new String[]{"r??h"});
  assertAnalyzesTo(cz,"r??????m",new String[]{"r??h"});
  assertAnalyzesTo(cz,"r??????ch",new String[]{"r??h"});
  assertAnalyzesTo(cz,"r????emi",new String[]{"r??h"});
  assertAnalyzesTo(cz,"r????i",new String[]{"r??h"});
  assertAnalyzesTo(cz,"??ena",new String[]{"??n"});
  assertAnalyzesTo(cz,"??eny",new String[]{"??n"});
  assertAnalyzesTo(cz,"??en",new String[]{"??n"});
  assertAnalyzesTo(cz,"??en??",new String[]{"??n"});
  assertAnalyzesTo(cz,"??en??m",new String[]{"??n"});
  assertAnalyzesTo(cz,"??enu",new String[]{"??n"});
  assertAnalyzesTo(cz,"??eno",new String[]{"??n"});
  assertAnalyzesTo(cz,"??en??ch",new String[]{"??n"});
  assertAnalyzesTo(cz,"??enou",new String[]{"??n"});
  assertAnalyzesTo(cz,"??enami",new String[]{"??n"});
  cz.close();
}
