{
  Directory dir=new RAMDirectory();
  dir.close();
  try {
    dir.createOutput("test");
    fail("did not hit expected exception");
  }
 catch (  AlreadyClosedException ace) {
  }
  dir=FSDirectory.open(TEMP_DIR);
  dir.close();
  try {
    dir.createOutput("test");
    fail("did not hit expected exception");
  }
 catch (  AlreadyClosedException ace) {
  }
}
