{
  int low=0;
  int high=numOrd - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    seekExact(mid);
    int cmp=term.compareTo(text);
    if (cmp < 0)     low=mid + 1;
 else     if (cmp > 0)     high=mid - 1;
 else {
      return SeekStatus.FOUND;
    }
  }
  if (low == numOrd) {
    return SeekStatus.END;
  }
 else {
    seekExact(low);
    return SeekStatus.NOT_FOUND;
  }
}
