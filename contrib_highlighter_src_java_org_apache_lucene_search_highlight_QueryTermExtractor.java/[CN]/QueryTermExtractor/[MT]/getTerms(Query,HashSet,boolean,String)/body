{
  if (query instanceof BooleanQuery)   getTermsFromBooleanQuery((BooleanQuery)query,terms,prohibited,fieldName);
 else   if (query instanceof PhraseQuery)   getTermsFromPhraseQuery((PhraseQuery)query,terms,fieldName);
 else   if (query instanceof TermQuery)   getTermsFromTermQuery((TermQuery)query,terms,fieldName);
 else   if (query instanceof SpanNearQuery)   getTermsFromSpanNearQuery((SpanNearQuery)query,terms,fieldName);
}
