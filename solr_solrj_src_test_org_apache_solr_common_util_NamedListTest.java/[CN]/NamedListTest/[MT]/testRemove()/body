{
  NamedList<String> nl=new NamedList<>();
  nl.add("key1","value1");
  nl.add("key2","value2");
  assertEquals(2,nl.size());
  String value=null;
  value=nl.remove(0);
  assertEquals("value1",value);
  assertEquals(1,nl.size());
  value=nl.remove("key2");
  assertEquals("value2",value);
  assertEquals(0,nl.size());
}
