{
  int version=CodecUtil.checkHeader(input,TempBlockTreeTermsWriter.TERMS_CODEC_NAME,TempBlockTreeTermsWriter.TERMS_VERSION_START,TempBlockTreeTermsWriter.TERMS_VERSION_CURRENT);
  if (version < TempBlockTreeTermsWriter.TERMS_VERSION_APPEND_ONLY) {
    dirOffset=input.readLong();
  }
  return version;
}
