{
  int version=CodecUtil.checkHeader(input,TempBlockTreeTermsWriter.TERMS_INDEX_CODEC_NAME,TempBlockTreeTermsWriter.TERMS_INDEX_VERSION_START,TempBlockTreeTermsWriter.TERMS_INDEX_VERSION_CURRENT);
  if (version < TempBlockTreeTermsWriter.TERMS_INDEX_VERSION_APPEND_ONLY) {
    indexDirOffset=input.readLong();
  }
  return version;
}
