{
  String parseableQuery=func(field,funcTemplate);
  List<String> tests=new ArrayList<String>();
  for (int i=0; i < results.length; i+=2) {
    String xpath="//doc[./float[@name='" + field + "']='"+ results[i]+ "' and ./float[@name='score']='"+ results[i + 1]+ "']";
    tests.add(xpath);
  }
  assertQ(req("q",parseableQuery,"fl","*,score"),tests.toArray(new String[tests.size()]));
}
