{
  if (isStored && isIndexed && !isTokenized)   return "Keyword<" + name + ":"+ stringValue+ ">";
 else   if (isStored && !isIndexed && !isTokenized)   return "Unindexed<" + name + ":"+ stringValue+ ">";
 else   if (isStored && isIndexed && isTokenized&& stringValue != null)   return "Text<" + name + ":"+ stringValue+ ">";
 else   if (!isStored && isIndexed && isTokenized&& readerValue != null)   return "Text<" + name + ":"+ readerValue+ ">";
 else   return super.toString();
}
