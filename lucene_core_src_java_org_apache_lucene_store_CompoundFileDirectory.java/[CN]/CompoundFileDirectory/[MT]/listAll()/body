{
  ensureOpen();
  String[] res;
  if (writer != null) {
    res=writer.listAll();
  }
 else {
    res=entries.keySet().toArray(new String[entries.size()]);
    String seg=IndexFileNames.parseSegmentName(fileName);
    for (int i=0; i < res.length; i++) {
      res[i]=seg + res[i];
    }
  }
  return res;
}
