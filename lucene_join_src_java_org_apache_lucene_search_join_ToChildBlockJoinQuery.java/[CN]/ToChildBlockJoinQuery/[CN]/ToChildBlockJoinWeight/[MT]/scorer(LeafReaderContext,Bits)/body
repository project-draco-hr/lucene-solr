{
  final Scorer parentScorer=parentWeight.scorer(readerContext,null);
  if (parentScorer == null) {
    return null;
  }
  final DocIdSet parents=parentsFilter.getDocIdSet(readerContext,null);
  if (parents == null) {
    return null;
  }
  if (!(parents instanceof FixedBitDocIdSet)) {
    throw new IllegalStateException("parentFilter must return FixedBitSet; got " + parents);
  }
  return new ToChildBlockJoinScorer(this,parentScorer,((FixedBitDocIdSet)parents).bits(),doScores,acceptDocs);
}
