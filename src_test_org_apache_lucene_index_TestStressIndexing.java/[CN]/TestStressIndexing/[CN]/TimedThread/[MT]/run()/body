{
  final long stopTime=System.currentTimeMillis() + 1000 * RUN_TIME_SEC;
  count=0;
  try {
    while (System.currentTimeMillis() < stopTime && !anyErrors()) {
      doWork();
      count++;
    }
  }
 catch (  Throwable e) {
    System.out.println(Thread.currentThread() + ": exc");
    e.printStackTrace(System.out);
    failed=true;
  }
}
