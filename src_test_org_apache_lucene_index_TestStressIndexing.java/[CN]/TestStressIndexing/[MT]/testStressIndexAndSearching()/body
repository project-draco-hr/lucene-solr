{
  Directory directory=new RAMDirectory();
  runStressTest(directory);
  directory.close();
  String tempDir=System.getProperty("java.io.tmpdir");
  File dirPath=new File(tempDir,"lucene.test.stress");
  directory=FSDirectory.getDirectory(dirPath);
  runStressTest(directory);
  directory.close();
  rmDir(dirPath);
}
