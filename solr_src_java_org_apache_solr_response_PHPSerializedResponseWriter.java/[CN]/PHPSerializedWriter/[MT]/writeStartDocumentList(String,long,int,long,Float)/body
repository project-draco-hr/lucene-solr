{
  writeMapOpener((maxScore == null) ? 3 : 4);
  writeKey("numFound",false);
  writeLong(null,numFound);
  writeKey("start",false);
  writeLong(null,start);
  if (maxScore != null) {
    writeKey("maxScore",false);
    writeFloat(null,maxScore);
  }
  writeKey("docs",false);
  writeArrayOpener(size);
}
