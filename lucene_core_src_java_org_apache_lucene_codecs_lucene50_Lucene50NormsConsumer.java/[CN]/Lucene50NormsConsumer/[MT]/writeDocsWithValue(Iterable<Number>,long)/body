{
  data.writeLong(commonValue);
  data.writeVInt(PackedInts.VERSION_CURRENT);
  data.writeVInt(BLOCK_SIZE);
  final MonotonicBlockPackedWriter writer=new MonotonicBlockPackedWriter(data,BLOCK_SIZE);
  int doc=0;
  for (  Number n : values) {
    long v=n.longValue();
    if (v != commonValue) {
      writer.add(doc);
    }
    doc++;
  }
  writer.finish();
}
