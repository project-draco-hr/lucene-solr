{
  PatternAnalyzer a=new PatternAnalyzer(Version.LUCENE_CURRENT,PatternAnalyzer.NON_WORD_PATTERN,false,null);
  check(a,"The quick brown Fox,the abcd1234 (56.78) dc.",new String[]{"The","quick","brown","Fox","the","abcd","dc"});
  PatternAnalyzer b=new PatternAnalyzer(Version.LUCENE_CURRENT,PatternAnalyzer.NON_WORD_PATTERN,true,StopAnalyzer.ENGLISH_STOP_WORDS_SET);
  check(b,"The quick brown Fox,the abcd1234 (56.78) dc.",new String[]{"quick","brown","fox","abcd","dc"});
}
