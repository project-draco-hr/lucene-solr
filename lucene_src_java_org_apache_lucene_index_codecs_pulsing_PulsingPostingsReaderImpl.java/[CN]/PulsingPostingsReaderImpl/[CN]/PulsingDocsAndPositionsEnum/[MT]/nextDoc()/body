{
  while (true) {
    skipPositions();
    if (postings.eof()) {
      return docID=NO_MORE_DOCS;
    }
    final int code=postings.readVInt();
    docID+=code >>> 1;
    if ((code & 1) != 0) {
      freq=1;
    }
 else {
      freq=postings.readVInt();
    }
    posPending=freq;
    if (skipDocs == null || !skipDocs.get(docID)) {
      position=0;
      return docID;
    }
  }
}
