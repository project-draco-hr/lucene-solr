{
  int start=0;
  final int limit=term.length < lastTerm.length ? term.length : lastTerm.length;
  while (start < limit) {
    if (term.bytes[start + term.offset] != lastTerm.bytes[start + lastTerm.offset])     break;
    start++;
  }
  final int length=term.length - start;
  output.writeVInt(start);
  output.writeVInt(length);
  output.writeBytes(term.bytes,start + term.offset,length);
  output.writeVInt(fieldNumber);
  lastTerm.copy(term);
}
