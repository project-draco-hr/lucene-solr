{
  sortvals=new double[digests.length];
  double sortp=percentiles.get(0) * 0.01;
  for (int i=0; i < digests.length; i++) {
    AVLTreeDigest digest=digests[i];
    if (digest == null) {
      sortvals[i]=Double.NEGATIVE_INFINITY;
    }
 else {
      sortvals[i]=digest.quantile(sortp);
    }
  }
}
