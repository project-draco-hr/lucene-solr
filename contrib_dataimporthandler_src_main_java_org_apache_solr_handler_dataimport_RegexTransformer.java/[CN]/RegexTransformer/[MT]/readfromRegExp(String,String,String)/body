{
  Pattern regexp=getPattern(reStr);
  Matcher m=regexp.matcher(value);
  if (m.find() && m.groupCount() > 0) {
    if (m.groupCount() > 1) {
      List l=new ArrayList();
      for (int i=1; i <= m.groupCount(); i++) {
        try {
          l.add(m.group(i));
        }
 catch (        Exception e) {
          LOG.log(Level.WARNING,"Parsing failed for field : " + columnName,e);
        }
      }
      return l;
    }
 else {
      return m.group(1);
    }
  }
  return null;
}
