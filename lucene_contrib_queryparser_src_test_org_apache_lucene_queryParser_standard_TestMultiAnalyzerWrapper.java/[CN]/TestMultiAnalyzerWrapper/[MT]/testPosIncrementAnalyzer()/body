{
  QueryParserWrapper qp=new QueryParserWrapper("",new PosIncrementAnalyzer());
  assertEquals("quick brown",qp.parse("the quick brown").toString());
  assertEquals("\"quick brown\"",qp.parse("\"the quick brown\"").toString());
  assertEquals("quick brown fox",qp.parse("the quick brown fox").toString());
  assertEquals("\"quick brown fox\"",qp.parse("\"the quick brown fox\"").toString());
}
