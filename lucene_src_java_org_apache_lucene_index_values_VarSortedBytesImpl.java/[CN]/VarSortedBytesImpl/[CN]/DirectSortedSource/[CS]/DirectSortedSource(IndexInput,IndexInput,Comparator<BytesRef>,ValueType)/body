{
  super(type,comparator);
  idxIn.readLong();
  ordToOffsetIndex=PackedInts.getRandomAccessReaderIterator(idxIn);
  valueCount=ordToOffsetIndex.size() - 1;
  ordToOffsetIndex.advance(valueCount);
  docToOrdIndex=PackedInts.getRandomAccessReaderIterator((IndexInput)idxIn.clone());
  basePointer=datIn.getFilePointer();
  this.datIn=datIn;
}
