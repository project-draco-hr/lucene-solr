{
  super(dir,id,CODEC_NAME,VERSION_CURRENT,bytesUsed);
  this.hash=new BytesRefHash(new ByteBlockPool(allocator),BytesRefHash.DEFAULT_CAPACITY,new TrackingDirectBytesStartArray(BytesRefHash.DEFAULT_CAPACITY,bytesUsed));
  this.comp=comp;
  docToEntry=new int[1];
  docToEntry[0]=-1;
  bytesUsed.addAndGet(RamUsageEstimator.NUM_BYTES_INT);
}
