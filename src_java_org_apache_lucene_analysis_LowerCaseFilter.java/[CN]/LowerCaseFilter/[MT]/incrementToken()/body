{
  if (input.incrementToken()) {
    final char[] buffer=termAtt.termBuffer();
    final int length=termAtt.termLength();
    for (int i=0; i < length; ) {
      i+=Character.toChars(Character.toLowerCase(charUtils.codePointAt(buffer,i)),buffer,i);
    }
    return true;
  }
 else   return false;
}
