{
  final int insideRectangle=isShapeInsideBBox(path);
  if (insideRectangle == SOME_INSIDE)   return OVERLAPS;
  final boolean insideShape=path.isWithin(ULHC);
  if (insideRectangle == ALL_INSIDE && insideShape)   return OVERLAPS;
  if (path.intersects(topPlane,bottomPlane,leftPlane,rightPlane) || path.intersects(bottomPlane,topPlane,leftPlane,rightPlane) || path.intersects(leftPlane,topPlane,bottomPlane,rightPlane)|| path.intersects(rightPlane,leftPlane,topPlane,bottomPlane))   return OVERLAPS;
  if (insideRectangle == ALL_INSIDE)   return WITHIN;
  if (insideShape)   return CONTAINS;
  return DISJOINT;
}
