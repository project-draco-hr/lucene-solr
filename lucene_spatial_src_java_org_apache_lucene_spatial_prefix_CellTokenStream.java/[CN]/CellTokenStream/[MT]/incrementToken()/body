{
  if (iter == null)   throw new IllegalStateException("call setCells() before usage");
  clearAttributes();
  if (cellAtt.getOmitLeafByte()) {
    cellAtt.setOmitLeafByte(false);
    return true;
  }
  if (!iter.hasNext())   return false;
  cellAtt.setCell(iter.next());
  if (cellAtt.getCell().isLeaf())   cellAtt.setOmitLeafByte(true);
  return true;
}
