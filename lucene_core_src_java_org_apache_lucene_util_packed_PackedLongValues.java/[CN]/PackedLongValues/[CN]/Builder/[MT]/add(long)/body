{
  if (pending == null) {
    throw new IllegalStateException("Cannot be reused after build()");
  }
  if (pendingOff == pending.length) {
    if (values.length == valuesOff) {
      final int newLength=ArrayUtil.oversize(valuesOff + 1,8);
      grow(newLength);
    }
    pack();
  }
  pending[pendingOff++]=l;
  size+=1;
  return this;
}
