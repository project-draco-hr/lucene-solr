{
  final ScorerWrapper[] heap=this.heap;
  final int size=this.size;
  ScorerWrapper list=heap[0];
  list.next=null;
  if (size >= 3) {
    list=topList(list,heap,size,1);
    list=topList(list,heap,size,2);
  }
 else   if (size == 2 && heap[1].doc == list.doc) {
    list=prepend(heap[1],list);
  }
  return list;
}
