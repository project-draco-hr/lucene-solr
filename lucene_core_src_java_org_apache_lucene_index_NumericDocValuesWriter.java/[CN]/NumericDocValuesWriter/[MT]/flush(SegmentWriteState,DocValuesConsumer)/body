{
  final PackedLongValues values=pending.build();
  dvConsumer.addNumericField(fieldInfo,new EmptyDocValuesProducer(){
    @Override public NumericDocValues getNumeric(    FieldInfo fieldInfo){
      if (fieldInfo != NumericDocValuesWriter.this.fieldInfo) {
        throw new IllegalArgumentException("wrong fieldInfo");
      }
      return new BufferedNumericDocValues(values,docsWithField);
    }
  }
);
}
