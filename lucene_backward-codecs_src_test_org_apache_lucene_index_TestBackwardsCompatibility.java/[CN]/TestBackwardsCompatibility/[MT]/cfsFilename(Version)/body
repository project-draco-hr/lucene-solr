{
  String bugfix="";
  if (v.bugfix != 0) {
    bugfix=Integer.toString(v.bugfix);
  }
  String prerelease="";
  if (v.minor == 0 && v.bugfix == 0) {
    if (v.prerelease == 0) {
      prerelease="a";
    }
 else     if (v.prerelease == 1) {
      prerelease="b";
    }
  }
  return Integer.toString(v.major) + v.minor + bugfix+ prerelease+ ".cfs";
}
