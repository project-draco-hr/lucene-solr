{
  super.doInit();
  if (isExisting()) {
    typeName=(String)getRequestAttributes().get(SchemaRestApi.NAME_VARIABLE);
    try {
      typeName=null == typeName ? "" : urlDecode(typeName.trim()).trim();
    }
 catch (    UnsupportedEncodingException e) {
      throw new ResourceException(e);
    }
  }
}
