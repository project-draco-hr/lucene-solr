{
  if (termIsPrefix) {
    MultiTermQuery rewritten=new PrefixQuery(term.createTerm(term.text().substring(0,term.text().indexOf('*'))));
    rewritten.setBoost(getBoost());
    rewritten.setRewriteMethod(getRewriteMethod());
    return rewritten;
  }
 else {
    return super.rewrite(reader);
  }
}
