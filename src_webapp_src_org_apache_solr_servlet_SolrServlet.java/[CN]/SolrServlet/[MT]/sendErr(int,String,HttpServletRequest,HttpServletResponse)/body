{
  try {
    try {
      response.setContentType(CONTENT_TYPE);
    }
 catch (    Exception e) {
    }
    try {
      response.setStatus(rc);
    }
 catch (    Exception e) {
    }
    PrintWriter writer=response.getWriter();
    writer.write(msg);
  }
 catch (  IOException e) {
    SolrException.log(log,e);
  }
}
