{
  Boolean b=process(node.getLeft(),tuple);
  if (node.getType() == LogicalBinaryExpression.Type.AND) {
    if (!b) {
      return false;
    }
 else {
      return process(node.getRight(),tuple);
    }
  }
 else {
    if (b) {
      return true;
    }
 else {
      return process(node.getRight(),tuple);
    }
  }
}
