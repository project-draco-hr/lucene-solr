{
  LightAutomaton a=BasicOperations.concatenateLight(Arrays.asList(BasicAutomata.makeStringLight("m"),BasicAutomata.makeAnyStringLight(),BasicAutomata.makeStringLight("n"),BasicAutomata.makeAnyStringLight()));
  a=BasicOperations.determinize(a);
  assertTrue(BasicOperations.run(a,"mn"));
  assertTrue(BasicOperations.run(a,"mone"));
  assertFalse(BasicOperations.run(a,"m"));
}
