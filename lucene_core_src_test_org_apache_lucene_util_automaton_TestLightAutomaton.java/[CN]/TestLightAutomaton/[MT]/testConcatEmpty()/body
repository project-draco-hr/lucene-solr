{
  LightAutomaton a=BasicOperations.concatenateLight(BasicAutomata.makeEmptyLight(),BasicAutomata.makeStringLight("foo"));
  assertEquals(new HashSet<IntsRef>(),SpecialOperations.getFiniteStrings(a,-1));
  a=BasicOperations.concatenateLight(BasicAutomata.makeStringLight("foo"),BasicAutomata.makeEmptyLight());
  assertEquals(new HashSet<IntsRef>(),SpecialOperations.getFiniteStrings(a,-1));
}
