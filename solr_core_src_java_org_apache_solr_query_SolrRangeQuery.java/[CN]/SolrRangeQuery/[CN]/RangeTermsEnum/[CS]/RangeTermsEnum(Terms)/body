{
  if (terms == null) {
    positioned=true;
  }
 else {
    te=terms.iterator();
    if (lower != null) {
      TermsEnum.SeekStatus status=te.seekCeil(lower);
      if (status == TermsEnum.SeekStatus.END) {
        positioned=true;
        curr=null;
      }
 else       if (status == SeekStatus.FOUND) {
        positioned=includeLower();
        curr=te.term();
      }
 else {
        positioned=true;
        curr=te.term();
      }
    }
  }
}
