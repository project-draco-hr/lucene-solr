{
  byte[] arr=f.binaryValue();
  if (arr == null)   return badFieldString(f);
switch (type) {
case INTEGER:
    return toInt(arr);
case FLOAT:
  return toFloat(arr);
case LONG:
return toLong(arr);
case DOUBLE:
return toDouble(arr);
case DATE:
return new Date(toLong(arr));
default :
throw new SolrException(SolrException.ErrorCode.SERVER_ERROR,"Unknown type for trie field: " + f.name());
}
}
