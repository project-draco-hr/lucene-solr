{
  if (deletable != null) {
    List<String> oldDeletable=deletable;
    deletable=null;
    int size=oldDeletable.size();
    for (int i=0; i < size; i++) {
      if (infoStream != null)       message("delete pending file " + oldDeletable.get(i));
      deleteFile(oldDeletable.get(i));
    }
  }
}
