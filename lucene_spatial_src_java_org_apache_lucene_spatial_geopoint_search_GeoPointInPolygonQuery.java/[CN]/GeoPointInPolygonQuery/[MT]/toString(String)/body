{
  final StringBuilder sb=new StringBuilder();
  sb.append(getClass().getSimpleName());
  sb.append(':');
  if (!getField().equals(field)) {
    sb.append(" field=");
    sb.append(getField());
    sb.append(':');
  }
  sb.append(" Polygon: ");
  sb.append(Arrays.toString(polygons));
  return sb.toString();
}
