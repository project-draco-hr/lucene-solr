{
  if (next != null)   next.processDelete(cmd);
  if (cmd.id != null) {
    if (deletes == null) {
      deletes=new ArrayList<String>();
      toLog.add("delete",deletes);
    }
    if (deletes.size() < maxNumToLog) {
      deletes.add(cmd.id);
    }
  }
 else {
    if (toLog.size() < maxNumToLog) {
      toLog.add("deleteByQuery",cmd.query);
    }
  }
  numDeletes++;
}
