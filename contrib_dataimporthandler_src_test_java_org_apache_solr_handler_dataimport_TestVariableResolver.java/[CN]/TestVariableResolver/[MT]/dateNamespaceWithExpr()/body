{
  VariableResolverImpl vri=new VariableResolverImpl();
  vri.addNamespace("dataimporter.functions",EvaluatorBag.getFunctionsNamespace(vri,Collections.EMPTY_LIST,null));
  String s=vri.replaceTokens("${dataimporter.functions.formatDate('NOW',yyyy-MM-dd HH:mm)}");
  Assert.assertEquals(new SimpleDateFormat("yyyy-MM-dd HH:mm").format(new Date()),s);
}
