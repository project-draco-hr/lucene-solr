{
  if (ignoreCase)   for (int i=0; i < text.length; i++)   text[i]=Character.toLowerCase(text[i]);
  int code=getHashCode(text,text.length);
  int pos=code & mask;
  char[] text2=entries[pos];
  if (text2 != null) {
    final int inc=code * 1347 | 1;
    do {
      code+=inc;
      pos=code & mask;
      text2=entries[pos];
    }
 while (text2 != null);
  }
  entries[pos]=text;
  count++;
  if (((double)count) / entries.length > MAX_LOAD_FACTOR) {
    rehash();
  }
}
