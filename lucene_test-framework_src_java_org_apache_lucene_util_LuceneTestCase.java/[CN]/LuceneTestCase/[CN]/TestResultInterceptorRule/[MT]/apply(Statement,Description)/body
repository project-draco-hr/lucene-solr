{
  return new Statement(){
    @Override public void evaluate() throws Throwable {
      starting(description);
      try {
        base.evaluate();
      }
 catch (      AssumptionViolatedException e) {
        assumptionIgnored(e,description);
        throw e;
      }
catch (      Throwable t) {
        failed(t,description);
        throw t;
      }
 finally {
        ending(description);
      }
    }
  }
;
}
