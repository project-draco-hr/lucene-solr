{
  StringBuilder b=new StringBuilder();
  b.append("NOTE: reproduce with: ant test -Dtestcase=").append(getClass().getSimpleName());
  if (getName() != null) {
    b.append(" -Dtestmethod=").append(getName());
  }
  b.append(" -Dtests.seed=").append(new ThreeLongs(staticSeed,seed,LuceneTestCaseRunner.runnerSeed)).append(reproduceWithExtraParams());
  System.err.println(b.toString());
}
