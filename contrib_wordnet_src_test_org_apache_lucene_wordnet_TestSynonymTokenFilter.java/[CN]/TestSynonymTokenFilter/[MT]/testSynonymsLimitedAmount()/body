{
  SynonymMap map=new SynonymMap(getClass().getResourceAsStream(testFile));
  Analyzer analyzer=new SynonymWhitespaceAnalyzer(map,1);
  assertAnalyzesTo(analyzer,"Lost in the woods",new String[]{"lost","in","the","woods","wood"},new int[]{0,5,8,12,12},new int[]{4,7,11,17,17},new int[]{1,1,1,1,0});
}
