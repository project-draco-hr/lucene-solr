{
  CharArraySet set=new CharArraySet(Version.LUCENE_CURRENT,1,true);
  set.add("fischen");
  GermanStemFilter filter=new GermanStemFilter(new KeywordMarkerTokenFilter(new LowerCaseTokenizer(Version.LUCENE_CURRENT,new StringReader("Fischen Trinken")),set));
  assertTokenStreamContents(filter,new String[]{"fischen","trink"});
}
