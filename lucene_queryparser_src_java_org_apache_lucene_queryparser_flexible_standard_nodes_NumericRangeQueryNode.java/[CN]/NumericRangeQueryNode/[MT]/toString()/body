{
  StringBuilder sb=new StringBuilder("<numericRange lowerInclusive='");
  sb.append(isLowerInclusive()).append("' upperInclusive='").append(isUpperInclusive()).append("' precisionStep='" + numericConfig.getPrecisionStep()).append("' type='" + numericConfig.getType()).append("'>\n");
  sb.append(getLowerBound()).append('\n');
  sb.append(getUpperBound()).append('\n');
  sb.append("</numericRange>");
  return sb.toString();
}
