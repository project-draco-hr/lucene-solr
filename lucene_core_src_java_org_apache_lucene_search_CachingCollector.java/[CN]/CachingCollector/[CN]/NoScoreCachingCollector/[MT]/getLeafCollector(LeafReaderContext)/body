{
  postCollection();
  final LeafCollector in=this.in.getLeafCollector(context);
  if (contexts != null) {
    contexts.add(context);
    acceptDocsOutOfOrders.add(in.acceptsDocsOutOfOrder());
  }
  if (maxDocsToCache >= 0) {
    return lastCollector=wrap(in,maxDocsToCache);
  }
 else {
    return in;
  }
}
