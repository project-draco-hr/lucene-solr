{
  final OpenBitSet bits=new OpenBitSet(reader.maxDoc());
  final TermDocs termDocs=reader.termDocs();
  try {
    final int count=splitRange(reader,termDocs,bits,min,trieVariant.TRIE_CODED_NUMERIC_MIN.equals(min),max,trieVariant.TRIE_CODED_NUMERIC_MAX.equals(max));
    lastNumberOfTerms=new Integer(count);
  }
  finally {
    termDocs.close();
  }
  return bits;
}
