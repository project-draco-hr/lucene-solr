{
  final StringBuffer sb=new StringBuffer();
  if (!this.field.equals(field))   sb.append(this.field).append(':');
  return sb.append(minInclusive ? '[' : '{').append((minUnconverted == null) ? "*" : minUnconverted.toString()).append(" TO ").append((maxUnconverted == null) ? "*" : maxUnconverted.toString()).append(maxInclusive ? ']' : '}').toString();
}
