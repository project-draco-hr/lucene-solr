{
  int tpPos=pp.position + pp.offset;
  for (int i=0; i < repeats.length; i++) {
    PhrasePositions pp2=repeats[i];
    if (pp2 == pp) {
      continue;
    }
    if (pp.offset == pp2.offset) {
      continue;
    }
    int tpPos2=pp2.position + pp2.offset;
    if (tpPos2 == tpPos) {
      return pp.offset > pp2.offset ? pp : pp2;
    }
  }
  return null;
}
