{
  this.buffer.addDeleted(ENTRYID,FEEDID);
  assertNull(this.buffer.getSortedEntries(FEEDID));
  assertEquals(1,this.buffer.getExculdList().length);
  assertEquals(ENTRYID,this.buffer.getExculdList()[0]);
  this.buffer.addDeleted(ENTRYID,FEEDID);
  assertNull(this.buffer.getSortedEntries(FEEDID));
  assertEquals(1,this.buffer.getExculdList().length);
  assertEquals(ENTRYID,this.buffer.getExculdList()[0]);
}
