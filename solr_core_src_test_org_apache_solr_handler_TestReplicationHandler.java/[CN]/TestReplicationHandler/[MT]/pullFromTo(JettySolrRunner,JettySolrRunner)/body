{
  String masterUrl;
  URL url;
  InputStream stream;
  masterUrl=buildUrl(to.getLocalPort()) + "/" + DEFAULT_TEST_CORENAME+ ReplicationHandler.PATH+ "?wait=true&command=fetchindex&masterUrl="+ buildUrl(from.getLocalPort())+ "/"+ DEFAULT_TEST_CORENAME+ ReplicationHandler.PATH;
