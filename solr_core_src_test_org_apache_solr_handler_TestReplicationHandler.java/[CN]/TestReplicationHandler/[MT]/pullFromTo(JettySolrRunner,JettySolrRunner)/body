{
  String masterUrl;
  URL url;
  InputStream stream;
  masterUrl=buildUrl(to.getLocalPort()) + "/replication?wait=true&command=fetchindex&masterUrl=" + buildUrl(from.getLocalPort())+ "/replication";
  url=new URL(masterUrl);
  stream=url.openStream();
