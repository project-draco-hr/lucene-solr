{
  final int totalDocs=TestUtil.nextInt(random(),1,10);
  for (int i=0; i < totalDocs; i++) {
    try {
      index(masterClient,"id",i + startId,"name",TestUtil.randomSimpleString(random(),1000,5000));
    }
 catch (    Exception e) {
    }
  }
  try {
    masterClient.commit();
  }
 catch (  Exception e) {
  }
}
