{
  final IndexOutput output=dir.createOutput(name);
  boolean success=false;
  long version;
  try {
    version=map.write(output);
    success=true;
  }
  finally {
    try {
      output.close();
    }
 catch (    Throwable t) {
    }
    if (!success) {
      try {
        dir.deleteFile(name);
      }
 catch (      Throwable t) {
      }
    }
 else {
      dir.sync(Collections.singleton(name));
    }
  }
  return version;
}
