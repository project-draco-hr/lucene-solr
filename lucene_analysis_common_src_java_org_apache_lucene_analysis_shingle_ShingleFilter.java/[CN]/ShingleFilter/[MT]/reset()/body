{
  super.reset();
  gramSize.reset();
  inputWindow.clear();
  nextInputStreamToken=null;
  isNextInputStreamToken=false;
  numFillerTokensToInsert=0;
  isOutputHere=false;
  noShingleOutput=true;
  exhausted=false;
  endState=null;
  if (outputUnigramsIfNoShingles && !outputUnigrams) {
    gramSize.minValue=minShingleSize;
  }
}
