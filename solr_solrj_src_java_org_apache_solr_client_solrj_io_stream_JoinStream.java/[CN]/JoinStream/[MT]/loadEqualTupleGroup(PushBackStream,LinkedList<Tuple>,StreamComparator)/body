{
  Tuple firstMember=stream.read();
  if (!firstMember.EOF) {
    group.add(firstMember);
    BREAKPOINT:     while (true) {
      Tuple nMember=stream.read();
      if (!nMember.EOF && 0 == groupComparator.compare(firstMember,nMember)) {
        group.add(nMember);
      }
 else {
        stream.pushBack(nMember);
        break BREAKPOINT;
      }
    }
  }
  return firstMember;
}
