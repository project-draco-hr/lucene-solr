{
  if (maps.size() != ids.length) {
    throw new Exception("Expected id count != actual map count:" + ids.length + ":"+ maps.size());
  }
  int i=0;
  for (  int val : ids) {
    Map t=maps.get(i);
    Long tip=(Long)t.get("id");
    if (tip.intValue() != val) {
      throw new Exception("Found value:" + tip.intValue() + " expecting:"+ val);
    }
    ++i;
  }
  return true;
}
