{
  final Class<?> superClazz=clazz.getSuperclass();
  for (  Method m : superClazz.getMethods()) {
    final int mods=m.getModifiers();
    if (Modifier.isStatic(mods) || Modifier.isAbstract(mods) || Modifier.isFinal(mods)|| m.isSynthetic()|| m.getName().equals("attributes")|| m.getName().equals("getStats")) {
      continue;
    }
    final Method subM=clazz.getMethod(m.getName(),m.getParameterTypes());
    if (subM.getDeclaringClass() == clazz && m.getDeclaringClass() != Object.class && m.getDeclaringClass() != subM.getDeclaringClass()) {
      fail(clazz + " overrides " + m+ " although it has a default impl");
    }
  }
}
