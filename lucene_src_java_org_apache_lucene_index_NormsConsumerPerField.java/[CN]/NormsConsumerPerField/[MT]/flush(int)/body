{
  DocValuesConsumer consumer=this.consumer;
  if (consumer == null && fieldInfo.isIndexed) {
    consumer=getConsumer();
    spare.bytes[0]=0x00;
    value.setBytes(spare,Type.BYTES_FIXED_STRAIGHT);
    consumer.add(docCount - 1,value);
  }
  if (consumer != null) {
    consumer.finish(docCount);
  }
}
