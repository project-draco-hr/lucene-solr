{
  assertU(adoc("id","0"));
  assertU(adoc("id","1"));
  assertU(adoc("id","2","datedv","1900-12-31T23:59:59.999Z"));
  assertU(adoc("id","3","datedv","2005-12-31T23:59:59.999Z"));
  assertU(commit());
  assertQ(req("q","*:*","facet","true","facet.field","datedv","facet.mincount","1","facet.missing","true"),"//lst[@name='facet_fields']/lst[@name='datedv']/int[@name='1900-12-31T23:59:59.999Z'][.=1]","//lst[@name='facet_fields']/lst[@name='datedv']/int[@name='2005-12-31T23:59:59.999Z'][.=1]","//lst[@name='facet_fields']/lst[@name='datedv']/int[.=2]");
}
