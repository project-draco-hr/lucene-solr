{
  assertU(adoc("id","0"));
  assertU(adoc("id","1","datedv","1900-12-31T23:59:59.999Z"));
  assertU(adoc("id","2","datedv","2005-12-31T23:59:59.999Z"));
  assertU(commit());
  assertQ(req("q","*:*","fl","e:exists(datedv)","sort","id asc"),"//result/doc[1]/bool[@name='e'][.='false']","//result/doc[2]/bool[@name='e'][.='true']","//result/doc[3]/bool[@name='e'][.='true']");
}
