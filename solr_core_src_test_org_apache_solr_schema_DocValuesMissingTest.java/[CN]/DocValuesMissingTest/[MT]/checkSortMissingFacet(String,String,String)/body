{
  assertU(adoc("id","0"));
  assertU(adoc("id","1"));
  assertU(adoc("id","2",field,low));
  assertU(adoc("id","3",field,high));
  assertU(commit());
  assertQ(req("q","*:*","facet","true","facet.field",field,"facet.mincount","1","facet.missing","true"),"//lst[@name='facet_fields']/lst[@name='" + field + "']/int[@name='"+ low+ "'][.=1]","//lst[@name='facet_fields']/lst[@name='" + field + "']/int[@name='"+ high+ "'][.=1]","//lst[@name='facet_fields']/lst[@name='" + field + "']/int[.=2]");
}
