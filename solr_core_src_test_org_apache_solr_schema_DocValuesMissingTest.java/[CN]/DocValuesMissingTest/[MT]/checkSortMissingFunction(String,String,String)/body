{
  assertU(adoc("id","0"));
  assertU(adoc("id","1",field,low));
  assertU(adoc("id","2",field,high));
  assertU(commit());
  assertQ(req("q","*:*","fl","e:exists(" + field + ")","sort","id asc"),"//result/doc[1]/bool[@name='e'][.='false']","//result/doc[2]/bool[@name='e'][.='true']","//result/doc[3]/bool[@name='e'][.='true']");
}
