{
  if (exhausted) {
    return false;
  }
  if (input.incrementToken()) {
    tokenPosition+=posIncAtt.getPositionIncrement();
    if (tokenPosition <= maxTokenPosition) {
      return true;
    }
 else {
      while (consumeAllTokens && input.incrementToken()) {
      }
      exhausted=true;
      return false;
    }
  }
 else {
    exhausted=true;
    return false;
  }
}
