{
  if (args.length != 2) {
    printUsage();
  }
  File wikipedia=new File(args[0]);
  if (wikipedia.exists()) {
    File outputDir=new File(args[1]);
    outputDir.mkdirs();
    ExtractWikipedia extractor=new ExtractWikipedia(wikipedia,outputDir);
    extractor.extract();
  }
 else {
    printUsage();
  }
}
