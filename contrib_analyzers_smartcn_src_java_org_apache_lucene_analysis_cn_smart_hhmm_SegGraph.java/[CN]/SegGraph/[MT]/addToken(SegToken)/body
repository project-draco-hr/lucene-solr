{
  int s=token.startOffset;
  if (!isStartExist(s)) {
    ArrayList newlist=new ArrayList();
    newlist.add(token);
    tokenListTable.put((Object)(new Integer(s)),newlist);
  }
 else {
    List tokenList=(List)tokenListTable.get((Object)(new Integer(s)));
    tokenList.add(token);
  }
  if (s > maxStart)   maxStart=s;
}
