{
  List result=new ArrayList();
  int s=-1, count=0, size=tokenListTable.size();
  List tokenList;
  short index=0;
  while (count < size) {
    if (isStartExist(s)) {
      tokenList=(List)tokenListTable.get(Integer.valueOf(s));
      for (Iterator iter=tokenList.iterator(); iter.hasNext(); ) {
        SegToken st=(SegToken)iter.next();
        st.index=index;
        result.add(st);
        index++;
      }
      count++;
    }
    s++;
  }
  return result;
}
