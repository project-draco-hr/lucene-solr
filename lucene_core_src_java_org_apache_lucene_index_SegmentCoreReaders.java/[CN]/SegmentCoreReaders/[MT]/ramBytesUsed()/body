{
  return ((dvProducer != null) ? dvProducer.ramBytesUsed() : 0) + ((normsProducer != null) ? normsProducer.ramBytesUsed() : 0) + ((fields != null) ? fields.ramBytesUsed() : 0)+ ((fieldsReaderOrig != null) ? fieldsReaderOrig.ramBytesUsed() : 0)+ ((termVectorsReaderOrig != null) ? termVectorsReaderOrig.ramBytesUsed() : 0);
}
