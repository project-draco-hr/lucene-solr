{
  FieldInfo fi=fieldInfos.fieldInfo(field);
  if (fi == null) {
    return null;
  }
  if (fi.getDocValuesType() == null) {
    return null;
  }
  if (!DocValues.isBytes(fi.getDocValuesType())) {
    return null;
  }
  if (simpleDVProducer == null) {
    return null;
  }
  return simpleDocValuesLocal.get().getBinary(fi);
}
