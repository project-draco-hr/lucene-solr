{
  try {
    rowToModelIndex.clear();
    for (int t=0; t < hits.length(); t++) {
      Document document=hits.doc(t);
      Fieldable field=document.getField(ROW_NUMBER);
      rowToModelIndex.add(Integer.valueOf(field.stringValue()));
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
