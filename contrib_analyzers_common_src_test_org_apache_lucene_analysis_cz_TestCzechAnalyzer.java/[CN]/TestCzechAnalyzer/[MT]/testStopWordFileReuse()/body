{
  CzechAnalyzer cz=new CzechAnalyzer(Version.LUCENE_CURRENT);
  assertAnalyzesToReuse(cz,"??esk?? Republika",new String[]{"??esk??","republika"});
  InputStream stopwords=new FileInputStream(customStopFile);
  cz.loadStopWords(stopwords,"UTF-8");
  assertAnalyzesToReuse(cz,"??esk?? Republika",new String[]{"??esk??"});
}
