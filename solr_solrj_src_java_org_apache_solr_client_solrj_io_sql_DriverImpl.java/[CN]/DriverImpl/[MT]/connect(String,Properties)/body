{
  if (!acceptsURL(url)) {
    return null;
  }
  StringBuilder buf=new StringBuilder(url);
  boolean needsAmp=true;
  if (!url.contains("?")) {
    buf.append("?");
    needsAmp=false;
  }
  for (  Object key : props.keySet()) {
    Object value=props.get(key);
    if (needsAmp) {
      buf.append("&");
    }
    buf.append(key.toString()).append("=").append(value);
    needsAmp=true;
  }
  return connect(buf.toString());
}
