{
  assertQueryEquals("b:one t:one","one");
  assertQueryEquals("(b:one b:two) (t:one t:two)","one two");
  assertQueryEquals("(b:one -b:two) (t:one -t:two)","one -two");
  assertQueryEquals("(b:one -(b:two b:three)) (t:one -(t:two t:three))","one -(two three)");
  assertQueryEquals("(+b:one +b:two) (+t:one +t:two)","+one +two");
}
