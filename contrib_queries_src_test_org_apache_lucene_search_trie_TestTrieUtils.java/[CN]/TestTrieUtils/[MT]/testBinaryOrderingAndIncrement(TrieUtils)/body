{
  String last=null;
  for (long l=-100000L; l < 100000L; l++) {
    String act=variant.longToTrieCoded(l);
    if (last != null) {
      assertTrue(last.compareTo(act) < 0);
      assertEquals(variant.incrementTrieCoded(last),act);
      assertEquals(last,variant.decrementTrieCoded(act));
    }
    last=act;
  }
}
