{
switch (valueType) {
case FIXED_INTS_64:
    bytesRef.copy(v);
  break;
case FIXED_INTS_32:
bytesRef.copy((int)(0xFFFFFFFF & v));
break;
case FIXED_INTS_16:
bytesRef.copy((short)(0xFFFFL & v));
break;
case FIXED_INTS_8:
bytesRef.bytes[0]=(byte)(0xFFL & v);
break;
default :
throw new IllegalStateException("illegal type " + valueType);
}
add(docID,bytesRef);
}
