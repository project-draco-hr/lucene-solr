{
  if (!(o instanceof XYZSolid))   return false;
  XYZSolid other=(XYZSolid)o;
  if (!super.equals(other) || other.isWholeWorld != isWholeWorld) {
    return false;
  }
  if (!isWholeWorld) {
    return other.minXPlane.equals(minXPlane) && other.maxXPlane.equals(maxXPlane) && other.minYPlane.equals(minYPlane)&& other.maxYPlane.equals(maxYPlane)&& other.minZPlane.equals(minZPlane)&& other.maxZPlane.equals(maxZPlane);
  }
  return true;
}
