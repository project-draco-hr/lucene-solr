{
  super();
  if (aLocalName == null)   throw new IllegalArgumentException("localname must not be null");
  if (aNamespace == null)   throw new IllegalArgumentException("Namespace must not be null");
  this.localName=aLocalName;
  this.qname=new QName(aNamespace.getNamespaceUri(),this.localName,aNamespace.getNamespacePrefix());
}
