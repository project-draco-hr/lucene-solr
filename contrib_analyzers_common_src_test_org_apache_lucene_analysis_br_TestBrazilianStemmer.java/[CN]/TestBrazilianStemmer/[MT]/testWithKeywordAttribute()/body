{
  CharArraySet set=new CharArraySet(Version.LUCENE_CURRENT,1,true);
  set.add("Bras??lia");
  BrazilianStemFilter filter=new BrazilianStemFilter(new KeywordMarkerTokenFilter(new LowerCaseTokenizer(new StringReader("Bras??lia Brasilia")),set));
  assertTokenStreamContents(filter,new String[]{"bras??lia","brasil"});
}
