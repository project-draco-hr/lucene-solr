{
  CharArraySet set=new CharArraySet(TEST_VERSION_CURRENT,1,true);
  set.add("Bras??lia");
  BrazilianStemFilter filter=new BrazilianStemFilter(new LowerCaseTokenizer(TEST_VERSION_CURRENT,new StringReader("Bras??lia Brasilia")),set);
  assertTokenStreamContents(filter,new String[]{"bras??lia","brasil"});
}
