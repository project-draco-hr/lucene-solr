{
  final LeafCache leafCache=cache.get(context.reader().getCoreCacheKey());
  if (leafCache == null) {
    missCount+=1;
    return null;
  }
  final Filter singleton=uniqueFilters.get(filter);
  if (singleton == null) {
    missCount+=1;
    return null;
  }
  final DocIdSet cached=leafCache.get(singleton);
  if (cached == null) {
    missCount+=1;
  }
 else {
    hitCount+=1;
  }
  return cached;
}
