{
  if (this.terms == null) {
    termsStream.open();
    this.terms=new ArrayList<>();
    this.idfs=new ArrayList();
    while (true) {
      Tuple termTuple=termsStream.read();
      if (termTuple.EOF) {
        break;
      }
 else {
        terms.add(termTuple.getString("term_s"));
        idfs.add(termTuple.getDouble("idf_d"));
      }
    }
    termsStream.close();
  }
}
