{
  if (iterator == null) {
    initializeIterator();
  }
  if (iterator.hasNext()) {
    clearAttributes();
    AnnotationFS next=iterator.next();
    termAttr.append(next.getCoveredText());
    offsetAttr.setOffset(correctOffset(next.getBegin()),correctOffset(next.getEnd()));
    typeAttr.setType(featurePath.getValueAsString(next));
    return true;
  }
 else {
    return false;
  }
}
