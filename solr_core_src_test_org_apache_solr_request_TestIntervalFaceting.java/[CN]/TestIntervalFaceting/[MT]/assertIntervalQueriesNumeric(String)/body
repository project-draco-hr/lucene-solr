{
  assertIntervalQuery(field,"[0,1]","2");
  assertIntervalQuery(field,"(0,2)","1");
  assertIntervalQuery(field,"[0,2)","2");
  assertIntervalQuery(field,"(0,2]","2");
  assertIntervalQuery(field,"[*,5]","6");
  assertIntervalQuery(field,"[*,3)","3","[2,5)","3","[6,8)","2","[3,*]","7","[10,10]","1","[10,10]","1","[10,10]","1");
}
