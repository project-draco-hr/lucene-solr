{
  assertU(adoc("id","1","test_s_dv","dog","test_l_dv","1"));
  assertU(adoc("id","2","test_s_dv","cat","test_l_dv","2"));
  assertU(commit());
  assertQ(req("q","*:*","facet","true","facet.interval","{!key=foo}test_s_dv","facet.interval","{!key=bar}test_l_dv","f.test_s_dv.facet.interval.set","[cat,dog]","f.test_l_dv.facet.interval.set","[0,1]","f.test_l_dv.facet.interval.set","[2,*]"),"//lst[@name='facet_intervals']/lst[@name='foo']/int[@name='[cat,dog]'][.=2]","//lst[@name='facet_intervals']/lst[@name='bar']/int[@name='[0,1]'][.=1]","//lst[@name='facet_intervals']/lst[@name='bar']/int[@name='[2,*]'][.=1]");
}
