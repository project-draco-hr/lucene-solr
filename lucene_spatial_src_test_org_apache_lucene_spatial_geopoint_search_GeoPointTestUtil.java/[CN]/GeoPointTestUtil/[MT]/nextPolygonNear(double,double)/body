{
  if (random().nextBoolean()) {
    return surpriseMePolygon(otherLatitude,otherLongitude);
  }
  Rectangle box=nextBoxInternal(nextLatitudeNear(otherLatitude),nextLatitudeNear(otherLatitude),nextLongitudeNear(otherLongitude),nextLongitudeNear(otherLongitude),false);
  if (random().nextBoolean()) {
    return boxPolygon(box);
  }
 else {
    return trianglePolygon(box);
  }
}
