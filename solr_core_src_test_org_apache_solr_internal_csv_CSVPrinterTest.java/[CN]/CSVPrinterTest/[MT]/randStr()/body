{
  int sz=r.nextInt(20);
  char[] buf=new char[sz];
  for (int i=0; i < sz; i++) {
    char ch;
    int what=r.nextInt(20);
switch (what) {
case 0:
      ch='\r';
    break;
case 1:
  ch='\n';
break;
case 2:
ch='\t';
break;
case 3:
ch='\f';
break;
case 4:
ch=' ';
break;
case 5:
ch=',';
break;
case 6:
ch='"';
break;
case 7:
ch='\'';
break;
case 8:
ch='\\';
break;
default :
ch=(char)r.nextInt(300);
break;
}
buf[i]=ch;
}
return new String(buf);
}
