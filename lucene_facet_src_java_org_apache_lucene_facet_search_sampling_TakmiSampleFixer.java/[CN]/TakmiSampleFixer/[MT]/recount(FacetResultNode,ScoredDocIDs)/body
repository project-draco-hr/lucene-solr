{
  CategoryPath catPath=fresNode.getLabel(taxonomyReader);
  Term drillDownTerm=DrillDown.term(searchParams,catPath);
  Bits liveDocs=MultiFields.getLiveDocs(indexReader);
  int updatedCount=countIntersection(MultiFields.getTermDocsEnum(indexReader,liveDocs,drillDownTerm.field(),drillDownTerm.bytes(),false),docIds.iterator());
  fresNode.setValue(updatedCount);
}
