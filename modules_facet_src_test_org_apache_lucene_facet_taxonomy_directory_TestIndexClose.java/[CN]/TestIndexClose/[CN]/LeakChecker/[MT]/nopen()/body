{
  int ret=0;
  for (  DirectoryReader r : readers) {
    try {
      r.getTopReaderContext();
      System.err.println("reader " + r + " still open");
      ret++;
    }
 catch (    AlreadyClosedException e) {
    }
  }
  for (  int i : openWriters) {
    System.err.println("writer " + i + " still open");
    ret++;
  }
  return ret;
}
