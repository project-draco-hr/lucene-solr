{
  Term t1=randomTerm();
  Term t2=randomTerm();
  SpanQuery subquery[]=new SpanQuery[]{span(new SpanTermQuery(t1)),span(new SpanTermQuery(t2))};
  SpanQuery nearQuery=span(new SpanNearQuery(subquery,10,true));
  Query q1=span(new SpanFirstQuery(nearQuery,Integer.MAX_VALUE));
  Query q2=nearQuery;
  assertSameSet(q1,q2);
}
