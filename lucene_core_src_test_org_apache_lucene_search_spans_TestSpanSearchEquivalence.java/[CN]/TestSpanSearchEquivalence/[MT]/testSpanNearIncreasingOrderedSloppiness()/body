{
  Term t1=randomTerm();
  Term t2=randomTerm();
  SpanQuery subquery[]=new SpanQuery[]{span(new SpanTermQuery(t1)),span(new SpanTermQuery(t2))};
  for (int i=0; i < 10; i++) {
    SpanQuery q1=span(new SpanNearQuery(subquery,i,false));
    SpanQuery q2=span(new SpanNearQuery(subquery,i + 1,false));
    assertSubsetOf(q1,q2);
  }
}
