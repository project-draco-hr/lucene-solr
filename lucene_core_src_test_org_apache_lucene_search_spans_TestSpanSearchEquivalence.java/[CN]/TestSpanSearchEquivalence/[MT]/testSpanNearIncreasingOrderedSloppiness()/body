{
  Term t1=randomTerm();
  Term t2=randomTerm();
  SpanQuery subquery[]=new SpanQuery[]{new SpanTermQuery(t1),new SpanTermQuery(t2)};
  for (int i=0; i < 10; i++) {
    SpanNearQuery q1=new SpanNearQuery(subquery,i,false);
    SpanNearQuery q2=new SpanNearQuery(subquery,i + 1,false);
    assertSubsetOf(q1,q2);
  }
}
