{
  Term t1=randomTerm();
  Term t2=randomTerm();
  SpanQuery subquery[]=new SpanQuery[]{spanQuery(new SpanTermQuery(t1)),spanQuery(new SpanTermQuery(t2))};
  SpanQuery q1=spanQuery(new SpanNearQuery(subquery,0,true));
  PhraseQuery q2=new PhraseQuery();
  q2.add(t1);
  q2.add(t2);
  assertSameSet(q1,q2);
}
