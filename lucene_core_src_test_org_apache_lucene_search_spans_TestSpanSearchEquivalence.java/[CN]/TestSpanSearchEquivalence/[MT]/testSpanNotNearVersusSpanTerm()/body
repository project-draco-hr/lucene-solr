{
  Term t1=randomTerm();
  Term t2=randomTerm();
  Term t3=randomTerm();
  SpanQuery near=span(new SpanNearQuery(new SpanQuery[]{span(new SpanTermQuery(t2)),span(new SpanTermQuery(t3))},10,random().nextBoolean()));
  assertSubsetOf(span(new SpanNotQuery(span(new SpanTermQuery(t1)),near)),span(new SpanTermQuery(t1)));
}
