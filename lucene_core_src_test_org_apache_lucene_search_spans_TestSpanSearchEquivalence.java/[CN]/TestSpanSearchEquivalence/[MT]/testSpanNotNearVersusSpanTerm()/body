{
  Term t1=randomTerm();
  Term t2=randomTerm();
  Term t3=randomTerm();
  SpanQuery near=spanQuery(new SpanNearQuery(new SpanQuery[]{spanQuery(new SpanTermQuery(t2)),spanQuery(new SpanTermQuery(t3))},10,random().nextBoolean()));
  assertSubsetOf(spanQuery(new SpanNotQuery(spanQuery(new SpanTermQuery(t1)),near)),spanQuery(new SpanTermQuery(t1)));
}
