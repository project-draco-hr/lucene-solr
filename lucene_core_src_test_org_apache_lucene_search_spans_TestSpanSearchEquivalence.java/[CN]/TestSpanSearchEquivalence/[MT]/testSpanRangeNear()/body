{
  Term t1=randomTerm();
  Term t2=randomTerm();
  SpanQuery subquery[]=new SpanQuery[]{new SpanTermQuery(t1),new SpanTermQuery(t2)};
  SpanQuery nearQuery=new SpanNearQuery(subquery,10,true);
  for (int i=0; i < 5; i++) {
    for (int j=0; j < 5; j++) {
      Query q1=new SpanPositionRangeQuery(nearQuery,i,i + j);
      Query q2=nearQuery;
      assertSubsetOf(q1,q2);
    }
  }
}
