{
  Term t1=randomTerm();
  Term t2=randomTerm();
  SpanQuery subquery[]=new SpanQuery[]{new SpanTermQuery(t1),new SpanTermQuery(t2)};
  SpanQuery nearQuery=new SpanNearQuery(subquery,10,true);
  Query q1=new SpanPositionRangeQuery(nearQuery,0,Integer.MAX_VALUE);
  Query q2=nearQuery;
  assertSameSet(q1,q2);
}
