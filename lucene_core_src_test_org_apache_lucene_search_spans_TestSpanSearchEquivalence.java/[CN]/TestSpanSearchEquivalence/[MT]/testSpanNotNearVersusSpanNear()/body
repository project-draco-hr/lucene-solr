{
  Term t1=randomTerm();
  Term t2=randomTerm();
  Term t3=randomTerm();
  Term t4=randomTerm();
  SpanQuery near1=spanQuery(new SpanNearQuery(new SpanQuery[]{spanQuery(new SpanTermQuery(t1)),spanQuery(new SpanTermQuery(t2))},10,random().nextBoolean()));
  SpanQuery near2=spanQuery(new SpanNearQuery(new SpanQuery[]{spanQuery(new SpanTermQuery(t3)),spanQuery(new SpanTermQuery(t4))},10,random().nextBoolean()));
  assertSubsetOf(spanQuery(new SpanNotQuery(near1,near2)),near1);
}
