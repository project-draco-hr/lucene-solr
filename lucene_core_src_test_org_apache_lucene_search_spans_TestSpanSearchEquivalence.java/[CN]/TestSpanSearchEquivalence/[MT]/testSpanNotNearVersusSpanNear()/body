{
  Term t1=randomTerm();
  Term t2=randomTerm();
  Term t3=randomTerm();
  Term t4=randomTerm();
  SpanQuery near1=span(new SpanNearQuery(new SpanQuery[]{span(new SpanTermQuery(t1)),span(new SpanTermQuery(t2))},10,random().nextBoolean()));
  SpanQuery near2=span(new SpanNearQuery(new SpanQuery[]{span(new SpanTermQuery(t3)),span(new SpanTermQuery(t4))},10,random().nextBoolean()));
  assertSubsetOf(span(new SpanNotQuery(near1,near2)),near1);
}
