{
  Term t1=randomTerm();
  Term t2=randomTerm();
  SpanQuery subquery[]=new SpanQuery[]{spanQuery(new SpanTermQuery(t1)),spanQuery(new SpanTermQuery(t2))};
  SpanQuery q1=spanQuery(new SpanNearQuery(subquery,0,false));
  SpanQuery q2=spanQuery(new SpanNearQuery(subquery,1,false));
  assertSubsetOf(q1,q2);
}
