{
  if (dir.isDirectory()) {
    Matcher m=dirNamePattern.matcher(dir.getName());
    if (m.find()) {
      try {
        this.dir=dir;
        this.timestamp=new SimpleDateFormat(SnapShooter.DATE_FMT,Locale.ROOT).parse(m.group(1));
      }
 catch (      Exception e) {
        this.dir=null;
        this.timestamp=null;
      }
    }
  }
}
