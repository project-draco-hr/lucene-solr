{
  final StringBuilder sb=new StringBuilder(getClass().getSimpleName());
  sb.append("(name=").append(getName());
  sb.append(",trees=[");
  for (int ii=0; ii < trees.size(); ++ii) {
    if (ii > 0) {
      sb.append(',');
    }
    sb.append(trees.get(ii));
  }
  sb.append("])");
  return sb.toString();
}
