{
  if (o == this)   return true;
  if (o == null)   return false;
  if (o instanceof NumericRangeQuery) {
    final NumericRangeQuery q=(NumericRangeQuery)o;
    return (field == q.field && (q.min == null ? min == null : q.min.equals(min)) && (q.max == null ? max == null : q.max.equals(max)) && minInclusive == q.minInclusive && maxInclusive == q.maxInclusive && precisionStep == q.precisionStep && getBoost() == q.getBoost());
  }
  return false;
}
