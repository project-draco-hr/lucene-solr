{
  try {
    jetty.start();
  }
 catch (  Exception e) {
    jetty.stop();
    Thread.sleep(2000);
    try {
      jetty.start();
    }
 catch (    Exception e2) {
      jetty.stop();
      Thread.sleep(5000);
      try {
        jetty.start();
      }
 catch (      Exception e3) {
        log.error("",e3);
        jetty.stop();
        return false;
      }
    }
  }
  return true;
}
