{
  try {
    jetty.start();
  }
 catch (  BindException e) {
    jetty.stop();
    Thread.sleep(2000);
    try {
      jetty.start();
    }
 catch (    BindException e2) {
      jetty.stop();
      Thread.sleep(5000);
      try {
        jetty.start();
      }
 catch (      BindException e3) {
        jetty.stop();
        return false;
      }
    }
  }
  return true;
}
