{
  if (addSynonym) {
    clearAttributes();
    termAtt.setEmpty().append("dog");
    posIncAtt.setPositionIncrement(0);
    addSynonym=false;
    return true;
  }
  if (input.incrementToken()) {
    addSynonym=termAtt.toString().equals("dogs");
    return true;
  }
 else {
    return false;
  }
}
