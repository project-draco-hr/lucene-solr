{
  HashMap<String,Float> scoreMap=new HashMap<String,Float>();
  int n=hits.length;
  for (int i=0; i < n; ++i) {
    Document doc=searcher.doc(hits[i].doc);
    String[] v=doc.getValues("tracer");
    assertEquals(v.length,1);
    scoreMap.put(v[0],Float.valueOf(hits[i].score));
  }
  return scoreMap;
}
