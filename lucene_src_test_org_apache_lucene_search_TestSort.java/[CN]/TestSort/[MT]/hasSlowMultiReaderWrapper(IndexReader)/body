{
  if (r instanceof SlowMultiReaderWrapper) {
    return true;
  }
 else {
    IndexReader[] subReaders=r.getSequentialSubReaders();
    if (subReaders != null) {
      for (      IndexReader subReader : subReaders) {
        if (hasSlowMultiReaderWrapper(subReader)) {
          return true;
        }
      }
    }
  }
  return false;
}
