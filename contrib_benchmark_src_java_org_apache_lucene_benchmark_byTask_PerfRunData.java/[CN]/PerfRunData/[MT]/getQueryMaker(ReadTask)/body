{
  Class<? extends ReadTask> readTaskClass=readTask.getClass();
  QueryMaker qm=readTaskQueryMaker.get(readTaskClass);
  if (qm == null) {
    try {
      qm=qmkrClass.newInstance();
      qm.setConfig(config);
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
    readTaskQueryMaker.put(readTaskClass,qm);
  }
  return qm;
}
