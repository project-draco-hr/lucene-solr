{
  CategoryListParams clp=new CategoryListParams("clp");
  PerDimensionIndexingParams tlfip=new PerDimensionIndexingParams(Collections.<FacetLabel,CategoryListParams>singletonMap(new FacetLabel("a"),clp));
  assertEquals("Expected category list field is " + clp.field,clp.field,tlfip.getCategoryListParams(new FacetLabel("a")).field);
  assertNotSame("Unexpected default category list " + clp.field,clp,tlfip.getCategoryListParams(null));
}
