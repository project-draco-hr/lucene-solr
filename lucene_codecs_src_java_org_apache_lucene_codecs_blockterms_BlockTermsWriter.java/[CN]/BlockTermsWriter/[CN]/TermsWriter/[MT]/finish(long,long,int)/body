{
  if (pendingCount > 0) {
    flushBlock();
  }
  out.writeVInt(0);
  this.sumTotalTermFreq=sumTotalTermFreq;
  this.sumDocFreq=sumDocFreq;
  this.docCount=docCount;
  fieldIndexWriter.finish(out.getFilePointer());
  if (numTerms > 0) {
    fields.add(new FieldMetaData(fieldInfo,numTerms,termsStartPointer,sumTotalTermFreq,sumDocFreq,docCount));
  }
}
