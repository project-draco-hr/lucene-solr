{
  super(runData);
  collapsable=(name == null);
  name=(name != null ? name : (parallel ? "Par" : "Seq"));
  setName(name);
  setSequenceName();
  this.parent=parent;
  this.parallel=parallel;
  tasks=new ArrayList<PerfTask>();
  logByTimeMsec=runData.getConfig().get("report.time.step.msec",0);
}
