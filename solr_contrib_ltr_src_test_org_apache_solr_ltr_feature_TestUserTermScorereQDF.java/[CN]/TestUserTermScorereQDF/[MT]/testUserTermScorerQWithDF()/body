{
  loadFeature("matchedTitleDF",SolrFeature.class.getCanonicalName(),"{\"q\":\"w5\",\"df\":\"title\"}");
  loadModel("Term-matchedTitleDF",LinearModel.class.getCanonicalName(),new String[]{"matchedTitleDF"},"{\"weights\":{\"matchedTitleDF\":1.0}}");
  final SolrQuery query=new SolrQuery();
  query.setQuery("title:w1");
  query.add("fl","*, score");
  query.add("rows","2");
  query.add("rq","{!ltr model=Term-matchedTitleDF reRankDocs=4}");
  query.set("debugQuery","on");
  assertJQ("/query" + query.toQueryString(),"/response/numFound/==4");
  assertJQ("/query" + query.toQueryString(),"/response/docs/[0]/id=='7'");
  assertJQ("/query" + query.toQueryString(),"/response/docs/[1]/score==0.0");
}
