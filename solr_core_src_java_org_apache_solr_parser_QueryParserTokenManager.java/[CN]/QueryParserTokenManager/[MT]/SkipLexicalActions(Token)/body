{
switch (jjmatchedKind) {
case 9:
    image.append(input_stream.GetSuffix(jjimageLen + (lengthOfMatch=jjmatchedPos + 1)));
  commentNestingDepth++;
break;
case 10:
image.append(input_stream.GetSuffix(jjimageLen + (lengthOfMatch=jjmatchedPos + 1)));
commentNestingDepth-=1;
SwitchTo(commentNestingDepth == 0 ? DEFAULT : COMMENT);
break;
default :
break;
}
}
