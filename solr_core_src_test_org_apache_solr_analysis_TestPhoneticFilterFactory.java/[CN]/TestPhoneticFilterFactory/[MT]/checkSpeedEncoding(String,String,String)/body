{
  long start=System.currentTimeMillis();
  for (int i=0; i < REPEATS; i++) {
    assertAlgorithm(encoder,"false",toBeEncoded,new String[]{estimated});
  }
  long duration=System.currentTimeMillis() - start;
  if (VERBOSE)   System.out.println(encoder + " encodings per msec: " + (REPEATS / duration));
}
