{
  super(maxDoc,tempDir,tempFileNamePrefix,numDims,bytesPerDim,maxPointsInLeafNode,maxMBSortInHeap,totalPointCount,getRandomSingleValuePerDoc(singleValuePerDoc,randomSeed),getRandomLongOrds(totalPointCount,singleValuePerDoc,randomSeed),getRandomOfflineSorterBufferMB(randomSeed),getRandomOfflineSorterMaxTempFiles(randomSeed));
  this.random=new Random(randomSeed);
