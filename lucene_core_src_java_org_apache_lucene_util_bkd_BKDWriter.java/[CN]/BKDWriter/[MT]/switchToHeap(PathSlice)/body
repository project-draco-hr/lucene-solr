{
  int count=Math.toIntExact(source.count);
  try (PointWriter writer=new HeapPointWriter(count,count,packedBytesLength);PointReader reader=source.writer.getReader(source.start)){
    for (int i=0; i < count; i++) {
      boolean hasNext=reader.next();
      assert hasNext;
      writer.append(reader.packedValue(),reader.ord(),reader.docID());
    }
    return new PathSlice(writer,0,count);
  }
 }
