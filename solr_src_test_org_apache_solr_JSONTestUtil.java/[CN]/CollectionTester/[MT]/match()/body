{
  if (expected == null && val == null) {
    return true;
  }
  if (expected instanceof List) {
    return matchList();
  }
  if (expected instanceof Map) {
    return matchMap();
  }
  if (!expected.equals(val)) {
    setErr("mismatch: '" + expected + "'!='"+ val+ "'");
    return false;
  }
  return true;
}
