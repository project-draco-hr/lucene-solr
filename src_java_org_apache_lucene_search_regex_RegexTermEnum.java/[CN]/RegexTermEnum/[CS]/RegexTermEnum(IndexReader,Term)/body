{
  super();
  field=term.field();
  String text=term.text();
  pattern=Pattern.compile(text);
  int index=0;
  while (index < text.length()) {
    char c=text.charAt(index);
    if (c == '*' || c == '[' || c == '?' || c == '.')     break;
    index++;
  }
  pre=text.substring(0,index);
  setEnum(reader.terms(new Term(term.field(),pre)));
}
