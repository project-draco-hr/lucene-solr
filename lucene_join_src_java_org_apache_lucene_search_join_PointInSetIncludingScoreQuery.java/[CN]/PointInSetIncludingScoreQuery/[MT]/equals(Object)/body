{
  if (super.equals(other)) {
    final PointInSetIncludingScoreQuery q=(PointInSetIncludingScoreQuery)other;
    return q.field.equals(field) && q.originalQuery.equals(originalQuery) && q.bytesPerDim == bytesPerDim && q.sortedPackedPointsHashCode == sortedPackedPointsHashCode && q.sortedPackedPoints.equals(sortedPackedPoints);
  }
  return false;
}
