{
  double lat0=randomLat(small);
  double lat1=randomLat(small);
  double lon0=randomLon(small);
  double lon1=randomLon(small);
  if (lat1 < lat0) {
    double x=lat0;
    lat0=lat1;
    lat1=x;
  }
  if (canCrossDateLine == false && lon1 < lon0) {
    double x=lon0;
    lon0=lon1;
    lon1=x;
  }
  return new GeoRect(lon0,lon1,lat0,lat1);
}
