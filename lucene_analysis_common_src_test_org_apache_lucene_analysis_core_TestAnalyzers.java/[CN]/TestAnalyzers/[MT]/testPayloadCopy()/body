{
  String s="how now brown cow";
  TokenStream ts;
  ts=new WhitespaceTokenizer();
  ((Tokenizer)ts).setReader(new StringReader(s));
  ts=new PayloadSetter(ts);
  verifyPayload(ts);
  ts=new WhitespaceTokenizer();
  ((Tokenizer)ts).setReader(new StringReader(s));
  ts=new PayloadSetter(ts);
  verifyPayload(ts);
}
