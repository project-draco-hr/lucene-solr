{
  super.setUp();
  httpClient=HttpClientUtil.createClient(null);
  HttpClientUtil.setConnectionTimeout(httpClient,1000);
  for (int i=0; i < solr.length; i++) {
    solr[i]=new SolrInstance("solr/collection1" + i,createTempDir("instance-" + i).toFile(),0);
    solr[i].setUp();
    solr[i].startJetty();
    addDocs(solr[i]);
  }
}
