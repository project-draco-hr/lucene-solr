{
  long sz=6 * 8 + 12;
  sz+=bigTerms.size() * 64;
  for (  TopTerm tt : bigTerms.values()) {
    sz+=tt.memSize();
  }
  if (index != null)   sz+=index.length * 4;
  if (tnums != null) {
    for (    byte[] arr : tnums)     if (arr != null)     sz+=arr.length;
  }
  if (maxTermCounts != null)   sz+=maxTermCounts.length * 4;
  sz+=ti.memSize();
  return sz;
}
