{
  if (spans1.docID() == spans2.docID()) {
    if (spans1.startPosition() == spans2.startPosition()) {
      return spans1.endPosition() < spans2.endPosition();
    }
 else {
      return spans1.startPosition() < spans2.startPosition();
    }
  }
 else {
    return spans1.docID() < spans2.docID();
  }
}
