{
  TokenStream stream=new WhitespaceTokenizer(Version.LUCENE_CURRENT,new StringReader("\u0049\u0307STANBUL \u0049\u0307ZM\u0049\u0307R ISPARTA"));
  TurkishLowerCaseFilter filter=new TurkishLowerCaseFilter(stream);
  assertTokenStreamContents(filter,new String[]{"istanbul","izmir","\u0131sparta"});
}
