{
  Analyzer analyzer=new MockAnalyzer(random());
  final TokenStream input=analyzer.tokenStream("field","abc");
  CachingTokenFilter buffer=new CachingTokenFilter(input);
  buffer.reset();
  boolean madeIt=false;
  try {
    buffer.reset();
    madeIt=true;
  }
 catch (  Throwable e) {
  }
  assertFalse(madeIt);
}
