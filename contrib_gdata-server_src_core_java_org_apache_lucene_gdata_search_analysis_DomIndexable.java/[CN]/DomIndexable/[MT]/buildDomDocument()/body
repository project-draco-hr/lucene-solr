{
  StringWriter stringWriter=new StringWriter();
  ExtensionProfile profile=this.applyAble.getServiceConfig().getExtensionProfile();
  if (profile == null)   throw new IllegalStateException("ExtensionProfile is not set");
  XmlWriter writer=new XmlWriter(stringWriter);
  this.applyAble.generateAtom(writer,profile);
  DocumentBuilder builder=DocumentBuilderFactory.newInstance().newDocumentBuilder();
  return builder.parse(new InputSource(new StringReader(stringWriter.toString())));
}
