{
  Node currentNode=this;
  while (true) {
    final int result=edgeComparator.compare(edge,newEdge);
    if (result < 0) {
      if (lesser == null) {
        lesser=new Node(newEdge);
        return;
      }
      currentNode=lesser;
    }
 else     if (result > 0) {
      if (greater == null) {
        greater=new Node(newEdge);
        return;
      }
      currentNode=greater;
    }
 else {
      if (overlaps == null) {
        overlaps=new Node(newEdge);
        return;
      }
      currentNode=overlaps;
    }
  }
}
