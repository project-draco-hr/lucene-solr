{
  Node currentNode=this;
  while (currentNode != null) {
    final int result=edgeComparator.compare(currentNode.edge,value);
    if (result < 0) {
      currentNode=lesser;
    }
 else     if (result > 0) {
      currentNode=greater;
    }
 else {
      if (!edgeIterator.matches(edge)) {
        return false;
      }
      currentNode=overlaps;
    }
  }
  return true;
}
