{
  HashSet<IntsRef> strings=new HashSet<IntsRef>();
  if (a.isSingleton()) {
    if (limit > 0) {
      strings.add(Util.toUTF32(a.singleton,new IntsRef()));
    }
 else {
      return null;
    }
  }
 else   if (!getFiniteStrings(a.initial,new HashSet<State>(),strings,new IntsRef(),limit)) {
    return null;
  }
  return strings;
}
