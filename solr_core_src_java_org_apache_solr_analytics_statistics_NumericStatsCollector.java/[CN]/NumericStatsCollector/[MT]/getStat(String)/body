{
  if (stat.equals("sum")) {
    return new Double(sum);
  }
  if (stat.equals("sumofsquares")) {
    return new Double(sumOfSquares);
  }
  if (stat.equals("mean")) {
    return new Double(mean);
  }
  if (stat.equals("stddev")) {
    return new Double(stddev);
  }
  return super.getStat(stat);
}
