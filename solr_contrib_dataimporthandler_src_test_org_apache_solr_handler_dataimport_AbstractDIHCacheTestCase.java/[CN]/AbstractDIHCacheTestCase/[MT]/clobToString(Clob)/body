{
  StringBuilder sb=new StringBuilder();
  try {
    Reader in=cl.getCharacterStream();
    char[] cbuf=new char[1024];
    int numGot=-1;
    while ((numGot=in.read(cbuf)) != -1) {
      sb.append(String.valueOf(cbuf,0,numGot));
    }
  }
 catch (  Exception e) {
    Assert.fail(e.toString());
  }
  return sb.toString();
}
