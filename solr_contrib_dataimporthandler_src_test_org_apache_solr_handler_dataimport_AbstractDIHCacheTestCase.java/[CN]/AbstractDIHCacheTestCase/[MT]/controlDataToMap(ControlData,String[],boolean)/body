{
  Map<String,Object> rec=null;
  if (keepOrdered) {
    rec=new LinkedHashMap<String,Object>();
  }
 else {
    rec=new HashMap<String,Object>();
  }
  for (int i=0; i < cd.data.length; i++) {
    String fieldName=theFieldNames[i];
    Object data=cd.data[i];
    rec.put(fieldName,data);
  }
  return rec;
}
