{
  String eName=localName;
  if ("".equals(eName)) {
    eName=qualifiedName;
  }
  if (attrs != null) {
    for (int i=0; i < attrs.getLength(); i++) {
      String aName=attrs.getLocalName(i);
      if ("".equals(aName)) {
        aName=attrs.getQName(i);
      }
      System.out.println("attr " + aName + "="+ attrs.getValue(i));
    }
  }
  elementBuffer.setLength(0);
}
