{
  double[] px={0,10,10,0,-8,-10,-8,0,10,10,0};
  double[] py={0,5,9,10,9,0,-9,-10,-9,-5,0};
  double xMin=2;
  double xMax=11;
  double yMin=-1;
  double yMax=1;
  Polygon polygon=new Polygon(py,px);
  assertTrue(polygon.crosses(yMin,yMax,xMin,xMax));
  assertFalse(polygon.contains(yMin,yMax,xMin,xMax));
}
