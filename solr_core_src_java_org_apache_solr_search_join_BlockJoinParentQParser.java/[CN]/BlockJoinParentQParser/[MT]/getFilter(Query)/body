{
  SolrCache parentCache=req.getSearcher().getCache(CACHE_NAME);
  Filter filter=null;
  if (parentCache != null) {
    filter=(Filter)parentCache.get(parentList);
  }
  Filter result;
  if (filter == null) {
    result=createParentFilter(parentList);
    if (parentCache != null) {
      parentCache.put(parentList,result);
    }
  }
 else {
    result=filter;
  }
  return result;
}
