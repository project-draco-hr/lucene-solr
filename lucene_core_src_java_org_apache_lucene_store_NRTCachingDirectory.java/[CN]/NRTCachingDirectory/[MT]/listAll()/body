{
  final Set<String> files=new HashSet<>();
  for (  String f : cache.listAll()) {
    files.add(f);
  }
  try {
    for (    String f : in.listAll()) {
      files.add(f);
    }
  }
 catch (  NoSuchDirectoryException ex) {
    if (files.isEmpty()) {
      throw ex;
    }
  }
  return files.toArray(new String[files.size()]);
}
