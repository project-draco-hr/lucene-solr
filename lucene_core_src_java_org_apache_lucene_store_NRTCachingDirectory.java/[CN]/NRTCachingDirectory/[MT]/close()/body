{
  boolean success=false;
  try {
    if (cache.isOpen) {
      for (      String fileName : cache.listAll()) {
        unCache(fileName);
      }
    }
    success=true;
  }
  finally {
    if (success) {
      IOUtils.close(cache,in);
    }
 else {
      IOUtils.closeWhileHandlingException(cache,in);
    }
  }
}
