{
  if (getChildren() == null || getChildren().size() == 0)   return "<multiPhrase/>";
  StringBuilder sb=new StringBuilder();
  sb.append("<multiPhrase>");
  for (  QueryNode child : getChildren()) {
    sb.append("\n");
    sb.append(child.toString());
  }
  sb.append("\n</multiPhrase>");
  return sb.toString();
}
