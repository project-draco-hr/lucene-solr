{
  super.setUp();
  directory=newDirectory();
  IndexWriter writer=new IndexWriter(directory,new IndexWriterConfig(TEST_VERSION_CURRENT,new SimpleAnalyzer(TEST_VERSION_CURRENT)));
  Document doc=new Document();
  doc.add(new Field("partnum","Q36",StringField.TYPE_STORED));
  doc.add(new Field("description","Illidium Space Modulator",TextField.TYPE_STORED));
  writer.addDocument(doc);
  writer.close();
  reader=IndexReader.open(directory);
  searcher=new IndexSearcher(reader);
}
