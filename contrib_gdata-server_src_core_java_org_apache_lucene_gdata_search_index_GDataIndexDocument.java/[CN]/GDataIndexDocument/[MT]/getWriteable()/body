{
  Document retVal=new Document();
  retVal.add(new Field(FIELD_ENTRY_ID,this.id,Field.Store.YES,Field.Index.UN_TOKENIZED));
  retVal.add(new Field(FIELD_FEED_ID,this.feedId,Field.Store.YES,Field.Index.UN_TOKENIZED));
  for (  ContentStrategy strategy : this.fields) {
    Field[] fieldArray=strategy.createLuceneField();
    for (int i=0; i < fieldArray.length; i++) {
      retVal.add(fieldArray[i]);
    }
  }
  return retVal;
}
