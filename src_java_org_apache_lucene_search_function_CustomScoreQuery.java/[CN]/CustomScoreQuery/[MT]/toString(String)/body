{
  StringBuffer sb=new StringBuffer(name()).append("(");
  sb.append(subQuery.toString(field));
  if (valSrcQuery != null) {
    sb.append(", ").append(valSrcQuery.toString(field));
  }
  sb.append(")");
  sb.append(strict ? " STRICT" : "");
  return sb.toString() + ToStringUtils.boost(getBoost());
}
