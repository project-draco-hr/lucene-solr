{
  List l=(List)m.get(RULE);
  if (l != null) {
    for (    Object o : l) {
      Map map=(Map)o;
      try {
        new Rule(map);
      }
 catch (      Exception e) {
        throw new SolrException(SolrException.ErrorCode.BAD_REQUEST,"Error in rule " + m,e);
      }
    }
  }
  ReplicaAssigner.verifySnitchConf(cc,(List)m.get(SNITCH));
}
