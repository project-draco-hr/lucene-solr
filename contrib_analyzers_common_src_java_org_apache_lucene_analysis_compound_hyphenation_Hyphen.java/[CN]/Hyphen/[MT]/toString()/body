{
  if (noBreak == null && postBreak == null && preBreak != null && preBreak.equals("-")) {
    return "-";
  }
  StringBuffer res=new StringBuffer("{");
  res.append(preBreak);
  res.append("}{");
  res.append(postBreak);
  res.append("}{");
  res.append(noBreak);
  res.append('}');
  return res.toString();
}
