{
{
    SegmentReader reader=SegmentReader.get(true,info,indexDivisor,IOContext.DEFAULT);
    assertTrue(reader != null);
    DocsEnum termDocs=reader.termDocsEnum(reader.getDeletedDocs(),"textField2",new BytesRef("bad"));
    assertNull(termDocs);
    reader.close();
  }
{
    SegmentReader reader=SegmentReader.get(true,info,indexDivisor,IOContext.DEFAULT);
    assertTrue(reader != null);
    DocsEnum termDocs=reader.termDocsEnum(reader.getDeletedDocs(),"junk",new BytesRef("bad"));
    assertNull(termDocs);
    reader.close();
  }
}
