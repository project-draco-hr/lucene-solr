{
{
    SegmentReader reader=SegmentReader.get(info,indexDivisor,newIOContext(random));
    assertTrue(reader != null);
    DocsEnum termDocs=_TestUtil.docs(random,reader,"textField2",new BytesRef("bad"),reader.getLiveDocs(),null,false);
    assertNull(termDocs);
    reader.close();
  }
{
    SegmentReader reader=SegmentReader.get(info,indexDivisor,newIOContext(random));
    assertTrue(reader != null);
    DocsEnum termDocs=_TestUtil.docs(random,reader,"junk",new BytesRef("bad"),reader.getLiveDocs(),null,false);
    assertNull(termDocs);
    reader.close();
  }
}
