{
  boolean bool;
  bool=(contains(r.getMinX(),r.getMinY()));
  if (contains(r.getMinX(),r.getMaxY())) {
    if (!bool)     return 1;
  }
 else {
    if (bool)     return 1;
  }
  if (contains(r.getMaxX(),r.getMinY())) {
    if (!bool)     return 1;
  }
 else {
    if (bool)     return 1;
  }
  if (contains(r.getMaxX(),r.getMaxY())) {
    if (!bool)     return 1;
  }
 else {
    if (bool)     return 1;
  }
  return bool ? 4 : 0;
}
