{
  Arc<Long> arc=new Arc<Long>();
  Long result=null;
  try {
    result=lookupPrefix(new BytesRef(key),arc);
  }
 catch (  IOException bogus) {
    throw new RuntimeException(bogus);
  }
  if (result == null || !arc.isFinal()) {
    return null;
  }
 else {
    return decodeWeight(result + arc.nextFinalOutput);
  }
}
