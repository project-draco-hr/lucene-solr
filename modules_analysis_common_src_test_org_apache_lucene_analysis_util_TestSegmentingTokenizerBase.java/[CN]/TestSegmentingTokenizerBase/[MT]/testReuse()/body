{
  assertAnalyzesToReuse(sentenceAndWord,"He said, \"Are you going?\"",new String[]{"He","said","Are","you","going"},new int[]{0,3,10,14,18},new int[]{2,7,13,17,23},new int[]{1,1,1,1,1});
  assertAnalyzesToReuse(sentenceAndWord,"John shook his head.",new String[]{"John","shook","his","head"},new int[]{0,5,11,15},new int[]{4,10,14,19},new int[]{1,1,1,1});
}
