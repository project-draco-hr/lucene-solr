{
  Token buf=result;
  if (!prefixExhausted) {
    result=prefix.next(result);
    if (result == null) {
      prefixExhausted=true;
    }
 else {
      previousPrefixToken.copyFrom(result);
      return result;
    }
  }
  result=suffix.next(buf);
  if (result == null) {
    return null;
  }
  return updateSuffixToken(result,previousPrefixToken);
}
