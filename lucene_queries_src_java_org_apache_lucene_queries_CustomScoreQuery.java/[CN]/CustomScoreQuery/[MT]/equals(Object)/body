{
  if (this == o)   return true;
  if (!super.equals(o))   return false;
  if (getClass() != o.getClass()) {
    return false;
  }
  CustomScoreQuery other=(CustomScoreQuery)o;
  if (this.getBoost() != other.getBoost() || !this.subQuery.equals(other.subQuery) || this.strict != other.strict || this.scoringQueries.length != other.scoringQueries.length) {
    return false;
  }
  return Arrays.equals(scoringQueries,other.scoringQueries);
}
