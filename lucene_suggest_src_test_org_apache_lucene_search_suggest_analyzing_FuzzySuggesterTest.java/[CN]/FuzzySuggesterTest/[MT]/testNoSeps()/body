{
  TermFreqPayload[] keys=new TermFreqPayload[]{new TermFreqPayload("ab cd",0),new TermFreqPayload("abcd",1)};
  int options=0;
  Analyzer a=new MockAnalyzer(random());
  FuzzySuggester suggester=new FuzzySuggester(a,a,options,256,-1,1,true,1,3,false);
  suggester.build(new TermFreqPayloadArrayIterator(keys));
  List<LookupResult> r=suggester.lookup(_TestUtil.stringToCharSequence("ab c",random()),false,2);
  assertEquals(2,r.size());
  assertEquals("abcd",r.get(0).key.toString());
}
