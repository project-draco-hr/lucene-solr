{
  PerfRunData runData=getRunData();
  Config config=runData.getConfig();
  IndexWriter writer=new IndexWriter(runData.getDirectory(),runData.getAnalyzer(),true,getIndexDeletionPolicy(config),IndexWriter.MaxFieldLength.LIMITED);
  setIndexWriterConfig(writer,config);
  runData.setIndexWriter(writer);
  return 1;
}
