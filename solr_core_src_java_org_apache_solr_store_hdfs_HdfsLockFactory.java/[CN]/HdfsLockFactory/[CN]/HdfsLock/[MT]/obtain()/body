{
  FSDataOutputStream file=null;
  FileSystem fs=null;
  try {
    fs=FileSystem.newInstance(lockPath.toUri(),conf);
    file=fs.create(new Path(lockPath,lockName),false);
  }
 catch (  IOException e) {
    return false;
  }
 finally {
    IOUtils.closeQuietly(file);
    IOUtils.closeQuietly(fs);
  }
  return true;
}
