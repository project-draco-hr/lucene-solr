{
  if (obtained) {
    FileSystem fs=FileSystem.get(lockPath.toUri(),conf);
    try {
      if (fs.exists(new Path(lockPath,lockName)) && !fs.delete(new Path(lockPath,lockName),false))       throw new LockReleaseFailedException("failed to delete " + new Path(lockPath,lockName));
    }
  finally {
      obtained=false;
      IOUtils.closeQuietly(fs);
    }
  }
}
