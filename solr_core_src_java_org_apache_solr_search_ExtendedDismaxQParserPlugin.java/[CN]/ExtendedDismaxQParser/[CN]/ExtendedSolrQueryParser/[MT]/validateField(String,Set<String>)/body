{
  if (this.getAlias(field) == null) {
    return false;
  }
  boolean hascycle=false;
  for (  String referencedField : this.getAlias(field).fields.keySet()) {
    if (!set.add(referencedField)) {
      hascycle=true;
    }
 else {
      if (validateField(referencedField,set)) {
        hascycle=true;
      }
      set.remove(referencedField);
    }
  }
  return hascycle;
}
