{
  BooleanQuery bq=new BooleanQuery.Builder().add(new TermQuery(new Term("foo",TestUtil.randomSimpleString(random()))),Occur.SHOULD).add(new TermQuery(new Term("foo",TestUtil.randomSimpleString(random()))),Occur.SHOULD).build();
  final int hashCode=bq.hashCode();
  assertEquals(hashCode,bq.hashCode());
}
