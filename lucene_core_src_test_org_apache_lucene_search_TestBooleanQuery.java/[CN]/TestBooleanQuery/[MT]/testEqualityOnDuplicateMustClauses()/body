{
  BooleanQuery bq1=new BooleanQuery.Builder().setMinimumNumberShouldMatch(random().nextInt(2)).add(new TermQuery(new Term("foo","bar")),Occur.MUST).build();
  BooleanQuery bq2=new BooleanQuery.Builder().setMinimumNumberShouldMatch(bq1.getMinimumNumberShouldMatch()).add(new TermQuery(new Term("foo","bar")),Occur.MUST).add(new TermQuery(new Term("foo","bar")),Occur.MUST).build();
  QueryUtils.checkUnequal(bq1,bq2);
}
