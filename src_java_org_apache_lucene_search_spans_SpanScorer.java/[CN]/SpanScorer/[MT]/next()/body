{
  if (firstTime) {
    more=spans.next();
    firstTime=false;
  }
  if (!more)   return false;
  freq=0.0f;
  doc=spans.doc();
  while (more && doc == spans.doc()) {
    int matchLength=spans.end() - spans.start();
    freq+=getSimilarity().sloppyFreq(matchLength);
    more=spans.next();
  }
  return more || freq != 0.0f;
}
