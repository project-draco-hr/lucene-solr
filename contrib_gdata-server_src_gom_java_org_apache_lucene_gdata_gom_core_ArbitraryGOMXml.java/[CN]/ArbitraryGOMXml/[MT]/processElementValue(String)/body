{
  if (aValue == null)   throw new GDataParseException("Element value must not be null");
  if (this.children.size() > 0)   throw new GDataParseException(String.format(AtomParser.UNEXPECTED_ELEMENT_VALUE,this.localName));
  if (this.textValue != null)   throw new GDataParseException(String.format(AtomParser.UNEXPECTED_ELEMENT_VALUE,this.localName));
  this.textValue=aValue;
}
