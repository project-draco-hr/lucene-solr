{
  return new SourceEnum(attrSource,type(),this,maxDoc){
    @Override public int advance(    int target) throws IOException {
      if (target >= numDocs) {
        return pos=NO_MORE_DOCS;
      }
      bytesRef.length=1;
      bytesRef.bytes=data;
      bytesRef.offset=target;
      return pos=target;
    }
  }
;
}
