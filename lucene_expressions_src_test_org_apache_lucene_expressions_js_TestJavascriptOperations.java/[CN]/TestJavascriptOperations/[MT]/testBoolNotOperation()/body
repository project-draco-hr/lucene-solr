{
  testComputedExpression("boolnot0","!1",0);
  testComputedExpression("boolnot0","!!1",1);
  testComputedExpression("boolnot0","!0",1);
  testComputedExpression("boolnot0","!!0",0);
  testComputedExpression("boolnot0","!-1",0);
  testComputedExpression("boolnot0","!2",0);
  testComputedExpression("boolnot0","!-2",0);
}
