{
  if (nextTerm != 0 && text.compareTo(term) < 0) {
    nextTerm=0;
    tvf.seek(tvfFP);
  }
  while (next() != null) {
    final int cmp=text.compareTo(term);
    if (cmp < 0) {
      return SeekStatus.NOT_FOUND;
    }
 else     if (cmp == 0) {
      return SeekStatus.FOUND;
    }
  }
  return SeekStatus.END;
}
