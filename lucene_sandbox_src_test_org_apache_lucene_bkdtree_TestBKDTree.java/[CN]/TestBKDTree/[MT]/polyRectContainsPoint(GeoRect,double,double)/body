{
  if (Math.abs(rect.minLat - pointLat) < POLY_TOLERANCE || Math.abs(rect.maxLat - pointLat) < POLY_TOLERANCE || Math.abs(rect.minLon - pointLon) < POLY_TOLERANCE || Math.abs(rect.maxLon - pointLon) < POLY_TOLERANCE) {
    return null;
  }
 else {
    return rectContainsPoint(rect,pointLat,pointLon);
  }
}
