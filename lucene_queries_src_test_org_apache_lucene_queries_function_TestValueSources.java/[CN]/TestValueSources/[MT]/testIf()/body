{
  assertHits(new FunctionQuery(new IfFunction(new BytesRefFieldSource("id"),new ConstValueSource(1.0f),new ConstValueSource(2.0f))),new float[]{1f,1f});
  assertHits(new FunctionQuery(new IfFunction(new LiteralValueSource("false"),new ConstValueSource(1.0f),new ConstValueSource(2.0f))),new float[]{1f,1f});
}
