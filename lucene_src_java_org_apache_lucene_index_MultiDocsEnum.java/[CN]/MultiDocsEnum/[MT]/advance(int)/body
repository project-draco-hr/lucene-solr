{
  while (true) {
    if (current != null) {
      final int doc=current.advance(target - currentBase);
      if (doc == NO_MORE_DOCS) {
        current=null;
      }
 else {
        return this.doc=doc + currentBase;
      }
    }
 else     if (upto == numSubs - 1) {
      return this.doc=NO_MORE_DOCS;
    }
 else {
      upto++;
      current=subs[upto].docsEnum;
      currentBase=subs[upto].slice.start;
    }
  }
}
