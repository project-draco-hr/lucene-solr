{
  super(source,type);
switch (type) {
case FIXED_INTS_16:
    sizeInByte=2;
  break;
case FIXED_INTS_32:
sizeInByte=4;
break;
case FIXED_INTS_64:
sizeInByte=8;
break;
case FIXED_INTS_8:
sizeInByte=1;
break;
default :
throw new IllegalStateException("type " + type + " is not a fixed int type");
}
intsRef.offset=0;
this.dataIn=dataIn;
this.maxDoc=maxDoc;
}
