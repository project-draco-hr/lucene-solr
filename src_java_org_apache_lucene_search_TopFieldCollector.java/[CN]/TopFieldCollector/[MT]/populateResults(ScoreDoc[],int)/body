{
  if (fillFields) {
    FieldValueHitQueue queue=(FieldValueHitQueue)pq;
    for (int i=howMany - 1; i >= 0; i--) {
      results[i]=queue.fillFields(queue.pop());
    }
  }
 else {
    for (int i=howMany - 1; i >= 0; i--) {
      Entry entry=pq.pop();
      results[i]=new FieldDoc(entry.docID,entry.score);
    }
  }
}
