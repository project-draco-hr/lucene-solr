{
  FacetResultNode top=pq.top();
  int numResults=0;
  while (ordinal != TaxonomyReader.INVALID_ORDINAL) {
    float value=values[ordinal];
    if (value > 0.0f) {
      ++numResults;
      if (value > top.value) {
        top.value=value;
        top.ordinal=ordinal;
        top=pq.updateTop();
      }
    }
    ordinal=siblings[ordinal];
  }
  return numResults;
}
