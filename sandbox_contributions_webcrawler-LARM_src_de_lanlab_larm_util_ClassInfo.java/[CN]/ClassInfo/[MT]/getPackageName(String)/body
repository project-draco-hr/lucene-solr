{
  if (className.charAt(0) == '[') {
switch (className.charAt(1)) {
case 'L':
      return getPackageName(className.substring(2,className.length() - 1));
default :
    return "";
}
}
String name=className.lastIndexOf(".") != -1 ? className.substring(0,className.lastIndexOf(".")) : "";
return name;
}
