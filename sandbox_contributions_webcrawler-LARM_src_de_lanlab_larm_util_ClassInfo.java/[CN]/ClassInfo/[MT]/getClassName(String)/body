{
  if (className.charAt(0) == '[') {
switch (className.charAt(1)) {
case 'L':
      return getClassName(className.substring(2,className.length() - 1)) + "[]";
case 'C':
    return "char[]";
case 'I':
  return "int[]";
case 'B':
return "byte[]";
}
}
String name=(className.lastIndexOf(".") > -1) ? className.substring(className.lastIndexOf(".") + 1) : className;
return name;
}
