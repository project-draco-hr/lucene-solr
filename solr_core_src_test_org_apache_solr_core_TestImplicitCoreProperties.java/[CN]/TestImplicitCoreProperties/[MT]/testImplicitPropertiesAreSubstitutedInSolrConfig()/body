{
  CoreContainer cc=createCoreContainer(TEST_HOME(),SOLRXML);
  try {
    cc.load();
    assertQ(req("q","*:*"),"//str[@name='dummy1'][.='collection1']","//str[@name='dummy2'][.='data/']","//str[@name='dummy3'][.='solrconfig-implicitproperties.xml']","//str[@name='dummy4'][.='schema.xml']","//str[@name='dummy5'][.='false']");
  }
  finally {
    cc.shutdown();
  }
}
