{
  super.setUp();
  httpClient=new HttpClient(new MultiThreadedHttpConnectionManager());
  httpClient.getParams().setParameter("http.connection.timeout",new Integer(1000));
  for (int i=0; i < solr.length; i++) {
    solr[i]=new SolrInstance("solr" + i,0);
    solr[i].setUp();
    solr[i].startJetty();
    addDocs(solr[i]);
  }
}
