{
  SolrInputDocument d=processAdd("clone-regex-replaceall",doc(f("id","1111"),f("foo_x2_s","123456789","",42,"abcd"),f("foo_x3_x7_s","xyz")));
  assertNotNull(d);
  assertEquals(Arrays.asList("123456789","",42,"abcd"),d.getFieldValues("foo_y2_s"));
  assertEquals("xyz",d.getFieldValue("foo_y3_y7_s"));
}
