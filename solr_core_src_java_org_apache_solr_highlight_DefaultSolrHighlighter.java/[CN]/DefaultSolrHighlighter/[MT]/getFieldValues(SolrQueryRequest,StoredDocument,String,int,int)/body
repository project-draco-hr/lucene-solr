{
  List<String> result=new ArrayList<>();
  for (  StorableField thisField : doc.getFields()) {
    if (!thisField.name().equals(fieldName)) {
      continue;
    }
    String value=thisField.stringValue();
    result.add(value);
    maxValues--;
    if (maxValues <= 0 || maxCharsToAnalyze <= 0) {
      break;
    }
  }
  return result;
}
