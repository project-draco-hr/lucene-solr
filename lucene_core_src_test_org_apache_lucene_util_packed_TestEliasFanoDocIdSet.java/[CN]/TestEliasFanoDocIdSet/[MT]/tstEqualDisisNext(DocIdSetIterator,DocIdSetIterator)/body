{
  assertEquals(disi0.docID(),disi1.docID());
  int d0=disi0.nextDoc();
  int d1=disi1.nextDoc();
  int i=0;
  while ((d0 != DocIdSetIterator.NO_MORE_DOCS) && (d1 != DocIdSetIterator.NO_MORE_DOCS)) {
    assertEquals("index " + i,d0,d1);
    i++;
    d0=disi0.nextDoc();
    d1=disi1.nextDoc();
  }
  assertEquals("at end",d0,d1);
}
