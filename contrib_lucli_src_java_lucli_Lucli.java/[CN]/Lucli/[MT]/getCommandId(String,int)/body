{
  name=name.toLowerCase();
  Command command=(Command)commandMap.get(name);
  if (command == null) {
    return (UNKOWN);
  }
 else {
    if (command.params > params) {
      error(command.name + " needs at least " + command.params+ " arguments.");
      return (NOCOMMAND);
    }
    return (command.id);
  }
}
