{
  initCore("solrconfig-phrasesuggest.xml","schema-phrasesuggest.xml");
  assertU(adoc("id","9999991","text","true","stext","change"));
  assertU(adoc("id","9999992","text","true","stext","charge"));
  assertU(adoc("id","9999992","text","true","stext","chance"));
  assertU(commit());
  assertQ(req("qt",REQUEST_URI,"q","",SuggesterParams.SUGGEST_DICT,DICT_NAME,SuggesterParams.SUGGEST_BUILD,"true"));
}
