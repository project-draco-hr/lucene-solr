{
  addDocs(random,dir,12,true);
  IndexReader ir=IndexReader.open(dir,false);
  verifyIndex(ir);
  modifyNormsForF1(ir);
  IndexReader irc=(IndexReader)ir.clone();
  verifyIndex(irc);
  modifyNormsForF1(irc);
  IndexReader irc3=(IndexReader)irc.clone();
  verifyIndex(irc3);
  modifyNormsForF1(irc3);
  verifyIndex(irc3);
  irc3.flush();
  ir.close();
  irc.close();
  irc3.close();
}
