{
  Document d=new Document();
  float boost=nextNorm("anyfield");
  FieldType customType=new FieldType(TextField.TYPE_UNSTORED);
  customType.setTokenized(false);
  for (int i=0; i < 10; i++) {
    Field f=newField("f" + i,"v" + i,customType);
    f.setBoost(boost);
    d.add(f);
  }
  return d;
}
