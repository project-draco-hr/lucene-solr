{
  byte[] binary=new byte[]{(byte)0x23,(byte)0x98,(byte)0x13,(byte)0xE4,(byte)0x76,(byte)0x41,(byte)0xB2,(byte)0xC9,(byte)0x7F,(byte)0x0A,(byte)0xA6,(byte)0xD8};
  ByteBuffer binaryBuf=ByteBuffer.wrap(binary);
  CharBuffer encoded=IndexableBinaryStringTools.encode(binaryBuf);
  ByteBuffer decoded=IndexableBinaryStringTools.decode(encoded);
  assertEquals("Round trip decode/decode returned different results:" + System.getProperty("line.separator") + "original: "+ binaryDump(binaryBuf)+ System.getProperty("line.separator")+ " encoded: "+ charArrayDump(encoded)+ System.getProperty("line.separator")+ " decoded: "+ binaryDump(decoded),binaryBuf,decoded);
}
