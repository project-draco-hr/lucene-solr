{
  if (aName == null)   throw new GDataParseException("QName must not be null");
  if (this.contentType == ContentType.XHTML && aName.getLocalPart().equals("div")) {
    if (this.blobParser != null)     throw new GDataParseException(String.format(DUPLICATE_ELEMENT,"div"));
    this.blobParser=new XMLBlobContentParser();
    return this.blobParser.getChildParser(aName);
  }
  return super.getChildParser(aName);
}
