{
synchronized (this) {
    CacheValue cacheValue=byDirectoryCache.get(directory);
    if (cacheValue == null) {
      throw new IllegalArgumentException("Unknown directory: " + directory + " "+ byDirectoryCache);
    }
    cacheValue.doneWithDir=true;
    if (cacheValue.refCnt == 0) {
      cacheValue.refCnt++;
      close(directory);
    }
  }
}
