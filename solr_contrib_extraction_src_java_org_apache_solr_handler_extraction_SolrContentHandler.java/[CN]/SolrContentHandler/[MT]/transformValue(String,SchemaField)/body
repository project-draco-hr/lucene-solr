{
  String result=val;
  if (schFld != null && schFld.getType() instanceof DateField) {
    try {
      Date date=DateUtil.parseDate(val,dateFormats);
      DateFormat df=DateUtil.getThreadLocalDateFormat();
      result=df.format(date);
    }
 catch (    Exception e) {
    }
  }
  return result;
}
