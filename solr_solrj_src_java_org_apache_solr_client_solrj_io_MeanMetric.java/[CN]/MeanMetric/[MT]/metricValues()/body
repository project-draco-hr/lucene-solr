{
  Map m=new HashMap();
  double dcount=(double)count;
  m.put(COUNT,dcount);
  if (isDouble) {
    double ave=doubleSum / dcount;
    m.put(MEAN,ave);
    m.put(SUM,doubleSum);
  }
 else {
    double ave=longSum / dcount;
    doubleSum=(double)longSum;
    m.put(MEAN,ave);
    m.put(SUM,doubleSum);
  }
  return m;
}
