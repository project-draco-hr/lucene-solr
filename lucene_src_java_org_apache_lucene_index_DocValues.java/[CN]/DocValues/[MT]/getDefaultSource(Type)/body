{
  return new Source(type){
    @Override public long getInt(    int docID){
      return 0;
    }
    @Override public double getFloat(    int docID){
      return 0.0;
    }
    @Override public BytesRef getBytes(    int docID,    BytesRef ref){
      ref.length=0;
      return ref;
    }
  }
;
}
