{
  long xRange=(long)maxX - (long)minX;
  long yRange=(long)maxY - (long)minY;
  long zRange=(long)maxZ - (long)minZ;
  if (xRange > yRange) {
    if (xRange > zRange) {
      return 0;
    }
 else {
      return 2;
    }
  }
 else   if (yRange > zRange) {
    return 1;
  }
 else {
    return 2;
  }
}
