{
  if (!hasNext()) {
    throw new NoSuchElementException();
  }
  if (upto < size) {
    int length=(int)lengthsIterator.next();
    value.grow(length);
    value.length=length;
    try {
      bytesIterator.readBytes(value.bytes,value.offset,value.length);
    }
 catch (    IOException ioe) {
      throw new RuntimeException(ioe);
    }
  }
 else {
    value.length=0;
  }
  upto++;
  return value;
}
