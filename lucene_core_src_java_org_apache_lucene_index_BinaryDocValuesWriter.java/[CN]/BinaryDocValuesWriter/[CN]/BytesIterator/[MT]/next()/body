{
  if (!hasNext()) {
    throw new NoSuchElementException();
  }
  final BytesRef v;
  if (upto < size) {
    int length=(int)lengthsIterator.next();
    value.grow(length);
    value.length=length;
    pool.readBytes(byteOffset,value.bytes,value.offset,value.length);
    byteOffset+=length;
    if (docsWithField.get(upto)) {
      v=value;
    }
 else {
      v=null;
    }
  }
 else {
    v=null;
  }
  upto++;
  return v;
}
