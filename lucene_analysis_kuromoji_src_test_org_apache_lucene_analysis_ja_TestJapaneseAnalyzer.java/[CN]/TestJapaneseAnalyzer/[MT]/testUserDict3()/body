{
  final Analyzer a=new JapaneseAnalyzer(TEST_VERSION_CURRENT,TestJapaneseTokenizer.readDict(),Mode.SEARCH,JapaneseAnalyzer.getDefaultStopSet(),JapaneseAnalyzer.getDefaultStopTags());
  assertTokenStreamContents(a.tokenStream("foo","abcd"),new String[]{"a","b","cd"},new int[]{0,1,2},new int[]{1,2,4},new Integer(4));
}
