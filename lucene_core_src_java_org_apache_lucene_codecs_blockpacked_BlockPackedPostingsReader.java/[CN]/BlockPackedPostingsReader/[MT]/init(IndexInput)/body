{
  CodecUtil.checkHeader(termsIn,BlockPackedPostingsWriter.TERMS_CODEC,BlockPackedPostingsWriter.VERSION_START,BlockPackedPostingsWriter.VERSION_START);
  final int indexBlockSize=termsIn.readVInt();
  if (indexBlockSize != BLOCK_SIZE) {
    throw new IllegalStateException("index-time blockSize (" + indexBlockSize + ") != read-time blockSize ("+ BLOCK_SIZE+ ")");
  }
}
