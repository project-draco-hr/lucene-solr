{
  int count=0;
  for (  final GeoPoint[] pointArray : pointArrays) {
    count+=pointArray.length;
  }
  final GeoPoint[] rval=new GeoPoint[count];
  count=0;
  for (  final GeoPoint[] pointArray : pointArrays) {
    for (    final GeoPoint point : pointArray) {
      rval[count++]=point;
    }
  }
  return rval;
}
