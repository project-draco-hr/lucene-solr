{
  final GeoPoint[] edgePoints=getEdgePoints();
  if (edgePoints.length == 0) {
    return NO_EDGEPOINTS;
  }
  boolean foundOutside=false;
  boolean foundInside=false;
  for (  final GeoPoint p : edgePoints) {
    if (path.isWithin(p)) {
      foundInside=true;
    }
 else {
      foundOutside=true;
    }
    if (foundInside && foundOutside) {
      return SOME_INSIDE;
    }
  }
  if (!foundInside && !foundOutside)   return NONE_INSIDE;
  if (foundInside && !foundOutside)   return ALL_INSIDE;
  if (foundOutside && !foundInside)   return NONE_INSIDE;
  return SOME_INSIDE;
}
