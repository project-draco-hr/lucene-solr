{
  if (numberType == null) {
    return null;
  }
switch (numberType) {
case POSITIVE:
    return RANDOM_NUMBER_MAP.get(fieldName);
case NEGATIVE:
  Number number=RANDOM_NUMBER_MAP.get(fieldName);
if (NumericType.LONG.name().equals(fieldName) || DATE_FIELD_NAME.equals(fieldName)) {
  number=-number.longValue();
}
 else if (NumericType.DOUBLE.name().equals(fieldName)) {
  number=-number.doubleValue();
}
 else if (NumericType.FLOAT.name().equals(fieldName)) {
  number=-number.floatValue();
}
 else if (NumericType.INT.name().equals(fieldName)) {
  number=-number.intValue();
}
 else {
  throw new IllegalArgumentException("field name not found: " + fieldName);
}
return number;
default :
return 0;
}
}
