{
  q=extractInnerQuery(q);
  final BooleanQuery bq=(BooleanQuery)q;
  Term last=null, act;
  for (  BooleanClause clause : bq.clauses()) {
    act=extractTerm(clause.getQuery());
    if (last != null) {
      assertTrue("sort order of terms in BQ violated",last.compareTo(act) < 0);
    }
    last=act;
  }
}
