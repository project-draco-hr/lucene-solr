{
  BytesRef[] terms=vector.getTerms();
  int freqs[]=vector.getTermFrequencies();
  for (int j=0; j < terms.length; j++) {
    String term=terms[j].utf8ToString();
    if (isNoiseWord(term)) {
      continue;
    }
    Int cnt=termFreqMap.get(term);
    if (cnt == null) {
      cnt=new Int();
      termFreqMap.put(term,cnt);
      cnt.x=freqs[j];
    }
 else {
      cnt.x+=freqs[j];
    }
  }
}
