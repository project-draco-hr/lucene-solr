{
  if (results == null) {
    return EMPTY_TOPDOCS;
  }
  float maxScore=Float.NaN;
  if (start == 0) {
    maxScore=results[0].score;
  }
 else {
    for (int i=globalQueue.size(); i > 1; i--) {
      globalQueue.pop();
    }
    maxScore=globalQueue.pop().score;
  }
  return new TopDocs(totalHits,results,maxScore);
}
