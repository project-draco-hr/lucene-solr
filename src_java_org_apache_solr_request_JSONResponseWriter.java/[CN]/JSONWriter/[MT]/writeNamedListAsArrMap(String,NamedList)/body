{
  int sz=val.size();
  indent();
  writer.write('[');
  incLevel();
  boolean first=true;
  for (int i=0; i < sz; i++) {
    String key=val.getName(i);
    if (first) {
      first=false;
    }
 else {
      writer.write(',');
    }
    indent();
    if (key == null) {
      writeVal(null,val.getVal(i));
    }
 else {
      writer.write('{');
      writeKey(key,true);
      writeVal(key,val.getVal(i));
      writer.write('}');
    }
  }
  decLevel();
  writer.write(']');
}
