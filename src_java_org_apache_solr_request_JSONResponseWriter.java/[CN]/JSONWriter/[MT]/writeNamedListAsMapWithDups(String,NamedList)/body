{
  int sz=val.size();
  writer.write('{');
  incLevel();
  for (int i=0; i < sz; i++) {
    if (i != 0) {
      writer.write(',');
    }
    String key=val.getName(i);
    if (key == null)     key="";
    indent();
    writeKey(key,true);
    writeVal(key,val.getVal(i));
  }
  decLevel();
  writer.write('}');
}
