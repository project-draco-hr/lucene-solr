{
  int sz=val.size();
  writer.write('[');
  incLevel();
  for (int i=0; i < sz; i++) {
    if (i != 0) {
      writer.write(',');
    }
    String key=val.getName(i);
    indent();
    if (key == null) {
      writeNull(null);
    }
 else {
      writeStr(null,key,true);
    }
    writer.write(',');
    writeVal(key,val.getVal(i));
  }
  decLevel();
  writer.write(']');
}
