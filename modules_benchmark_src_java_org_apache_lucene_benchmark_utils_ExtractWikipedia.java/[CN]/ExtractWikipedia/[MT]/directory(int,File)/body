{
  if (directory == null) {
    directory=outputDir;
  }
  int base=BASE;
  while (base <= count) {
    base*=BASE;
  }
  if (count < BASE) {
    return directory;
  }
  directory=new File(directory,(Integer.toString(base / BASE)));
  directory=new File(directory,(Integer.toString(count / (base / BASE))));
  return directory(count % (base / BASE),directory);
}
