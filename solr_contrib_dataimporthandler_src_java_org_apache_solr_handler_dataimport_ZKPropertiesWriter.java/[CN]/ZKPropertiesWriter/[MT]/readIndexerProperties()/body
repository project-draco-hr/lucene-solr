{
  Properties props=new Properties();
  try {
    byte[] data=zkClient.getData(path,null,null,false);
    if (data != null) {
      ByteArrayInputStream input=new ByteArrayInputStream(data);
      props.load(input);
    }
  }
 catch (  Throwable e) {
    log.warn("Could not read DIH properties from " + path + " :"+ e.getClass(),e);
  }
  return props;
}
