{
  for (; thisTerm != null && curVNode.cell.isWithin(curVNodeTerm,thisTerm); thisTerm=termsEnum.next()) {
    scanCell=grid.getCell(thisTerm.bytes,thisTerm.offset,thisTerm.length,scanCell);
    int termLevel=scanCell.getLevel();
    if (termLevel < scanDetailLevel) {
      if (scanCell.isLeaf())       visitScanned(scanCell);
    }
 else     if (termLevel == scanDetailLevel) {
      if (!scanCell.isLeaf())       visitScanned(scanCell);
    }
  }
}
