{
  for (; thisTerm != null; thisTerm=termsEnum.next()) {
    scanCell.readCell(thisTerm);
    if (!curVNode.cell.isPrefixOf(scanCell))     break;
    int termLevel=scanCell.getLevel();
    if (termLevel < scanDetailLevel) {
      if (scanCell.isLeaf())       visitScanned(scanCell);
    }
 else     if (termLevel == scanDetailLevel) {
      if (!scanCell.isLeaf())       visitScanned(scanCell);
    }
  }
}
