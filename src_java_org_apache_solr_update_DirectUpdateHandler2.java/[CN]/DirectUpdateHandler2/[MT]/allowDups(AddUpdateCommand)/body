{
  if (cmd.indexedId == null) {
    cmd.indexedId=getIndexedIdOptional(cmd.doc);
  }
synchronized (this) {
    doAdd(cmd.doc);
    if (cmd.indexedId != null) {
      Integer saveCount=pset.get(cmd.indexedId);
      if (saveCount == null)       return 1;
      if (saveCount == ZERO)       saveCount=ONE;
 else       saveCount++;
      pset.put(cmd.indexedId,saveCount);
    }
  }
  return 1;
}
