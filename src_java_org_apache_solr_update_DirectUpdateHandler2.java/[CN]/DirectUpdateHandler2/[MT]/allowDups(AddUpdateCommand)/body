{
  if (cmd.id == null) {
    cmd.id=getOptId(cmd.doc);
  }
synchronized (this) {
    doAdd(cmd.doc);
    if (cmd.id != null) {
      Integer saveCount=pset.get(cmd.id);
      if (saveCount == null)       return 1;
      if (saveCount == ZERO)       saveCount=ONE;
 else       saveCount++;
      pset.put(cmd.id,saveCount);
    }
  }
  return 1;
}
