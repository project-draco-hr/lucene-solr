{
  TokenStream ts=createTokenStream(5,"woof woof woof woof woof" + " " + "woof woof woof woof puff",1,100,1,false);
  ArrayList<String> tokens=getTokens(ts);
  ts.close();
  assertEquals(2,tokens.size());
  ts=createTokenStream(5,"woof woof woof woof woof" + " " + "woof woof woof woof puff",1,100,1,true);
  tokens=getTokens(ts);
  ts.close();
  assertEquals(100,tokens.size());
  HashSet<String> set=new HashSet<>(tokens);
  assertEquals(2,set.size());
  boolean rolled=false;
  String first=null;
  String last=null;
  for (  String current : tokens) {
    if (first == null) {
      first=current;
    }
    if (last != null) {
      if (!rolled) {
        if (current.compareTo(last) >= 0) {
        }
 else         if (current.equals(first)) {
          rolled=true;
        }
 else {
          fail("Incorrect hash order");
        }
      }
 else {
        if (!current.equals(first)) {
          fail("Incorrect hash order");
        }
      }
    }
    last=current;
  }
}
