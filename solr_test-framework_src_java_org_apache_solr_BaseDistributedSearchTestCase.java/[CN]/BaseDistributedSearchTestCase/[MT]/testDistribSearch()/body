{
  if (fixShardCount) {
    createServers(shardCount);
    RandVal.uniqueValues=new HashSet();
    doTest();
    destroyServers();
  }
 else {
    for (int nServers=1; nServers < shardCount; nServers++) {
      createServers(nServers);
      RandVal.uniqueValues=new HashSet();
      doTest();
      destroyServers();
    }
  }
}
