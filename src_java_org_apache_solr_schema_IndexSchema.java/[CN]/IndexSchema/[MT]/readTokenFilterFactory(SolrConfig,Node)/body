{
  NamedNodeMap attrs=node.getAttributes();
  String className=DOMUtil.getAttr(attrs,"class","token filter");
  TokenFilterFactory tfac=(TokenFilterFactory)solrConfig.newInstance(className);
  if (tfac instanceof SolrConfig.Initializable)   ((SolrConfig.Initializable)tfac).init(solrConfig,DOMUtil.toMapExcept(attrs,"class"));
 else   tfac.init(DOMUtil.toMapExcept(attrs,"class"));
  return tfac;
}
