{
  StringBuilder builder=new StringBuilder("updated:[");
  if (updateMin != null)   builder.append(Long.toString(DateTime.parseDateTime(updateMin).getValue()));
 else   builder.append(UPDATED_MIN);
  builder.append(" TO ");
  if (updateMax != null)   builder.append(Long.toString(DateTime.parseDateTime(updateMax).getValue() - 1));
 else   builder.append(UPDATED_MAX);
  builder.append("]");
  return builder.toString();
}
