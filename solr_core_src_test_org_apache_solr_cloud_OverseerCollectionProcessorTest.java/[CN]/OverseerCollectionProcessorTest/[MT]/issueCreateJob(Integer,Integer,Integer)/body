{
  ZkNodeProps props=new ZkNodeProps(Overseer.QUEUE_OPERATION,OverseerCollectionProcessor.CREATECOLLECTION,OverseerCollectionProcessor.REPLICATION_FACTOR,replicationFactor.toString(),"name",COLLECTION_NAME,"collection.configName",CONFIG_NAME,OverseerCollectionProcessor.NUM_SLICES,numberOfSlices.toString(),OverseerCollectionProcessor.MAX_SHARDS_PER_NODE,maxShardsPerNode.toString());
  queue.add(ZkStateReader.toJSON(props));
}
