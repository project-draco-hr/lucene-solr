{
  ZkNodeProps props;
  if (sendCreateNodeList) {
    props=new ZkNodeProps(Overseer.QUEUE_OPERATION,CollectionParams.CollectionAction.CREATE.toLower(),ZkStateReader.REPLICATION_FACTOR,replicationFactor.toString(),"name",COLLECTION_NAME,"collection.configName",CONFIG_NAME,OverseerCollectionProcessor.NUM_SLICES,numberOfSlices.toString(),ZkStateReader.MAX_SHARDS_PER_NODE,maxShardsPerNode.toString(),OverseerCollectionProcessor.CREATE_NODE_SET,(createNodeList != null) ? StrUtils.join(createNodeList,',') : null);
  }
 else {
    props=new ZkNodeProps(Overseer.QUEUE_OPERATION,CollectionParams.CollectionAction.CREATE.toLower(),ZkStateReader.REPLICATION_FACTOR,replicationFactor.toString(),"name",COLLECTION_NAME,"collection.configName",CONFIG_NAME,OverseerCollectionProcessor.NUM_SLICES,numberOfSlices.toString(),ZkStateReader.MAX_SHARDS_PER_NODE,maxShardsPerNode.toString());
  }
  QueueEvent qe=new QueueEvent("id",ZkStateReader.toJSON(props),null){
    @Override public void setBytes(    byte[] bytes){
      lastProcessMessageResult=SolrResponse.deserialize(bytes);
    }
  }
;
  queue.add(qe);
}
