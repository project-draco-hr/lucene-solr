{
  if (pos < 0 || sliceToTest.length - pos < other.length) {
    return false;
  }
  int i=sliceToTest.offset + pos;
  int j=other.offset;
  final int k=other.offset + other.length;
  while (j < k) {
    if (sliceToTest.bytes[i++] != other.bytes[j++]) {
      return false;
    }
  }
  return true;
}
