    doc.add(newField(FIELD,"tangfulin",Field.Store.YES,Field.Index.NOT_ANALYZED));
    writer.addDocument(doc);
  }
  reader=writer.getReader();
  searcher=new IndexSearcher(reader);
  writer.close();
}
