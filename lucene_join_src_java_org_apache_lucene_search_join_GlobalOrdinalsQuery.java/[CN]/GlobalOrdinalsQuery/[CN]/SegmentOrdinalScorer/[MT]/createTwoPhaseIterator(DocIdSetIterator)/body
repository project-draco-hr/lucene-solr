{
  return new TwoPhaseIterator(approximation){
    @Override public boolean matches() throws IOException {
      final long segmentOrd=values.getOrd(approximation.docID());
      if (segmentOrd != -1) {
        if (foundOrds.get(segmentOrd)) {
          return true;
        }
      }
      return false;
    }
    @Override public float matchCost(){
      return 100;
    }
  }
;
}
