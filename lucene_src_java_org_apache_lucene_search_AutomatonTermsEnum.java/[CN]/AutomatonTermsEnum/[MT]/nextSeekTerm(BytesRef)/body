{
  if (term == null) {
    seekBytesRef.copy("");
    if (runAutomaton.run(seekBytesRef.bytes,seekBytesRef.offset,seekBytesRef.length)) {
      return seekBytesRef;
    }
  }
 else {
    seekBytesRef.copy(term);
  }
  if (nextString()) {
    linear=false;
    if (linear)     setLinear(infinitePosition);
    return seekBytesRef;
  }
  return null;
}
