{
  while (position > 0) {
    int nextChar=seekBytesRef.bytes[position - 1] & 0xff;
    nextChar=incrementUTF16(nextChar);
    if (nextChar != -1) {
      seekBytesRef.bytes[position - 1]=(byte)nextChar;
      seekBytesRef.length=position;
      return true;
    }
    position--;
  }
  return false;
}
