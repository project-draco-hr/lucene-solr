{
  int state;
  int pos=0;
  while (true) {
    curGen++;
    linear=false;
    state=runAutomaton.getInitialState();
    for (pos=0; pos < seekBytesRef.length; pos++) {
      visited[state]=curGen;
      int nextState=runAutomaton.step(state,seekBytesRef.bytes[pos] & 0xff);
      if (nextState == -1)       break;
      if (!finite && !linear && visited[nextState] == curGen) {
        linear=true;
        infinitePosition=pos;
      }
      state=nextState;
    }
    if (nextString(state,pos)) {
      return true;
    }
 else {
      if (!backtrack(pos))       return false;
 else       if (runAutomaton.run(seekBytesRef.bytes,0,seekBytesRef.length))       return true;
    }
  }
}
