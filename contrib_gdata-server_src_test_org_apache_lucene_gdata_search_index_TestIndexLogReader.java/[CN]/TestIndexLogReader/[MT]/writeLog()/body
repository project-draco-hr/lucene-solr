{
  this.actions=new HashMap<String,IndexAction>();
  IndexLogWriter writer=new IndexLogWriter(this.logFile);
  boolean totwice=true;
  for (int i=0; i < 10; i++) {
    IndexAction a=null;
    if (i % 3 == 0)     a=IndexAction.INSERT;
    if (i % 3 == 1)     a=IndexAction.UPDATE;
    if (i % 3 == 2)     a=IndexAction.DELETE;
    this.actions.put("" + i,a);
    writer.writeAction("" + i,a);
    if (i == 9 && totwice) {
      i=0;
      totwice=false;
    }
  }
  return writer;
}
