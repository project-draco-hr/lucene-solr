{
  if (docIdSet == null) {
    return EMPTY;
  }
 else   if (docIdSet instanceof BitDocIdSet) {
    return docIdSet;
  }
 else {
    final DocIdSetIterator it=docIdSet.iterator();
    if (it == null) {
      return EMPTY;
    }
 else {
      final FixedBitSet copy=new FixedBitSet(reader.maxDoc());
      copy.or(it);
      return new BitDocIdSet(copy);
    }
  }
}
