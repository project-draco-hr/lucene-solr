{
  if (valSize == 0)   throw new IllegalStateException("call set???Value() before usage");
  shift+=precisionStep;
  if (shift >= valSize) {
    shift-=precisionStep;
    return false;
  }
  clearAttributes();
  typeAtt.setType((shift == 0) ? TOKEN_TYPE_FULL_PREC : TOKEN_TYPE_LOWER_PREC);
  posIncrAtt.setPositionIncrement((shift == 0) ? 1 : 0);
  return true;
}
