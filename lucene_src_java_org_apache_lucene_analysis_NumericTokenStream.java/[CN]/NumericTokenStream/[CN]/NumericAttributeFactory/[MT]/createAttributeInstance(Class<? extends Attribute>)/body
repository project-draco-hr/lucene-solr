{
  if (attClass == NumericTermAttribute.class)   return new NumericTermAttributeImpl(ts);
  if (attClass.isAssignableFrom(CharTermAttribute.class) || attClass.isAssignableFrom(TermAttribute.class))   throw new IllegalArgumentException("NumericTokenStream does not support CharTermAttribute/TermAttribute.");
  return delegate.createAttributeInstance(attClass);
}
