{
  Arrays.sort(actual);
  if (expected.length != actual.length) {
    return false;
  }
  for (  int e : expected) {
    boolean found=false;
    for (    int a : actual) {
      if (e == a) {
        found=true;
        break;
      }
    }
    if (!found) {
      return false;
    }
  }
  return true;
}
