{
  Object prevVal=row.get(key);
  if (prevVal != null) {
    if (prevVal instanceof List) {
      ((List)prevVal).add(value);
    }
 else {
      ArrayList<Object> valList=new ArrayList<Object>();
      valList.add(prevVal);
      valList.add(value);
      row.put(key,valList);
    }
  }
 else {
    row.put(key,value);
  }
}
