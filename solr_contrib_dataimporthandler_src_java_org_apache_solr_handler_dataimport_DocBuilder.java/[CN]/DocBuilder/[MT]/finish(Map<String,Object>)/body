{
  LOG.info("Import completed successfully");
  statusMessages.put("","Indexing completed. Added/Updated: " + importStatistics.docCount + " documents. Deleted "+ importStatistics.deletedDocCount+ " documents.");
  if (reqParams.isCommit()) {
    writer.commit(reqParams.isOptimize());
    addStatusMessage("Committed");
    if (reqParams.isOptimize())     addStatusMessage("Optimized");
  }
  try {
    propWriter.persist(lastIndexTimeProps);
  }
 catch (  Exception e) {
    LOG.error("Could not write property file",e);
    statusMessages.put("error","Could not write property file. Delta imports will not work. " + "Make sure your conf directory is writable");
  }
}
