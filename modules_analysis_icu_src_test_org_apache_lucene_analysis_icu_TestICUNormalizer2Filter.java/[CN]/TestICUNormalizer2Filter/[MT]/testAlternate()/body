{
  Analyzer a=new Analyzer(){
    @Override public TokenStream tokenStream(    String fieldName,    Reader reader){
      return new ICUNormalizer2Filter(new WhitespaceTokenizer(TEST_VERSION_CURRENT,reader),Normalizer2.getInstance(null,"nfc",Normalizer2.Mode.DECOMPOSE));
    }
  }
;
  assertAnalyzesTo(a,"\u00E9",new String[]{"\u0065\u0301"});
}
