{
  Analyzer a=new ReusableAnalyzerBase(){
    @Override public TokenStreamComponents createComponents(    String fieldName,    Reader reader){
      Tokenizer tokenizer=new WhitespaceTokenizer(TEST_VERSION_CURRENT,reader);
      return new TokenStreamComponents(tokenizer,new ICUNormalizer2Filter(tokenizer,Normalizer2.getInstance(null,"nfc",Normalizer2.Mode.DECOMPOSE)));
    }
  }
;
  assertAnalyzesTo(a,"\u00E9",new String[]{"\u0065\u0301"});
}
