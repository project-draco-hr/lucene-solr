{
  if (exhausted) {
    return false;
  }
 else   if (tokenCount < maxTokenCount) {
    if (input.incrementToken()) {
      tokenCount++;
      return true;
    }
 else {
      exhausted=true;
      return false;
    }
  }
 else {
    while (consumeAllTokens && input.incrementToken()) {
    }
    return false;
  }
}
