{
  if (t == null) {
    threadDone=false;
    t=new Thread(this);
    t.setDaemon(true);
    t.start();
  }
  String[] result;
synchronized (this) {
    while (tuple == null && nmde == null && !threadDone) {
      try {
        wait();
      }
 catch (      InterruptedException ie) {
      }
    }
    if (nmde != null) {
      t=null;
      throw nmde;
    }
    if (t != null && threadDone)     throw new NoMoreDataException();
    result=tuple;
    tuple=null;
    notify();
  }
  return result;
}
