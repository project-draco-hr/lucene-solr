{
  DocsEnum tdocs=_TestUtil.docs(random,reader,term.field(),new BytesRef(term.text()),MultiFields.getLiveDocs(reader),null,false);
  int count=0;
  if (tdocs != null) {
    while (tdocs.nextDoc() != DocIdSetIterator.NO_MORE_DOCS) {
      count++;
    }
  }
  assertEquals(msg + ", count mismatch",expected,count);
}
