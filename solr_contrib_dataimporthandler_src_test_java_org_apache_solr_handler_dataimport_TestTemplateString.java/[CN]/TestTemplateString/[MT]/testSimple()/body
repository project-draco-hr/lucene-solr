{
  VariableResolverImpl vri=new VariableResolverImpl();
  Map<String,Object> ns=new HashMap<String,Object>();
  ns.put("last_index_time",Long.valueOf(1199429363730l));
  vri.addNamespace("indexer",ns);
  Assert.assertEquals("select id from subject where last_modified > 1199429363730",new TemplateString().replaceTokens("select id from subject where last_modified > ${indexer.last_index_time}",vri));
}
