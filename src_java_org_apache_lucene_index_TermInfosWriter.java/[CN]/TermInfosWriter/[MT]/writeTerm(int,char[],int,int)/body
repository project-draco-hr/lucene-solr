{
  int start=0;
  final int limit=termTextLength < lastTermTextLength ? termTextLength : lastTermTextLength;
  while (start < limit) {
    if (termText[termTextStart + start] != lastTermText[start])     break;
    start++;
  }
  int length=termTextLength - start;
  output.writeVInt(start);
  output.writeVInt(length);
  output.writeChars(termText,start + termTextStart,length);
  output.writeVInt(fieldNumber);
}
