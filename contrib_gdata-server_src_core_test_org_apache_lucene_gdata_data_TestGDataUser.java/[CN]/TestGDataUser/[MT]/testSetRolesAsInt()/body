{
  this.user.setRolesAsInt(2);
  this.user.setRolesAsInt(4);
  this.user.setRolesAsInt(8);
  assertEquals(4,this.user.getRoles().size());
  this.user.setRolesAsInt(15);
  assertEquals(4,this.user.getRoles().size());
  this.user=new GDataAccount();
  this.user.setName("simon");
  this.user.setRolesAsInt(15);
  assertEquals(4,this.user.getRoles().size());
}
