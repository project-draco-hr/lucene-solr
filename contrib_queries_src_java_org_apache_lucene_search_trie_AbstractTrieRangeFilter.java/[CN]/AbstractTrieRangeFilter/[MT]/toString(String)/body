{
  final StringBuffer sb=new StringBuffer();
  if (!this.fields[0].equals(field))   sb.append(this.fields[0]).append(':');
  return sb.append(minInclusive ? '[' : '{').append((min == null) ? "*" : min.toString()).append(" TO ").append((max == null) ? "*" : max.toString()).append(maxInclusive ? ']' : '}').toString();
}
