{
  if (array.length < minSize) {
    @SuppressWarnings("unchecked") final T[] newArray=(T[])Array.newInstance(array.getClass().getComponentType(),oversize(minSize,RamUsageEstimator.NUM_BYTES_OBJECT_REF));
    System.arraycopy(array,0,newArray,0,array.length);
    return newArray;
  }
 else   return array;
}
