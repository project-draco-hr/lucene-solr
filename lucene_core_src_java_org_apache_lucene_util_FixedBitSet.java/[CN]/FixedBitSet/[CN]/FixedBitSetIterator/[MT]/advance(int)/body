{
  if (doc == NO_MORE_DOCS || target >= numBits) {
    return doc=NO_MORE_DOCS;
  }
  int i=target >> 6;
  long word=bits[i] >> target;
  if (word != 0) {
    return doc=target + Long.numberOfTrailingZeros(word);
  }
  while (++i < numWords) {
    word=bits[i];
    if (word != 0) {
      return doc=(i << 6) + Long.numberOfTrailingZeros(word);
    }
  }
  return doc=NO_MORE_DOCS;
}
