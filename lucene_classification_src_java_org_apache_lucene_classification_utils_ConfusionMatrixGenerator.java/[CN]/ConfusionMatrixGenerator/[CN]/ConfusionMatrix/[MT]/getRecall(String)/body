{
  Map<String,Long> classifications=linearizedMatrix.get(klass);
  double tp=0;
  double fn=0;
  if (classifications != null) {
    for (    Map.Entry<String,Long> entry : classifications.entrySet()) {
      if (klass.equals(entry.getKey())) {
        tp+=entry.getValue();
      }
 else {
        fn+=entry.getValue();
      }
    }
  }
  return tp + fn > 0 ? tp / (tp + fn) : 0;
}
