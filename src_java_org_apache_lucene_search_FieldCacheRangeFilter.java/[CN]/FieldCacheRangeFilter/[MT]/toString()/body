{
  StringBuffer buffer=new StringBuffer();
  buffer.append(field);
  buffer.append(":");
  buffer.append(includeLower ? "[" : "{");
  if (null != lowerVal) {
    buffer.append(lowerVal);
  }
  buffer.append("-");
  if (null != upperVal) {
    buffer.append(upperVal);
  }
  buffer.append(includeUpper ? "]" : "}");
  return buffer.toString();
}
