{
{
    final HLL hll=newHLL(128);
    hll.addRaw(1L);
    assertEquals(hll.cardinality(),1L);
  }
{
    final HLL hll=newHLL(128);
    hll.addRaw(-1L);
    assertEquals(hll.cardinality(),1L);
  }
{
    final HLL hll=newHLL(128);
    hll.addRaw(1L);
    assertEquals(hll.cardinality(),1L);
    assertEquals(hll.cardinality(),1L);
  }
}
