{
{
    final int explicitThreshold=128;
    final HLL hll=new HLL(11,5,explicitThreshold,256,HLLType.EXPLICIT);
    for (int i=0; i < explicitThreshold + 1; i++) {
      hll.addRaw(i);
    }
    assertEquals(hll.getType(),HLLType.SPARSE);
  }
{
    final HLL hll=new HLL(11,5,4,false,HLLType.EXPLICIT);
    for (int i=0; i < 9; i++) {
      hll.addRaw(i);
    }
    assertEquals(hll.getType(),HLLType.FULL);
  }
}
