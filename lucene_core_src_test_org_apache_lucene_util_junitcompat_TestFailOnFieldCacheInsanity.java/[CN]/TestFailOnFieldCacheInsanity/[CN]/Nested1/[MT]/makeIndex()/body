{
  d=new MockDirectoryWrapper(random(),new RAMDirectory());
  @SuppressWarnings("resource") RandomIndexWriter w=new RandomIndexWriter(random(),d);
  Document doc=new Document();
  doc.add(newField("ints","1",StringField.TYPE_NOT_STORED));
  w.addDocument(doc);
  w.forceMerge(1);
  r=w.getReader();
  w.shutdown();
  subR=r.leaves().get(0).reader();
}
