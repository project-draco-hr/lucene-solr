{
  super(in);
  this.insaneField=insaneField;
  ArrayList<FieldInfo> filteredInfos=new ArrayList<>();
  for (  FieldInfo fi : in.getFieldInfos()) {
    if (fi.name.equals(insaneField)) {
      filteredInfos.add(new FieldInfo(fi.name,fi.number,fi.hasVectors(),fi.omitsNorms(),fi.hasPayloads(),fi.getIndexOptions(),DocValuesType.NONE,-1,Collections.emptyMap(),fi.getDimensionCount(),fi.getDimensionNumBytes()));
    }
 else {
      filteredInfos.add(fi);
    }
  }
  fieldInfos=new FieldInfos(filteredInfos.toArray(new FieldInfo[filteredInfos.size()]));
}
