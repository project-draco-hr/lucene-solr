{
  if (save != null) {
    Token temp=save;
    save=null;
    return temp;
  }
  Token t=input.next();
  if (t != null) {
    String value=t.termText();
    try {
      value=encoder.encode(t.termText()).toString();
    }
 catch (    Exception ignored) {
    }
    Token m=new Token(value,t.startOffset(),t.endOffset(),name);
    if (inject) {
      m.setPositionIncrement(0);
      save=m;
    }
 else {
      return m;
    }
  }
  return t;
}
