{
  super.setUp();
  dir=newDirectory();
  RandomIndexWriter iw=new RandomIndexWriter(random(),dir);
  for (int i=0; i < 5; i++) {
    Document doc=new Document();
    doc.add(new TextField("field","this is document " + i,Field.Store.NO));
    iw.addDocument(doc);
  }
  reader=iw.getReader();
  searcher=newSearcher(reader);
  iw.close();
}
