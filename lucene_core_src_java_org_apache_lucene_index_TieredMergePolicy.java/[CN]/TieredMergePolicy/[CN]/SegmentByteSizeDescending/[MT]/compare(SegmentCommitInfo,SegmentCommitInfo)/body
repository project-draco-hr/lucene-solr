{
  try {
    final long sz1=size(o1,writer);
    final long sz2=size(o2,writer);
    if (sz1 > sz2) {
      return -1;
    }
 else     if (sz2 > sz1) {
      return 1;
    }
 else {
      return o1.info.name.compareTo(o2.info.name);
    }
  }
 catch (  IOException ioe) {
    throw new RuntimeException(ioe);
  }
}
