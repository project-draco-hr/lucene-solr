{
  int[] strengths=new int[]{256,192,128};
  Exception e=null;
  for (  int strength : strengths) {
    try {
      return decodeAES(base64CipherTxt,pwd,strength);
    }
 catch (    Exception exp) {
      e=exp;
    }
  }
  throw new SolrException(SolrException.ErrorCode.BAD_REQUEST,"Error decoding ",e);
}
