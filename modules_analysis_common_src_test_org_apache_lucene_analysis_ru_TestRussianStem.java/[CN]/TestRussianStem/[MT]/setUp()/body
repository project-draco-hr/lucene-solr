{
  super.setUp();
  String str;
  BufferedReader inWords=new BufferedReader(new InputStreamReader(getClass().getResourceAsStream("wordsUTF8.txt"),"UTF-8"));
  while ((str=inWords.readLine()) != null) {
    words.add(str);
  }
  inWords.close();
  BufferedReader inStems=new BufferedReader(new InputStreamReader(getClass().getResourceAsStream("stemsUTF8.txt"),"UTF-8"));
  while ((str=inStems.readLine()) != null) {
    stems.add(str);
  }
  inStems.close();
}
