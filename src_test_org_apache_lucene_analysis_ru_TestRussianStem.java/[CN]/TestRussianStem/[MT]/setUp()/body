{
  super.setUp();
  String str;
  BufferedReader inWords=new BufferedReader(new InputStreamReader(new FileInputStream("src/test/org/apache/lucene/analysis/ru/wordsUnicode.txt"),"Unicode"));
  while ((str=inWords.readLine()) != null) {
    words.add(str);
  }
  inWords.close();
  BufferedReader inStems=new BufferedReader(new InputStreamReader(new FileInputStream("src/test/org/apache/lucene/analysis/ru/stemsUnicode.txt"),"Unicode"));
  while ((str=inStems.readLine()) != null) {
    stems.add(str);
  }
  inStems.close();
}
