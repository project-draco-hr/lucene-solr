{
  assertEquals(InetAddressPoint.newRangeQuery("a",InetAddress.getByName("1.2.3.0"),InetAddress.getByName("1.2.3.255")),InetAddressPoint.newPrefixQuery("a",InetAddress.getByName("1.2.3.127"),24));
  assertEquals(InetAddressPoint.newRangeQuery("a",InetAddress.getByName("1.2.3.128"),InetAddress.getByName("1.2.3.255")),InetAddressPoint.newPrefixQuery("a",InetAddress.getByName("1.2.3.213"),25));
  assertEquals(InetAddressPoint.newRangeQuery("a",InetAddress.getByName("2001::a000:0"),InetAddress.getByName("2001::afff:ffff")),InetAddressPoint.newPrefixQuery("a",InetAddress.getByName("2001::a6bd:fc80"),100));
}
