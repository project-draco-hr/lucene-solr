{
  setuptest("solrconfig-multiseg.xml","schema.xml");
  for (int i=0; i < 400; i=i + 20) {
    assertU(adoc("id",new Integer(i).toString(),"popularity","201","description","apple is a company " + randomString(i % 6 + 3),"normHits","0.1"));
