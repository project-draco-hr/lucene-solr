{
  if (seq == null)   return;
  extrct.add(seq);
  ArrayList t=sequence.getTasks();
  if (t == null)   return;
  for (int i=0; i < t.size(); i++) {
    PerfTask p=(PerfTask)t.get(0);
    if (p instanceof TaskSequence) {
      extractTasks(extrct,(TaskSequence)p);
    }
 else {
      extrct.add(p);
    }
  }
}
