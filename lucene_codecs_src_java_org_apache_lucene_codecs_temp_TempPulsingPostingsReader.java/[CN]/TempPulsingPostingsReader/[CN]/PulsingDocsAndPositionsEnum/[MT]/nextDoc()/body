{
  while (true) {
    skipPositions();
    if (postings.eof()) {
      return docID=NO_MORE_DOCS;
    }
    final int code=postings.readVInt();
    accum+=code >>> 1;
    if ((code & 1) != 0) {
      freq=1;
    }
 else {
      freq=postings.readVInt();
    }
    posPending=freq;
    startOffset=storeOffsets ? 0 : -1;
    if (liveDocs == null || liveDocs.get(accum)) {
      position=0;
      return (docID=accum);
    }
  }
}
