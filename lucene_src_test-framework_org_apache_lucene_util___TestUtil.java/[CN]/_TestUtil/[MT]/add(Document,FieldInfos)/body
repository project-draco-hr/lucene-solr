{
  List<Fieldable> fields=doc.getFields();
  for (  Fieldable field : fields) {
    fieldInfos.addOrUpdate(field.name(),field.isIndexed(),field.isTermVectorStored(),field.isStorePositionWithTermVector(),field.isStoreOffsetWithTermVector(),field.getOmitNorms(),false,field.getOmitTermFreqAndPositions(),field.docValuesType());
  }
}
