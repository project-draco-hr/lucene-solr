{
  String s=args.remove(name);
  if (s == null) {
    return null;
  }
 else {
    Set<String> set=null;
    Matcher matcher=ITEM_PATTERN.matcher(s);
    if (matcher.find()) {
      set=new HashSet<>();
      set.add(matcher.group(0));
      while (matcher.find()) {
        set.add(matcher.group(0));
      }
    }
    return set;
  }
}
