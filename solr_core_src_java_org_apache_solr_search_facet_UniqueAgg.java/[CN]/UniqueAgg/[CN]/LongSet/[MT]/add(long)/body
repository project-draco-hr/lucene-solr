{
  if (val == 0) {
    zeroCount=1;
    return;
  }
  if (cardinality >= threshold) {
    rehash();
  }
  int h=(int)(val + (val >>> 44) + (val >>> 15));
  for (int slot=h & mask; ; slot=(slot + 1) & mask) {
    long v=vals[slot];
    if (v == 0) {
      vals[slot]=val;
      cardinality++;
      break;
    }
 else     if (v == val) {
      break;
    }
  }
}
