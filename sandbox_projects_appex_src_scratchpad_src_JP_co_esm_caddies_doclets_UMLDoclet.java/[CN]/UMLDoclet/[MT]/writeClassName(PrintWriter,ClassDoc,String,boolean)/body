{
  if (cls.isIncluded()) {
    String prefix=PREFIX;
    if (thiscls) {
      if (!configuration().withstandard) {
        out.println(cls.name());
        return;
      }
      prefix="";
    }
    out.print("<a href=" + dstpath + getPath(cls.containingPackage())+ prefix+ cls.name()+ ".html>");
    out.print(cls.name());
    out.println("</a>");
  }
 else   out.println(cls.name());
}
