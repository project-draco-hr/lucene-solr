{
switch (fi.getNormType()) {
case FLOAT_32:
case FLOAT_64:
case VAR_INTS:
case FIXED_INTS_16:
case FIXED_INTS_32:
case FIXED_INTS_64:
case FIXED_INTS_8:
    checkNumericDocValues(fi.name,reader,reader.simpleNormValues(fi.name));
  break;
default :
throw new AssertionError("wtf: " + fi.getNormType());
}
}
