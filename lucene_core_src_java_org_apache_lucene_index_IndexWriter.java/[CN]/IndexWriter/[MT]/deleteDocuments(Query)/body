{
  ensureOpen();
  for (  Query query : queries) {
    if (query.getClass() == MatchAllDocsQuery.class) {
      deleteAll();
      return;
    }
  }
  try {
    if (docWriter.deleteQueries(queries)) {
      processEvents(true,false);
    }
  }
 catch (  VirtualMachineError tragedy) {
    tragicEvent(tragedy,"deleteDocuments(Query..)");
  }
}
