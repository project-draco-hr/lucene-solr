{
  Iterable<TermFreqPayload> keys=shuffle(new TermFreqPayload("foo bar bar bar bar",50));
  Analyzer a=new MockAnalyzer(random());
  FreeTextSuggester sug=new FreeTextSuggester(a,a,2,(byte)0x20);
  sug.build(new TermFreqPayloadArrayIterator(keys));
  assertEquals("foo bar/1.00",toString(sug.lookup("foo b",10)));
}
