{
  super.setUp();
  directory=new RAMDirectory();
  IndexWriter writer=new IndexWriter(directory,new IndexWriterConfig(TEST_VERSION_CURRENT,new SimpleAnalyzer(TEST_VERSION_CURRENT)));
  Document doc=new Document();
  doc.add(new Field("partnum",StringField.TYPE_STORED,"Q36"));
  doc.add(new Field("description",TextField.TYPE_STORED,"Illidium Space Modulator"));
  writer.addDocument(doc);
  writer.close();
  searcher=new IndexSearcher(directory,true);
}
