{
  assertQ(req("*:*"),"//result[@numFound='0']");
  try {
    runFullImport(generateDIHConfig("query='bogus:3' rows='2' fields='id,desc' onError='abort'"));
  }
 catch (  Exception e) {
    LOG.error(e.getMessage(),e);
    fail(e.getMessage());
  }
  assertQ(req("*:*"),"//result[@numFound='0']");
}
