{
  assertQ(req("*:*"),"//result[@numFound='0']");
  try {
    MockDataSource.setIterator("select * from x",DB_DOCS.iterator());
    addDocumentsToSolr(SOLR_DOCS);
    runFullImport(getDihConfigTagsInnerEntity(jetty.getLocalPort()));
  }
 catch (  Exception e) {
    LOG.error(e.getMessage(),e);
    fail(e.getMessage());
  }
 finally {
    MockDataSource.clearCache();
  }
  assertQ(req("*:*"),"//result[@numFound='1']");
  assertQ(req("id:1"),"//result/doc/str[@name='id'][.='1']","//result/doc/str[@name='dbdesc_s'][.='DbDescription']","//result/doc/str[@name='dbid_s'][.='1']","//result/doc/arr[@name='desc'][.='SolrDescription']");
}
