{
  Map map=new HashMap();
  List<Map> list=new ArrayList();
  LinkedList ll=new LinkedList();
  while (priorityQueue.size() > 0) {
    ll.addFirst(priorityQueue.poll().getMap());
  }
  list.addAll(ll);
  Map groupHead=list.get(0);
  map.putAll(groupHead);
  map.put("group",list);
  return new Tuple(map);
}
