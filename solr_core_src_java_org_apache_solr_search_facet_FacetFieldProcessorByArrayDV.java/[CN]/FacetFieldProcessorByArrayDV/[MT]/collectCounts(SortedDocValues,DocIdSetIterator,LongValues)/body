{
  int doc;
  while ((doc=disi.nextDoc()) != DocIdSetIterator.NO_MORE_DOCS) {
    if (doc > singleDv.docID()) {
      singleDv.advance(doc);
    }
    if (doc == singleDv.docID()) {
      int segOrd=singleDv.ordValue();
      int ord=(int)toGlobal.get(segOrd);
      countAcc.incrementCount(ord,1);
    }
  }
}
