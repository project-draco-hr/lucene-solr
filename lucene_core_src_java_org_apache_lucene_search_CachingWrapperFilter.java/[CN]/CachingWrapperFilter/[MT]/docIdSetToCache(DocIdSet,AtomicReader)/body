{
  if (docIdSet == null) {
    return EMPTY_DOCIDSET;
  }
 else   if (docIdSet.isCacheable()) {
    return docIdSet;
  }
 else {
    final DocIdSetIterator it=docIdSet.iterator();
    if (it == null) {
      return EMPTY_DOCIDSET;
    }
 else {
      final FixedBitSet bits=new FixedBitSet(reader.maxDoc());
      bits.or(it);
      return bits;
    }
  }
}
