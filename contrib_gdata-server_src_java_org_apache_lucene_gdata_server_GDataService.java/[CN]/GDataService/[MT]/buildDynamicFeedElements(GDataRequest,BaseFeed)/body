{
  feed.setGenerator(generator);
  feed.setItemsPerPage(request.getItemsPerPage());
  feed.setStartIndex(request.getStartIndex());
  feed.setId(request.getContextPath());
  feed.getLinks().add(buildLink(Link.Rel.SELF,Link.Type.ATOM,request.getSelfId()));
  feed.getLinks().add(buildLink(Link.Rel.NEXT,XMLMIME,request.getNextId()));
}
