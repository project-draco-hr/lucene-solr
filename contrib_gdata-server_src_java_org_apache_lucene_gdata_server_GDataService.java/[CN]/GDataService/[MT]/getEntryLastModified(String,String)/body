{
  try {
    return new Date(this.storage.getEntryLastModified(entryId,feedId));
  }
 catch (  StorageException e) {
    ServiceException ex=new ServiceException("Could not get Last update for entry  -- " + entryId,e,GDataResponse.SERVER_ERROR);
    ex.setStackTrace(e.getStackTrace());
    throw ex;
  }
}
