{
  try {
    ServerBaseEntry entry=new ServerBaseEntry(GDataEntityBuilder.buildEntry(request));
    return entry;
  }
 catch (  ParseException e) {
    ServiceException ex=new ServiceException("Could not parse entry from incoming request",e,GDataResponse.BAD_REQUEST);
    ex.setStackTrace(e.getStackTrace());
    throw ex;
  }
catch (  IOException e) {
    ServiceException ex=new ServiceException("Could not read or open input stream",e,GDataResponse.BAD_REQUEST);
    ex.setStackTrace(e.getStackTrace());
    throw ex;
  }
}
