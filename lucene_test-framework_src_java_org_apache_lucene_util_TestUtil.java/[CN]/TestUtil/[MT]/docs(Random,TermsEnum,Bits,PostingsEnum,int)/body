{
  if (random.nextBoolean()) {
    if (random.nextBoolean()) {
      final int posFlags;
switch (random.nextInt(4)) {
case 0:
        posFlags=PostingsEnum.POSITIONS;
      break;
case 1:
    posFlags=PostingsEnum.OFFSETS;
  break;
case 2:
posFlags=PostingsEnum.PAYLOADS;
break;
default :
posFlags=PostingsEnum.ALL;
break;
}
return termsEnum.postings(liveDocs,null,posFlags);
}
flags|=PostingsEnum.FREQS;
}
return termsEnum.postings(liveDocs,reuse,flags);
}
