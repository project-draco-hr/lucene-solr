{
  int num=CodecProvider.getDefault().getFieldCodec("field").equals("PreFlex") ? 100 * RANDOM_MULTIPLIER : atLeast(1000);
  for (int i=0; i < num; i++) {
    String reg=AutomatonTestUtil.randomRegexp(random);
    if (VERBOSE) {
      System.out.println("TEST: regexp=" + reg);
    }
    assertSame(reg);
  }
}
