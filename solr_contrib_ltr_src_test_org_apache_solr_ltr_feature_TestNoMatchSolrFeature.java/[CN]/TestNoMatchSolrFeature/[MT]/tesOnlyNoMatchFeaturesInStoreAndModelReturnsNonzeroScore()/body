{
  loadModel("nomatchmodel4",MultipleAdditiveTreesModel.class.getCanonicalName(),new String[]{"nomatchfeature4"},"noMatchFeaturesStore","{\"trees\":[{\"weight\":\"1f\", \"root\":{\"feature\": \"matchedTitle\",\"threshold\": \"0.5f\",\"left\":{\"value\" : \"-10\"},\"right\":{\"value\" : \"9\"}}}]}");
  final SolrQuery query=new SolrQuery();
  query.setQuery("*:*");
  query.add("fl","*, score,fv:[fv]");
  query.add("rows","4");
  query.add("rq","{!ltr model=nomatchmodel4 reRankDocs=4}");
  assertJQ("/query" + query.toQueryString(),"/response/docs/[0]/score==0.0");
  assertJQ("/query" + query.toQueryString(),"/response/docs/[0]/fv==''");
}
