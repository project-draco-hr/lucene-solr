{
  Set<String> extensions=new HashSet<String>();
  if (random().nextBoolean()) {
    extensions.add("cfs");
  }
  if (random().nextBoolean()) {
    extensions.add("prx");
  }
  if (random().nextBoolean()) {
    extensions.add("frq");
  }
  if (random().nextBoolean()) {
    extensions.add("tip");
  }
  if (random().nextBoolean()) {
    extensions.add("tim");
  }
  if (random().nextBoolean()) {
    extensions.add("del");
  }
  return newFSSwitchDirectory(extensions);
}
