{
  SpatialRelation r=relateApprox(other);
  if (r == CONTAINS)   return r;
  if (r == DISJOINT)   return r;
  if (r == WITHIN && !biasContainsThenWithin)   return r;
  boolean pairTouches=shape1.relate(shape2).intersects();
  if (!pairTouches)   return r;
  Rectangle oRect=(Rectangle)other;
  if (relate(ctx.makePoint(oRect.getMinX(),oRect.getMinY())) == CONTAINS && relate(ctx.makePoint(oRect.getMinX(),oRect.getMaxY())) == CONTAINS && relate(ctx.makePoint(oRect.getMaxX(),oRect.getMinY())) == CONTAINS && relate(ctx.makePoint(oRect.getMaxX(),oRect.getMaxY())) == CONTAINS)   return CONTAINS;
  return r;
}
