{
  Spans bigSpans=bigWeight.getSpans(context,acceptDocs,postings);
  if (bigSpans == null) {
    return null;
  }
  Spans littleSpans=littleWeight.getSpans(context,acceptDocs,postings);
  if (littleSpans == null) {
    return null;
  }
  ArrayList<Spans> bigAndLittle=new ArrayList<>();
  bigAndLittle.add(bigSpans);
  bigAndLittle.add(littleSpans);
  return bigAndLittle;
}
