{
  super(comparatorSlot,parentDoc,parentScore);
  docs=new int[numJoins][];
  for (int joinID=0; joinID < numJoins; joinID++) {
    docs[joinID]=new int[5];
  }
  if (doScores) {
    scores=new float[numJoins][];
    for (int joinID=0; joinID < numJoins; joinID++) {
      scores[joinID]=new float[5];
    }
  }
  counts=new int[numJoins];
}
