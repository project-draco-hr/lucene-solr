{
  int len=in.readVInt();
  BytesRef bytes;
  if (len == 0) {
    bytes=NO_BYTES;
  }
 else {
    bytes=new BytesRef(len);
    in.readBytes(bytes.bytes,0,len);
    bytes.length=len;
  }
  long startOrd=in.readVLong();
  long endOrd=in.readVLong();
  Output result=newOutput(bytes,startOrd,endOrd);
  return result;
}
