{
  Analyzer analyzer=new Analyzer(){
    @Override protected TokenStreamComponents createComponents(    String fieldName,    Reader reader){
      Tokenizer tokenizer=new MockTokenizer(reader,MockTokenizer.WHITESPACE,false);
      TokenFilter filters=new ASCIIFoldingFilter(tokenizer);
      filters=new WordTokenFilter(filters);
      return new TokenStreamComponents(tokenizer,filters);
    }
  }
;
  assertAnalyzesTo(analyzer,"mosfellsb??r",new String[]{"mosfellsbaer"},new int[]{0},new int[]{11});
}
