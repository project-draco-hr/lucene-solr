{
  Analyzer ca=new SmartChineseAnalyzer();
  String sentence="??????????????????????????????";
  String result[]={"???","??????","???","??????","???","??????"};
  assertAnalyzesTo(ca,sentence,result);
  ca.close();
  ca=new SmartChineseAnalyzer(SmartChineseAnalyzer.getDefaultStopSet());
  assertAnalyzesTo(ca,sentence,result);
  ca.close();
}
