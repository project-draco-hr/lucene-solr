{
  Explanation queryExpl=explainQuery(stats);
  Explanation fieldExpl=explainField(doc,freq,stats,norms);
  if (queryExpl.getValue() == 1f) {
    return fieldExpl;
  }
  return Explanation.match(queryExpl.getValue() * fieldExpl.getValue(),"score(doc=" + doc + ",freq="+ freq.getValue()+ "), product of:",queryExpl,fieldExpl);
}
