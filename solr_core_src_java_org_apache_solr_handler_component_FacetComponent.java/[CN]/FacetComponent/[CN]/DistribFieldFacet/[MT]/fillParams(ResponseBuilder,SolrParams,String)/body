{
  super.fillParams(rb,params,field);
  this.overrequestRatio=params.getFieldDouble(field,FacetParams.FACET_OVERREQUEST_RATIO,1.5);
  this.overrequestCount=params.getFieldInt(field,FacetParams.FACET_OVERREQUEST_COUNT,10);
  this.mco=params.getFieldBool(field,FacetParams.FACET_DISTRIB_MCO,false);
}
