{
  while (true) {
    pushCurrentDoc();
    popCurrentDoc();
    if (docID == NO_MORE_DOCS) {
      return docID;
    }
    countMatches();
    if (freq > 0) {
      return docID;
    }
    for (int i=0; i < numSubsOnDoc; i++) {
      EnumAndScorer sub=subsOnDoc[i];
      if (sub.posEnum.nextDoc() != NO_MORE_DOCS) {
        sub.posLeft=sub.posEnum.freq() - 1;
        sub.pos=sub.posEnum.nextPosition();
      }
    }
  }
}
