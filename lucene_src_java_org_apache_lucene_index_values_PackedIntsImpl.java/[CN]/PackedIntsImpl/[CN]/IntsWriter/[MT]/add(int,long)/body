{
  if (!started) {
    minValue=maxValue=v;
    minDocID=maxDocID=docID;
    started=true;
  }
 else {
    if (v < minValue) {
      minValue=v;
    }
 else     if (v > maxValue) {
      maxValue=v;
    }
    if (docID < minDocID) {
      minDocID=docID;
    }
 else     if (docID > maxDocID) {
      maxDocID=docID;
    }
  }
  if (docID >= docToValue.length) {
    docToValue=ArrayUtil.grow(docToValue,1 + docID);
  }
  docToValue[docID]=v;
}
