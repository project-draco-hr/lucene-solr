{
  int code=getHashCode(text);
  int pos=code & (entries.length - 1);
  char[] text2=entries[pos];
  if (text2 != null && !equals(text,text2)) {
    final int inc=((code >> 8) + code) | 1;
    do {
      code+=inc;
      pos=code & (entries.length - 1);
      text2=entries[pos];
    }
 while (text2 != null && !equals(text,text2));
  }
  return pos;
}
