{
  if (ignoreCase)   for (int i=0; i < text.length; i++)   text[i]=Character.toLowerCase(text[i]);
  int slot=getSlot(text,0,text.length);
  if (entries[slot] != null)   return false;
  entries[slot]=text;
  count++;
  if (count + (count >> 2) > entries.length) {
    rehash();
  }
  return true;
}
