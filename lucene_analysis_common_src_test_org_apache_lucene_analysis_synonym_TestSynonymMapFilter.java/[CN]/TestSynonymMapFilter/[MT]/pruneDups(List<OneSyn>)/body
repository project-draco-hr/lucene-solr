{
  Set<String> seen=new HashSet<String>();
  for (  OneSyn syn : syns) {
    int idx=0;
    while (idx < syn.out.size()) {
      String out=syn.out.get(idx);
      if (!seen.contains(out)) {
        seen.add(out);
        idx++;
      }
 else {
        syn.out.remove(idx);
      }
    }
    seen.clear();
  }
}
