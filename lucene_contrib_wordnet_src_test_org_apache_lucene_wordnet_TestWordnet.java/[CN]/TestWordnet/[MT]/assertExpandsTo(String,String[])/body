{
  Query expandedQuery=SynExpand.expand(term,searcher,new MockAnalyzer(random),"field",1F);
  BooleanQuery expectedQuery=new BooleanQuery();
  for (  String t : expected)   expectedQuery.add(new TermQuery(new Term("field",t)),BooleanClause.Occur.SHOULD);
  assertEquals(expectedQuery,expandedQuery);
}
