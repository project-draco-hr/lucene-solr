{
  super.close();
  if (!isClone) {
synchronized (dir) {
      Integer v=(Integer)dir.openFiles.get(name);
      if (v != null) {
        if (v.intValue() == 1) {
          dir.openFiles.remove(name);
        }
 else {
          v=new Integer(v.intValue() - 1);
          dir.openFiles.put(name,v);
        }
      }
    }
  }
}
