{
  final String coordMethod;
  if (coordType == 0) {
    coordMethod="no";
  }
 else   if (coordType == 1) {
    coordMethod="yes";
  }
 else {
    coordMethod="crazy";
  }
  return "RandomSimilarityProvider(queryNorm=" + shouldQueryNorm + ",coord="+ coordMethod+ "): "+ previousMappings.toString();
}
