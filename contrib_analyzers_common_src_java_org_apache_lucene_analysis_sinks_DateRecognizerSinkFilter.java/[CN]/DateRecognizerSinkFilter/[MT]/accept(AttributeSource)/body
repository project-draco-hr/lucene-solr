{
  if (termAtt == null) {
    termAtt=(TermAttribute)source.getAttribute(TermAttribute.class);
  }
  if (termAtt != null) {
    try {
      Date date=dateFormat.parse(termAtt.term());
      if (date != null) {
        return true;
      }
    }
 catch (    ParseException e) {
    }
  }
  return false;
}
