{
  Analyzer a=new LowerCaseWhitespaceAnalyzerBWComp();
  assertAnalyzesTo(a,"AbaCaDabA",new String[]{"abacadaba"});
  assertAnalyzesTo(a,"\ud801\udc16\ud801\udc16\ud801\udc16\ud801\udc16",new String[]{"\ud801\udc16\ud801\udc16\ud801\udc16\ud801\udc16"});
  assertAnalyzesTo(a,"AbaCa\ud801\udc16DabA",new String[]{"abaca\ud801\udc16daba"});
  assertAnalyzesTo(a,"AbaC\uD801AdaBa",new String[]{"abac\uD801adaba"});
  assertAnalyzesTo(a,"AbaC\uDC16AdaBa",new String[]{"abac\uDC16adaba"});
}
