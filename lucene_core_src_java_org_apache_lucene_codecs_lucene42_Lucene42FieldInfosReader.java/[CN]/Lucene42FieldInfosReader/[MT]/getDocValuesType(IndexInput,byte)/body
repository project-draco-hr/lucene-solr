{
  if (b == 0) {
    return null;
  }
 else   if (b == 1) {
    return DocValuesType.NUMERIC;
  }
 else   if (b == 2) {
    return DocValuesType.BINARY;
  }
 else   if (b == 3) {
    return DocValuesType.SORTED;
  }
 else   if (b == 4) {
    return DocValuesType.SORTED_SET;
  }
 else {
    throw new CorruptIndexException("invalid docvalues byte: " + b + " (resource="+ input+ ")");
  }
}
