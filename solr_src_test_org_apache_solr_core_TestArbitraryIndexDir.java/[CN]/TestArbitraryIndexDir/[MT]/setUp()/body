{
  super.setUp();
  dataDir=new File(System.getProperty("java.io.tmpdir") + System.getProperty("file.separator") + getClass().getName()+ "-"+ System.currentTimeMillis()+ System.getProperty("file.separator")+ "solr"+ System.getProperty("file.separator")+ "data");
