{
  for (  final boolean consumeAll : new boolean[]{true,false}) {
    Reader reader=new StringReader("A1 B2 C3 D4 E5 F6");
    MockTokenizer tokenizer=whitespaceMockTokenizer(reader);
    tokenizer.setEnableChecks(consumeAll);
    TokenStream stream=tokenizer;
    stream=tokenFilterFactory("LimitTokenPosition",LimitTokenPositionFilterFactory.MAX_TOKEN_POSITION_KEY,"1",LimitTokenPositionFilterFactory.CONSUME_ALL_TOKENS_KEY,Boolean.toString(consumeAll)).create(stream);
    assertTokenStreamContents(stream,new String[]{"A1"});
  }
}
