{
  if (childId.equals(parent.getFieldValue("id"))) {
    return parent;
  }
  if (!parent.hasChildDocuments()) {
    return null;
  }
  for (  SolrInputDocument kid : parent.getChildDocuments()) {
    SolrInputDocument result=findDecendent(kid,childId);
    if (null != result) {
      return result;
    }
  }
  return null;
}
