{
  mergeLength(value.length);
  int ord=hash.add(value);
  if (ord < 0) {
    ord=-ord - 1;
  }
 else {
    bytesUsed+=value.length;
  }
  pending.add(ord);
  bytesUsed+=(int)(RamUsageEstimator.NUM_BYTES_OBJECT_REF * 1.25) + RamUsageEstimator.NUM_BYTES_OBJECT_HEADER + RamUsageEstimator.NUM_BYTES_INT;
}
