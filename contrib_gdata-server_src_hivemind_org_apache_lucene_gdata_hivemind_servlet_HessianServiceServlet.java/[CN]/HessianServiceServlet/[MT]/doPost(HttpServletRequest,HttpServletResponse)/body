{
  try {
    HessianSkeletonProvider provider=getSkeletonProvider(arg0);
    HessianServiceSkeletonInvoker invoker=provider.getServiceSkeletonInvoker(arg0);
    invoker.invoke(arg0,arg1);
  }
 catch (  Throwable e) {
    throw new ServletException("Nested Exception occured -- Message: " + e.getMessage(),e);
  }
}
