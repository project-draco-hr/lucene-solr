{
  assertU(adoc("id","myid1","test_is_dvo","101","test_is_dvo","102","test_is_dvo","103"));
  assertU(adoc("id","myid2","test_is_dvo","201","test_is_dvo","202"));
  assertU(adoc("id","myid3","test_is_dvo","301","test_is_dvo","302","test_is_dvo","303","test_is_dvo","304"));
  assertU(adoc("id","myid4","test_s_dvo","hello","test_is_dvo","401","test_is_dvo","402"));
  assertU(adoc("id","myid5","nonstored_dv_str","dont see me"));
  assertU(adoc("id","myid6","nonstored_dv_str","dont see me","test_s_dvo","hello"));
  assertU(commit());
  assertJQ(req("q","id:myid*","fl","*"),"/response/docs==[" + "{'id':'myid1','test_is_dvo':[101,102,103]}," + "{'id':'myid2','test_is_dvo':[201,202]},"+ "{'id':'myid3','test_is_dvo':[301,302,303,304]},"+ "{'id':'myid4','test_s_dvo':'hello','test_is_dvo':[401,402]},"+ "{'id':'myid5'},"+ "{'id':'myid6','test_s_dvo':'hello'}"+ "]");
}
