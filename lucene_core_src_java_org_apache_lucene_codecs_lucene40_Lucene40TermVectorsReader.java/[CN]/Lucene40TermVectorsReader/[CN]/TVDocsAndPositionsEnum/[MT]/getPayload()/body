{
  if (payloadOffsets == null) {
    return null;
  }
 else {
    int off=payloadOffsets[nextPos - 1];
    int end=nextPos == payloadOffsets.length ? payloadBytes.length : payloadOffsets[nextPos];
    if (end - off == 0) {
      return null;
    }
    payload.bytes=payloadBytes;
    payload.offset=off;
    payload.length=end - off;
    return payload;
  }
}
