{
  if (lastElement > ord) {
    int offset=offsets[ord];
    int length=ord == lastElement - 1 ? currentOffset - offset : offsets[ord + 1] - offset;
    pool.readBytes(spare,offset,length);
    return spare;
  }
  throw new IndexOutOfBoundsException("index " + ord + " must be less than the size: "+ lastElement);
}
