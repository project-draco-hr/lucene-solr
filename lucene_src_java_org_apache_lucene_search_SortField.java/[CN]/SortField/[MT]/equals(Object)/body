{
  if (this == o)   return true;
  if (!(o instanceof SortField))   return false;
  final SortField other=(SortField)o;
  return (other.field == this.field && other.type == this.type && other.reverse == this.reverse && (other.comparatorSource == null ? this.comparatorSource == null : other.comparatorSource.equals(this.comparatorSource)) && (other.creator == null ? this.creator == null : other.creator.equals(this.creator)));
}
