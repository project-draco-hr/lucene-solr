{
  boolean loadFromObject=false;
  try {
    ObjectInputStream input=new ObjectInputStream(new FileInputStream(serialObj));
    bigramHashTable=(long[])input.readObject();
    frequencyTable=(int[])input.readObject();
    loadFromObject=true;
    input.close();
  }
 catch (  Exception e) {
  }
  return loadFromObject;
}
