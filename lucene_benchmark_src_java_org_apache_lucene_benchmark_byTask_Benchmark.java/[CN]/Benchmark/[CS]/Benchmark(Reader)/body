{
  try {
    runData=new PerfRunData(new Config(algReader));
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new Exception("Error: cannot init PerfRunData!",e);
  }
  try {
    algorithm=new Algorithm(runData);
  }
 catch (  Exception e) {
    throw new Exception("Error: cannot understand algorithm!",e);
  }
}
