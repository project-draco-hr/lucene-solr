{
  String test="one<script attr= bare><!-- action('<!-- comment -->', \"\\\"-->\\\"\"); --></script>two";
  String gold="one\ntwo";
  assertHTMLStripsTo(test,gold,null);
  test="hello<script><!-- f('<!--internal--></script>'); --></script>";
  gold="hello\n";
  assertHTMLStripsTo(test,gold,null);
}
