{
  boolean flag=_innerIter.skipTo(n);
  if (flag) {
    int doc=_innerIter.doc();
    if (match(doc)) {
      _currentDoc=doc;
      return true;
    }
 else {
      while (_innerIter.next()) {
        int docid=_innerIter.doc();
        if (match(docid)) {
          _currentDoc=docid;
          return true;
        }
      }
      return false;
    }
  }
  return flag;
}
