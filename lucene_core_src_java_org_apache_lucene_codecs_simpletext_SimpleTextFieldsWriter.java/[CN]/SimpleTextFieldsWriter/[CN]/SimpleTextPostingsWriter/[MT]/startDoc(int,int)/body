{
  if (!wroteTerm) {
    write(TERM);
    write(term);
    newline();
    wroteTerm=true;
  }
  write(DOC);
  write(Integer.toString(docID));
  newline();
  if (indexOptions != IndexOptions.DOCS_ONLY) {
    write(FREQ);
    write(Integer.toString(termDocFreq));
    newline();
  }
  lastStartOffset=0;
}
