{
  int i=name.lastIndexOf('.');
  if (i != -1) {
    String extension=name.substring(1 + i);
    if (extensions.contains(extension) && !extension.equals("del") && !extension.equals("gen")&& !extension.equals("cfs")) {
      return true;
    }
    if (extension.startsWith("f") && extension.matches("f\\d+")) {
      return true;
    }
  }
  return false;
}
