{
  if (state != SEEN_DATA) {
    int end=start + length;
    while (end-- > start) {
      if (!isXMLWhitespace(ch[end]))       break;
    }
    if (end < start) {
      saveWhitespace(ch,start,length);
    }
 else {
      state=SEEN_DATA;
      emitWhitespace();
    }
  }
  if (state == SEEN_DATA) {
    super.characters(ch,start,length);
  }
}
