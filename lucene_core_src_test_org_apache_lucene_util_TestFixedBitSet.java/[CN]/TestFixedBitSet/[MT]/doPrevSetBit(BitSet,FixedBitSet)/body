{
  int aa=a.size() + random.nextInt(100);
  int bb=aa;
  do {
    aa--;
    while ((aa >= 0) && (!a.get(aa))) {
      aa--;
    }
    if (b.length() == 0) {
      bb=-1;
    }
 else     if (bb > b.length() - 1) {
      bb=b.prevSetBit(b.length() - 1);
    }
 else     if (bb < 1) {
      bb=-1;
    }
 else {
      bb=bb >= 1 ? b.prevSetBit(bb - 1) : -1;
    }
    assertEquals(aa,bb);
  }
 while (aa >= 0);
}
