{
  assertEquals(a.cardinality(),b.cardinality());
  int aa=-1;
  int bb=-1;
  do {
    aa=a.nextSetBit(aa + 1);
    if (aa == -1) {
      aa=DocIdSetIterator.NO_MORE_DOCS;
    }
    bb=bb < b.length() - 1 ? b.nextSetBit(bb + 1) : DocIdSetIterator.NO_MORE_DOCS;
    assertEquals(aa,bb);
  }
 while (aa != DocIdSetIterator.NO_MORE_DOCS);
}
