{
  FixedBitSet bits=new FixedBitSet(72);
  bits.set(1);
  bits.set(4);
  bits.set(69);
  FixedBitSet newBits=new FixedBitSet(bits,66);
  assertTrue(newBits.get(1));
  assertTrue(newBits.get(4));
  boolean hitError=true;
  try {
    newBits.get(69);
    hitError=false;
  }
 catch (  AssertionError e) {
    hitError=true;
  }
  assertTrue(hitError);
  newBits=new FixedBitSet(bits,8);
  assertTrue(newBits.get(1));
  assertTrue(newBits.get(4));
  hitError=true;
  try {
    newBits.get(69);
    hitError=false;
  }
 catch (  AssertionError e) {
    hitError=true;
  }
  assertTrue(hitError);
}
