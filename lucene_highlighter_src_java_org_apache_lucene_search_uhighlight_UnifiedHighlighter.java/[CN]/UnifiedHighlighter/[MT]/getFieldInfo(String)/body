{
  if (searcher == null) {
    return null;
  }
  FieldInfos fieldInfos=this.fieldInfos;
  if (fieldInfos == null) {
synchronized (this) {
      fieldInfos=this.fieldInfos;
      if (fieldInfos == null) {
        fieldInfos=MultiFields.getMergedFieldInfos(searcher.getIndexReader());
        this.fieldInfos=fieldInfos;
      }
    }
  }
  return fieldInfos.fieldInfo(field);
}
