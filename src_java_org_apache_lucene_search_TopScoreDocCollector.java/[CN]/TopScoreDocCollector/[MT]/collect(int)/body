{
  float score=scorer.score();
  totalHits++;
  if (reusableSD == null) {
    reusableSD=new ScoreDoc(doc + docBase,score);
  }
 else   if (score >= reusableSD.score) {
    reusableSD.doc=doc + docBase;
    reusableSD.score=score;
  }
 else {
    return;
  }
  reusableSD=(ScoreDoc)pq.insertWithOverflow(reusableSD);
}
