{
  int ord=sortDoc.ord;
  FixedBitSet set=sets[ord];
  set.clear(sortDoc.docId);
  LeafReaderContext context=leaves.get(ord);
  int fieldIndex=0;
  for (  FieldWriter fieldWriter : fieldWriters) {
    if (fieldWriter.write(sortDoc.docId,context.reader(),out,fieldIndex)) {
      ++fieldIndex;
    }
  }
}
