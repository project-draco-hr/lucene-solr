{
  if (docId > currentVals.docID()) {
    currentVals.advance(docId);
  }
  if (docId == currentVals.docID()) {
    int ord=currentVals.ordValue();
    if (globalOrds != null) {
      currentOrd=(int)globalOrds.get(ord);
    }
 else {
      currentOrd=ord;
    }
  }
 else {
    currentOrd=-1;
  }
}
