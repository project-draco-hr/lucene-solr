{
  NumericDocValues vals=DocValues.getNumeric(reader,this.field);
  int val;
  if (vals.advance(docId) == docId) {
    val=(int)vals.longValue();
  }
 else {
    val=0;
  }
  if (fieldIndex > 0) {
    out.write(',');
  }
  out.write('"');
  out.write(this.field);
  out.write('"');
  out.write(':');
  out.write(Integer.toString(val));
  return true;
}
