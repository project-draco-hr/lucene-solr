{
  if (docId < lastDocID) {
    throw new AssertionError("docs were sent out-of-order: lastDocID=" + lastDocID + " vs doc="+ docId);
  }
  lastDocID=docId;
  int curDocID=vals.docID();
  if (docId > curDocID) {
    curDocID=vals.advance(docId);
  }
  if (docId == curDocID) {
    currentValue=(int)vals.longValue();
  }
 else {
    currentValue=0;
  }
}
