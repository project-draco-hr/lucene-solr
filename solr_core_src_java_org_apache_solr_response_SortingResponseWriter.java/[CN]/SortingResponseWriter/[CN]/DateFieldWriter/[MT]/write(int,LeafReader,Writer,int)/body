{
  NumericDocValues vals=DocValues.getNumeric(reader,this.field);
  long val=vals.get(docId);
  if (fieldIndex > 0) {
    out.write(',');
  }
  out.write('"');
  out.write(this.field);
  out.write('"');
  out.write(':');
  out.write('"');
  writeStr(new Date(val).toInstant().toString(),out);
  out.write('"');
  return true;
}
