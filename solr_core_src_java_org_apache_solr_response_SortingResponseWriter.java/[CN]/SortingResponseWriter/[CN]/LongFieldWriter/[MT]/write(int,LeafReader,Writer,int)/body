{
  NumericDocValues vals=DocValues.getNumeric(reader,this.field);
  long val;
  if (vals.advance(docId) == docId) {
    val=vals.longValue();
  }
 else {
    val=0;
  }
  if (fieldIndex > 0) {
    out.write(',');
  }
  out.write('"');
  out.write(this.field);
  out.write('"');
  out.write(':');
  out.write(Long.toString(val));
  return true;
}
