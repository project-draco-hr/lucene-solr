{
  super("MockRandom");
  if (random == null) {
    this.seedRandom=new Random(0L){
      @Override protected int next(      int arg0){
        throw new IllegalStateException("Please use MockRandomPostingsFormat(Random)");
      }
    }
;
  }
 else {
    this.seedRandom=new Random(random.nextLong());
  }
}
