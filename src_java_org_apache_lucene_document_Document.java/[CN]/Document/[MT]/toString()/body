{
  StringBuffer buffer=new StringBuffer();
  buffer.append("Document<");
  for (int i=0; i < fields.size(); i++) {
    Field field=(Field)fields.get(i);
    buffer.append(field.toString());
    if (i != fields.size() - 1)     buffer.append(" ");
  }
  buffer.append(">");
  return buffer.toString();
}
