{
  this.readLock.lock();
  try {
    if (!this.bufferMap.containsKey(feedId))     return null;
    Map<String,StorageEntryWrapper> tempMap=this.bufferMap.get(feedId);
    if (tempMap == null)     return null;
    Collection<StorageEntryWrapper> col=tempMap.values();
    List<StorageEntryWrapper> returnList=new ArrayList<StorageEntryWrapper>(col);
    Collections.sort(returnList);
    return returnList;
  }
  finally {
    this.readLock.unlock();
  }
}
