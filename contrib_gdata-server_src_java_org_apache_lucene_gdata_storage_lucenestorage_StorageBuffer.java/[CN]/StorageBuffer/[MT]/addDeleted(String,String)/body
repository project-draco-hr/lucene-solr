{
  this.writeLock.lock();
  try {
    Map<String,StorageEntryWrapper> tempMap=this.bufferMap.get(feedId);
    if (tempMap != null) {
      tempMap.remove(entryId);
      this.addLastModified(feedId,new Long(System.currentTimeMillis()));
    }
    if (!this.excludeList.contains(entryId))     this.excludeList.add(entryId);
  }
  finally {
    this.writeLock.unlock();
  }
}
