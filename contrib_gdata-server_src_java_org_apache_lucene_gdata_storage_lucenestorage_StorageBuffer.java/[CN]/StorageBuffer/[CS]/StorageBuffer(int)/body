{
  this.bufferSize=expectedBufferCount < DEFAULT_BUFFER_COUNT ? DEFAULT_BUFFER_COUNT : expectedBufferCount;
  this.bufferMap=new HashMap<String,Map<String,StorageEntryWrapper>>(this.bufferSize);
  this.excludeList=new ArrayList<String>(this.bufferSize);
  this.modifiyMap=new HashMap<String,Long>(this.bufferSize);
}
