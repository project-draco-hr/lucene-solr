{
  try {
    SolrQueryResponse rsp=queryAndResponse(handler,req);
    StringWriter sw=new StringWriter(32000);
    QueryResponseWriter responseWriter=core.getQueryResponseWriter(req);
    responseWriter.write(sw,req,rsp);
    req.close();
    return sw.toString();
  }
  finally {
    req.close();
  }
}
