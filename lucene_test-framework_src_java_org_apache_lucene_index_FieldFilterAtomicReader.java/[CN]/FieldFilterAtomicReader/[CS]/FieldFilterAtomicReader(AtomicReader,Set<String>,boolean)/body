{
  super(in);
  this.fields=fields;
  this.negate=negate;
  ArrayList<FieldInfo> filteredInfos=new ArrayList<FieldInfo>();
  for (  FieldInfo fi : in.getFieldInfos()) {
    if (hasField(fi.name)) {
      filteredInfos.add(fi);
    }
  }
  fieldInfos=new ReadOnlyFieldInfos(filteredInfos.toArray(new FieldInfo[filteredInfos.size()]));
}
