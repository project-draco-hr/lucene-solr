{
  try {
    super.processDelete(cmd);
  }
 catch (  Throwable t) {
    firstErrTracker.caught(t);
    ToleratedUpdateError err=new ToleratedUpdateError(cmd.isDeleteById() ? CmdType.DELID : CmdType.DELQ,cmd.isDeleteById() ? cmd.id : cmd.query,t.getMessage());
    knownErrors.add(err);
    if (CmdType.DELQ.equals(err.getType())) {
      knownDBQErrors.add(err);
    }
    if (knownErrors.size() > maxErrors) {
      firstErrTracker.throwFirst();
    }
  }
}
