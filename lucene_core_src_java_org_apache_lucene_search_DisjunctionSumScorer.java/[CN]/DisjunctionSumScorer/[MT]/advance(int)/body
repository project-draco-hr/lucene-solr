{
  if (numScorers == 0)   return doc=NO_MORE_DOCS;
  while (subScorers[0].docID() < target) {
    if (subScorers[0].advance(target) != NO_MORE_DOCS) {
      heapAdjust(0);
    }
 else {
      heapRemoveRoot();
      if (numScorers == 0) {
        return doc=NO_MORE_DOCS;
      }
    }
  }
  afterNext();
  if (nrMatchers >= minimumNrMatchers) {
    return doc;
  }
 else {
    return nextDoc();
  }
}
