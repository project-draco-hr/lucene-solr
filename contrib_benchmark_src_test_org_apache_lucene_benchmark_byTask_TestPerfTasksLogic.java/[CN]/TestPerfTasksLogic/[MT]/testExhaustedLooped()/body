{
  String algLines[]={"# ----- properties ","doc.maker=" + Reuters20DocMaker.class.getName(),"doc.add.log.step=3","doc.term.vector=false","doc.maker.forever=false","directory=RAMDirectory","doc.stored=false","doc.tokenized=false","debug.level=1","# ----- alg ","{ \"Rounds\"","  ResetSystemErase","  CreateIndex","  { \"AddDocs\"  AddDoc > : * ","  CloseIndex","} : 2"};
  Benchmark benchmark=execBenchmark(algLines);
  IndexReader ir=IndexReader.open(benchmark.getRunData().getDirectory());
  int ndocsExpected=20;
  assertEquals("wrong number of docs in the index!",ndocsExpected,ir.numDocs());
  ir.close();
}
