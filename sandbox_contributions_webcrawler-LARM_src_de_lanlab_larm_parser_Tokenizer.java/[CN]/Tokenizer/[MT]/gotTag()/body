{
  if (linkAttrFound && isStartTag) {
switch (linkTagType) {
case LINKTYPE_AREA:
      linkHandler.handleLink(linkValue,null,false);
    break;
case LINKTYPE_FRAME:
  linkHandler.handleLink(linkValue,null,true);
break;
case LINKTYPE_BASE:
linkHandler.handleBase(linkValue);
break;
case LINKTYPE_A:
state=ST_IN_ANCHOR;
return;
}
}
toStart();
}
