{
  StringBuffer buffer=new StringBuffer();
  buffer.append("<ul>\n");
  buffer.append("<li>");
  buffer.append(getValue());
  buffer.append(" = ");
  buffer.append(getDescription());
  buffer.append("</li>\n");
  Explanation[] details=getDetails();
  if (details != null) {
    for (int i=0; i < details.length; i++) {
      buffer.append(details[i].toHtml());
    }
  }
  buffer.append("</ul>\n");
  return buffer.toString();
}
