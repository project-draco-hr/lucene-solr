{
  if (spansQueue.isEmpty()) {
    return NO_MORE_DOCS;
  }
  while (true) {
    Spans spans=spansQueue.peek();
    if (spans.docID() >= target) {
      return spans.docID();
    }
    spansQueue.remove();
    if (spans.advance(target) != NO_MORE_DOCS) {
      spansQueue.add(spans);
    }
 else     if (spansQueue.isEmpty()) {
      return NO_MORE_DOCS;
    }
  }
}
