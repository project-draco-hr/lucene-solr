{
  boolean atDocStart=false;
  while (spansQueue.peek().startPosition() == -1) {
    atDocStart=true;
    Spans headSpans=spansQueue.remove();
    headSpans.nextStartPosition();
    spansQueue.add(headSpans);
  }
  if (!atDocStart) {
    Spans headSpans=spansQueue.remove();
    headSpans.nextStartPosition();
    spansQueue.add(headSpans);
  }
  return startPosition();
}
