{
  while (buf.offset < upto) {
    if (values.length == values.ints.length) {
      values.grow(values.length + 10);
    }
    values.ints[values.length++]=((buf.bytes[buf.offset++] & 0xFF) << 24) | ((buf.bytes[buf.offset++] & 0xFF) << 16) | ((buf.bytes[buf.offset++] & 0xFF) << 8)| (buf.bytes[buf.offset++] & 0xFF);
  }
}
