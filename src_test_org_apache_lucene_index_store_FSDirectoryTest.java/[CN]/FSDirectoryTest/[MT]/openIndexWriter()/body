{
  IndexWriter iw=null;
  String ret=null;
  try {
    iw=new IndexWriter(TEST_INDEX_DIR,new StandardAnalyzer(),true);
  }
 catch (  IOException e) {
    ret=e.toString();
    e.printStackTrace();
  }
catch (  NullPointerException e) {
    ret=e.toString();
    e.printStackTrace();
  }
 finally {
    if (iw != null) {
      try {
        iw.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
  return ret;
}
