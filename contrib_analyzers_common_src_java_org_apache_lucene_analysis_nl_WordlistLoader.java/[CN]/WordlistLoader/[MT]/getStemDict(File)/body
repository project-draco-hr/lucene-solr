{
  if (wordstemfile == null) {
    return new HashMap();
  }
  HashMap result=new HashMap();
  try {
    LineNumberReader lnr=new LineNumberReader(new FileReader(wordstemfile));
    String line;
    String[] wordstem;
    while ((line=lnr.readLine()) != null) {
      wordstem=line.split("\t",2);
      result.put(wordstem[0],wordstem[1]);
    }
  }
 catch (  IOException e) {
  }
  return result;
}
