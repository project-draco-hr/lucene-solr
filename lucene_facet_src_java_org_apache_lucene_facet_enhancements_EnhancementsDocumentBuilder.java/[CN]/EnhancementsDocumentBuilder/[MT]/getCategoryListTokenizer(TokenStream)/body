{
  CategoryListTokenizer tokenizer=super.getCategoryListTokenizer(categoryStream);
  for (  CategoryEnhancement enhancement : ((EnhancementsIndexingParams)indexingParams).getCategoryEnhancements()) {
    if (enhancement.generatesCategoryList()) {
      tokenizer=enhancement.getCategoryListTokenizer(tokenizer,(EnhancementsIndexingParams)indexingParams,taxonomyWriter);
    }
  }
  return tokenizer;
}
