{
  List<Class<? extends CategoryProperty>> toRetainList=((EnhancementsIndexingParams)indexingParams).getRetainableProperties();
  if (toRetainList != null) {
    CategoryParentsStream categoryParentsStream=new CategoryParentsStream(categoryAttributesStream,taxonomyWriter,indexingParams);
    for (    Class<? extends CategoryProperty> toRetain : toRetainList) {
      categoryParentsStream.addRetainableProperty(toRetain);
    }
    return categoryParentsStream;
  }
  return super.getParentsStream(categoryAttributesStream);
}
