{
  if (Event.EventType.None.equals(event.getType())) {
    return;
  }
  LOG.info("{} fired on path {} state {} latchEventType {}",event.getType(),event.getPath(),event.getState(),latchEventType);
  if (latchEventType == null || event.getType() == latchEventType) {
synchronized (lock) {
      this.event=event;
      lock.notifyAll();
    }
  }
}
