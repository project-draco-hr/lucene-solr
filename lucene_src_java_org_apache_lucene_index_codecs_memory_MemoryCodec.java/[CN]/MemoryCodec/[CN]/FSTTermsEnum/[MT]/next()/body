{
  if (VERBOSE)   System.out.println("te.next");
  current=fstEnum.next();
  if (current == null) {
    if (VERBOSE)     System.out.println("  END");
    return null;
  }
  readTermStats();
  if (VERBOSE)   System.out.println("  term=" + field.name + ":"+ current.input.utf8ToString());
  return current.input;
}
