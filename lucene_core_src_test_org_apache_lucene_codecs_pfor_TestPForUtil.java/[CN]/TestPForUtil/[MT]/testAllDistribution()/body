{
  initRandom();
  int sz=ForPostingsFormat.DEFAULT_BLOCK_SIZE;
  int[] data=new int[sz];
  for (int i=0; i <= 32; ++i) {
    double alpha=gen.nextDouble();
    for (int j=0; j <= 32; ++j) {
      createDistribution(data,sz,alpha,MASK[i],MASK[j]);
      tryCompressAndDecompress(data,sz);
    }
  }
}
