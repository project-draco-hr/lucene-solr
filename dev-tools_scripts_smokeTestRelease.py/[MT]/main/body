def main():
    if (len(sys.argv) != 4):
        print 
        print ('Usage python -u %s BaseURL version tmpDir' % sys.argv[0])
        print 
        sys.exit(1)
    baseURL = sys.argv[1]
    version = sys.argv[2]
    tmpDir = os.path.abspath(sys.argv[3])
    smokeTest(baseURL, version, tmpDir, True)
