def download(name, urlString, tmpDir, quiet=False):
    fileName = ('%s/%s' % (tmpDir, name))
    if (DEBUG and os.path.exists(fileName)):
        if ((not quiet) and (fileName.find('.asc') == (-1))):
            print ('    already done: %.1f MB' % ((os.path.getsize(fileName) / 1024.0) / 1024.0))
        return
    fIn = urllib2.urlopen(urlString)
    fOut = open(fileName, 'wb')
    success = False
    try:
        while True:
            s = fIn.read(65536)
            if (s == ''):
                break
            fOut.write(s)
        fOut.close()
        fIn.close()
        success = True
    finally:
        fIn.close()
        fOut.close()
        if (not success):
            os.remove(fileName)
    if ((not quiet) and (fileName.find('.asc') == (-1))):
        print ('    %.1f MB' % ((os.path.getsize(fileName) / 1024.0) / 1024.0))
