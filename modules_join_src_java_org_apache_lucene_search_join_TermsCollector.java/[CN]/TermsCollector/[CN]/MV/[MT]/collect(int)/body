{
  reuse=docTermOrds.lookup(doc,reuse);
  int[] buffer=new int[5];
  int chunk;
  do {
    chunk=reuse.read(buffer);
    if (chunk == 0) {
      return;
    }
    for (int idx=0; idx < chunk; idx++) {
      int key=buffer[idx];
      docTermsEnum.seekExact((long)key);
      collectorTerms.add(docTermsEnum.term());
    }
  }
 while (chunk >= buffer.length);
}
