{
  if (minNrShouldMatch > 1) {
    return false;
  }
  int optionalCount=0;
  for (  BooleanClause c : clauses) {
    if (c.isRequired()) {
      return false;
    }
 else     if (!c.isProhibited()) {
      optionalCount++;
    }
  }
  if (optionalCount == minNrShouldMatch) {
    return false;
  }
  return true;
}
