{
  if (input.incrementToken()) {
    if (!keywordAtt.isKeyword()) {
      String stem=dictionary.get(termAtt.termBuffer(),0,termAtt.termLength());
      if (stem != null) {
        termAtt.setTermBuffer(stem);
        keywordAtt.setKeyword(true);
      }
    }
    return true;
  }
 else {
    return false;
  }
}
