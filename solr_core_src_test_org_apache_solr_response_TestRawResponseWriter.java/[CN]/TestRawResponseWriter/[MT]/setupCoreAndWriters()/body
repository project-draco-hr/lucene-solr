{
  initCore("solrconfig.xml","schema.xml");
  writerNoBase=newRawResponseWriter(null);
  writerXmlBase=newRawResponseWriter("xml");
  writerJsonBase=newRawResponseWriter("json");
  writerBinBase=newRawResponseWriter("javabin");
  allWriters=new RawResponseWriter[]{writerXmlBase,writerJsonBase,writerBinBase,writerNoBase};
}
