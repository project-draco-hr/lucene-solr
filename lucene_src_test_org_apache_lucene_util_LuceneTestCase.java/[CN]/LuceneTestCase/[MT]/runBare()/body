{
  try {
    seed=null;
    super.runBare();
  }
 catch (  Throwable e) {
    System.out.println("NOTE: random codec of testcase '" + getName() + "' was: "+ codec);
    if (TEST_LOCALE.equals("random"))     System.out.println("NOTE: random locale of testcase '" + getName() + "' was: "+ locale);
    if (TEST_TIMEZONE.equals("random"))     System.out.println("NOTE: random timezone of testcase '" + getName() + "' was: "+ timeZone.getID());
    if (seed != null) {
      System.out.println("NOTE: random seed of testcase '" + getName() + "' was: "+ seed);
    }
    throw e;
  }
}
