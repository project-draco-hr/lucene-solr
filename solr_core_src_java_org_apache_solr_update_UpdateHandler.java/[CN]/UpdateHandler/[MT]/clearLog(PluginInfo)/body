{
  File tlogDir=UpdateLog.getTlogDir(ulogPluginInfo);
  if (tlogDir.exists()) {
    String[] files=UpdateLog.getLogList(tlogDir);
    for (    String file : files) {
      File f=new File(file);
      boolean s=f.delete();
      if (!s) {
        log.error("Could not remove tlog file:" + f);
      }
    }
  }
}
