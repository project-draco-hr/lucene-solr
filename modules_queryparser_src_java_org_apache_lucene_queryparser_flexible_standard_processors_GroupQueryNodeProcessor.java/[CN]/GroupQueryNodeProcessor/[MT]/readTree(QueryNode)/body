{
  if (node instanceof BooleanQueryNode) {
    List<QueryNode> children=node.getChildren();
    if (children != null && children.size() > 0) {
      for (int i=0; i < children.size() - 1; i++) {
        readTree(children.get(i));
      }
      processNode(node);
      readTree(children.get(children.size() - 1));
    }
 else {
      processNode(node);
    }
  }
 else {
    processNode(node);
  }
}
