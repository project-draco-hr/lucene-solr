{
  if (aStreamWriter == null)   throw new GDataParseException("GOMWriter must not be null");
  StringBuilder builder=new StringBuilder("");
  if (this.email != null)   builder.append(this.email);
  if (this.name != null)   builder.append("(").append(this.name).append(")");
  aStreamWriter.writeSimpleXMLElement(aRssName,getXmlNamespaceAttributes(),builder.toString());
}
