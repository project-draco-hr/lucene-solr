{
  assertEquals("foo({\"result\":\"0\"})",h.query(req("q","*:*","wt","velocity",VelocityResponseWriter.TEMPLATE,"numFound",VelocityResponseWriter.JSON,"foo")));
  assertEquals("foo({\"result\":\"{{{0}}}\"})",h.query(req("q","*:*","wt","velocity",VelocityResponseWriter.TEMPLATE,"numFound",VelocityResponseWriter.JSON,"foo",VelocityResponseWriter.LAYOUT,"layout")));
}
