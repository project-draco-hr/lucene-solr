{
  String leastScore=getLeastScore("parent_s:a");
  assertTrue(leastScore + " > 0.0",Float.parseFloat(leastScore) > 0.0);
  assertQ(req("q","{!child of=\"parent_s:[* TO *]\"}parent_s:a","fl","score"),"//*[@numFound='6']","(//float[@name='score'])[" + (random().nextInt(6) + 1) + "]>='"+ leastScore+ "'");
}
