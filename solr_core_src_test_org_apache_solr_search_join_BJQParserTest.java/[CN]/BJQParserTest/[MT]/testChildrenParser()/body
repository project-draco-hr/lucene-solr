{
  assertQ(req("q","{!child of=\"parent_s:[* TO *]\"}parent_s:a","fq","NOT grand_s:[* TO *]"),"//*[@numFound='3']","//doc/arr[@name=\"child_s\"]/str='k'","//doc/arr[@name=\"child_s\"]/str='l'","//doc/arr[@name=\"child_s\"]/str='m'");
  assertQ(req("q","{!child of=\"parent_s:[* TO *]\"}parent_s:b","fq","-parentchild_s:bm","fq","-grand_s:*"),"//*[@numFound='2']","//doc/arr[@name=\"child_s\"]/str='k'","//doc/arr[@name=\"child_s\"]/str='l'");
}
