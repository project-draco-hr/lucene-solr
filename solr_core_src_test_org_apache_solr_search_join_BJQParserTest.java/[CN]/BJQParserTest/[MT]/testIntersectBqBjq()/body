{
  assertQ(req("q","+parent_s:(e b) +_query_:\"{!parent which=$pq v=$chq}\"","chq","child_s:l","pq","parent_s:[* TO *]"),beParents);
  assertQ(req("fq","{!parent which=$pq v=$chq}\"","q","parent_s:(e b)","chq","child_s:l","pq","parent_s:[* TO *]"),beParents);
  assertQ(req("q","*:*","fq","{!parent which=$pq v=$chq}\"","fq","parent_s:(e b)","chq","child_s:l","pq","parent_s:[* TO *]"),beParents);
}
