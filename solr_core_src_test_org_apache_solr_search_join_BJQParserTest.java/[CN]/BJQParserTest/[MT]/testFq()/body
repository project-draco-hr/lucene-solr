{
  assertQ(req("q","{!parent which=$pq v=$chq}","fq","parent_s:(e b)","chq","child_s:l","pq","parent_s:[* TO *]"),beParents);
  boolean qfq=random().nextBoolean();
  assertQ(req(qfq ? "q" : "fq","parent_s:(a e b)",(!qfq) ? "q" : "fq","{!parent which=$pq v=$chq}","chq","parentchild_s:(bm ek cl)","pq","parent_s:[* TO *]"),beParents);
}
