{
  int containsCount=0;
  if (componentContains(minLat,minLon)) {
    containsCount++;
  }
  if (componentContains(minLat,maxLon)) {
    containsCount++;
  }
  if (containsCount == 1) {
    return containsCount;
  }
  if (componentContains(maxLat,maxLon)) {
    containsCount++;
  }
  if (containsCount == 2) {
    return containsCount;
  }
  if (componentContains(maxLat,minLon)) {
    containsCount++;
  }
  return containsCount;
}
