{
  url=normalizedURL(host,port,path);
  if (defaultConfig == null) {
    this.defaultConfig=RequestConfig.custom().setConnectionRequestTimeout(DEFAULT_CONNECTION_TIMEOUT).setSocketTimeout(DEFAULT_SO_TIMEOUT).build();
  }
 else {
    this.defaultConfig=defaultConfig;
  }
  httpc=HttpClientBuilder.create().setConnectionManager(conMgr).setDefaultRequestConfig(this.defaultConfig).build();
}
