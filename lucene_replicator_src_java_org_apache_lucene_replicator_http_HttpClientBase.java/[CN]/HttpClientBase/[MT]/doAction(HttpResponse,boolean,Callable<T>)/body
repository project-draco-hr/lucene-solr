{
  IOException error=null;
  try {
    return call.call();
  }
 catch (  IOException e) {
    error=e;
  }
catch (  Exception e) {
    error=new IOException(e);
  }
 finally {
    try {
      verifyStatus(response);
    }
  finally {
      if (consume) {
        try {
          EntityUtils.consume(response.getEntity());
        }
 catch (        Exception e) {
        }
      }
    }
  }
  throw error;
}
