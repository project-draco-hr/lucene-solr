{
  while (low <= high) {
    int mid=(low + high) >>> 1;
    bytesRef.offset=mid * size;
    int cmp=comp.compare(bytesRef,b);
    if (cmp < 0) {
      low=mid + 1;
    }
 else     if (cmp > 0) {
      high=mid - 1;
    }
 else {
      lookupResult.ord=mid + 1;
      lookupResult.found=true;
      return lookupResult;
    }
  }
  lookupResult.ord=low;
  lookupResult.found=false;
  return lookupResult;
}
