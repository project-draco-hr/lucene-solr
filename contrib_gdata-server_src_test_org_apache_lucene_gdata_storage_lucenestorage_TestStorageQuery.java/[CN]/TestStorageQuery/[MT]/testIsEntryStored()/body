{
  assertTrue(this.query.get().isEntryStored("" + (this.count - 1),feedId));
  assertFalse(this.query.get().isEntryStored("someOther",feedId));
  this.modifier.forceWrite();
  assertTrue(this.query.get().isEntryStored("" + (this.count - 1),feedId));
  this.query=this.controller.getStorageQuery();
  assertTrue(this.query.get().isEntryStored("" + (this.count - 1),feedId));
  assertFalse(this.query.get().isEntryStored("someOther",feedId));
}
