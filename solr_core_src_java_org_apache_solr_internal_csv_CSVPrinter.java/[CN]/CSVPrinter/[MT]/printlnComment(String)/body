{
  if (this.strategy.isCommentingDisabled()) {
    return;
  }
  if (!newLine) {
    println();
  }
  out.write(this.strategy.getCommentStart());
  out.write(' ');
  for (int i=0; i < comment.length(); i++) {
    char c=comment.charAt(i);
switch (c) {
case '\r':
      if (i + 1 < comment.length() && comment.charAt(i + 1) == '\n') {
        i++;
      }
case '\n':
    println();
  out.write(this.strategy.getCommentStart());
out.write(' ');
break;
default :
out.write(c);
break;
}
}
println();
}
