{
  int ITERS=atLeast(100);
  for (int i=0; i < ITERS; i++) {
    Automaton a=AutomatonTestUtil.randomAutomaton(random());
    Automaton ra=Operations.reverse(a);
    Automaton rra=Operations.reverse(ra);
    assertTrue(Operations.sameLanguage(Operations.determinize(Operations.removeDeadStates(a),Integer.MAX_VALUE),Operations.determinize(Operations.removeDeadStates(rra),Integer.MAX_VALUE)));
  }
}
