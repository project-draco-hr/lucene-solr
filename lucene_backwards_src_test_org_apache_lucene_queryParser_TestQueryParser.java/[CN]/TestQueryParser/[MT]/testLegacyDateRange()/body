{
  String startDate=getLocalizedDate(2002,1,1,false);
  String endDate=getLocalizedDate(2002,1,4,false);
  Calendar endDateExpected=new GregorianCalendar();
  endDateExpected.set(2002,1,4,23,59,59);
  endDateExpected.set(Calendar.MILLISECOND,999);
  assertQueryEquals("[ " + escapeDateString(startDate) + " TO "+ escapeDateString(endDate)+ "]",null,"[" + getLegacyDate(startDate) + " TO "+ DateField.dateToString(endDateExpected.getTime())+ "]");
  assertQueryEquals("{  " + escapeDateString(startDate) + "    "+ escapeDateString(endDate)+ "   }",null,"{" + getLegacyDate(startDate) + " TO "+ getLegacyDate(endDate)+ "}");
}
