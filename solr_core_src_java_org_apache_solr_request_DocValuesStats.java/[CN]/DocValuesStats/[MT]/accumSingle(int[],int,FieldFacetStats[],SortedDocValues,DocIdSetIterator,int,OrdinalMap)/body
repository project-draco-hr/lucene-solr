{
  int doc;
  while ((doc=disi.nextDoc()) != DocIdSetIterator.NO_MORE_DOCS) {
    int term=si.getOrd(doc);
    if (term >= 0) {
      if (map != null) {
        term=(int)map.getGlobalOrd(subIndex,term);
      }
      counts[term]++;
      for (      FieldFacetStats f : facetStats) {
        f.facetTermNum(docBase + doc,term);
      }
    }
  }
}
