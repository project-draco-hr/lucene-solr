{
  if (!allJarsLoaded) {
    loadJars();
  }
  String path=name.replace('.','/').concat(".class");
  ByteBuffer buf=null;
  for (  PluginBag.RuntimeLib lib : libs) {
    try {
      buf=lib.getFileContent(path);
      if (buf != null) {
        jarName.set(lib.name);
        break;
      }
    }
 catch (    Exception exp) {
      throw new ClassNotFoundException("Unable to load class :" + name,exp);
    }
  }
  return buf;
}
