{
  super.getBounds(bounds);
  bounds.addHorizontalPlane(planetModel,topLat,topPlane,leftPlane,rightPlane).addVerticalPlane(planetModel,leftLon,leftPlane,topPlane,rightPlane).addVerticalPlane(planetModel,rightLon,rightPlane,topPlane,leftPlane).addPoint(URHC).addPoint(ULHC).addPoint(planetModel.SOUTH_POLE);
}
