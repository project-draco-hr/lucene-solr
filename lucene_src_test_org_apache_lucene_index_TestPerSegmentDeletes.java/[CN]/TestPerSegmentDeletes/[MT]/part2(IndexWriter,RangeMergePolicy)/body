{
  for (int x=20; x < 25; x++) {
    writer.addDocument(TestIndexWriterReader.createDocument(x,"5",2));
  }
  writer.flush(false,true,false);
  for (int x=25; x < 30; x++) {
    writer.addDocument(TestIndexWriterReader.createDocument(x,"5",2));
  }
  writer.flush(false,true,false);
  Term delterm=new Term("id","8");
  writer.deleteDocuments(delterm);
  fsmp.doMerge=true;
  fsmp.start=1;
  fsmp.length=2;
  writer.maybeMerge();
}
