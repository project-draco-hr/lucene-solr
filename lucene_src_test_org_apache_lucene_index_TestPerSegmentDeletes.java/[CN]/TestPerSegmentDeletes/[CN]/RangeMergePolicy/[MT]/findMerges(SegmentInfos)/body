{
  MergeSpecification ms=new MergeSpecification();
  if (doMerge) {
    SegmentInfos mergeInfos=new SegmentInfos();
    for (int x=start; x < (start + length); x++) {
      mergeInfos.add(segmentInfos.get(x));
    }
    OneMerge om=new OneMerge(mergeInfos);
    ms.add(om);
    doMerge=false;
    return ms;
  }
  return null;
}
