{
  sort();
  other.sort();
  int myIndex=0;
  int otherIndex=0;
  int newSize=0;
  if (this.size > other.size) {
    while (otherIndex < other.size && myIndex < size) {
      while (otherIndex < other.size && other.data[otherIndex] < data[myIndex]) {
        ++otherIndex;
      }
      if (otherIndex == other.size) {
        break;
      }
      while (myIndex < size && other.data[otherIndex] > data[myIndex]) {
        ++myIndex;
      }
      if (other.data[otherIndex] == data[myIndex]) {
        data[newSize++]=data[myIndex];
        ++otherIndex;
        ++myIndex;
      }
    }
  }
 else {
    while (otherIndex < other.size && myIndex < size) {
      while (myIndex < size && other.data[otherIndex] > data[myIndex]) {
        ++myIndex;
      }
      if (myIndex == size) {
        break;
      }
      while (otherIndex < other.size && other.data[otherIndex] < data[myIndex]) {
        ++otherIndex;
      }
      if (other.data[otherIndex] == data[myIndex]) {
        data[newSize++]=data[myIndex];
        ++otherIndex;
        ++myIndex;
      }
    }
  }
  this.size=newSize;
}
