{
  if (getChildren() == null || getChildren().size() == 0)   return "<any field='" + this.field + "'  matchelements="+ this.minimumMatchingmElements+ "/>";
  StringBuilder sb=new StringBuilder();
  sb.append("<any field='" + this.field + "'  matchelements="+ this.minimumMatchingmElements+ ">");
  for (  QueryNode clause : getChildren()) {
    sb.append("\n");
    sb.append(clause.toString());
  }
  sb.append("\n</any>");
  return sb.toString();
}
