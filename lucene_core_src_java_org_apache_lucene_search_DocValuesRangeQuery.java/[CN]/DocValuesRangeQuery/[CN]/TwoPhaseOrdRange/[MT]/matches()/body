{
  final int doc=approximation.docID();
  if (acceptDocs == null || acceptDocs.get(doc)) {
    values.setDocument(doc);
    for (long ord=values.nextOrd(); ord != SortedSetDocValues.NO_MORE_ORDS; ord=values.nextOrd()) {
      if (ord >= minOrd && ord <= maxOrd) {
        return true;
      }
    }
  }
  return false;
}
