{
  final int doc=approximation.docID();
  if (acceptDocs == null || acceptDocs.get(doc)) {
    values.setDocument(doc);
    final int count=values.count();
    for (int i=0; i < count; ++i) {
      final long value=values.valueAt(i);
      if (value >= min && value <= max) {
        return true;
      }
    }
  }
  return false;
}
