{
  CharTermAttributeImpl t=new CharTermAttributeImpl();
  t.append("test");
  assertEquals("test",t.toString());
  try {
    t.charAt(-1);
    fail("Should throw IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException iobe) {
  }
  try {
    t.charAt(4);
    fail("Should throw IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException iobe) {
  }
  try {
    t.subSequence(0,5);
    fail("Should throw IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException iobe) {
  }
  try {
    t.subSequence(5,0);
    fail("Should throw IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException iobe) {
  }
}
