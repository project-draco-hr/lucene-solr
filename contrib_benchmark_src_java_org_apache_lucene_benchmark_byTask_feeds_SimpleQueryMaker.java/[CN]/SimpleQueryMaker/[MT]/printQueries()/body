{
  String newline=System.getProperty("line.separator");
  StringBuffer sb=new StringBuffer();
  if (queries != null) {
    for (int i=0; i < queries.length; i++) {
      sb.append(i + ". " + Format.simpleName(queries[i].getClass())+ " - "+ queries[i].toString());
      sb.append(newline);
    }
  }
  return sb.toString();
}
