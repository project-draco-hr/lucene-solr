{
  long time=0;
  Date date=(Date)valsArr[0].objectVal(doc);
  try {
    parser.setNow(date);
    for (int count=1; count < valsArr.length; count++) {
      date=parser.parseMath(valsArr[count].strVal(doc));
      parser.setNow(date);
    }
    time=parser.getNow().getTime();
  }
 catch (  ParseException e) {
    e.printStackTrace();
    time=date.getTime();
  }
  return time;
}
