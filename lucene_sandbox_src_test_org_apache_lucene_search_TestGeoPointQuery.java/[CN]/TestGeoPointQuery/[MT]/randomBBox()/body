{
  double lat0=randomLat();
  double lat1=randomLat();
  double lon0=randomLon();
  double lon1=randomLon();
  if (lat1 < lat0) {
    double x=lat0;
    lat0=lat1;
    lat1=x;
  }
  if (lon1 < lon0) {
    double x=lon0;
    lon0=lon1;
    lon1=x;
  }
  return new GeoBoundingBox(lon0,lon1,lat0,lat1);
}
