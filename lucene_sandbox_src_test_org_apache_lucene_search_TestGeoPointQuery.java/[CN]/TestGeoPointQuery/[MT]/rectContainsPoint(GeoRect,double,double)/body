{
  if (GeoUtils.compare(pointLon,rect.minLon) == 0.0 || GeoUtils.compare(pointLon,rect.maxLon) == 0.0 || GeoUtils.compare(pointLat,rect.minLat) == 0.0 || GeoUtils.compare(pointLat,rect.maxLat) == 0.0) {
    return null;
  }
  if (rect.minLon < rect.maxLon) {
    return GeoRelationUtils.pointInRect(pointLon,pointLat,rect.minLon,rect.minLat,rect.maxLon,rect.maxLat);
  }
 else {
    return GeoRelationUtils.pointInRect(pointLon,pointLat,-180.0,rect.minLat,rect.maxLon,rect.maxLat) || GeoRelationUtils.pointInRect(pointLon,pointLat,rect.minLon,rect.minLat,180.0,rect.maxLat);
  }
}
