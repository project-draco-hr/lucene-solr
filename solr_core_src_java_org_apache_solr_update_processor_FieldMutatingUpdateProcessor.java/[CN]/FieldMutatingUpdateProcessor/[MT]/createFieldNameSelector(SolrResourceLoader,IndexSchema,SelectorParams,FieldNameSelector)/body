{
  if (params.noSelectorsSpecified()) {
    return defSelector;
  }
  final ConfigurableFieldNameSelectorHelper helper=new ConfigurableFieldNameSelectorHelper(loader,params);
  return new FieldNameSelector(){
    @Override public boolean shouldMutate(    String fieldName){
      return helper.shouldMutateBasedOnSchema(fieldName,schema);
    }
  }
;
}
