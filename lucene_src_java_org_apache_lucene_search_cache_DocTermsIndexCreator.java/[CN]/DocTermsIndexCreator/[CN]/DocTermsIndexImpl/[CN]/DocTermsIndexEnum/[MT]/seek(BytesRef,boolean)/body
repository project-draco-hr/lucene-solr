{
  int low=1;
  int high=numOrd - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    seek(mid);
    int cmp=term.compareTo(text);
    if (cmp < 0)     low=mid + 1;
 else     if (cmp > 0)     high=mid - 1;
 else     return SeekStatus.FOUND;
  }
  if (low == numOrd) {
    return SeekStatus.END;
  }
 else {
    seek(low);
    return SeekStatus.NOT_FOUND;
  }
}
