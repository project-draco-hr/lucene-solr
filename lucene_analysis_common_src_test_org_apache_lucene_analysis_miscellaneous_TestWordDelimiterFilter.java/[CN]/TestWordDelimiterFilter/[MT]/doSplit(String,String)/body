{
  int flags=GENERATE_WORD_PARTS | GENERATE_NUMBER_PARTS | SPLIT_ON_CASE_CHANGE| SPLIT_ON_NUMERICS| STEM_ENGLISH_POSSESSIVE;
  WordDelimiterFilter wdf=new WordDelimiterFilter(new MockTokenizer(new StringReader(input),MockTokenizer.KEYWORD,false),WordDelimiterIterator.DEFAULT_WORD_DELIM_TABLE,flags,null);
  assertTokenStreamContents(wdf,output);
}
