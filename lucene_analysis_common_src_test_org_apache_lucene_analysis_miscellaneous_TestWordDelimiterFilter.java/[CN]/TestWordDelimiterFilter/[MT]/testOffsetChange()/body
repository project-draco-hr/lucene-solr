{
  int flags=GENERATE_WORD_PARTS | GENERATE_NUMBER_PARTS | CATENATE_ALL| SPLIT_ON_CASE_CHANGE| SPLIT_ON_NUMERICS| STEM_ENGLISH_POSSESSIVE;
  WordDelimiterFilter wdf=new WordDelimiterFilter(new SingleTokenTokenStream(new Token("??belkeit)",7,16)),DEFAULT_WORD_DELIM_TABLE,flags,null);
  assertTokenStreamContents(wdf,new String[]{"??belkeit"},new int[]{7},new int[]{15});
}
