{
  QueryResponse rsp=cluster.getSolrClient().query(COLLECTION,new SolrQuery("q","*:*","fl","id","sort","payload desc","rows","20"));
  assertFieldValues(rsp.getResults(),id,"11","19","13","8","9","5","3","12","10","2","4","6","1","7");
  rsp=cluster.getSolrClient().query(COLLECTION,new SolrQuery("q","*:*","fl","id,score","sort","payload asc","rows","20"));
  assertFieldValues(rsp.getResults(),id,"7","1","6","4","2","10","12","3","5","9","8","13","19","11");
}
