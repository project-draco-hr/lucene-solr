{
  List<String> l=new ArrayList<>();
  for (int i=0; i < args.length; i+=2) {
    if (args[i + 1].equals("0") || args[i + 1].equals("0.0") || args[i + 1].equals("1800-12-31T23:59:59Z")|| args[i + 1].equals("str0")|| args[i + 1].equals("this is the firststr0")|| args[i + 1].equals("this is the secondstr0")) {
      continue;
    }
    l.add(args[i]);
    l.add(args[i + 1]);
  }
  return l.toArray(new String[0]);
}
