{
  TokenStream ts=new TrimFilter(new IterTokenStream(new Token(" a ",1,5),new Token("b   ",6,10),new Token("cCc",11,15),new Token("   ",16,20)));
  assertEquals("a",ts.next().termText());
  assertEquals("b",ts.next().termText());
  assertEquals("cCc",ts.next().termText());
  assertEquals("",ts.next().termText());
  assertNull(ts.next());
}
