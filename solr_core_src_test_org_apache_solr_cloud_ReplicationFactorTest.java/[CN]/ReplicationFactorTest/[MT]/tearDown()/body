{
  log.info("tearing down replicationFactorTest!");
  System.clearProperty("numShards");
  try {
    super.tearDown();
  }
 catch (  Exception exc) {
  }
  resetExceptionIgnores();
  log.info("super.tearDown complete, closing all socket proxies");
  if (!proxies.isEmpty()) {
    for (    SocketProxy proxy : proxies.values()) {
      proxy.close();
    }
  }
}
