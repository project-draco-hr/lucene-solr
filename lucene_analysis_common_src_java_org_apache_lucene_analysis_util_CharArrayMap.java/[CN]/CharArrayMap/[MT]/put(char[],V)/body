{
  if (ignoreCase) {
    charUtils.toLowerCase(text,0,text.length);
  }
  int slot=getSlot(text,0,text.length);
  if (keys[slot] != null) {
    final V oldValue=values[slot];
    values[slot]=value;
    return oldValue;
  }
  keys[slot]=text;
  values[slot]=value;
  count++;
  if (count + (count >> 2) > keys.length) {
    rehash();
  }
  return null;
}
