{
  if (this.term == null)   throw new GDataParseException(String.format(AtomParser.MISSING_ELEMENT_ATTRIBUTE,this.localName,TERM_ATTRIBUTE));
  if (this.scheme != null) {
    try {
      AtomParserUtils.getAbsolutAtomURI(this.xmlBase,this.scheme);
    }
 catch (    URISyntaxException e) {
      throw new GDataParseException(String.format(AtomParser.INVALID_ATTRIBUTE,this.localName + " attribute " + GOMCategory.SCHEME_ATTRIBUTE,"absolute uri"),e);
    }
  }
}
