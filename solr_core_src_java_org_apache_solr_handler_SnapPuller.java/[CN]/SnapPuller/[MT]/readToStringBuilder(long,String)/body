{
  StringBuilder sb=new StringBuilder();
  List<String> l=new ArrayList<String>();
  if (str != null && str.length() != 0) {
    String[] ss=str.split(",");
    for (int i=0; i < ss.length; i++) {
      l.add(ss[i]);
    }
  }
  sb.append(replicationTime);
  if (!l.isEmpty()) {
    for (int i=0; i < l.size() || i < 9; i++) {
      if (i == l.size() || i == 9)       break;
      String s=l.get(i);
      sb.append(",").append(s);
    }
  }
  return sb;
}
