{
  SolrCore core=getCore(name);
  try {
    if (core != null) {
      registerCore(false,toName,core,false);
      name=checkDefault(name);
      SolrCore old=solrCores.remove(name,false);
      coresLocator.rename(this,old.getCoreDescriptor(),core.getCoreDescriptor());
    }
  }
  finally {
    if (core != null) {
      core.close();
    }
  }
}
