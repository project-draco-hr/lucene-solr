{
  if (dstName.endsWith("/")) {
    int pos=srcName.lastIndexOf("/");
    if (pos < 0) {
      dstName+=srcName;
    }
 else {
      dstName+=srcName.substring(pos + 1);
    }
  }
 else   if (dstName.equals(".")) {
    dstName=Paths.get(".").normalize().toAbsolutePath().toString();
  }
  return dstName;
}
