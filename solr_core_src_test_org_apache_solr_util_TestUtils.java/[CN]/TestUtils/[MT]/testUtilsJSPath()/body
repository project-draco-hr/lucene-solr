{
  String json="{\n" + "  'authorization':{\n" + "    'class':'solr.RuleBasedAuthorizationPlugin',\n"+ "    'user-role':{\n"+ "      'solr':'admin',\n"+ "      'harry':'admin'},\n"+ "    'permissions':[{\n"+ "        'name':'security-edit',\n"+ "        'role':'admin'},\n"+ "      {\n"+ "        'name':'x-update',\n"+ "        'collection':'x',\n"+ "        'path':'/update/*',\n"+ "        'role':'dev'}],\n"+ "    '':{'v':4}}}";
  Map m=(Map)Utils.fromJSONString(json);
  assertEquals("x-update",Utils.getObjectByPath(m,false,"authorization/permissions[1]/name"));
}
