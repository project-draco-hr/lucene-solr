{
  assertEquals("a",StrUtils.escapeTextWithSeparator("a",'|'));
  assertEquals("a",StrUtils.escapeTextWithSeparator("a",','));
  assertEquals("a\\|b",StrUtils.escapeTextWithSeparator("a|b",'|'));
  assertEquals("a|b",StrUtils.escapeTextWithSeparator("a|b",','));
  assertEquals("a,b",StrUtils.escapeTextWithSeparator("a,b",'|'));
  assertEquals("a\\,b",StrUtils.escapeTextWithSeparator("a,b",','));
  assertEquals("a\\\\b",StrUtils.escapeTextWithSeparator("a\\b",','));
  assertEquals("a\\\\\\,b",StrUtils.escapeTextWithSeparator("a\\,b",','));
}
