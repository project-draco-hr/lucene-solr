{
  if (this.profile == null)   throw new StorageException("Can process ExtensionProfile not set -- is null");
  if (feedId == null)   throw new StorageException("No feed ID specified -- is null");
  if (LOG.isInfoEnabled())   LOG.info("get feed: " + feedId + " startindex: "+ startIndex+ " resultCount: "+ resultCount);
  ReferenceCounter<StorageQuery> query=null;
  try {
    query=this.controller.getStorageQuery();
    List<BaseEntry> resultList=query.get().getLatestFeedQuery(feedId,resultCount,startIndex,this.profile);
    BaseFeed feed=new Feed();
    feed.getEntries().addAll(resultList);
    return feed;
  }
 catch (  Exception e) {
    LOG.error("Can't get latest feed for feedID: " + feedId + " -- "+ e.getMessage(),e);
    StorageException ex=new StorageException("Can't create Entry -- " + e.getMessage(),e);
    ex.setStackTrace(e.getStackTrace());
    throw ex;
  }
 finally {
    if (query != null)     query.decrementRef();
  }
}
