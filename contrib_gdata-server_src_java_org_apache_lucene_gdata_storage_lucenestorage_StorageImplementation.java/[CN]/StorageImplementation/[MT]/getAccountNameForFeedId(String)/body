{
  if (feedId == null)   throw new StorageException("feedid must not be null");
  ReferenceCounter<StorageQuery> query=null;
  try {
    query=this.controller.getStorageQuery();
    String accountName=query.get().getAccountNameForFeedId(feedId);
    if (accountName == null)     throw new StorageException("no feed for feedId " + feedId + " found");
    return accountName;
  }
 catch (  IOException e) {
    throw new StorageException("Can not access storage - " + e.getMessage(),e);
  }
 finally {
    if (query != null)     query.decrementRef();
  }
}
