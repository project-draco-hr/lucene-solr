{
  if (this.profile == null)   throw new StorageException("Can process ExtensionProfile not set -- is null");
  if (feedId == null)   throw new StorageException("No feed ID specified -- is null");
  StorageModifier modifier=this.controller.getStorageModifier();
  String id=this.controller.releaseID();
  entry.setId(feedId + id);
  if (LOG.isInfoEnabled())   LOG.info("Store entry " + id + " -- feed: "+ feedId);
  try {
    StorageEntryWrapper wrapper=new StorageEntryWrapper(entry,feedId,StorageOperation.INSERT,this.profile);
    modifier.insertEntry(wrapper);
  }
 catch (  IOException e) {
    StorageException ex=new StorageException("Can't create Entry -- " + e.getMessage(),e);
    ex.setStackTrace(e.getStackTrace());
    throw ex;
  }
  return entry;
}
