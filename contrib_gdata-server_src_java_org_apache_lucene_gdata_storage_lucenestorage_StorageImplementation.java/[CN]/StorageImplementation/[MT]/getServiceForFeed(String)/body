{
  if (feedId == null)   throw new StorageException("no feed for the feedID == null");
  ReferenceCounter<StorageQuery> query=null;
  try {
    query=this.controller.getStorageQuery();
    String type=query.get().getService(feedId);
    if (type == null)     throw new StorageException("no feed for the feedID == " + feedId + " found");
    return type;
  }
 catch (  Exception e) {
    throw new StorageException("Can not access storage",e);
  }
 finally {
    if (query != null)     query.decrementRef();
  }
}
