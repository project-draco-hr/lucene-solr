{
  DumbQueryParser qp=new DumbQueryParser("",new MultiAnalyzer());
  qp.setPhraseSlop(99);
  assertEquals("\"foo bar\"~99",qp.getSuperFieldQuery("","foo bar").toString());
  assertEquals("\"(multi multi2) bar\"~99",qp.getSuperFieldQuery("","multi bar").toString());
  assertEquals("\"(multi multi2) foo\"~99 bar",qp.parse("\"multi foo\" bar").toString());
}
