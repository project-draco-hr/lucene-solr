{
  for (  PointReader reader : mergeState.pointReaders) {
    if (reader != null) {
      reader.checkIntegrity();
    }
  }
  for (  FieldInfo fieldInfo : mergeState.mergeFieldInfos) {
    if (fieldInfo.getPointDimensionCount() != 0) {
      mergeOneField(mergeState,fieldInfo);
    }
  }
  finish();
}
