{
  if (parentArray == null) {
synchronized (this) {
      if (parentArray == null) {
        initReaderManager();
        parentArray=new ParentArray();
        DirectoryReader reader=readerManager.acquire();
        try {
          parentArray.refresh(reader);
        }
  finally {
          readerManager.release(reader);
        }
      }
    }
  }
  return parentArray;
}
