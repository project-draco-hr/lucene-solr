{
  if (taxoArrays == null) {
synchronized (this) {
      if (taxoArrays == null) {
        initReaderManager();
        DirectoryReader reader=readerManager.acquire();
        try {
          ParallelTaxonomyArrays tmpArrays=new ParallelTaxonomyArrays(reader);
          taxoArrays=tmpArrays;
        }
  finally {
          readerManager.release(reader);
        }
      }
    }
  }
  return taxoArrays;
}
