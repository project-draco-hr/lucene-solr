{
  StringBuilder sb=new StringBuilder();
  for (  LegacyNumericType type : LegacyNumericType.values()) {
    String numberStr=numberToString(getNumberType(numberType,type.name()));
    sb.append('+').append(type.name()).append(":\"").append(numberStr).append("\" ");
  }
  String dateStr=ESCAPER.escape(DATE_FORMAT.format(new Date(getNumberType(numberType,DATE_FIELD_NAME).longValue())),LOCALE,EscapeQuerySyntax.Type.STRING).toString();
  sb.append('+').append(DATE_FIELD_NAME).append(":\"").append(dateStr).append('"');
  testQuery(sb.toString(),expectedDocCount);
}
