{
  StringBuilder sb=new StringBuilder();
  if (this.field.equals(field) == false) {
    sb.append(this.field);
    sb.append(':');
  }
  sb.append("<ranges:");
  sb.append(toString(ranges,0));
  for (int d=1; d < numDims; ++d) {
    sb.append(' ');
    sb.append(toString(ranges,d));
  }
  sb.append('>');
  return sb.toString();
}
