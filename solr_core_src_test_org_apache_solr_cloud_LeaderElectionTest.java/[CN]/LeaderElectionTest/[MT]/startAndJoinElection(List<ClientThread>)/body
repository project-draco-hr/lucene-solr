{
  for (  Thread thread : threads) {
    thread.start();
  }
  while (true) {
    int doneCount=0;
    for (    ClientThread thread : threads) {
      if (thread.electionDone) {
        doneCount++;
      }
    }
    if (doneCount == threads.size()) {
      break;
    }
    Thread.sleep(100);
  }
}
