{
  final TermsEnum termsEnum=vector.iterator(null);
  if (termsEnum.next() != null) {
    DocsAndPositionsEnum dpEnum=termsEnum.docsAndPositions(null,null,false);
    if (dpEnum != null) {
      int pos=dpEnum.nextPosition();
      if (pos >= 0) {
        return true;
      }
    }
  }
  return false;
}
