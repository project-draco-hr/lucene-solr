{
  final TokenStream ts=new CannedTokenStream(new Token[]{token("a",1,1),token("x",0,3),token("b",3,1)});
  final Automaton actual=(new TokenStreamToAutomaton()).toAutomaton(ts);
  final Automaton a1=join(s2a("a"),SEP_A,HOLE_A,SEP_A,HOLE_A,SEP_A,s2a("b"));
  final Automaton a2=join(s2a("x"),SEP_A,s2a("b"));
  final Automaton expected=Operations.union(a1,a2);
  assertTrue(Operations.sameLanguage(Operations.determinize(Operations.removeDeadStates(expected)),Operations.determinize(Operations.removeDeadStates(actual))));
}
