{
  final TokenStream ts=new CannedTokenStream(new Token[]{token("a",1,1),token("X",0,10)});
  final Automaton actual=(new TokenStreamToAutomaton()).toAutomaton(ts);
  final Automaton expected=BasicOperations.union(BasicAutomata.makeString("a"),BasicAutomata.makeString("X"));
  assertTrue(BasicOperations.sameLanguage(expected,actual));
}
