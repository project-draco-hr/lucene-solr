{
  final TokenStream ts=new CannedTokenStream(new Token[]{token("a",1,1),token("X",0,2),token("b",2,1)});
  final Automaton actual=(new TokenStreamToAutomaton()).toAutomaton(ts);
  final Automaton a1=BasicOperations.union(join(s2a("a"),SEP_A,HOLE_A),BasicAutomata.makeString("X"));
  final Automaton expected=BasicOperations.concatenate(a1,join(SEP_A,s2a("b")));
  assertTrue(BasicOperations.sameLanguage(expected,actual));
}
