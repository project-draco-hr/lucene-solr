{
  if (firstTime) {
    init();
    firstTime=false;
  }
 else   if (more) {
    more=last.next();
  }
  while (more) {
    while (more && first.doc < last.doc) {
      more=first.skipTo(last.doc);
      firstToLast();
    }
    if (more) {
      freq=phraseFreq();
      if (freq == 0.0f)       more=last.next();
 else       return true;
    }
  }
  return false;
}
