{
  if (splitBy != null) {
    return readBySplit(splitBy,value);
  }
 else   if (replaceWith != null) {
    Pattern p=getPattern(reStr);
    Matcher m=p.matcher(value);
    return m.find() ? m.replaceAll(replaceWith) : value;
  }
 else {
    return readfromRegExp(reStr,value,col,groupNames);
  }
}
