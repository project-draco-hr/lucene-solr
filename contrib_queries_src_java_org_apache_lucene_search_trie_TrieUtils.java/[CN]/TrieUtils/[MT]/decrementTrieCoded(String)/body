{
  final int l=v.length();
  final char[] buf=new char[l];
  boolean dec=true;
  for (int i=l - 1; i >= 0; i--) {
    int b=v.charAt(i) - TRIE_CODED_SYMBOL_MIN;
    if (dec)     b--;
    if (dec=(b < 0))     b=(int)mask;
    buf[i]=(char)(TRIE_CODED_SYMBOL_MIN + b);
  }
  return new String(buf);
}
