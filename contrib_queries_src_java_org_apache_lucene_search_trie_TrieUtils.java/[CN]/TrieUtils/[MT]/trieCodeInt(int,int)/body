{
  if (precisionStep < 1 || precisionStep > 32)   throw new IllegalArgumentException("precisionStep may only be 1..32");
  String[] arr=new String[31 / precisionStep + 1];
  int idx=0;
  for (int shift=0; shift < 32; shift+=precisionStep) {
    arr[idx++]=intToPrefixCoded(val,shift);
  }
  return arr;
}
