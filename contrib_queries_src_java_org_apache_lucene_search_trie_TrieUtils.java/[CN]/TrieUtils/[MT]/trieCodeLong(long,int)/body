{
  if (precisionStep < 1 || precisionStep > 64)   throw new IllegalArgumentException("precisionStep may only be 1..64");
  String[] arr=new String[63 / precisionStep + 1];
  int idx=0;
  for (int shift=0; shift < 64; shift+=precisionStep) {
    arr[idx++]=longToPrefixCoded(val,shift);
  }
  return arr;
}
