{
  int num=2000 * RANDOM_MULTIPLIER;
  for (int i=0; i < num; i++) {
    Automaton a=AutomatonTestUtil.randomAutomaton(random);
    Automaton b=a.clone();
    AutomatonTestUtil.determinizeSimple(a);
    b.deterministic=false;
    b.determinize();
    assertTrue(BasicOperations.sameLanguage(a,b));
  }
}
