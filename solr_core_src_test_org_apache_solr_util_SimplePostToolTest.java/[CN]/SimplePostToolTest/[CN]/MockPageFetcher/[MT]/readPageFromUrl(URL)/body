{
  PageFetcherResult res=(new SimplePostTool()).new PageFetcherResult();
  if (isDisallowedByRobots(u)) {
    res.httpStatus=403;
    return res;
  }
  res.httpStatus=200;
  res.contentType="text/html";
  res.content=ByteBuffer.wrap(htmlMap.get(u.toString()).getBytes(StandardCharsets.UTF_8));
  return res;
}
