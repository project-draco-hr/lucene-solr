{
  String input="?????????????????????????????????????????????????????????????????????";
  String[] surfaceForms={"????????????","??????????????????","???","??????","??????","???","??????????????????","???"};
  List<Token> tokens=segmenter.tokenize(input);
  assertTrue(tokens.size() == surfaceForms.length);
  for (int i=0; i < tokens.size(); i++) {
    assertEquals(surfaceForms[i],tokens.get(i).getSurfaceFormString());
  }
}
