{
  return new DataSource(){
    public void init(    Context context,    Properties initProps){
      ds.init(context,initProps);
    }
    public void close(){
      ds.close();
    }
    public Object getData(    String query){
      writer.log(SolrWriter.ENTITY_META,"query",query);
      long start=System.currentTimeMillis();
      try {
        return ds.getData(query);
      }
 catch (      DataImportHandlerException de) {
        writer.log(SolrWriter.ENTITY_EXCEPTION,null,de);
        throw de;
      }
catch (      Exception e) {
        writer.log(SolrWriter.ENTITY_EXCEPTION,null,e);
        DataImportHandlerException de=new DataImportHandlerException(DataImportHandlerException.SEVERE,"",e);
        de.debugged=true;
        throw de;
      }
 finally {
        writer.log(SolrWriter.ENTITY_META,"time-taken",DocBuilder.getTimeElapsedSince(start));
      }
    }
  }
;
}
