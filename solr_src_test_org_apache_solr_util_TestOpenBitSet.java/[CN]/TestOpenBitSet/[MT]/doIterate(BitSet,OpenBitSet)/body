{
  int aa=-1, bb=-1;
  OpenBitSetIterator iterator=new OpenBitSetIterator(b);
  do {
    aa=a.nextSetBit(aa + 1);
    if (rand.nextBoolean()) {
      iterator.next();
      bb=iterator.doc();
    }
 else {
      iterator.skipTo(bb + 1);
      bb=iterator.doc();
    }
    assertEquals(aa == -1 ? DocIdSetIterator.NO_MORE_DOCS : aa,bb);
  }
 while (aa >= 0);
}
