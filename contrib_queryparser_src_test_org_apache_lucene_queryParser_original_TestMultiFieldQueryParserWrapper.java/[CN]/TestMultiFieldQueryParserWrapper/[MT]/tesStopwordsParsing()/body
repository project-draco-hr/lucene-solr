{
  assertStopQueryEquals("one","b:one t:one");
  assertStopQueryEquals("one stop","b:one t:one");
  assertStopQueryEquals("one (stop)","b:one t:one");
  assertStopQueryEquals("one ((stop))","b:one t:one");
  assertStopQueryEquals("stop","");
  assertStopQueryEquals("(stop)","");
  assertStopQueryEquals("((stop))","");
}
