{
  if (getChildren() == null || getChildren().size() == 0)   return "";
  StringBuilder sb=new StringBuilder();
  String filler="";
  for (  QueryNode child : getChildren()) {
    sb.append(filler).append(child.toQueryString(escapeSyntaxParser));
    filler=",";
  }
  return "[TP[" + sb.toString() + "]]";
}
