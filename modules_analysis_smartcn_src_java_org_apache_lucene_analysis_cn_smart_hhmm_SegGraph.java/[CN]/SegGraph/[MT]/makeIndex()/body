{
  List<SegToken> result=new ArrayList<SegToken>();
  int s=-1, count=0, size=tokenListTable.size();
  List<SegToken> tokenList;
  int index=0;
  while (count < size) {
    if (isStartExist(s)) {
      tokenList=tokenListTable.get(s);
      for (      SegToken st : tokenList) {
        st.index=index;
        result.add(st);
        index++;
      }
      count++;
    }
    s++;
  }
  return result;
}
