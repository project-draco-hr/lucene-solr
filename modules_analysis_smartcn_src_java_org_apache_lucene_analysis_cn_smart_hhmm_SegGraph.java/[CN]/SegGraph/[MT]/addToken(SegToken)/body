{
  int s=token.startOffset;
  if (!isStartExist(s)) {
    ArrayList<SegToken> newlist=new ArrayList<SegToken>();
    newlist.add(token);
    tokenListTable.put(s,newlist);
  }
 else {
    List<SegToken> tokenList=tokenListTable.get(s);
    tokenList.add(token);
  }
  if (s > maxStart)   maxStart=s;
}
