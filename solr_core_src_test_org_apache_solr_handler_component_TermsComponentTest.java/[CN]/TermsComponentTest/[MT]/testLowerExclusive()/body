{
  assertQ(req("indent","true","qt","/terms","terms","true","terms.fl","lowerfilt","terms.lower","a","terms.lower.incl","false","terms.upper","b"),"count(//lst[@name='lowerfilt']/*)=5","//int[@name='aa'] ","//int[@name='aaa'] ","//int[@name='ab'] ","//int[@name='abb'] ","//int[@name='abc'] ");
  assertQ(req("indent","true","qt","/terms","terms","true","terms.fl","standardfilt","terms.lower","cc","terms.lower.incl","false","terms.upper","d"),"count(//lst[@name='standardfilt']/*)=2");
}
