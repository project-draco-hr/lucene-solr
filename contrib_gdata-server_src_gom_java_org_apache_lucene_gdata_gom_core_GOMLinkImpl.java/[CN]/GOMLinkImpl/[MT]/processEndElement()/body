{
  if (this.href == null)   throw new GDataParseException(String.format(MISSING_ELEMENT_ATTRIBUTE,this.localName,"href"));
  try {
    AtomParserUtils.getAbsolutAtomURI(this.xmlBase,this.href);
  }
 catch (  URISyntaxException e) {
    throw new GDataParseException(String.format(INVALID_ATTRIBUTE,"href","absolute uri"),e);
  }
}
