{
  super.init(config,loader);
  String accuracy=(String)config.get(ACCURACY);
  threshold=config.get(THRESHOLD_TOKEN_FREQUENCY) == null ? 0.0f : (Float)config.get(THRESHOLD_TOKEN_FREQUENCY);
  if (accuracy != null) {
    try {
      this.accuracy=Float.parseFloat(accuracy);
      spellChecker.setAccuracy(this.accuracy);
    }
 catch (    NumberFormatException e) {
      throw new RuntimeException("Unparseable accuracy given for dictionary: " + name,e);
    }
  }
  initSourceReader();
  return name;
}
