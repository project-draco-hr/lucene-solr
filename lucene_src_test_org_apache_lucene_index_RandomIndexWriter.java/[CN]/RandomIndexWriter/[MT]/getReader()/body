{
  if (w.codecs.getWriter(null).name.equals("PreFlex")) {
    w.commit();
    return IndexReader.open(w.getDirectory(),null,false,_TestUtil.nextInt(r,1,10),_TestUtil.alwaysCodec(new PreFlexCodec()));
  }
  if (r.nextBoolean()) {
    return w.getReader();
  }
 else {
    w.commit();
    return IndexReader.open(w.getDirectory(),new KeepOnlyLastCommitDeletionPolicy(),r.nextBoolean(),_TestUtil.nextInt(r,1,10));
  }
}
