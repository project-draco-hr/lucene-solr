{
  checkCluster(cluster,expectedSubclusterCount > 0);
  assertEquals("number of docs in cluster",expectedDocCount,getDocs(cluster).size());
  assertEquals("number of labels in cluster",expectedLabelCount,getLabels(cluster).size());
  if (expectedSubclusterCount > 0) {
    List<NamedList<Object>> subclusters=getSubclusters(cluster);
    assertEquals("numClusters",expectedSubclusterCount,subclusters.size());
    assertEquals("number of subclusters in cluster",expectedSubclusterCount,subclusters.size());
  }
}
