{
  StringBuilder b=new StringBuilder();
  FacetResultNode node=fr.getFacetResultNode();
  b.append(node.label);
  b.append(":");
  for (  FacetResultNode childNode : node.subResults) {
    b.append(' ');
    b.append(childNode.label.components[1]);
    b.append('=');
    b.append((int)childNode.value);
  }
  return b.toString();
}
