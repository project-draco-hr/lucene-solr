{
  T strategy=fieldStrategyMap.get(fieldName);
  if (strategy == null) {
synchronized (fieldStrategyMap) {
      strategy=fieldStrategyMap.get(fieldName);
      if (strategy == null) {
        strategy=newSpatialStrategy(fieldName);
        fieldStrategyMap.put(fieldName,strategy);
      }
    }
  }
  return strategy;
}
