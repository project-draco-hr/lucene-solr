{
  Query q1=new TermQuery(new Term("foo","bar"));
  q1.setBoost(2);
  Query q2=q1.clone();
  q2.setBoost(3);
  Query q3=q1.clone();
  q3.setBoost(4);
  UsageTrackingQueryCachingPolicy policy=new UsageTrackingQueryCachingPolicy();
  policy.onUse(q1);
  policy.onUse(q2);
  assertEquals(2,policy.frequency(q3));
}
