{
  final BytesRef payload=postings.getPayload();
  readPayload=true;
  final byte[] bytes;
  if (payload != null) {
    bytes=new byte[payload.length];
    System.arraycopy(payload.bytes,payload.offset,bytes,0,payload.length);
  }
 else {
    bytes=null;
  }
  return Collections.singletonList(bytes);
}
