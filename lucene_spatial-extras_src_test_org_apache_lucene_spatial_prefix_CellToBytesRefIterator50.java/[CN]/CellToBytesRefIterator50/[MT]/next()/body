{
  if (repeatCell != null) {
    bytesRef=repeatCell.getTokenBytesWithLeaf(bytesRef);
    repeatCell=null;
    return bytesRef;
  }
  if (!cellIter.hasNext()) {
    return null;
  }
  Cell cell=cellIter.next();
  bytesRef=cell.getTokenBytesNoLeaf(bytesRef);
  if (cell.isLeaf()) {
    repeatCell=cell;
  }
  return bytesRef;
}
