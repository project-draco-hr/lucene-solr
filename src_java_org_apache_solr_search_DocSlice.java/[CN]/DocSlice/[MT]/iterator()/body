{
  return new DocIterator(){
    int pos=offset;
    final int end=offset + len;
    public boolean hasNext(){
      return pos < end;
    }
    public Integer next(){
      return nextDoc();
    }
    public void remove(){
    }
    public int nextDoc(){
      return docs[pos++];
    }
    public float score(){
      return scores[pos - 1];
    }
  }
;
}
