{
  output.writeString(name);
  output.writeInt(docCount);
  output.writeLong(delGen);
  if (normGen == null) {
    output.writeInt(-1);
  }
 else {
    output.writeInt(normGen.length);
    for (int j=0; j < normGen.length; j++) {
      output.writeLong(normGen[j]);
    }
  }
  output.writeByte(isCompoundFile);
}
