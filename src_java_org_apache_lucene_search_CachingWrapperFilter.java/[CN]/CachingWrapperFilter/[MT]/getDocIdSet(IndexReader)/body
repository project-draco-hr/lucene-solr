{
  if (cache == null) {
    cache=new WeakHashMap();
  }
synchronized (cache) {
    DocIdSet cached=(DocIdSet)cache.get(reader);
    if (cached != null) {
      return cached;
    }
  }
  final DocIdSet docIdSet=filter.getDocIdSet(reader);
synchronized (cache) {
    cache.put(reader,docIdSet);
  }
  return docIdSet;
}
