{
  if (ft != null) {
    try {
      CharsRef readable=new CharsRef();
      ft.indexedToReadable(val,readable);
      out.append(readable);
    }
 catch (    Exception e) {
      out.append("EXCEPTION(val=");
      out.append(val.utf8ToString());
      out.append(")");
    }
  }
 else {
    out.append(val.utf8ToString());
  }
}
