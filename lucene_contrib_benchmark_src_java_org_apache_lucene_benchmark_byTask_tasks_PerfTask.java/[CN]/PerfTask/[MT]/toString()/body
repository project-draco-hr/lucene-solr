{
  String padd=getPadding();
  StringBuffer sb=new StringBuffer(padd);
  if (disableCounting) {
    sb.append('-');
  }
  sb.append(getName());
  if (getRunInBackground()) {
    sb.append(" &");
    int x=getBackgroundDeltaPriority();
    if (x != 0) {
      sb.append(x);
    }
  }
  return sb.toString();
}
