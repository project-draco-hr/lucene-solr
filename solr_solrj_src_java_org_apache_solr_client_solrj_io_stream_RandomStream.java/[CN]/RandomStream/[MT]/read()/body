{
  if (documentIterator.hasNext()) {
    Map map=new HashMap();
    SolrDocument doc=documentIterator.next();
    for (    String key : doc.keySet()) {
      map.put(key,doc.get(key));
    }
    return new Tuple(map);
  }
 else {
    Map fields=new HashMap();
    fields.put("EOF",true);
    Tuple tuple=new Tuple(fields);
    return tuple;
  }
}
