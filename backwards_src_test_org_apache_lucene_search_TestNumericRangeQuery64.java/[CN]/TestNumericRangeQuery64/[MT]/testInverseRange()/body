{
  NumericRangeFilter<Long> f=NumericRangeFilter.newLongRange("field8",8,1000L,-1000L,true,true);
  assertSame("A inverse range should return the EMPTY_DOCIDSET instance",DocIdSet.EMPTY_DOCIDSET,f.getDocIdSet(searcher.getIndexReader()));
  f=NumericRangeFilter.newLongRange("field8",8,Long.MAX_VALUE,null,false,false);
  assertSame("A exclusive range starting with Long.MAX_VALUE should return the EMPTY_DOCIDSET instance",DocIdSet.EMPTY_DOCIDSET,f.getDocIdSet(searcher.getIndexReader()));
  f=NumericRangeFilter.newLongRange("field8",8,null,Long.MIN_VALUE,false,false);
  assertSame("A exclusive range ending with Long.MIN_VALUE should return the EMPTY_DOCIDSET instance",DocIdSet.EMPTY_DOCIDSET,f.getDocIdSet(searcher.getIndexReader()));
}
