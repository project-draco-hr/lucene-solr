{
  if (termEnum == null)   return false;
  boolean next=termEnum.next();
  if (next && termEnum.term().field() == field)   return true;
  termEnum.close();
  field=(String)fieldToReader.tailMap(field).firstKey();
  if (field != null) {
    termEnum=((IndexReader)fieldToReader.get(field)).terms();
    return true;
  }
  return false;
}
