{
  if (field == null)   return false;
  boolean next=enum.next();
  if (next && enum.term().field() == field)   return true;
  enum.close();
  field=(String)fieldToReader.tailMap(field).firstKey();
  if (field != null) {
    enum=((IndexReader)fieldToReader.get(field)).terms();
    return true;
  }
  return false;
}
