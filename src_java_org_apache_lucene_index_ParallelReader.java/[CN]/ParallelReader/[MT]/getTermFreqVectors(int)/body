{
  ArrayList results=new ArrayList();
  Iterator i=fieldToReader.entrySet().iterator();
  while (i.hasNext()) {
    Map.Entry e=(Map.Entry)i.next();
    IndexReader reader=(IndexReader)e.getKey();
    String field=(String)e.getValue();
    TermFreqVector vector=reader.getTermFreqVector(n,field);
    if (vector != null)     results.add(vector);
  }
  return (TermFreqVector[])results.toArray(new TermFreqVector[results.size()]);
}
