{
  if (fieldNumber < 0) {
    throw new IllegalArgumentException("Illegal field number: " + fieldNumber);
  }
  if (byNumberTable != null) {
    if (fieldNumber >= byNumberTable.length) {
      return null;
    }
    return byNumberTable[fieldNumber];
  }
 else {
    return byNumberMap.get(fieldNumber);
  }
}
