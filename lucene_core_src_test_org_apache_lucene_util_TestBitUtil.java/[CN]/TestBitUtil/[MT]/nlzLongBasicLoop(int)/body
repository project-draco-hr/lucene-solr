{
  long sumRes=0;
  while (iters-- >= 0) {
    for (int i=1; i <= 63; i++) {
      long a=testArg(i);
      sumRes+=Long.numberOfLeadingZeros(a);
      sumRes+=Long.numberOfLeadingZeros(a + 1);
      sumRes+=Long.numberOfLeadingZeros(a - 1);
      sumRes+=Long.numberOfLeadingZeros(a + 10);
      sumRes+=Long.numberOfLeadingZeros(a - 10);
    }
  }
  return sumRes;
}
