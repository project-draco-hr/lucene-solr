{
  long bytesUsed=RamUsageEstimator.alignObjectSize(baseRamBytesUsed()) + RamUsageEstimator.NUM_BYTES_LONG + RamUsageEstimator.sizeOf(pending)+ RamUsageEstimator.sizeOf(minValues)+ RamUsageEstimator.alignObjectSize(RamUsageEstimator.NUM_BYTES_ARRAY_HEADER + (long)RamUsageEstimator.NUM_BYTES_OBJECT_REF * deltas.length);
  return bytesUsed + deltasBytes;
}
