{
  parent.onIndexOutputClosed(name);
  if (isOpen) {
    boolean success=false;
    try {
      super.close();
      success=true;
    }
  finally {
      isOpen=false;
      if (success) {
        IOUtils.close(file);
      }
 else {
        IOUtils.closeWhileHandlingException(file);
      }
    }
  }
}
