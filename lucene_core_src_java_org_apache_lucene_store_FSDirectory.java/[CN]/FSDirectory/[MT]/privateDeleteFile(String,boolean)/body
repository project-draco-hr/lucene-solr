{
  try {
    Files.delete(directory.resolve(name));
    pendingDeletes.remove(name);
  }
 catch (  NoSuchFileException|FileNotFoundException e) {
    pendingDeletes.remove(name);
    if (isPendingDelete && Constants.WINDOWS) {
    }
 else {
      throw e;
    }
  }
catch (  IOException ioe) {
    pendingDeletes.add(name);
  }
}
