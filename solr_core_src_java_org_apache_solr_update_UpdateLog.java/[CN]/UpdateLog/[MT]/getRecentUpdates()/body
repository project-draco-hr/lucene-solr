{
  Deque<TransactionLog> logList;
synchronized (this) {
    logList=new LinkedList<>(logs);
    for (    TransactionLog log : logList) {
      log.incref();
    }
    if (prevTlog != null) {
      prevTlog.incref();
      logList.addFirst(prevTlog);
    }
    if (tlog != null) {
      tlog.incref();
      logList.addFirst(tlog);
    }
  }
  return new RecentUpdates(logList);
}
