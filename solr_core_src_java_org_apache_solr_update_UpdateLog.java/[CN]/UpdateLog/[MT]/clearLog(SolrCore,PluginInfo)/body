{
  if (ulogPluginInfo == null)   return;
  File tlogDir=new File(getTlogDir(core,ulogPluginInfo));
  if (tlogDir.exists()) {
    String[] files=getLogList(tlogDir);
    for (    String file : files) {
      File f=new File(tlogDir,file);
      boolean s=f.delete();
      if (!s) {
        log.error("Could not remove tlog file:" + f);
      }
    }
  }
}
