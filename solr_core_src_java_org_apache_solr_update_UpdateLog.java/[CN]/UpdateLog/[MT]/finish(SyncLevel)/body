{
  if (syncLevel == null) {
    syncLevel=defaultSyncLevel;
  }
  if (syncLevel == SyncLevel.NONE) {
    return;
  }
  TransactionLog currLog;
synchronized (this) {
    currLog=tlog;
    if (currLog == null)     return;
    currLog.incref();
  }
  try {
    currLog.finish(syncLevel);
  }
  finally {
    currLog.decref();
  }
}
