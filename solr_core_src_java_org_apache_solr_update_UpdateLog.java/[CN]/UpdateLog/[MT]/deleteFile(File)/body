{
  boolean success=false;
  try {
    success=file.delete();
    if (!success) {
      log.error("Error deleting file: " + file);
    }
  }
 catch (  Exception e) {
    log.error("Error deleting file: " + file,e);
  }
  if (!success) {
    try {
      file.deleteOnExit();
    }
 catch (    Exception e) {
      log.error("Error deleting file on exit: " + file,e);
    }
  }
}
