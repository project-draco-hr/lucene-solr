{
synchronized (this) {
    try {
      RefCounted<SolrIndexSearcher> holder=uhandler.core.openNewSearcher(true,true);
      holder.decref();
    }
 catch (    Throwable e) {
      SolrException.log(log,"Error opening realtime searcher for deleteByQuery",e);
    }
    map.clear();
    oldDeletes.clear();
    deleteByQueries.clear();
  }
}
