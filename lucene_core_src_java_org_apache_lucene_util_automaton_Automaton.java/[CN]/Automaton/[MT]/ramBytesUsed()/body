{
  return RamUsageEstimator.NUM_BYTES_OBJECT_HEADER + RamUsageEstimator.sizeOf(states) + RamUsageEstimator.sizeOf(transitions)+ RamUsageEstimator.NUM_BYTES_OBJECT_HEADER+ (isAccept.size() / 8)+ RamUsageEstimator.NUM_BYTES_OBJECT_REF+ 2 * RamUsageEstimator.NUM_BYTES_OBJECT_REF + 3 * Integer.BYTES + 1;
}
