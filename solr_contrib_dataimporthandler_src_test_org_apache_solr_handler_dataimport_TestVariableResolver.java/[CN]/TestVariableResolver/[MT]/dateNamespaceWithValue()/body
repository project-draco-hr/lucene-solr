{
  VariableResolver vri=new VariableResolver();
  vri.setEvaluators(new DataImporter().getEvaluators(Collections.<Map<String,String>>emptyList()));
  Map<String,Object> ns=new HashMap<String,Object>();
  Date d=new Date();
  ns.put("dt",d);
  vri.addNamespace("A",ns);
  assertEquals(new SimpleDateFormat("yyyy-MM-dd HH:mm:ss",Locale.ROOT).format(d),vri.replaceTokens("${dataimporter.functions.formatDate(A.dt,'yyyy-MM-dd HH:mm:ss')}"));
}
