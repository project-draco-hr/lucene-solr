{
  double bytes=(double)lbytes;
  if (bytes >= ONEGBYTE) {
    return fractionFormat.format((bytes / ONEGBYTE)) + " GB";
  }
 else   if (bytes >= MBYTE) {
    return fractionFormat.format(bytes / MBYTE) + " MB";
  }
 else   if (bytes >= KBYTE) {
    return fractionFormat.format(bytes / KBYTE) + " KB";
  }
 else {
    return fractionFormat.format(bytes) + " Bytes";
  }
}
