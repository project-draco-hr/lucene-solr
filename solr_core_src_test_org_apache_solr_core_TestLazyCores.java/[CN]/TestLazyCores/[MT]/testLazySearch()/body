{
  CoreContainer cc=init();
  try {
    checkNotInCores(cc,"collectionLazy4");
    SolrCore core4=cc.getCore("collectionLazy4");
    checkSearch(core4);
    assertQ("test raw query",req("q","{!raw f=v_t}hello","wt","xml"),"//result[@numFound='0']");
    checkInCores(cc,"collectionLazy4");
    core4.close();
  }
  finally {
    cc.shutdown();
  }
}
