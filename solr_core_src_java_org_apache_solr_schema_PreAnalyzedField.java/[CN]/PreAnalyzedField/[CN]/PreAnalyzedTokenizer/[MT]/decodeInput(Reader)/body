{
  removeAllAttributes();
  cachedStates.clear();
  stringValue=null;
  binaryValue=null;
  try {
    ParseResult res=parser.parse(reader,this);
    if (res != null) {
      stringValue=res.str;
      binaryValue=res.bin;
      if (res.states != null) {
        cachedStates.addAll(res.states);
      }
    }
  }
 catch (  IOException e) {
    removeAllAttributes();
    throw e;
  }
}
