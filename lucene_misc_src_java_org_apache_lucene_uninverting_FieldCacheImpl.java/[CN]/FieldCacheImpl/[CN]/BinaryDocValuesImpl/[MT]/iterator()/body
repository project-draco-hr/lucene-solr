{
  final BytesRef term=new BytesRef();
  return new BinaryDocValues(){
    @Override public BytesRef get(    int docID){
      final int pointer=(int)docToOffset.get(docID);
      if (pointer == 0) {
        term.length=0;
      }
 else {
        bytes.fill(term,pointer);
      }
      return term;
    }
  }
;
}
