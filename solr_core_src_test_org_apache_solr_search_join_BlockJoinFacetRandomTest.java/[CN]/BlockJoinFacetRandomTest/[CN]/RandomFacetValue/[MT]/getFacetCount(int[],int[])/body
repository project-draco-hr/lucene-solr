{
  int result=0;
  if (parentNumbers != null) {
    for (    int parentNumber : parentNumbers) {
      if (postings[parentNumber].isMatched(childNumbers)) {
        result++;
      }
    }
  }
 else {
    for (int i=0; i < NUMBER_OF_PARENTS; i++) {
      if (postings[i].isMatched(childNumbers)) {
        result++;
      }
    }
  }
  return result;
}
