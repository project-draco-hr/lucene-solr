{
  int[] randomParents=getRandomArray(NUMBER_OF_PARENTS);
  int[] randomChildren=getRandomArray(NUMBER_OF_CHILDREN);
  int[] randomFacets=getRandomArray(facets.length);
  assertQ("Random facets for all parent docs should be calculated",req(randomFacetsRequest(randomParents,randomChildren,null,null,null,randomFacets)),expectedResponse(randomParents,randomChildren,randomFacets));
}
