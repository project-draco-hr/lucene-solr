{
  String s=f.stringValue();
  int len=s.length();
  if (len >= 2) {
    char ch=s.charAt(0);
    if ((ch == '0') || (ch == '-' && s.charAt(1) == '0')) {
      s=Integer.toString(Integer.parseInt(s));
    }
  }
 else   if (len == 0) {
    writer.writeNull(name);
    return;
  }
  writer.writeInt(name,s);
}
