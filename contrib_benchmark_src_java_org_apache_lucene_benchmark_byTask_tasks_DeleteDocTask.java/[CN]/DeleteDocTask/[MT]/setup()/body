{
  super.setup();
  if (logStep < 0) {
    logStep=getRunData().getConfig().get("doc.delete.log.step",DEFAULT_DELETE_DOC_LOG_STEP);
  }
  if (deleteStep < 0) {
    deleteStep=getRunData().getConfig().get("doc.delete.step",DEFAULT_DOC_DELETE_STEP);
  }
  docid=(byStep ? lastDeleted + deleteStep : docid);
}
