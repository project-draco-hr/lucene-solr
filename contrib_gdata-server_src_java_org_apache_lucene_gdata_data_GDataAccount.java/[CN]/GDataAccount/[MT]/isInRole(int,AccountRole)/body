{
  if (role == AccountRole.USER)   return (intRole & 1) > 0;
  if (role == AccountRole.ENTRYAMINISTRATOR)   return (intRole & 2) > 0;
 else   if (role == AccountRole.FEEDAMINISTRATOR)   return (intRole & 4) > 0;
 else   if (role == AccountRole.USERADMINISTRATOR)   return (intRole & 8) > 0;
  return false;
}
