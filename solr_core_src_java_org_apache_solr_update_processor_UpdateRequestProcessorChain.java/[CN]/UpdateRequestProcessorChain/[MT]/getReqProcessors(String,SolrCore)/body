{
  if (processor == null)   return Collections.emptyList();
  List<UpdateRequestProcessorFactory> result=new ArrayList<>();
  List<String> names=StrUtils.splitSmart(processor,',');
  for (  String s : names) {
    s=s.trim();
    if (s.isEmpty())     continue;
    UpdateRequestProcessorFactory p=core.getUpdateProcessors().get(s);
    if (p == null) {
      try {
        p=core.createInstance(s + "UpdateProcessorFactory",UpdateRequestProcessorFactory.class,"updateProcessor",null,core.getMemClassLoader());
        core.getUpdateProcessors().put(s,p);
      }
 catch (      SolrException e) {
      }
      if (p == null)       throw new SolrException(SolrException.ErrorCode.BAD_REQUEST,"No such processor " + s);
    }
    result.add(p);
  }
  return result;
}
