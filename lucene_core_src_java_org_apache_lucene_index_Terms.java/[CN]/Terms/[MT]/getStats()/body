{
  StringBuilder sb=new StringBuilder();
  sb.append("impl=" + getClass().getSimpleName());
  sb.append(",size=" + size());
  sb.append(",docCount=" + getDocCount());
  sb.append(",sumTotalTermFreq=" + getSumTotalTermFreq());
  sb.append(",sumDocFreq=" + getSumDocFreq());
  return sb.toString();
}
