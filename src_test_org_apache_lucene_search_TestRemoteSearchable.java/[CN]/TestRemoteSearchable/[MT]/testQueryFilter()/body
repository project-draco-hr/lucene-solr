{
  Searchable[] searchables={getRemote()};
  Searcher searcher=new MultiSearcher(searchables);
  Hits hits=searcher.search(new TermQuery(new Term("test","text")),new QueryWrapperFilter(new TermQuery(new Term("test","test"))));
  assertEquals(1,hits.length());
  Hits nohits=searcher.search(new TermQuery(new Term("test","text")),new QueryWrapperFilter(new TermQuery(new Term("test","non-existent-term"))));
  assertEquals(0,nohits.length());
}
