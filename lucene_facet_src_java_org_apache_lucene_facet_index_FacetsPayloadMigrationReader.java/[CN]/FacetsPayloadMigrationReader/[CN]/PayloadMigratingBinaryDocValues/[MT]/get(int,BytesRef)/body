{
  try {
    if (docID <= lastRequestedDocID) {
      dpe=getDPE();
      if (dpe == null) {
        curDocID=DocIdSetIterator.NO_MORE_DOCS;
      }
 else {
        curDocID=dpe.nextDoc();
      }
    }
    lastRequestedDocID=docID;
    if (curDocID > docID) {
      result.length=0;
      return;
    }
    if (curDocID < docID) {
      curDocID=dpe.advance(docID);
      if (curDocID != docID) {
        result.length=0;
        return;
      }
    }
    dpe.nextPosition();
    result.copyBytes(dpe.getPayload());
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
