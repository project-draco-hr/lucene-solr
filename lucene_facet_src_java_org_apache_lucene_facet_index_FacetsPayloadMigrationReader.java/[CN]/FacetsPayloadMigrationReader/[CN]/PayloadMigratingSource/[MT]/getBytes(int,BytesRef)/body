{
  if (curDocID > docID) {
    ref.length=0;
    return ref;
  }
  try {
    if (curDocID < docID) {
      curDocID=dpe.advance(docID);
      if (curDocID != docID) {
        ref.length=0;
        return ref;
      }
    }
    dpe.nextPosition();
    ref.copyBytes(dpe.getPayload());
    return ref;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
