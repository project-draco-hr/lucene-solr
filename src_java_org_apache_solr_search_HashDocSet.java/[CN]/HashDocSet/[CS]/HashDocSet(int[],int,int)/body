{
  int tsize=Math.max(nextHighestPowerOfTwo(len),1);
  if (tsize < len * inverseLoadfactor) {
    tsize<<=1;
  }
  tablesize=tsize;
  mask=tablesize - 1;
  table=new int[tablesize];
  for (int i=0; i < tablesize; i++)   table[i]=EMPTY;
  for (int i=offset; i < len; i++) {
    put(docs[i]);
  }
  size=len;
}
