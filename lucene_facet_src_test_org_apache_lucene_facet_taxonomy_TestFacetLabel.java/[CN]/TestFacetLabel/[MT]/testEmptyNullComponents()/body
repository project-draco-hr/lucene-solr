{
  String[][] components_tests=new String[][]{new String[]{"","test"},new String[]{"test",""},new String[]{"test","","foo"},new String[]{null,"test"},new String[]{"test",null},new String[]{"test",null,"foo"}};
  for (  String[] components : components_tests) {
    try {
      assertNotNull(new FacetLabel(components));
      fail("empty or null components should not be allowed: " + Arrays.toString(components));
    }
 catch (    IllegalArgumentException e) {
    }
  }
}
