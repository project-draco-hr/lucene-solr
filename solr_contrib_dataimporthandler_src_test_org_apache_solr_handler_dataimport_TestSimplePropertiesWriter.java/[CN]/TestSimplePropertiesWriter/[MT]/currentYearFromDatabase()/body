{
  Connection conn=null;
  Statement s=null;
  ResultSet rs=null;
  try {
    conn=newConnection();
    s=conn.createStatement();
    rs=s.executeQuery("select year(current_timestamp) from sysibm.sysdummy1");
    if (rs.next()) {
      return rs.getInt(1);
    }
    Assert.fail("We should have gotten a row from the db.");
  }
 catch (  SQLException e) {
    throw e;
  }
 finally {
    try {
      rs.close();
    }
 catch (    Exception ex) {
    }
    try {
      s.close();
    }
 catch (    Exception ex) {
    }
    try {
      conn.close();
    }
 catch (    Exception ex) {
    }
  }
  return 0;
}
