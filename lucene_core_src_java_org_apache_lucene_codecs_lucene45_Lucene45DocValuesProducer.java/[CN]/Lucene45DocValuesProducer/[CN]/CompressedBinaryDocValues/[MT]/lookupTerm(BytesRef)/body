{
  try {
    SeekStatus status=termsEnum.seekCeil(key);
    if (status == SeekStatus.END) {
      return -numValues - 1;
    }
 else     if (status == SeekStatus.FOUND) {
      return termsEnum.ord();
    }
 else {
      return -termsEnum.ord() - 1;
    }
  }
 catch (  IOException bogus) {
    throw new RuntimeException(bogus);
  }
}
