{
  final int charLength=s.length();
  int charIdx=0;
  int intIdx=0;
  while (charIdx < charLength) {
    if (intIdx == ir.ints.length) {
      ir.grow(intIdx + 1);
    }
    final int utf32=s.codePointAt(charIdx);
    ir.ints[intIdx]=utf32;
    charIdx+=Character.charCount(utf32);
    intIdx++;
  }
  ir.length=intIdx;
  return ir;
}
