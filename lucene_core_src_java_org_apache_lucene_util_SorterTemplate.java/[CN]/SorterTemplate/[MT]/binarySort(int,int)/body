{
  for (int i=lo + 1; i <= hi; ++i) {
    int l=lo;
    int h=i - 1;
    setPivot(i);
    while (l <= h) {
      final int mid=(l + h) >>> 1;
      final int cmp=comparePivot(mid);
      if (cmp < 0) {
        h=mid - 1;
      }
 else {
        l=mid + 1;
      }
    }
    for (int j=i; j > l; --j) {
      swap(j - 1,j);
    }
  }
}
