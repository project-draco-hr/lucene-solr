{
  final Directory dir1=newDirectory();
  TestIndexReaderReopen.createIndex(random,dir1,false);
  IndexReader r1=IndexReader.open(dir1,false);
  r1.incRef();
  IndexReader r2=r1.clone(false);
  r1.deleteDocument(5);
  r1.decRef();
  r1.incRef();
  r2.close();
  r1.decRef();
  r1.close();
  dir1.close();
}
