{
  this.info=info;
  this.delCount=delCount;
  this.delGen=delGen;
  if (delGen == -1) {
    nextWriteDelGen=1;
  }
 else {
    nextWriteDelGen=delGen + 1;
  }
  this.docValuesGen=docValuesGen;
  if (docValuesGen == -1) {
    nextWriteDocValuesGen=1;
  }
 else {
    nextWriteDocValuesGen=docValuesGen + 1;
  }
}
