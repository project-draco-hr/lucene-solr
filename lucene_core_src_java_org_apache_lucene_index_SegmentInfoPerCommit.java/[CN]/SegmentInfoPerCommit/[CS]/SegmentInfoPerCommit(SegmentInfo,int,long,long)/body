{
  this.info=info;
  this.delCount=delCount;
  this.delGen=delGen;
  if (delGen == -1) {
    nextWriteDelGen=1;
  }
 else {
    nextWriteDelGen=delGen + 1;
  }
  this.fieldInfosGen=fieldInfosGen;
  if (fieldInfosGen == -1) {
    nextWriteFieldInfosGen=1;
  }
 else {
    nextWriteFieldInfosGen=fieldInfosGen + 1;
  }
}
