{
  if (logic.length != chain.length)   throw new IllegalArgumentException("Invalid number of elements in logic array");
  OpenBitSetDISI result=initialResult(info,logic[0],index);
  for (; index[0] < chain.length; index[0]++) {
    doChain(result,logic[index[0]],chain[index[0]].getDocIdSet(info));
  }
  return result;
}
