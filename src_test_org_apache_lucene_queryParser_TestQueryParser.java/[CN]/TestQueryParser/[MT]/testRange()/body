{
  assertQueryEquals("[ a z]",null,"[a-z]");
  assertQueryEquals("[ a z ]",null,"[a-z]");
  assertQueryEquals("{ a z}",null,"{a-z}");
  assertQueryEquals("{ a z }",null,"{a-z}");
  assertQueryEquals("{ a z }^2.0",null,"{a-z}^2.0");
  assertQueryEquals("[ a z] OR bar",null,"[a-z] bar");
  assertQueryEquals("[ a z] AND bar",null,"+[a-z] +bar");
  assertQueryEquals("( bar blar { a z}) ",null,"(bar blar {a-z})");
}
