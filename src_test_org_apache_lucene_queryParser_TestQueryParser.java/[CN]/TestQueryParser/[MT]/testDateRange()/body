{
  String startDate=getLocalizedDate(2002,1,1);
  String endDate=getLocalizedDate(2002,1,4);
  assertQueryEquals("[ " + startDate + " TO "+ endDate+ "]",null,"[" + getDate(startDate) + "-"+ getDate(endDate)+ "]");
  assertQueryEquals("{  " + startDate + "    "+ endDate+ "   }",null,"{" + getDate(startDate) + "-"+ getDate(endDate)+ "}");
}
