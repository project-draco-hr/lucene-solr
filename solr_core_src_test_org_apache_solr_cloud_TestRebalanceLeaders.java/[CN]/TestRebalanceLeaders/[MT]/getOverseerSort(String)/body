{
  List<String> ret=null;
  try {
    ret=OverseerCollectionProcessor.getSortedElectionNodes(cloudClient.getZkStateReader().getZkClient(),"/collections/" + COLLECTION_NAME + "/leader_elect/"+ key+ "/election");
    return ret;
  }
 catch (  KeeperException e) {
    cloudClient.connect();
  }
catch (  InterruptedException e) {
    return null;
  }
  return null;
}
