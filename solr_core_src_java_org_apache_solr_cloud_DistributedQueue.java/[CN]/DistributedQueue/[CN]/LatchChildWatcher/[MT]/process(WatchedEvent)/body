{
  Event.EventType eventType=event.getType();
  LOG.info("LatchChildWatcher fired on path: " + event.getPath() + " state: "+ event.getState()+ " type "+ eventType);
  if (eventType == Event.EventType.NodeChildrenChanged) {
synchronized (lock) {
      this.event=event;
      lock.notifyAll();
    }
  }
}
