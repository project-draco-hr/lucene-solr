{
  if (Event.EventType.None.equals(event.getType()) && !Event.KeeperState.Expired.equals(event.getState())) {
    return;
  }
  updateLock.lock();
  try {
    if (lastWatcher == this) {
      lastWatcher=null;
    }
    isDirty=true;
    notEmpty.signalAll();
  }
  finally {
    updateLock.unlock();
  }
}
