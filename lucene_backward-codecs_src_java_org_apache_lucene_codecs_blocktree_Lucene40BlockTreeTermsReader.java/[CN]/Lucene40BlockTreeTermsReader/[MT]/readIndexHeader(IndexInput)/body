{
  int version=CodecUtil.checkHeader(input,Lucene40BlockTreeTermsWriter.TERMS_INDEX_CODEC_NAME,Lucene40BlockTreeTermsWriter.VERSION_START,Lucene40BlockTreeTermsWriter.VERSION_CURRENT);
  if (version < Lucene40BlockTreeTermsWriter.VERSION_APPEND_ONLY) {
    indexDirOffset=input.readLong();
  }
  return version;
}
