{
  if (length == other.length) {
    int otherUpto=other.offset;
    final double[] otherFloats=other.floats;
    final int end=offset + length;
    for (int upto=offset; upto < end; upto++, otherUpto++) {
      if (floats[upto] != otherFloats[otherUpto]) {
        return false;
      }
    }
    return true;
  }
 else {
    return false;
  }
}
