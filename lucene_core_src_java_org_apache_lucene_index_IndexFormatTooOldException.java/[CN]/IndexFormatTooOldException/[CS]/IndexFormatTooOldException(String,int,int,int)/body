{
  super("Format version is not supported (resource " + resourceDescription + "): "+ version+ " (needs to be between "+ minVersion+ " and "+ maxVersion+ "). This version of Lucene only supports indexes created with release 6.0 and later.");
  this.resourceDescription=resourceDescription;
  this.version=version;
  this.minVersion=minVersion;
  this.maxVersion=maxVersion;
  this.reason=null;
}
