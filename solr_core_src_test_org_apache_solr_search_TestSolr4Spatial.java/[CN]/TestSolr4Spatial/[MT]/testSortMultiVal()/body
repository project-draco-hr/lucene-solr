{
  RandomizedTest.assumeFalse("Multivalue not supported for this field",fieldName.equals("twodoubles"));
  assertU(adoc("id","100",fieldName,"1,2"));
  assertU(adoc("id","101",fieldName,"4,-1",fieldName,"3,5"));
  assertU(commit());
  assertJQ(req("q","{! score=distance}" + fieldName + ":\"Intersects(Circle(3,4 d=9))\"","fl","id,score","sort","score asc"),1e-4,"/response/docs/[0]/id=='101'","/response/docs/[0]/score==0.99862987");
}
