{
  super(args);
  assureMatchVersion();
  stopWordFiles=get(args,"words");
  format=get(args,"format",(null == stopWordFiles ? null : FORMAT_WORDSET));
  ignoreCase=getBoolean(args,"ignoreCase",false);
  if (!args.isEmpty()) {
    throw new IllegalArgumentException("Unknown parameters: " + args);
  }
}
