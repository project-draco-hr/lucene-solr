{
  if (!reader.hasDeletions()) {
    return;
  }
  Bits bits=MultiFields.getLiveDocs(reader);
  DocIdSetIterator it=set.iterator();
  int doc=DocIdSetIterator.NO_MORE_DOCS;
  while ((doc=it.nextDoc()) != DocIdSetIterator.NO_MORE_DOCS) {
    if (!bits.get(doc)) {
      set.fastClear(doc);
    }
  }
}
