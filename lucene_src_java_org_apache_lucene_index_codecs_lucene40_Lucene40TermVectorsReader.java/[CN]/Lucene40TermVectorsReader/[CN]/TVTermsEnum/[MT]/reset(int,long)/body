{
  this.numTerms=numTerms;
  nextTerm=0;
  tvf.seek(tvfFPStart);
  final byte bits=tvf.readByte();
  storePositions=(bits & STORE_POSITIONS_WITH_TERMVECTOR) != 0;
  storeOffsets=(bits & STORE_OFFSET_WITH_TERMVECTOR) != 0;
  tvfFP=1 + tvfFPStart;
  positions=null;
  startOffsets=null;
  endOffsets=null;
}
