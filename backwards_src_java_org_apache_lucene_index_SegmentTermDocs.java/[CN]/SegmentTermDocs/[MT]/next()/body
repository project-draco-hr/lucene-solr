{
  while (true) {
    if (count == df)     return false;
    final int docCode=freqStream.readVInt();
    if (currentFieldOmitTermFreqAndPositions) {
      doc+=docCode;
      freq=1;
    }
 else {
      doc+=docCode >>> 1;
      if ((docCode & 1) != 0)       freq=1;
 else       freq=freqStream.readVInt();
    }
    count++;
    if (deletedDocs == null || !deletedDocs.get(doc))     break;
    skippingDoc();
  }
  return true;
}
