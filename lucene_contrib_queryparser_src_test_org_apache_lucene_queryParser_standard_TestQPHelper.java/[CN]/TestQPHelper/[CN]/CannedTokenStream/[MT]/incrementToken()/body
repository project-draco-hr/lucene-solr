{
  clearAttributes();
  if (upto == 4) {
    return false;
  }
  if (upto == 0) {
    posIncr.setPositionIncrement(1);
    term.setTermBuffer("a");
  }
 else   if (upto == 1) {
    posIncr.setPositionIncrement(1);
    term.setTermBuffer("b");
  }
 else   if (upto == 2) {
    posIncr.setPositionIncrement(0);
    term.setTermBuffer("c");
  }
 else {
    posIncr.setPositionIncrement(0);
    term.setTermBuffer("d");
  }
  upto++;
  return true;
}
