{
  if (position + 1 > length)   throw new IOException(file.getName() + ": Reading past end of file");
  int blockPos=(int)(position++ & JEIndexOutput.BLOCK_MASK);
  byte b=block.getData()[blockPos];
  if (blockPos + 1 == JEIndexOutput.BLOCK_LEN) {
    block.seek(position);
    block.get(directory);
  }
  return b;
}
