{
  if (this.fieldName == null || this.content == null)   throw new GdataIndexerException("Required field not set fieldName: " + this.fieldName + " content: "+ this.content);
  if (this.content.length() == 0) {
    return new Field[0];
  }
  Field retValue=new Field(this.fieldName,this.content,this.store,this.index);
  float boost=this.config.getBoost();
  if (boost != 1.0f)   retValue.setBoost(boost);
  return new Field[]{retValue};
}
