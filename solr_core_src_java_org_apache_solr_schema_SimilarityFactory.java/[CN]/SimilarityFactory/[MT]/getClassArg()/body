{
  if (null != params) {
    String className=params.get(CLASS_NAME);
    if (null != className) {
      return className;
    }
  }
  String className=getClass().getName();
  if (className.startsWith("org.apache.solr.schema.IndexSchema$")) {
    className=getSimilarity().getClass().getName();
  }
  return className;
}
