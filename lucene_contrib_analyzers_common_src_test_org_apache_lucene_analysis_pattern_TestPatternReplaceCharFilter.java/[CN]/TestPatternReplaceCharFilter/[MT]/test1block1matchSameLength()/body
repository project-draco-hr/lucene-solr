{
  final String BLOCK="aa bb cc";
  CharStream cs=new PatternReplaceCharFilter(pattern("(aa)\\s+(bb)\\s+(cc)"),"$1#$2#$3",CharReader.get(new StringReader(BLOCK)));
  TokenStream ts=new WhitespaceTokenizer(TEST_VERSION_CURRENT,cs);
  assertTokenStreamContents(ts,new String[]{"aa#bb#cc"},new int[]{0},new int[]{8});
}
