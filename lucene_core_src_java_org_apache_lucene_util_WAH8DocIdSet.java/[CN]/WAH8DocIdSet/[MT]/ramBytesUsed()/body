{
  if (this == EMPTY) {
    return 0L;
  }
  long ramBytesUsed=BASE_RAM_BYTES_USED + RamUsageEstimator.sizeOf(data);
  if (positions != SINGLE_ZERO_BUFFER) {
    ramBytesUsed+=positions.ramBytesUsed();
  }
  if (wordNums != SINGLE_ZERO_BUFFER) {
    ramBytesUsed+=wordNums.ramBytesUsed();
  }
  return ramBytesUsed;
}
