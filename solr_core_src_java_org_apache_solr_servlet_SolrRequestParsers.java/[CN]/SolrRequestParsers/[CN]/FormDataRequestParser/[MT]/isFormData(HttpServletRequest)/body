{
  String contentType=req.getContentType();
  if (contentType != null) {
    int idx=contentType.indexOf(';');
    if (idx > 0) {
      contentType=contentType.substring(0,idx);
    }
    contentType=contentType.trim();
    if ("application/x-www-form-urlencoded".equalsIgnoreCase(contentType)) {
      return true;
    }
  }
  return false;
}
