{
  assertJQ(req("defType","edismax","q","*","df","doesnotexist_s"),"/response/docs/[0]==");
  assertJQ(req("defType","edismax","q","doesnotexist_s:*"),"/response/numFound==0");
  assertJQ(req("defType","edismax","q","doesnotexist_s:*"),"/response/numFound==0");
  assertJQ(req("defType","edismax","q","doesnotexist_s:( * * * )"),"/response/numFound==0");
}
