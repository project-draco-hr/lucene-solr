{
  assertQ(req("q","tekna","qf","text_sw","defType","edismax","bq","id:54^100","bq","id:53^10","fq","id:[52 TO 54]","fl","id,score"),"//doc[1]/str[@name='id'][.='54']","//doc[2]/str[@name='id'][.='53']","//doc[3]/str[@name='id'][.='52']");
  assertQ(req("q","tekna","qf","text_sw","defType","edismax","bq","(text_sw:blasdfadsf id:54)^100","bq","id:[53 TO 53]^10","fq","id:[52 TO 54]","fl","id,score"),"//doc[1]/str[@name='id'][.='54']","//doc[2]/str[@name='id'][.='53']","//doc[3]/str[@name='id'][.='52']");
  assertQ(req("q","tekna","qf","text_sw","defType","edismax","bq","(*:* -id:54)^100","bq","id:53^10","bq","id:52","fq","id:[52 TO 54]","fl","id,score"),"//doc[1]/str[@name='id'][.='53']","//doc[2]/str[@name='id'][.='52']","//doc[3]/str[@name='id'][.='54']");
}
