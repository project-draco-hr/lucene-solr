{
  if (urls == null)   return null;
  List<List<String>> shardUrls=new ArrayList<>(urls.size());
  List<String> list=null;
  int sz;
  if (numShards == null) {
    numShards=urls.size();
  }
  sz=(int)Math.ceil(urls.size() / (float)numShards);
  for (int i=0; i < urls.size(); i++) {
    if (i % sz == 0) {
      list=new ArrayList<>();
      shardUrls.add(list);
    }
    list.add((String)urls.get(i));
  }
  return shardUrls;
}
