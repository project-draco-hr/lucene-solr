{
  BufferedReader reader=new BufferedReader(new InputStreamReader(fs.open(fullInputList),StandardCharsets.UTF_8));
  try {
    String line;
    while ((line=reader.readLine()) != null) {
      runner.map(line,job.getConfiguration(),null);
    }
    runner.cleanup();
  }
  finally {
    reader.close();
  }
}
