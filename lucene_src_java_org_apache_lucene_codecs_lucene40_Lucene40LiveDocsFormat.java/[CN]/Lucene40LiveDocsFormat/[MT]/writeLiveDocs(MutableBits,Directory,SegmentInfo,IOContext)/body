{
  String filename=IndexFileNames.fileNameFromGeneration(info.name,IndexFileNames.DELETES_EXTENSION,info.getDelGen());
  boolean success=false;
  try {
    ((BitVector)bits).write(dir,filename,context);
    success=true;
  }
  finally {
    if (!success) {
      try {
        dir.deleteFile(filename);
      }
 catch (      Throwable t) {
      }
    }
  }
}
