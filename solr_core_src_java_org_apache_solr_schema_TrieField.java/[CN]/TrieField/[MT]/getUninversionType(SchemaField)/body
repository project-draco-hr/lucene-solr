{
  if (sf.multiValued()) {
switch (type) {
case INTEGER:
      return Type.SORTED_SET_INTEGER;
case LONG:
case DATE:
    return Type.SORTED_SET_LONG;
case FLOAT:
  return Type.SORTED_SET_FLOAT;
case DOUBLE:
return Type.SORTED_SET_DOUBLE;
default :
throw new AssertionError();
}
}
 else {
switch (type) {
case INTEGER:
return Type.INTEGER;
case LONG:
case DATE:
return Type.LONG;
case FLOAT:
return Type.FLOAT;
case DOUBLE:
return Type.DOUBLE;
default :
throw new AssertionError();
}
}
}
