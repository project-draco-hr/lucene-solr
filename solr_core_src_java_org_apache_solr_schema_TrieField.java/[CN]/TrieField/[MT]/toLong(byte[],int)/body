{
  int high=(arr[offset] << 24) | ((arr[offset + 1] & 0xff) << 16) | ((arr[offset + 2] & 0xff) << 8)| (arr[offset + 3] & 0xff);
  int low=(arr[offset + 4] << 24) | ((arr[offset + 5] & 0xff) << 16) | ((arr[offset + 6] & 0xff) << 8)| (arr[offset + 7] & 0xff);
  return (((long)high) << 32) | (low & 0x0ffffffffL);
}
