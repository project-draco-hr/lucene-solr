{
  if (!rb.doTerms || (sreq.purpose & ShardRequest.PURPOSE_GET_TERMS) == 0) {
    return;
  }
  TermsHelper th=rb._termsHelper;
  if (th != null) {
    for (    ShardResponse srsp : sreq.responses) {
      @SuppressWarnings("unchecked") NamedList<Object> terms=(NamedList<Object>)srsp.getSolrResponse().getResponse().get("terms");
      th.parse(terms);
    }
  }
}
