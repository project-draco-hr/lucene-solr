{
  float norm=lastNorm + normDelta;
  do {
    float norm1=Similarity.decodeNorm(Similarity.encodeNorm(norm));
    if (norm1 > lastNorm) {
      norm=norm1;
      break;
    }
    norm+=normDelta;
  }
 while (true);
  norms.add(numDocNorms,new Float(norm));
  modifiedNorms.add(numDocNorms,new Float(norm));
  numDocNorms++;
  lastNorm=(norm > 10 ? 0 : norm);
  return norm;
}
