{
  TokenStream ts=new LowerCaseFilterMock(new KeywordMarkerFilter(new KeywordMarkerFilter(new MockTokenizer(new StringReader("Dogs Trees Birds Houses"),MockTokenizer.WHITESPACE,false),new CharArraySet(TEST_VERSION_CURRENT,asSet("Birds","Houses"),false)),new CharArraySet(TEST_VERSION_CURRENT,asSet("Dogs","Trees"),false)));
  assertTokenStreamContents(ts,new String[]{"Dogs","Trees","Birds","Houses"});
}
