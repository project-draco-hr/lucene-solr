{
  TokenStream ts=new LowerCaseFilterMock(new KeywordMarkerFilter(new KeywordMarkerFilter(new MockTokenizer(new StringReader("Dogs Trees Birds Houses"),MockTokenizer.WHITESPACE,false),new HashSet<String>(Arrays.asList(new String[]{"Birds","Houses"}))),new HashSet<String>(Arrays.asList(new String[]{"Dogs","Trees"}))));
  assertTokenStreamContents(ts,new String[]{"Dogs","Trees","Birds","Houses"});
}
