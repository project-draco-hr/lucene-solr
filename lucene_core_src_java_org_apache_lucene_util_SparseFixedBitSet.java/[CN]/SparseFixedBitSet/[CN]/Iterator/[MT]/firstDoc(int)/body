{
  Block block=null;
  while (i4096 < blocks.length) {
    block=blocks[i4096];
    if (block != null) {
      final int i64=Long.numberOfTrailingZeros(block.index);
      return doc=(i4096 << 12) | (i64 << 6) | Long.numberOfTrailingZeros(block.bits[0]);
    }
