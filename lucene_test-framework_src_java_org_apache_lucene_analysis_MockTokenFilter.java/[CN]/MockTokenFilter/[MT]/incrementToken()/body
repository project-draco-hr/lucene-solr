{
  skippedPositions=0;
  while (input.incrementToken()) {
    if (!filter.run(termAtt.buffer(),0,termAtt.length())) {
      posIncrAtt.setPositionIncrement(posIncrAtt.getPositionIncrement() + skippedPositions);
      return true;
    }
    skippedPositions+=posIncrAtt.getPositionIncrement();
  }
  return false;
}
