{
  assertQ(req("q","*:*","debug","true"),"//str[@name='rawquerystring']='*:*'","//str[@name='querystring']='*:*'","//str[@name='parsedquery']='MatchAllDocsQuery(*:*)'","//str[@name='parsedquery_toString']='*:*'","//str[@name='QParser']","count(//lst[@name='explain']/*)=3","//lst[@name='explain']/str[@name='1']","//lst[@name='explain']/str[@name='2']","//lst[@name='explain']/str[@name='3']","count(//lst[@name='timing']/*)=3","//lst[@name='timing']/double[@name='time']","count(//lst[@name='prepare']/*)>0","//lst[@name='prepare']/double[@name='time']","count(//lst[@name='process']/*)>0","//lst[@name='process']/double[@name='time']");
  assertQ(req("q","*:*","debug",CommonParams.TIMING),"count(//str[@name='rawquerystring'])=0","count(//str[@name='querystring'])=0","count(//str[@name='parsedquery'])=0","count(//str[@name='parsedquery_toString'])=0","count(//lst[@name='explain']/*)=0","count(//str[@name='QParser'])=0","count(//lst[@name='timing']/*)=3","//lst[@name='timing']/double[@name='time']","count(//lst[@name='prepare']/*)>0","//lst[@name='prepare']/double[@name='time']","count(//lst[@name='process']/*)>0","//lst[@name='process']/double[@name='time']");
  assertQ(req("q","*:*","debug",CommonParams.QUERY),"//str[@name='rawquerystring']='*:*'","//str[@name='querystring']='*:*'","//str[@name='parsedquery']='MatchAllDocsQuery(*:*)'","//str[@name='parsedquery_toString']='*:*'","count(//lst[@name='explain']/*)=0","//str[@name='QParser']","count(//lst[@name='timing']/*)=0");
  assertQ(req("q","*:*","debug",CommonParams.RESULTS),"count(//str[@name='rawquerystring'])=0","count(//str[@name='querystring'])=0","count(//str[@name='parsedquery'])=0","count(//str[@name='parsedquery_toString'])=0","count(//lst[@name='explain']/*)=3","//lst[@name='explain']/str[@name='1']","//lst[@name='explain']/str[@name='2']","//lst[@name='explain']/str[@name='3']","count(//str[@name='QParser'])=0","count(//lst[@name='timing']/*)=0");
  assertQ(req("q","*:*","debug",CommonParams.RESULTS,"debug",CommonParams.QUERY),"//str[@name='rawquerystring']='*:*'","//str[@name='querystring']='*:*'","//str[@name='parsedquery']='MatchAllDocsQuery(*:*)'","//str[@name='parsedquery_toString']='*:*'","//str[@name='QParser']","count(//lst[@name='explain']/*)=3","//lst[@name='explain']/str[@name='1']","//lst[@name='explain']/str[@name='2']","//lst[@name='explain']/str[@name='3']","count(//lst[@name='timing']/*)=0");
}
