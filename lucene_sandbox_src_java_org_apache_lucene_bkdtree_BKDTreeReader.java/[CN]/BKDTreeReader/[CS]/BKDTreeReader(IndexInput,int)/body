{
  int numLeaves=in.readVInt();
  leafNodeOffset=numLeaves;
  splitValues=new int[numLeaves];
  for (int i=0; i < numLeaves; i++) {
    splitValues[i]=in.readInt();
  }
  leafBlockFPs=new long[numLeaves];
  for (int i=0; i < numLeaves; i++) {
    leafBlockFPs[i]=in.readVLong();
  }
  this.maxDoc=maxDoc;
  this.in=in;
}
