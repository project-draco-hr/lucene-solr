{
  final ModelException expectedException=new ModelException("MultipleAdditiveTreesModel tree node is missing threshold");
  try {
    createModelFromFiles("multipleadditivetreesmodel_no_threshold.json","multipleadditivetreesmodel_features.json");
    fail("multipleAdditiveTreesTestNoThreshold failed to throw exception: " + expectedException);
  }
 catch (  Exception actualException) {
    Throwable rootError=getRootCause(actualException);
    assertEquals(expectedException.toString(),rootError.toString());
  }
}
