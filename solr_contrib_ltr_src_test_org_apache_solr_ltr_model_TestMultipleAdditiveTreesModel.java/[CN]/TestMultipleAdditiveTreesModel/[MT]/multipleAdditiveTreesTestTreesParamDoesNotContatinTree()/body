{
  final ModelException expectedException=new ModelException("MultipleAdditiveTreesModel tree doesn't contain a tree");
  try {
    createModelFromFiles("multipleadditivetreesmodel_no_tree.json","multipleadditivetreesmodel_features.json");
    fail("multipleAdditiveTreesTestTreesParamDoesNotContatinTree failed to throw exception: " + expectedException);
  }
 catch (  Exception actualException) {
    Throwable rootError=getRootCause(actualException);
    assertEquals(expectedException.toString(),rootError.toString());
  }
}
