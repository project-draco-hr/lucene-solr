{
  final ModelException expectedException=new ModelException("MultipleAdditiveTreesModel tree node is leaf with left=-100.0 and right=75.0");
  try {
    createModelFromFiles("multipleadditivetreesmodel_no_feature.json","multipleadditivetreesmodel_features.json");
    fail("multipleAdditiveTreesTestMissingTreeFeature failed to throw exception: " + expectedException);
  }
 catch (  ModelException actualException) {
    assertEquals(expectedException.toString(),actualException.toString());
  }
}
