{
  final ModelException expectedException=new ModelException("no features declared for model multipleadditivetreesmodel_no_features");
  try {
    createModelFromFiles("multipleadditivetreesmodel_no_features.json","multipleadditivetreesmodel_features.json");
    fail("multipleAdditiveTreesTestNoFeaturesSpecified failed to throw exception: " + expectedException);
  }
 catch (  ModelException actualException) {
    assertEquals(expectedException.toString(),actualException.toString());
  }
}
