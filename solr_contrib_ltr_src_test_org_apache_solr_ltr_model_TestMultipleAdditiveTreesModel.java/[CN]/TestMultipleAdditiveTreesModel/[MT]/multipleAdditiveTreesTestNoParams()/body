{
  final ModelException expectedException=new ModelException("no trees declared for model multipleadditivetreesmodel_no_params");
  try {
    createModelFromFiles("multipleadditivetreesmodel_no_params.json","multipleadditivetreesmodel_features.json");
    fail("multipleAdditiveTreesTestNoParams failed to throw exception: " + expectedException);
  }
 catch (  Exception actualException) {
    Throwable rootError=getRootCause(actualException);
    assertEquals(expectedException.toString(),rootError.toString());
  }
}
