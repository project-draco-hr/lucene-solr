{
  if (nullFirst && nullLast) {
    throw new IllegalArgumentException("Cannot specify missing values as both first and last");
  }
  if (nullFirst && reverse) {
    in.setMissingValue(SortField.STRING_LAST);
  }
 else   if (nullLast && !reverse) {
    in.setMissingValue(SortField.STRING_LAST);
  }
}
