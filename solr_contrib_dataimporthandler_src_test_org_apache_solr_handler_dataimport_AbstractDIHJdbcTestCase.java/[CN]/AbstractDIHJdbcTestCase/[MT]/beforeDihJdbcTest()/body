{
  useSimpleCaches=false;
  countryEntity=false;
  countryCached=false;
  sportsEntity=false;
  sportsCached=false;
  rootTransformerName=null;
  countryTransformer=false;
  sportsTransformer=false;
  dbToUse=db;
  if (db == Database.RANDOM) {
    if (random().nextBoolean()) {
      dbToUse=Database.DERBY;
    }
 else {
      dbToUse=Database.HSQLDB;
    }
  }
  clearIndex();
  assertU(commit());
  buildDatabase();
}
