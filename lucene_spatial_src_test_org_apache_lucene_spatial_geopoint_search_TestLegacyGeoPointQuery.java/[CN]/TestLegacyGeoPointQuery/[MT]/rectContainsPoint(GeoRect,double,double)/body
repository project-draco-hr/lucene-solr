{
  if (GeoEncodingUtils.compare(pointLon,rect.minLon) == 0.0 || GeoEncodingUtils.compare(pointLon,rect.maxLon) == 0.0 || GeoEncodingUtils.compare(pointLat,rect.minLat) == 0.0 || GeoEncodingUtils.compare(pointLat,rect.maxLat) == 0.0) {
    return null;
  }
  if (rect.minLon < rect.maxLon) {
    return GeoRelationUtils.pointInRectPrecise(pointLat,pointLon,rect.minLat,rect.maxLat,rect.minLon,rect.maxLon);
  }
 else {
    return GeoRelationUtils.pointInRectPrecise(pointLat,pointLon,rect.minLat,rect.maxLat,-180.0,rect.maxLon) || GeoRelationUtils.pointInRectPrecise(pointLat,pointLon,rect.minLat,rect.maxLat,rect.minLon,180.0);
  }
}
