{
  if (!(toStore instanceof Serializable))   throw new IOException("Instance of " + toStore.getClass().getName() + " is not Serializable!");
  String storedId=getStoredResourceId(resourceId);
  ObjectOutputStream oos=null;
  try {
    oos=new ObjectOutputStream(storageIO.openOutputStream(storedId));
    oos.writeObject(toStore);
    oos.flush();
  }
  finally {
    if (oos != null) {
      try {
        oos.close();
      }
 catch (      Exception ignore) {
      }
    }
  }
}
