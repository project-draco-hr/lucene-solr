{
  ByteArrayOutputStream out=new ByteArrayOutputStream();
  ObjectOutputStream oos=null;
  try {
    oos=new ObjectOutputStream(out);
    oos.writeObject(ser);
    oos.flush();
  }
  finally {
    if (oos != null) {
      try {
        oos.close();
      }
 catch (      Exception ignore) {
      }
    }
  }
  return new BytesRef(out.toByteArray());
}
