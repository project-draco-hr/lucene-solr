{
  if (!ReflectionUtils.extendsType(clazz,Analyzer.class)) {
    LOG.warn("Can not create analyzer for class " + clazz.getName());
    return null;
  }
  try {
    return clazz.newInstance();
  }
 catch (  Exception e) {
    LOG.warn("Can not create analyzer for class " + clazz.getName());
  }
  return null;
}
