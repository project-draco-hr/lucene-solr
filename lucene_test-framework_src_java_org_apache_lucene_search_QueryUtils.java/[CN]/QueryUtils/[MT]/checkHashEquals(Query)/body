{
  Query q2=q.clone();
  checkEqual(q,q2);
  Query q3=q.clone();
  q3.setBoost(7.21792348f);
  checkUnequal(q,q3);
  Query whacky=new Query(){
    @Override public String toString(    String field){
      return "My Whacky Query";
    }
  }
;
  whacky.setBoost(q.getBoost());
  checkUnequal(q,whacky);
  assertFalse(q.equals(null));
}
