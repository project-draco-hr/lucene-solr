{
  try {
    int cmp=Integer.compare(startOffset(),other.startOffset());
    if (cmp != 0) {
      return cmp;
    }
    if (this.term == null || other.term == null) {
      if (this.term == null && other.term == null) {
        return 0;
      }
 else       if (this.term == null) {
        return 1;
      }
 else {
        return -1;
      }
    }
    return term.compareTo(other.term);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
