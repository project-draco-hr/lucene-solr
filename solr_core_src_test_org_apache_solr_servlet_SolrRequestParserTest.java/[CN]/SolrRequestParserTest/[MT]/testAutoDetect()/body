{
  String curl="curl/7.30.0";
  for (  String method : new String[]{"GET","POST"}) {
    doAutoDetect(null,method,"{}=a",null,"{}","a");
    doAutoDetect(curl,method,"{}","application/json",null,null);
    doAutoDetect(curl,method,"  \t\n\r  {}  ","application/json",null,null);
    doAutoDetect(curl,method,"  \t\n\r  // how now brown cow\n {}  ","application/json",null,null);
    doAutoDetect(curl,method,"  \t\n\r  #different style comment\n {}  ","application/json",null,null);
    doAutoDetect(curl,method,"  \t\n\r  /* C style comment */\n {}  ","application/json",null,null);
    doAutoDetect(curl,method,"  \t\n\r  <tag>hi</tag>  ","text/xml",null,null);
    doAutoDetect(curl,method,"  \t\r\n  aaa=1&bbb=2&ccc=3",null,"bbb","2");
    doAutoDetect(curl,method,"/x=foo&aaa=1&bbb=2&ccc=3",null,"/x","foo");
    doAutoDetect(curl,method," \t\r\n /x=foo&aaa=1&bbb=2&ccc=3",null,"bbb","2");
  }
}
