{
  ensureOpen();
  DocsEnum docs=MultiFields.getTermDocsEnum(this,MultiFields.getDeletedDocs(this),term.field(),new BytesRef(term.text()));
  if (docs == null)   return 0;
  int n=0;
  int doc;
  while ((doc=docs.nextDoc()) != docs.NO_MORE_DOCS) {
    deleteDocument(doc);
    n++;
  }
  return n;
}
