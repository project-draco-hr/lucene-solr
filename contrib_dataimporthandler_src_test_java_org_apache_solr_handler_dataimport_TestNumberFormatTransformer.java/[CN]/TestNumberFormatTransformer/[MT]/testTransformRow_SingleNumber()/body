{
  List l=new ArrayList();
  l.add(AbstractDataImportHandlerTest.createMap("column","num",NumberFormatTransformer.FORMAT_STYLE,NumberFormatTransformer.NUMBER));
  Context c=AbstractDataImportHandlerTest.getContext(null,null,null,0,l,null);
  Map m=AbstractDataImportHandlerTest.createMap("num","123,567");
  new NumberFormatTransformer().transformRow(m,c);
  Assert.assertEquals(new Long(123567),m.get("num"));
}
