{
  final FeatureScorer r=scorer(context);
  float score=getDefaultValue();
  if (r != null) {
    r.iterator().advance(doc);
    if (r.docID() == doc) {
      score=r.score();
    }
    return Explanation.match(score,toString());
  }
 else {
    return Explanation.match(score,"The feature has no value");
  }
}
