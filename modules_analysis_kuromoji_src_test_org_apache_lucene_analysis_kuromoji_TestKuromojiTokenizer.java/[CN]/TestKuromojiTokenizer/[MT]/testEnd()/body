{
  assertTokenStreamContents(analyzerNoPunct.tokenStream("foo",new StringReader("????????????????????????")),new String[]{"??????","???","???","???","???","??????"},new int[]{0,2,3,4,5,6},new int[]{2,3,4,5,6,8},new Integer(8));
  assertTokenStreamContents(analyzerNoPunct.tokenStream("foo",new StringReader("????????????????????????    ")),new String[]{"??????","???","???","???","???","??????"},new int[]{0,2,3,4,5,6,8},new int[]{2,3,4,5,6,8,9},new Integer(12));
}
