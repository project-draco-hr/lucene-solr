{
  IOException ioe=null;
  for (int i=0; i < subReaders.length; i++) {
    try {
      if (decrefOnClose[i]) {
        subReaders[i].decRef();
      }
 else {
        subReaders[i].close();
      }
    }
 catch (    IOException e) {
      if (ioe == null)       ioe=e;
    }
  }
  if (ioe != null)   throw ioe;
}
