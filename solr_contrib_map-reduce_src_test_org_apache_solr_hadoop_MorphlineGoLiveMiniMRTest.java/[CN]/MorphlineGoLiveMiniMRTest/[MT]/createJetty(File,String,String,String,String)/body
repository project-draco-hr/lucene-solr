{
  JettySolrRunner jetty=new JettySolrRunner(solrHome.getAbsolutePath(),solrConfigOverride,schemaOverride,buildJettyConfig(context));
  jetty.setShards(shardList);
  if (System.getProperty("collection") == null) {
    System.setProperty("collection","collection1");
  }
  jetty.start();
  System.clearProperty("collection");
  return jetty;
}
