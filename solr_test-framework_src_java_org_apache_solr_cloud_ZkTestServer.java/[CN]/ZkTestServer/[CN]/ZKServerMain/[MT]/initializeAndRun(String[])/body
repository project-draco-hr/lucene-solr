{
  try {
    ManagedUtil.registerLog4jMBeans();
  }
 catch (  JMException e) {
    log.warn("Unable to register log4j JMX control",e);
  }
  ServerConfig config=new ServerConfig();
  if (args.length == 1) {
    config.parse(args[0]);
  }
 else {
    config.parse(args);
  }
  runFromConfig(config);
}
