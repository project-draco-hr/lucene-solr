{
  if (input.incrementToken()) {
    String originalTerm=termAtt.term();
    stemmer.setCurrent(originalTerm);
    stemmer.stem();
    String finalTerm=stemmer.getCurrent();
    if (!originalTerm.equals(finalTerm))     termAtt.setTermBuffer(finalTerm);
    return true;
  }
 else {
    return false;
  }
}
