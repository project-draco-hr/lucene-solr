{
  int version=CodecUtil.checkHeader(input,VersionBlockTreeTermsWriter.TERMS_CODEC_NAME,VersionBlockTreeTermsWriter.VERSION_START,VersionBlockTreeTermsWriter.VERSION_CURRENT);
  if (version < VersionBlockTreeTermsWriter.VERSION_APPEND_ONLY) {
    dirOffset=input.readLong();
  }
  return version;
}
