{
  String description="LRU Cache(maxSize=" + limit + ", initialSize="+ initialSize;
  if (isAutowarmingOn()) {
    description+=", " + getAutowarmDescription();
  }
  if (maxRamBytes != Long.MAX_VALUE) {
    description+=", maxRamMB=" + (maxRamBytes / 1024L / 1024L);
  }
  description+=')';
  return description;
}
