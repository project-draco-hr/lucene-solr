{
  record=Utils.getDeepCopy(record,2);
  Object oldVal=values.get(null);
  if (oldVal == null) {
    values.put(null,record);
  }
 else   if (oldVal instanceof List) {
    ((List)oldVal).add(record);
  }
 else {
    ArrayList l=new ArrayList();
    l.add(oldVal);
    l.add(record);
    values.put(null,l);
  }
}
