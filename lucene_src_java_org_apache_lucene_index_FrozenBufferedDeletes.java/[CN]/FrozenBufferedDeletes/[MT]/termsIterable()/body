{
  return new Iterable<Term>(){
    public Iterator<Term> iterator(){
      return new Iterator<Term>(){
        private int upto;
        public boolean hasNext(){
          return upto < terms.length;
        }
        public Term next(){
          return terms[upto++];
        }
        public void remove(){
          throw new UnsupportedOperationException();
        }
      }
;
    }
  }
;
}
