{
  String algLines[]={"# ----- properties ","content.source=org.apache.lucene.benchmark.byTask.feeds.LineDocSource","docs.file=" + getReuters20LinesFile(),"ram.flush.mb=-1","max.buffered=2","content.source.log.step=3","doc.term.vector=false","content.source.forever=false","directory=RAMDirectory","doc.stored=false","doc.tokenized=false","debug.level=1","# ----- alg ","{ \"Rounds\"","  ResetSystemErase","  CreateIndex","  { \"AddDocs\"  AddDoc > : * ","  CloseIndex(false)","} : 2"};
  Benchmark benchmark=execBenchmark(algLines);
  IndexReader ir=IndexReader.open(benchmark.getRunData().getDirectory(),true);
  int ndocsExpected=20;
  assertEquals("wrong number of docs in the index!",ndocsExpected,ir.numDocs());
  ir.close();
}
