{
  try {
    docOut.close();
  }
  finally {
    try {
      skipOut.close();
    }
  finally {
      if (freqOut != null) {
        try {
          freqOut.close();
        }
  finally {
          try {
            posOut.close();
          }
  finally {
            payloadOut.close();
          }
        }
      }
    }
  }
}
