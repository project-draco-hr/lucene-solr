{
  return new StoredFieldsReader(){
    @Override public void visitDocument(    int docID,    StoredFieldVisitor visitor) throws IOException {
      reader.document(docID,visitor);
    }
    @Override public StoredFieldsReader clone(){
      return readerToStoredFieldsReader(reader);
    }
    @Override public void checkIntegrity() throws IOException {
    }
    @Override public void close(){
    }
    @Override public long ramBytesUsed(){
      return 0;
    }
    @Override public Iterable<? extends Accountable> getChildResources(){
      return Collections.emptyList();
    }
  }
;
}
