{
  selector=FieldMutatingUpdateProcessor.createFieldNameSelector(core.getResourceLoader(),core,inclusions,getDefaultSelector(core));
  for (  SelectorParams exc : exclusions) {
    selector=FieldMutatingUpdateProcessor.wrap(selector,FieldMutatingUpdateProcessor.createFieldNameSelector(core.getResourceLoader(),core,exc,FieldMutatingUpdateProcessor.SELECT_NO_FIELDS));
  }
}
