{
  final IndexSchema schema=core.getSchema();
  selector=FieldMutatingUpdateProcessor.createFieldNameSelector(core.getResourceLoader(),core.getSchema(),inclusions.fieldName,inclusions.typeName,inclusions.typeClass,inclusions.fieldRegex,getDefaultSelector(core));
  for (  SelectorParams exc : exclusions) {
    selector=FieldMutatingUpdateProcessor.wrap(selector,FieldMutatingUpdateProcessor.createFieldNameSelector(core.getResourceLoader(),core.getSchema(),exc.fieldName,exc.typeName,exc.typeClass,exc.fieldRegex,FieldMutatingUpdateProcessor.SELECT_NO_FIELDS));
  }
}
