{
  if (!exists(files,txn))   throw new IOException("File does not exist: " + getName());
  File newFile=new File(name);
  if (newFile.exists(files,txn))   newFile.delete(files,blocks,txn);
  try {
    files.delete(txn,key,0);
    setName(name);
    files.put(txn,key,data,0);
  }
 catch (  DbException e) {
    throw new IOException(e.getMessage());
  }
}
