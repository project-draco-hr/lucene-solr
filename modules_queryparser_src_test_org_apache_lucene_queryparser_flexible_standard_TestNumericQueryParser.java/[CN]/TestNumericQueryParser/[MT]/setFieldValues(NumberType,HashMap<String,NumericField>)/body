{
  Number number=getNumberType(numberType,NumericField.DataType.DOUBLE.name());
  numericFieldMap.get(NumericField.DataType.DOUBLE.name()).setValue(number.doubleValue());
  number=getNumberType(numberType,NumericField.DataType.INT.name());
  numericFieldMap.get(NumericField.DataType.INT.name()).setValue(number.intValue());
  number=getNumberType(numberType,NumericField.DataType.LONG.name());
  numericFieldMap.get(NumericField.DataType.LONG.name()).setValue(number.longValue());
  number=getNumberType(numberType,NumericField.DataType.FLOAT.name());
  numericFieldMap.get(NumericField.DataType.FLOAT.name()).setValue(number.floatValue());
  number=getNumberType(numberType,DATE_FIELD_NAME);
  numericFieldMap.get(DATE_FIELD_NAME).setValue(number.longValue());
}
