{
  List rows=new ArrayList();
  rows.add(createMap("id","1","desc","one"));
  MockDataSource.setIterator("select * from x",rows.iterator());
  super.runFullImport(dataConfigWithDynamicTransformer);
  assertQ(req("id:1"),"//*[@numFound='1']");
