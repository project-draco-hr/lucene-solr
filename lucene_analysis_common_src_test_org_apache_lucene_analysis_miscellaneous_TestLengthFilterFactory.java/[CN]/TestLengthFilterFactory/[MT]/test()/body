{
  Reader reader=new StringReader("foo foobar super-duper-trooper");
  TokenStream stream=new MockTokenizer(reader,MockTokenizer.WHITESPACE,false);
  stream=tokenFilterFactory("Length",Version.LUCENE_43,new ClasspathResourceLoader(getClass()),"min","4","max","10","enablePositionIncrements","false").create(stream);
  assertTokenStreamContents(stream,new String[]{"foobar"},new int[]{1});
}
