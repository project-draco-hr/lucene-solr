{
  if (node == null) {
    return;
  }
  node.getLabel(this.taxonomyReader);
  if (null == node.getSubResults()) {
    return;
  }
  int numLabeled=0;
  for (  FacetResultNode frn : node.getSubResults()) {
    recursivelyLabel(frn,numToLabel);
    if (++numLabeled >= numToLabel) {
      return;
    }
  }
}
