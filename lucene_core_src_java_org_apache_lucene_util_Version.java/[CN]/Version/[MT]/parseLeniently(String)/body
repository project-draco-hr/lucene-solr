{
  if (version.equals("LATEST") || version.equals("LUCENE_CURRENT"))   return LATEST;
  final String parsedMatchVersion=version.toUpperCase(Locale.ROOT).replaceFirst("^LUCENE_(\\d+)_(\\d+)_(\\d+)$","$1.$2.$3").replaceFirst("^LUCENE_(\\d+)_(\\d+)$","$1.$2.0").replaceFirst("^LUCENE_(\\d)(\\d)$","$1.$2.0");
  return parse(parsedMatchVersion);
}
