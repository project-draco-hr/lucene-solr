{
  int major=(encodedValue >>> 18) & 0xFF;
  int minor=(encodedValue >>> 10) & 0xFF;
  int bugfix=(encodedValue >>> 2) & 0xFF;
  int prerelease=encodedValue & 0x3;
  if (prerelease == 3) {
    return "" + major + "."+ minor+ "."+ bugfix;
  }
  return "" + major + "."+ minor+ "."+ bugfix+ "."+ prerelease;
}
