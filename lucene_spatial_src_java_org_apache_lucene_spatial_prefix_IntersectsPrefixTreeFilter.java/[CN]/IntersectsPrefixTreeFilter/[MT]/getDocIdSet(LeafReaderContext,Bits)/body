{
  return new VisitorTemplate(context,acceptDocs,hasIndexedLeaves){
    private FixedBitSet results;
    @Override protected void start(){
      results=new FixedBitSet(maxDoc);
    }
    @Override protected DocIdSet finish(){
      return new FixedBitDocIdSet(results);
    }
    @Override protected boolean visit(    Cell cell) throws IOException {
      if (cell.getShapeRel() == SpatialRelation.WITHIN || cell.getLevel() == detailLevel) {
        collectDocs(results);
        return false;
      }
      return true;
    }
    @Override protected void visitLeaf(    Cell cell) throws IOException {
      collectDocs(results);
    }
    @Override protected void visitScanned(    Cell cell) throws IOException {
      if (queryShape.relate(cell.getShape()).intersects())       collectDocs(results);
    }
  }
.getDocIdSet();
}
