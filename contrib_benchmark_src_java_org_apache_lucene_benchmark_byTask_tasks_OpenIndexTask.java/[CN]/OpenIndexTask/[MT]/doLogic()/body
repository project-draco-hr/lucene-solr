{
  PerfRunData runData=getRunData();
  Config config=runData.getConfig();
  IndexWriter writer=new IndexWriter(runData.getDirectory(),config.get("autocommit",DEFAULT_AUTO_COMMIT),runData.getAnalyzer(),false,IndexWriter.MaxFieldLength.LIMITED);
  CreateIndexTask.setIndexWriterConfig(writer,config);
  runData.setIndexWriter(writer);
  return 1;
}
