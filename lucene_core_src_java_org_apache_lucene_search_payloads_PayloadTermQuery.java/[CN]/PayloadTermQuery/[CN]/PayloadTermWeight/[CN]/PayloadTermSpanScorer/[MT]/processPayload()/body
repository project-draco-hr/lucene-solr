{
  float payloadFactor=payloadCollector.payload == null ? 1F : docScorer.computePayloadFactor(docID(),spans.startPosition(),spans.endPosition(),payloadCollector.payload);
  payloadScore=function.currentScore(docID(),term.field(),spans.startPosition(),spans.endPosition(),payloadsSeen,payloadScore,payloadFactor);
  payloadsSeen++;
}
