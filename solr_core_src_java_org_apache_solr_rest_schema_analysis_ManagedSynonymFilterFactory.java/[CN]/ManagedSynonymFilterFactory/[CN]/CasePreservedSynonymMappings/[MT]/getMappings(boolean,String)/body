{
  Set<String> synMappings=null;
  if (ignoreCase) {
    if (mappings.size() == 1) {
      return mappings.values().iterator().next();
    }
    synMappings=new TreeSet<>();
    for (    Set<String> next : mappings.values())     synMappings.addAll(next);
  }
 else {
    synMappings=mappings.get(key);
  }
  return synMappings;
}
