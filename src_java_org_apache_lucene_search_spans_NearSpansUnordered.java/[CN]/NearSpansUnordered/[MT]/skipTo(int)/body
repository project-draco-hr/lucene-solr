{
  if (firstTime) {
    initList(false);
    for (SpansCell cell=first; more && cell != null; cell=cell.next) {
      more=cell.skipTo(target);
    }
    if (more) {
      listToQueue();
    }
    firstTime=false;
  }
 else {
    while (more && min().doc() < target) {
      if (min().skipTo(target)) {
        queue.adjustTop();
      }
 else {
        more=false;
      }
    }
  }
  return more && (atMatch() || next());
}
