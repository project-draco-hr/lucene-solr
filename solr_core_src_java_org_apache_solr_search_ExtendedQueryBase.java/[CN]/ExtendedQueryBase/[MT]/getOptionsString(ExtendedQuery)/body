{
  StringBuilder sb=new StringBuilder();
  if (!q.getCache()) {
    sb.append("{!cache=false");
    int cost=q.getCost();
    if (cost != 0) {
      sb.append(" cost=");
      sb.append(q.getCost());
    }
    sb.append("}");
  }
 else   if (q.getCacheSep()) {
    sb.append("{!cache=sep");
    sb.append("}");
  }
  return sb.toString();
}
