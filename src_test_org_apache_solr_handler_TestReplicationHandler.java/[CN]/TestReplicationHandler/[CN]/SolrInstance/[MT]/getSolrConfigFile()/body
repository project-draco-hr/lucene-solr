{
  String fname="";
  if (type == 1)   fname="." + File.separator + "solr"+ File.separator+ "conf"+ File.separator+ "solrconfig-master.xml";
  if (type == 0)   fname="." + File.separator + "solr"+ File.separator+ "conf"+ File.separator+ "solrconfig-slave.xml";
  System.out.println(fname);
  return fname;
}
