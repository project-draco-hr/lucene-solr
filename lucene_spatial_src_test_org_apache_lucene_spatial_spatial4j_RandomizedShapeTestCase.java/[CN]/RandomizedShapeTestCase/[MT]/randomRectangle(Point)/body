{
  Rectangle bounds=ctx.getWorldBounds();
  if (nearP == null)   nearP=randomPointIn(bounds);
  Range xRange=randomRange(rarely() ? 0 : nearP.getX(),Range.xRange(bounds,ctx));
  Range yRange=randomRange(rarely() ? 0 : nearP.getY(),Range.yRange(bounds,ctx));
  return makeNormRect(divisible(xRange.getMin()),divisible(xRange.getMax()),divisible(yRange.getMin()),divisible(yRange.getMax()));
}
