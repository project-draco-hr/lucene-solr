{
  DocsEnum tdocs=MultiFields.getTermDocsEnum(reader,MultiFields.getDeletedDocs(reader),term.field(),new BytesRef(term.text()));
  int count=0;
  if (tdocs != null) {
    while (tdocs.nextDoc() != tdocs.NO_MORE_DOCS) {
      count++;
    }
  }
  assertEquals(msg + ", count mismatch",expected,count);
}
