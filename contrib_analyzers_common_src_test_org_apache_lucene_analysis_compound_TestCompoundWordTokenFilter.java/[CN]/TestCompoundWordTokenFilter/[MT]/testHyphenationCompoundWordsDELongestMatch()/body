{
  String[] dict={"Rind","Fleisch","Draht","Schere","Gesetz","Aufgabe","??berwachung","Rindfleisch","??berwachungsgesetz"};
  Reader reader=getHyphenationReader("de_DR.xml");
  if (reader == null) {
    return;
  }
  HyphenationTree hyphenator=HyphenationCompoundWordTokenFilter.getHyphenationTree(reader);
  HyphenationCompoundWordTokenFilter tf=new HyphenationCompoundWordTokenFilter(new WhitespaceTokenizer(new StringReader("Rindfleisch??berwachungsgesetz")),hyphenator,dict,CompoundWordTokenFilterBase.DEFAULT_MIN_WORD_SIZE,CompoundWordTokenFilterBase.DEFAULT_MIN_SUBWORD_SIZE,40,true);
  assertTokenStreamContents(tf,new String[]{"Rindfleisch??berwachungsgesetz","Rindfleisch","fleisch","??berwachungsgesetz","gesetz"},new int[]{0,0,4,11,23},new int[]{29,11,11,29,29},new int[]{1,0,0,0,0});
}
