{
  if (!more)   return false;
  if (firstTime) {
    heapify();
    firstTime=false;
    return true;
  }
  int lastdoc=((Scorer)subScorers.get(0)).doc();
  do {
    if (((Scorer)subScorers.get(0)).next())     heapAdjust(0);
 else {
      heapRemoveRoot();
      if (subScorers.isEmpty())       return (more=false);
    }
  }
 while (((Scorer)subScorers.get(0)).doc() == lastdoc);
  return true;
}
