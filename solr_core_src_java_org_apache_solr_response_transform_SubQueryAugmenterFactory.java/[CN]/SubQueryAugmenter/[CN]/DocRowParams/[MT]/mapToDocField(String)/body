{
  if (param.startsWith(prefixDotRowDot)) {
    final String docFieldName=param.substring(prefixDotRowDot.length());
    final Collection<Object> vals=doc.getFieldValues(docFieldName);
    if (vals == null || vals.isEmpty()) {
      return null;
    }
 else {
      return vals;
    }
  }
  return null;
}
