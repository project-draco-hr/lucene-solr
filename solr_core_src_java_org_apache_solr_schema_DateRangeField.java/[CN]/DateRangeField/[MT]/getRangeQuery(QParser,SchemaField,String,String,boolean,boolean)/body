{
  if (!minInclusive || !maxInclusive)   throw new SolrException(SolrException.ErrorCode.BAD_REQUEST,"exclusive range boundary not supported");
  if (part1 == null)   part1="*";
  if (part2 == null)   part2="*";
  Shape shape=tree.toRangeShape(parseShape(part1),parseShape(part2));
  SpatialArgs spatialArgs=new SpatialArgs(SpatialOperation.Intersects,shape);
  return getQueryFromSpatialArgs(parser,field,spatialArgs);
}
