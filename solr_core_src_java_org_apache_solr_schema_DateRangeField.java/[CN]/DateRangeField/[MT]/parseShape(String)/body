{
  if (str.contains(" TO ")) {
    try {
      return tree.parseShape(str);
    }
 catch (    ParseException e) {
      throw new SolrException(SolrException.ErrorCode.BAD_REQUEST,"Couldn't parse date because: " + e.getMessage(),e);
    }
  }
 else {
    return tree.toShape(parseCalendar(str));
  }
}
