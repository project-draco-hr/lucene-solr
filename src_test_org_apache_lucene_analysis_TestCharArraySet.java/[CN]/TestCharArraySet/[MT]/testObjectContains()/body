{
  CharArraySet set=new CharArraySet(Version.LUCENE_CURRENT,10,true);
  Integer val=Integer.valueOf(1);
  set.add(val);
  assertTrue(set.contains(val));
  assertTrue(set.contains(Integer.valueOf(1)));
  set=CharArraySet.unmodifiableSet(set);
  assertTrue(set.contains(val));
  assertTrue(set.contains(Integer.valueOf(1)));
}
