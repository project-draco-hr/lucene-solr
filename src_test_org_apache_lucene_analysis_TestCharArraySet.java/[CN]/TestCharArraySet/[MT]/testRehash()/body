{
  CharArraySet cas=new CharArraySet(0,true);
  for (int i=0; i < StopAnalyzer.ENGLISH_STOP_WORDS.length; i++)   cas.add(StopAnalyzer.ENGLISH_STOP_WORDS[i]);
  assertEquals(StopAnalyzer.ENGLISH_STOP_WORDS.length,cas.size());
  for (int i=0; i < StopAnalyzer.ENGLISH_STOP_WORDS.length; i++)   assertTrue(cas.contains(StopAnalyzer.ENGLISH_STOP_WORDS[i]));
}
