{
  for (  float val : values) {
    String s=Float.toString(val);
    if (field != null)     assertU(adoc("id",s,field,s));
 else     assertU(adoc("id",s));
    if (random().nextInt(100) < 20) {
      if (field != null)       assertU(adoc("id",s,field,s));
 else       assertU(adoc("id",s));
    }
    if (random().nextInt(100) < 20) {
      assertU(commit());
    }
  }
  assertU(commit());
}
