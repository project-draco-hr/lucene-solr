{
  assertU(adoc("id","1","number_of_atoms_in_universe_l",Long.toString(Long.MAX_VALUE)));
  assertU(adoc("id","2","number_of_atoms_in_universe_l",Long.toString(Long.MAX_VALUE - 1)));
  assertU(commit());
  singleTest("number_of_atoms_in_universe_l","if(gt(number_of_atoms_in_universe_l," + Long.toString(Long.MAX_VALUE - 1) + "),5,2)",1,5,2,2);
  singleTest("number_of_atoms_in_universe_l","if(lt(number_of_atoms_in_universe_l," + Long.toString(Long.MAX_VALUE) + "),5,2)",2,5,1,2);
}
