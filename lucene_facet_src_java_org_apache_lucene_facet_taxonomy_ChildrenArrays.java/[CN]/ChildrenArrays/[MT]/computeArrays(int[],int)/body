{
  for (int i=first; i < parents.length; i++) {
    youngestChild[i]=TaxonomyReader.INVALID_ORDINAL;
  }
  if (first == 0) {
    first=1;
    olderSibling[0]=TaxonomyReader.INVALID_ORDINAL;
  }
  for (int i=first; i < parents.length; i++) {
    olderSibling[i]=youngestChild[parents[i]];
    youngestChild[parents[i]]=i;
  }
}
