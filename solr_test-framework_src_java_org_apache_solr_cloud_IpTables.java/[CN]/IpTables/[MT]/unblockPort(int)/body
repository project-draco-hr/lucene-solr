{
  if (ENABLED && BLOCK_PORTS.contains(port)) {
    log.info("Unblock port with iptables: " + port);
    runCmd(("iptables -D INPUT -p tcp --dport " + port + " -j DROP").split("\\s"));
    runCmd(("iptables -D OUTPUT -p tcp --dport " + port + " -j DROP").split("\\s"));
    BLOCK_PORTS.remove(port);
  }
}
