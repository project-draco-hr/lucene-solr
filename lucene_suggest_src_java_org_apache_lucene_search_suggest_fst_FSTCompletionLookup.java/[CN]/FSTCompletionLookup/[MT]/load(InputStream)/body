{
  try {
    this.higherWeightsCompletion=new FSTCompletion(new FST<Object>(new InputStreamDataInput(input),NoOutputs.getSingleton()));
    this.normalCompletion=new FSTCompletion(higherWeightsCompletion.getFST(),false,exactMatchFirst);
  }
  finally {
    IOUtils.close(input);
  }
  return true;
}
