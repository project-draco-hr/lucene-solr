{
  if (tvx != null) {
    int fieldNumber=fieldInfos.fieldNumber(field);
    tvx.seek(((docNum + docStoreOffset) * 8L) + TermVectorsWriter.FORMAT_SIZE);
    long position=tvx.readLong();
    tvd.seek(position);
    int fieldCount=tvd.readVInt();
    int number=0;
    int found=-1;
    for (int i=0; i < fieldCount; i++) {
      if (tvdFormat == TermVectorsWriter.FORMAT_VERSION)       number=tvd.readVInt();
 else       number+=tvd.readVInt();
      if (number == fieldNumber)       found=i;
    }
    if (found != -1) {
      position=0;
      for (int i=0; i <= found; i++)       position+=tvd.readVLong();
      readTermVector(field,position,mapper);
    }
 else {
    }
  }
 else {
  }
}
