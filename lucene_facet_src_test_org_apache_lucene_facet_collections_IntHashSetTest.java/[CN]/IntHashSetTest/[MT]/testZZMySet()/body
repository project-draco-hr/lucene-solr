{
  IntHashSet set=new IntHashSet();
  for (int j=0; j < 100; ++j) {
    for (int i=0; i < ArrayHashMapTest.RANDOM_TEST_NUM_ITERATIONS; ++i) {
      int value=random().nextInt() % 5000;
      boolean shouldAdd=random().nextBoolean();
      if (shouldAdd) {
        set.add(value);
      }
 else {
        set.remove(value);
      }
    }
    set.clear();
  }
}
