{
  Set<String> handlerNames=h.getCore().getRequestHandlers().keySet();
  for (  String handler : handlerNames) {
    try {
      if (handler.startsWith("/update")) {
        continue;
      }
      if (handler.startsWith("/mlt")) {
        continue;
      }
      if (handler.equals("/admin/ping"))       continue;
      assertQ("failure w/handler: '" + handler + "'",req("qt",handler,"q","foo:bar","omitHeader","false"),"//lst[@name='responseHeader']");
    }
 catch (    Exception e) {
      throw new RuntimeException("exception w/handler: '" + handler + "'",e);
    }
  }
}
