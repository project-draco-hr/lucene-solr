{
  boolean hasNext=input.incrementToken();
  if (!hasNext) {
    return false;
  }
  if (offset + length <= data.length && !termAttribute.toString().endsWith("NO PAYLOAD")) {
    BytesRef p=new BytesRef(data,offset,length);
    payloadAtt.setPayload(p);
    offset+=length;
  }
 else {
    payloadAtt.setPayload(null);
  }
  return true;
}
