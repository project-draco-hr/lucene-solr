{
  if (tokenizer.get() == null) {
    throw new IllegalStateException("You have to set at least a tokenizer.");
  }
  return new CustomAnalyzer(defaultMatchVersion.get(),charFilters.toArray(new CharFilterFactory[charFilters.size()]),tokenizer.get(),tokenFilters.toArray(new TokenFilterFactory[tokenFilters.size()]),posIncGap.get(),offsetGap.get());
}
