{
  int quoteChar=next();
  if (quoteChar != '\'' && quoteChar != '"')   return MISMATCH;
  while (true) {
    int ch=next();
    if (ch == quoteChar)     return MATCH;
 else     if (ch == '\\') {
      ch=next();
    }
 else     if (ch < 0) {
      return MISMATCH;
    }
 else     if (ch == '<') {
      eatSSI();
    }
  }
}
