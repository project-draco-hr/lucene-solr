{
  int ret=readComment(inScript);
  if (ret == MATCH)   return MATCH;
  if ((numRead - lastMark) < readAheadLimitMinus1 || peek() == '>') {
    int ch=next();
    if (ch == '>')     return MATCH;
    while ((numRead - lastMark) < readAheadLimitMinus1) {
      ch=next();
      if (ch == '>') {
        return MATCH;
      }
 else       if (ch < 0) {
        return MISMATCH;
      }
    }
  }
  return MISMATCH;
}
