{
  StringBuilder builder=new StringBuilder();
  int ch=read();
  builder.append((char)ch);
  if (!isFirstIdChar(ch))   return MISMATCH;
  ch=read();
  builder.append((char)ch);
  while (isIdChar(ch)) {
    ch=read();
    builder.append((char)ch);
  }
  if (ch != -1) {
    push(ch);
  }
  if (checkEscaped && escapedTags.contains(builder.substring(0,builder.length() - 1))) {
    return MISMATCH;
  }
  return MATCH;
}
