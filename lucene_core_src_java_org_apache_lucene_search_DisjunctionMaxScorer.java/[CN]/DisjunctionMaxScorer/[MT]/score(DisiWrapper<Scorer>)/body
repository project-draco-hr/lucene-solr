{
  float scoreSum=0;
  float scoreMax=0;
  for (DisiWrapper<Scorer> w=topList; w != null; w=w.next) {
    final float subScore=w.iterator.score();
    scoreSum+=subScore;
    if (subScore > scoreMax) {
      scoreMax=subScore;
    }
  }
  return scoreMax + (scoreSum - scoreMax) * tieBreakerMultiplier;
}
