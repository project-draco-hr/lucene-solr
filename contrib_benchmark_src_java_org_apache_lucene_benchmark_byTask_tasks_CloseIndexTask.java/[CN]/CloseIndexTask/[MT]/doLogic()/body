{
  IndexWriter iw=getRunData().getIndexWriter();
  if (iw != null) {
    PrintStream infoStream=iw.getInfoStream();
    if (infoStream != null && infoStream != System.out && infoStream != System.err) {
      infoStream.close();
    }
    iw.close(doWait);
    getRunData().setIndexWriter(null);
  }
  return 1;
}
