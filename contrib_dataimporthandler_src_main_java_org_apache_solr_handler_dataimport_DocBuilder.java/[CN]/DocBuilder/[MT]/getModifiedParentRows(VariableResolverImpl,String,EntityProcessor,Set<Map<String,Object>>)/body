{
  try {
    while (true) {
      Map<String,Object> parentRow=entityProcessor.nextModifiedParentRowKey();
      if (parentRow == null)       break;
      parentKeyList.add(parentRow);
      importStatistics.rowsCount.incrementAndGet();
      if (stop.get())       return;
    }
  }
  finally {
    resolver.removeNamespace(entity);
  }
}
