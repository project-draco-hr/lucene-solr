{
  if (requestParameters.commit)   writer.persistIndexStartTime(dataImporter.getIndexStartTime());
  LOG.info("Full Import completed successfully");
  statusMessages.put("","Indexing completed. Added/Updated: " + importStatistics.docCount + " documents. Deleted "+ importStatistics.deletedDocCount+ " documents.");
  writer.commit(requestParameters.optimize);
  addStatusMessage("Committed");
  if (requestParameters.optimize)   addStatusMessage("Optimized");
}
