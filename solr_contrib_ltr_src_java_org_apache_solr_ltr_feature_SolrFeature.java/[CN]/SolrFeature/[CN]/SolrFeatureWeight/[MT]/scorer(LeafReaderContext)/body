{
  Scorer solrScorer=null;
  if (solrQueryWeight != null) {
    solrScorer=solrQueryWeight.scorer(context);
  }
  final DocIdSetIterator idItr=getDocIdSetIteratorFromQueries(queryAndFilters,context);
  if (idItr != null) {
    return solrScorer == null ? new ValueFeatureScorer(this,1f,idItr) : new SolrFeatureScorer(this,solrScorer,new SolrFeatureScorerIterator(idItr,solrScorer.iterator()));
  }
 else {
    return null;
  }
}
