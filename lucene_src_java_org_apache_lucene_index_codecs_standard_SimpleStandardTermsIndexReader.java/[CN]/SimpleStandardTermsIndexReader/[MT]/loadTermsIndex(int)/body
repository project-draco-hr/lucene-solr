{
  if (!indexLoaded) {
    this.indexDivisor=indexDivisor;
    this.totalIndexInterval=indexInterval * indexDivisor;
    Iterator<FieldIndexReader> it=fields.values().iterator();
    while (it.hasNext()) {
      it.next().loadTermsIndex();
    }
    indexLoaded=true;
    in.close();
    termBytesReader=termBytes.freeze();
  }
}
