{
  String[] args=new String[]{"--input-list","file:///tmp","--morphline-file",MORPHLINE_FILE,"--output-dir","file:/tmp/foo","--solr-home-dir",MINIMR_INSTANCE_DIR.getPath(),"--shard-url","http://localhost:8983/solr/collection1","--shard-url","http://localhost:8983/solr/collection1","--go-live"};
  Integer result=parser.parseArgs(args,conf,opts);
  assertNull(result);
  assertEmptySystemErrAndEmptySystemOut();
}
