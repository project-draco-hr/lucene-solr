{
  String[] args=new String[]{"--input-list","file:///tmp","--morphline-file",MORPHLINE_FILE,"--output-dir","file:/tmp/foo","--solr-home-dir",MINIMR_INSTANCE_DIR.getPath(),"--shards","1","--update-conflict-resolver",NoChangeUpdateConflictResolver.class.getName()};
  assertNull(parser.parseArgs(args,conf,opts));
  assertEquals(NoChangeUpdateConflictResolver.class.getName(),opts.updateConflictResolver);
  assertEmptySystemErrAndEmptySystemOut();
}
