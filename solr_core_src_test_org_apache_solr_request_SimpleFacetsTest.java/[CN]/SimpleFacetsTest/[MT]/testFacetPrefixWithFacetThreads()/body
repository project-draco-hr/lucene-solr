{
  assertQ("Test facet.prefix with facet.thread",req("q","id:[101 TO 102]","facet","true","facet.field","{!key=key1 facet.prefix=foo}myfield_s","facet.field","{!key=key2 facet.prefix=bar}myfield_s","facet.threads","1"),"*[count(//lst[@name='facet_fields']/lst[@name='key1']/int[@name='foo'])=1]","*[count(//lst[@name='facet_fields']/lst[@name='key2']/int[@name='bar'])=1]");
}
