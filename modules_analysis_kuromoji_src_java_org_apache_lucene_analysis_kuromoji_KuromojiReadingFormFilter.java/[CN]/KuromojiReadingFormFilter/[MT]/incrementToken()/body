{
  if (input.incrementToken()) {
    String reading=readingAttr.getReading();
    if (reading != null) {
      if (useRomaji) {
        ToStringUtil.getRomanization(termAttr.setEmpty(),reading);
      }
 else {
        termAttr.setEmpty().append(reading);
      }
    }
    return true;
  }
 else {
    return false;
  }
}
