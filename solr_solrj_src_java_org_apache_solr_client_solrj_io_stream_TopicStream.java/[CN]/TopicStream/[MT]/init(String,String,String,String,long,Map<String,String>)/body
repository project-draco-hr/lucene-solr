{
  this.zkHost=zkHost;
  this.params=params;
  this.collection=collection;
  this.checkpointCollection=checkpointCollection;
  this.checkpointEvery=checkpointEvery;
  this.id=id;
  this.comp=new FieldComparator("_version_",ComparatorOrder.ASCENDING);
