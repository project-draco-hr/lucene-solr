{
  Tuple tuple=_read();
  if (tuple.EOF) {
    return tuple;
  }
  ++count;
  if (checkpointEvery > -1 && (count % checkpointEvery) == 0) {
    persistCheckpoints();
  }
  long version=tuple.getLong("_version_");
  String slice=tuple.getString("_SLICE_");
  checkpoints.put(slice,version);
  tuple.remove("_SLICE_");
  tuple.remove("_CORE_");
  return tuple;
}
