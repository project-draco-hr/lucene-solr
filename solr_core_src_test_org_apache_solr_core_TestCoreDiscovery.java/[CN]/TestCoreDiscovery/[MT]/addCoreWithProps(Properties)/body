{
  File propFile=new File(solrHomeDirectory,stockProps.getProperty(CoreDescriptor.CORE_NAME) + File.separator + SolrCoreDiscoverer.CORE_PROP_FILE);
  File parent=propFile.getParentFile();
  assertTrue("Failed to mkdirs for " + parent.getAbsolutePath(),parent.mkdirs());
  addCoreWithProps(stockProps,propFile);
}
