{
  assertStemsTo("drink","drink");
  assertStemsTo("Drink","drink");
  assertStemsTo("DRINK","drink");
  assertStemsTo("drinks","drink");
  assertStemsTo("Drinks","drink");
  assertStemsTo("DRINKS","drink");
  assertStemsTo("walk","walk");
  assertStemsTo("walks","walk");
  assertStemsTo("Walk");
  assertStemsTo("Walks");
  assertStemsTo("WALKS");
  assertStemsTo("test","test");
  assertStemsTo("Test");
  assertStemsTo("TEST");
}
