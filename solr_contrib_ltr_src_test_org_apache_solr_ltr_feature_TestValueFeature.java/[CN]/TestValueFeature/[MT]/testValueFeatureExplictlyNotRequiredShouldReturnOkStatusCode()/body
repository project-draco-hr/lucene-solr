{
  loadFeature("c7",ValueFeature.class.getCanonicalName(),"c7","{\"value\":\"${val7}\",\"required\":false}");
  loadModel("m7",LinearModel.class.getCanonicalName(),new String[]{"c7"},"c7","{\"weights\":{\"c7\":1.0}}");
  final SolrQuery query=new SolrQuery();
  query.setQuery("title:w1");
  query.add("fl","*, score,fvonly:[fvonly]");
  query.add("rows","4");
  query.add("wt","json");
  query.add("rq","{!ltr model=m7 reRankDocs=4}");
  assertJQ("/query" + query.toQueryString(),"/responseHeader/status==0");
}
