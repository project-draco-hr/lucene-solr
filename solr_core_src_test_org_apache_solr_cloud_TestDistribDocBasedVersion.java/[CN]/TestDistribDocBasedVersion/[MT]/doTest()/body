{
  boolean testFinished=false;
  try {
    handle.clear();
    handle.put("QTime",SKIPVAL);
    handle.put("timestamp",SKIPVAL);
    waitForRecoveriesToFinish(false);
    doTestDocVersions();
    doTestHardFail();
    commit();
    testFinished=true;
  }
  finally {
    if (!testFinished) {
      printLayoutOnTearDown=true;
    }
  }
}
