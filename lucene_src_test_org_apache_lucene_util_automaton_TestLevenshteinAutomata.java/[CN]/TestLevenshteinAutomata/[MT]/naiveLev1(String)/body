{
  Automaton a=BasicAutomata.makeString(s);
  a=BasicOperations.union(a,insertionsOf(s));
  MinimizationOperations.minimize(a);
  a=BasicOperations.union(a,deletionsOf(s));
  MinimizationOperations.minimize(a);
  a=BasicOperations.union(a,substitutionsOf(s));
  MinimizationOperations.minimize(a);
  return a;
}
