{
  if (pending) {
    pending=false;
    return term;
  }
  decoded=false;
  DFS:   while (level > 0) {
    Frame frame=newFrame();
    if (loadExpandFrame(topFrame(),frame) != null) {
      pushFrame(frame);
      if (isAccept(frame)) {
        return term;
      }
      continue;
    }
    frame=popFrame();
    while (level > 0) {
      if (loadNextFrame(topFrame(),frame) != null) {
        pushFrame(frame);
        if (isAccept(frame)) {
          return term;
        }
        continue DFS;
      }
      frame=popFrame();
    }
    return null;
  }
  return null;
}
