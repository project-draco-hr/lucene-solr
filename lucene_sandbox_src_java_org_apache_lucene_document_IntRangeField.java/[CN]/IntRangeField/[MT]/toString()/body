{
  StringBuilder sb=new StringBuilder();
  sb.append(getClass().getSimpleName());
  sb.append(" <");
  sb.append(name);
  sb.append(':');
  byte[] b=((BytesRef)fieldsData).bytes;
  toString(b,0);
  for (int d=1; d < type.pointDimensionCount(); ++d) {
    sb.append(' ');
    toString(b,d);
  }
  sb.append('>');
  return sb.toString();
}
