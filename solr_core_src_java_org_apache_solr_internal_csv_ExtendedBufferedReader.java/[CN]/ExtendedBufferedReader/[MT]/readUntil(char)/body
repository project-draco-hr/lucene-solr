{
  if (lookaheadChar == UNDEFINED) {
    lookaheadChar=super.read();
  }
  line.clear();
  while (lookaheadChar != c && lookaheadChar != END_OF_STREAM) {
    line.append((char)lookaheadChar);
    if (lookaheadChar == '\n') {
      lineCounter++;
    }
    lastChar=lookaheadChar;
    lookaheadChar=super.read();
  }
  return line.toString();
}
