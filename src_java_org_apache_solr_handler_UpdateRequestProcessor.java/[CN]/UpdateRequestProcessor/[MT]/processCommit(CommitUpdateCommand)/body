{
  updateHandler.commit(cmd);
  response.add(cmd.optimize ? "optimize" : "commit","");
  long now=System.currentTimeMillis();
  if (cmd.optimize) {
    log.info("optimize 0 " + (now - startTime) + " ["+ (now - startTime)+ "]");
  }
 else {
    log.info("commit 0 " + (now - startTime) + " ["+ (now - startTime)+ "]");
  }
}
