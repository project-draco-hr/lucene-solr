{
  final int segCounts[];
  if (map == null) {
    segCounts=counts;
  }
 else {
    segCounts=new int[1 + si.getValueCount()];
  }
  int doc;
  while ((doc=disi.nextDoc()) != DocIdSetIterator.NO_MORE_DOCS) {
    if (doc > si.docID()) {
      si.advance(doc);
    }
    if (doc == si.docID()) {
      segCounts[1 + si.ordValue()]++;
    }
 else {
      segCounts[0]++;
    }
  }
  if (map != null) {
    migrateGlobal(counts,segCounts,subIndex,map);
  }
}
