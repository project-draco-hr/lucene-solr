{
  currentReaderValues=getNumericDocValues(context,field);
  if (missingValue != null) {
    docsWithField=DocValues.getDocsWithField(context.reader(),field);
    if (docsWithField instanceof Bits.MatchAllBits) {
      docsWithField=null;
    }
  }
 else {
    docsWithField=null;
  }
  return this;
}
