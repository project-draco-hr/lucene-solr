{
  final float value=valueObj.floatValue();
  float docValue=currentReaderValues[doc];
  if (docsWithField != null && docValue == 0 && !docsWithField.get(doc)) {
    docValue=missingValue;
  }
  if (docValue < value) {
    return -1;
  }
 else   if (docValue > value) {
    return 1;
  }
 else {
    return 0;
  }
}
