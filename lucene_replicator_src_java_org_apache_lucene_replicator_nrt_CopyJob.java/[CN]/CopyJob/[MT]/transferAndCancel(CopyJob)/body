{
synchronized (prevJob) {
    dest.message("CopyJob: now transfer prevJob " + prevJob);
    try {
      _transferAndCancel(prevJob);
    }
 catch (    Throwable t) {
      dest.message("xfer: exc during transferAndCancel");
      cancel("exc during transferAndCancel",t);
      IOUtils.reThrow(t);
    }
  }
}
