{
  DistanceUnits units=DistanceUnits.KILOMETERS;
  DistanceCalculator distCalc=new GeodesicSphereDistCalc.Haversine(units.earthRadius());
switch (random.nextInt(3)) {
case 2:
    break;
case 1:
  distCalc=new GeodesicSphereDistCalc.Vincenty(units.earthRadius());
break;
}
return new SimpleSpatialContext(units,distCalc,SpatialContext.GEO_WORLDBOUNDS);
}
