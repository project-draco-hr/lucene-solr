{
  SimilarityBase sim=new DFRSimilarity(new BasicModelBE(),new AfterEffectB(),new NormalizationH1());
  float tfn=FREQ * AVG_FIELD_LENGTH / DOC_LEN;
  float b=(TOTAL_TERM_FREQ + 1) / (DOC_FREQ * (tfn + 1));
  float f=TOTAL_TERM_FREQ + tfn;
  float n=f + NUMBER_OF_DOCUMENTS;
  float n1=n + f - 1;
  float m1=n + f - tfn - 2;
  float n2=f;
  float m2=f - tfn;
  float be=(float)(-SimilarityBase.log2(n - 1) - SimilarityBase.log2(Math.E) + ((m1 + 0.5f) * SimilarityBase.log2(n1 / m1) + (n1 - m1) * SimilarityBase.log2(n1)) - ((m2 + 0.5f) * SimilarityBase.log2(n2 / m2) + (n2 - m2) * SimilarityBase.log2(n2)));
  float gold=b * be;
  correctnessTestCore(sim,gold);
}
