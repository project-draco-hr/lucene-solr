{
  Exception parsed=new Exception(message);
  boolean ok=true;
  for (  Future<JettySolrRunner> future : futures) {
    try {
      future.get();
    }
 catch (    ExecutionException e) {
      parsed.addSuppressed(e.getCause());
      ok=false;
    }
catch (    InterruptedException e) {
      Thread.interrupted();
      throw e;
    }
  }
  return ok ? null : parsed;
}
