{
  String[] klas=req.getParams().getParams("class");
  if (klas == null || klas.length == 0) {
    throw new SolrException(SolrException.ErrorCode.BAD_REQUEST,"class is a required param");
  }
  for (  String c : klas) {
    Map<String,Object> result=invokeAClass(req,c);
    rsp.add(c,result);
  }
}
