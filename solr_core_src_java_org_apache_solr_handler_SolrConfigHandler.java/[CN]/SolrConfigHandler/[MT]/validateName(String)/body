{
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if ((c >= 'A' && c <= 'Z') || (c >= 'a' && c <= 'z') || (c >= '0' && c <= '9')|| c == '_' || c == '-' || c == '.')     continue;
 else {
      return StrUtils.formatString("''{0}'' name should only have chars [a-zA-Z_-.0-9] ",s);
    }
  }
  return null;
}
