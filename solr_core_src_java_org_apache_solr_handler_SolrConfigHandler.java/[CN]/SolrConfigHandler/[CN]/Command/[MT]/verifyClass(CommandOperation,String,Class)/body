{
  if (clz == null)   return true;
  if (!"true".equals(String.valueOf(op.getStr("runtimeLib",null)))) {
    try {
      req.getCore().createInitInstance(new PluginInfo(SolrRequestHandler.TYPE,op.getDataMap()),expected,clz,"");
    }
 catch (    Exception e) {
      op.addError(e.getMessage());
      return false;
    }
  }
  return true;
}
