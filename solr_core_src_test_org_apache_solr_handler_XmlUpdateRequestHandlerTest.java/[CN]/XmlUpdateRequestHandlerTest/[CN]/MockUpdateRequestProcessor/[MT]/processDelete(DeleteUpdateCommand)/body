{
  DeleteUpdateCommand expected=deleteCommands.poll();
  assertNotNull("Unexpected delete command: [" + cmd + "]",expected);
  assertTrue("Expected [" + expected + "] but found ["+ cmd+ "]",ObjectUtils.equals(expected.id,cmd.id) && ObjectUtils.equals(expected.query,cmd.query) && expected.commitWithin == cmd.commitWithin);
}
