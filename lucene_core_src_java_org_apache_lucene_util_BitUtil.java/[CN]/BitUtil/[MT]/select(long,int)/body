{
  long s=x - ((x & 0xAAAAAAAAAAAAAAAAL) >>> 1);
  s=(s & 0x3333333333333333L) + ((s >>> 2) & 0x3333333333333333L);
  s=((s + (s >>> 4)) & 0x0F0F0F0F0F0F0F0FL) * L8_L;
  int b=(Long.numberOfTrailingZeros((s + psOverflow[r - 1]) & (L8_L << 7)) >> 3) << 3;
  long l=r - (((s << 8) >>> b) & 0xFFL);
  int selectIndex=(int)(((x >>> b) & 0xFFL) | ((l - 1) << 8));
  int res=b + select256[selectIndex];
  return res;
}
