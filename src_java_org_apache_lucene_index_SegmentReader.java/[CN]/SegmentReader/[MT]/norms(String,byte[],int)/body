{
  Norm norm=(Norm)norms.get(field);
  if (norm == null) {
    System.arraycopy(fakeNorms(),0,bytes,offset,maxDoc());
    return;
  }
  if (norm.bytes != null) {
    System.arraycopy(norm.bytes,0,bytes,offset,maxDoc());
    return;
  }
  norm.in.seek(norm.normSeek);
  norm.in.readBytes(bytes,offset,maxDoc());
}
