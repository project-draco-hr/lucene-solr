{
  if (fieldsReader != null) {
    fieldsReader.close();
  }
  if (tis != null) {
    tis.close();
  }
  if (freqStream != null)   freqStream.close();
  if (proxStream != null)   proxStream.close();
  closeNorms();
  if (termVectorsReaderOrig != null)   termVectorsReaderOrig.close();
  if (cfsReader != null)   cfsReader.close();
}
