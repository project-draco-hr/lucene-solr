{
  double[] theXs=new double[]{-10,0,10};
  for (  double x : theXs) {
    double[] theYs=new double[]{-20,0,20};
    for (    double y : theYs) {
      testCircle(x,y,0);
      testCircle(x,y,5);
    }
  }
  assertEquals("getX not getY",INTERSECTS,ctx.makeCircle(107,-81,147).relate(ctx.makeRect(92,121,-89,74),ctx));
  testCircleIntersect();
}
