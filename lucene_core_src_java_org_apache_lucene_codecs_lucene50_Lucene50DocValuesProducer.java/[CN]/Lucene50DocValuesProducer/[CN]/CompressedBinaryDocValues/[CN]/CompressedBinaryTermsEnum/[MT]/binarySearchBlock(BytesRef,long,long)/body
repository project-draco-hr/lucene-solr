{
  while (low <= high) {
    long mid=(low + high) >>> 1;
    input.seek(addresses.get(mid));
    term.length=input.readVInt();
    input.readBytes(term.bytes,0,term.length);
    int cmp=term.compareTo(text);
    if (cmp < 0) {
      low=mid + 1;
    }
 else     if (cmp > 0) {
      high=mid - 1;
    }
 else {
      return mid;
    }
  }
  return high;
}
