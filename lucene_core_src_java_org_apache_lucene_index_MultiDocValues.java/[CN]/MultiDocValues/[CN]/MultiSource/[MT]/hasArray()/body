{
  boolean oneRealSource=false;
  for (  DocValuesSlice slice : slices) {
    try {
      Source source=slice.docValues.getSource();
      if (source instanceof EmptySource) {
        continue;
      }
      oneRealSource=true;
      if (!source.hasArray()) {
        return false;
      }
    }
 catch (    IOException e) {
      throw new RuntimeException("load failed",e);
    }
  }
  return oneRealSource;
}
