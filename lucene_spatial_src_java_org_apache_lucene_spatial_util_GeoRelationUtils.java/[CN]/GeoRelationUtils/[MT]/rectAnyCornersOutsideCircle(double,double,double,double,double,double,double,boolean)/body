{
  if (approx == true) {
    return rectAnyCornersOutsideCircleSloppy(rMinLat,rMaxLat,rMinLon,rMaxLon,centerLat,centerLon,radiusMeters);
  }
  if (Math.abs(rMaxLon - rMinLon) <= 70.0) {
    return SloppyMath.haversinMeters(centerLat,centerLon,rMinLat,rMinLon) > radiusMeters || SloppyMath.haversinMeters(centerLat,centerLon,rMaxLat,rMinLon) > radiusMeters || SloppyMath.haversinMeters(centerLat,centerLon,rMaxLat,rMaxLon) > radiusMeters || SloppyMath.haversinMeters(centerLat,centerLon,rMinLat,rMaxLon) > radiusMeters;
  }
  return rectCrossesOblateCircle(centerLat,centerLon,radiusMeters,rMinLat,rMaxLat,rMinLon,rMaxLon);
}
