{
  if (approx == true) {
    return rectAnyCornersInCircle(rMinX,rMinY,rMaxX,rMaxY,centerLon,centerLat,radiusMeters,approx) || isClosestPointOnRectWithinRange(rMinX,rMinY,rMaxX,rMaxY,centerLon,centerLat,radiusMeters,approx);
  }
  return (rectAnyCornersInCircle(rMinX,rMinY,rMaxX,rMaxY,centerLon,centerLat,radiusMeters,approx) && rectAnyCornersOutsideCircle(rMinX,rMinY,rMaxX,rMaxY,centerLon,centerLat,radiusMeters,approx)) || isClosestPointOnRectWithinRange(rMinX,rMinY,rMaxX,rMaxY,centerLon,centerLat,radiusMeters,approx);
}
