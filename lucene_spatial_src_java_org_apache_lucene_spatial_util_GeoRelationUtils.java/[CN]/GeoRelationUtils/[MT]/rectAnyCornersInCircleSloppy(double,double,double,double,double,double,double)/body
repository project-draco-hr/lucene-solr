{
  return SloppyMath.haversinMeters(centerLat,centerLon,rMinY,rMinX) <= radiusMeters || SloppyMath.haversinMeters(centerLat,centerLon,rMaxY,rMinX) <= radiusMeters || SloppyMath.haversinMeters(centerLat,centerLon,rMaxY,rMaxX) <= radiusMeters || SloppyMath.haversinMeters(centerLat,centerLon,rMinY,rMaxX) <= radiusMeters;
}
