{
  if (deleteById == null) {
    deleteById=new LinkedHashMap<>();
  }
  Map<String,Object> params=(route == null && version == null) ? null : new HashMap<>(1);
  if (version != null)   params.put(VER,version);
  if (route != null)   params.put(ROUTE,route);
  deleteById.put(id,params);
  return this;
}
