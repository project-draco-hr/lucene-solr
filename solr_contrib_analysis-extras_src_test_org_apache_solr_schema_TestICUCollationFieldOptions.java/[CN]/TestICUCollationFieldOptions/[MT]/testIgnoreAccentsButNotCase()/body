{
  assertQ("Collated TQ: ",req("fl","id","q","sort_ignore_accents:resume","sort","id asc"),"//*[@numFound='2']","//result/doc[1]/int[@name='id'][.=6]","//result/doc[2]/int[@name='id'][.=9]");
