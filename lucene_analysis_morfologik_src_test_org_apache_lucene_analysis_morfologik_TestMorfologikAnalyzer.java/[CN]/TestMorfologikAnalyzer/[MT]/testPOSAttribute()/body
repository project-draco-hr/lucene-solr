{
  try (Analyzer a=getTestAnalyzer();TokenStream ts=a.tokenStream("dummy","li??cie")){
    ts.reset();
    assertPOSToken(ts,"li??cie","subst:sg:acc:n2","subst:sg:nom:n2","subst:sg:voc:n2");
    assertPOSToken(ts,"li????","subst:pl:acc:m3","subst:pl:nom:m3","subst:pl:voc:m3");
    assertPOSToken(ts,"list","subst:sg:loc:m3","subst:sg:voc:m3");
    assertPOSToken(ts,"lista","subst:sg:dat:f","subst:sg:loc:f");
    ts.end();
  }
 }
