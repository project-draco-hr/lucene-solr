{
  Analyzer a=getTestAnalyzer();
  assertAnalyzesTo(a,"li??cie danych",new String[]{"li??cie","li????","list","lista","dany","dana","dane","da??"},new int[]{0,0,0,0,7,7,7,7},new int[]{6,6,6,6,13,13,13,13},new int[]{1,0,0,0,1,0,0,0});
  assertAnalyzesTo(a,"T. Gl\u00FCcksberg",new String[]{"tom","tona","Gl\u00FCcksberg"},new int[]{0,0,3},new int[]{1,1,13},new int[]{1,0,1});
  a.close();
}
