{
  CharArr out=new CharArr();
  try {
    out.append('{');
    boolean first=true;
    for (    Object id : ids) {
      if (first)       first=false;
 else       out.append(',');
      out.append("\"delete\":{\"id\":");
      out.append(JSONUtil.toJSON(id));
      out.append('}');
    }
    out.append('}');
  }
 catch (  IOException e) {
  }
  return out.toString();
}
