{
  CharArr out=new CharArr();
  try {
    out.append('[');
    boolean firstField=true;
    for (    SolrInputDocument doc : docs) {
      if (firstField)       firstField=false;
 else       out.append(',');
      out.append(json(doc));
    }
    out.append(']');
  }
 catch (  IOException e) {
  }
  return out.toString();
}
