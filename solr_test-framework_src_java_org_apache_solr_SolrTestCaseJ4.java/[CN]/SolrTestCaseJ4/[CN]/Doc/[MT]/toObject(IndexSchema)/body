{
  Map<String,Object> result=new HashMap<String,Object>();
  for (  Fld fld : fields) {
    SchemaField sf=schema.getField(fld.ftype.fname);
    if (!sf.multiValued()) {
      result.put(fld.ftype.fname,fld.vals.get(0));
    }
 else {
      result.put(fld.ftype.fname,fld.vals);
    }
  }
  return result;
}
