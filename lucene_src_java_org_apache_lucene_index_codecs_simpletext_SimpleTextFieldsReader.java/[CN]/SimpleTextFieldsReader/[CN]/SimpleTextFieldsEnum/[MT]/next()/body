{
  while (true) {
    SimpleTextUtil.readLine(in,scratch);
    if (scratch.equals(END)) {
      current=null;
      return null;
    }
    if (scratch.startsWith(FIELD)) {
      return current=new String(scratch.bytes,scratch.offset + FIELD.length,scratch.length - FIELD.length,"UTF-8");
    }
  }
}
