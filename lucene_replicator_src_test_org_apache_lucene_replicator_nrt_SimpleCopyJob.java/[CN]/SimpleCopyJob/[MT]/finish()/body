{
  dest.message(String.format(Locale.ROOT,"top: file copy done; took %.1f msec to copy %d bytes; now rename %d tmp files",(System.nanoTime() - startNS) / 1000000.0,totBytesCopied,copiedFiles.size()));
  for (  Map.Entry<String,String> ent : copiedFiles.entrySet()) {
    String tmpFileName=ent.getValue();
    String fileName=ent.getKey();
    if (Node.VERBOSE_FILES) {
      dest.message("rename file " + tmpFileName + " to "+ fileName);
    }
    dest.dir.renameFile(tmpFileName,fileName);
  }
  copiedFiles.clear();
}
