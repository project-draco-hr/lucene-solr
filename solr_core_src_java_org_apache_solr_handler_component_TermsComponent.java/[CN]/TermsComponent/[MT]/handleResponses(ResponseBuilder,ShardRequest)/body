{
  if (!rb.doTerms || (sreq.purpose & ShardRequest.PURPOSE_GET_TERMS) == 0) {
    return;
  }
  TermsHelper th=rb._termsHelper;
  if (th != null) {
    for (    ShardResponse srsp : sreq.responses) {
      @SuppressWarnings("unchecked") NamedList<NamedList<Number>> terms=(NamedList<NamedList<Number>>)srsp.getSolrResponse().getResponse().get("terms");
      th.parse(terms);
      NamedList<Number> stats=(NamedList<Number>)srsp.getSolrResponse().getResponse().get("stats");
      if (stats != null) {
        th.numDocs+=stats.get("numDocs").longValue();
        th.stats=true;
      }
    }
  }
}
