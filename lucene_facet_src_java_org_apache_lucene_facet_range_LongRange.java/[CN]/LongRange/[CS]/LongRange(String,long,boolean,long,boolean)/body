{
  super(label);
  this.min=minIn;
  this.max=maxIn;
  this.minInclusive=minInclusive;
  this.maxInclusive=maxInclusive;
  if (!minInclusive) {
    if (minIn != Long.MAX_VALUE) {
      minIn++;
    }
 else {
      failNoMatch();
    }
  }
  if (!maxInclusive) {
    if (maxIn != Long.MIN_VALUE) {
      maxIn--;
    }
 else {
      failNoMatch();
    }
  }
  if (minIn > maxIn) {
    failNoMatch();
  }
  this.minIncl=minIn;
  this.maxIncl=maxIn;
}
