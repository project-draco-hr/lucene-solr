{
  String text="Fortieth, Quaranti??me, Cuadrag??simo";
  Benchmark benchmark=execBenchmark(getAnalyzerFactoryConfig("ascii folded, pattern replaced, standard tokenized, downcased, bigrammed.'analyzer'","positionIncrementGap:100,offsetGap:1111," + "MappingCharFilter(mapping:'test-mapping-ISOLatin1Accent-partial.txt')," + "PatternReplaceCharFilterFactory(pattern:'e(\\\\\\\\S*)m',replacement:\"$1xxx$1\"),"+ "StandardTokenizer,LowerCaseFilter,NGramTokenFilter(minGramSize:2,maxGramSize:2)"));
  BaseTokenStreamTestCase.assertAnalyzesTo(benchmark.getRunData().getAnalyzer(),text,new String[]{"fo","or","rt","ti","ie","et","th","qu","ua","ar","ra","an","nt","ti","ix","xx","xx","xe","cu","ua","ad","dr","ra","ag","gs","si","ix","xx","xx","xs","si","io"});
}
