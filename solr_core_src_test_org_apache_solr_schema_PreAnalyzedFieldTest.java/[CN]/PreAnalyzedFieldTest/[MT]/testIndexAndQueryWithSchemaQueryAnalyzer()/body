{
  assertU(addTwoDocs(5,"pre_with_query_analyzer"));
  assertU(commit());
  assertQ(req("q","id:(5 6)","sort","id asc"),"//result[@numFound='2']","//result/doc[1]/str[@name='id'][.='5']","//result/doc[1]/str[@name='pre_with_query_analyzer'][.='document one']","//result/doc[2]/str[@name='id'][.='6']","//result/doc[2]/str[@name='pre_with_query_analyzer'][.='document two']");
  assertQ(req("q","pre_with_query_analyzer:one,two"),"//result[@numFound='1']");
  assertQ(req("q","pre_with_query_analyzer:eleven,twelve"),"//result[@numFound='1']");
}
