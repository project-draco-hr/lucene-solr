  return "<add>\n" + doc("id",Integer.toString(firstId),field,json("{'v':'1','str':'document one','tokens':[{'t':'one'},{'t':'two'},{'t':'three','i':100}]}")) + doc("id",Integer.toString(firstId + 1),field,json("{'v':'1','str':'document two','tokens':[{'t':'eleven'},{'t':'twelve'},{'t':'thirteen','i':110}]}"))+ "</add>\n";
