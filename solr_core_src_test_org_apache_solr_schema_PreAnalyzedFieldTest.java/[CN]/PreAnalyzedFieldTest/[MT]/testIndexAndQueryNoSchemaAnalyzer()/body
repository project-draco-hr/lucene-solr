{
  assertU(addTwoDocs(1,"pre_no_analyzer"));
  assertU(commit());
  assertQ(req("q","id:(1 2)","sort","id asc"),"//result[@numFound='2']","//result/doc[1]/str[@name='id'][.='1']","//result/doc[1]/str[@name='pre_no_analyzer'][.='document one']","//result/doc[2]/str[@name='id'][.='2']","//result/doc[2]/str[@name='pre_no_analyzer'][.='document two']");
  assertQ(req("q","{!field f='pre_no_analyzer'}{'v':'1','tokens':[{'t':'two'}]}"),"//result[@numFound='1']");
  assertQ(req("q","{!field f='pre_no_analyzer'}{'v':'1','tokens':[{'t':'eleven'},{'t':'twelve'}]}"),"//result[@numFound='1']");
}
