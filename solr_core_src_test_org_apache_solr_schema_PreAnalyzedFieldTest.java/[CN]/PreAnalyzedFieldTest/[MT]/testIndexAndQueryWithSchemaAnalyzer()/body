{
  assertU(addTwoDocs(3,"pre_with_analyzer"));
  assertU(commit());
  assertQ(req("q","id:(3 4)","sort","id asc"),"//result[@numFound='2']","//result/doc[1]/str[@name='id'][.='3']","//result/doc[1]/str[@name='pre_with_analyzer'][.='document one']","//result/doc[2]/str[@name='id'][.='4']","//result/doc[2]/str[@name='pre_with_analyzer'][.='document two']");
  assertQ(req("q","pre_with_analyzer:(+two +three)"),"//result[@numFound='1']");
  assertQ(req("q","pre_with_analyzer:(+eleven +twelve)"),"//result[@numFound='1']");
}
