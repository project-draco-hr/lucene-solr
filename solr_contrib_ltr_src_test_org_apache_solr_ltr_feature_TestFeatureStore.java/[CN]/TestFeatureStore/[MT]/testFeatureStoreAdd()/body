{
  final FeatureStore fs=fstore.getFeatureStore("fstore-testFeature");
  for (int i=0; i < 5; i++) {
    final String name="c" + i;
    fstore.addFeature(TestManagedFeatureStore.createMap(name,OriginalScoreFeature.class.getCanonicalName(),null),"fstore-testFeature");
    final Feature f=fs.get(name);
    assertNotNull(f);
  }
  assertEquals(5,fs.getFeatures().size());
}
