{
  final FeatureStore fs=fstore.getFeatureStore("fstore-testFeature3");
  for (int i=0; i < 5; i++) {
    Map<String,Object> params=new HashMap<String,Object>();
    params.put("value",i);
    final String name="testc" + (float)i;
    fstore.addFeature(TestManagedFeatureStore.createMap(name,ValueFeature.class.getCanonicalName(),params),"fstore-testFeature3");
  }
  assertNull(fs.get("missing_feature_name"));
}
