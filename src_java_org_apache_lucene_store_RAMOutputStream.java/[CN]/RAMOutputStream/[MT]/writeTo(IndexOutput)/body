{
  flush();
  final long end=file.length;
  long pos=0;
  int buffer=0;
  while (pos < end) {
    int length=BUFFER_SIZE;
    long nextPos=pos + length;
    if (nextPos > end) {
      length=(int)(end - pos);
    }
    out.writeBytes((byte[])file.getBuffer(buffer++),length);
    pos=nextPos;
  }
}
