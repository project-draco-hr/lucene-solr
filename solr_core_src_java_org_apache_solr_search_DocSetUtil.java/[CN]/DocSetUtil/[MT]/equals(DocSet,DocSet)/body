{
  DocIterator iter1=a.iterator();
  DocIterator iter2=b.iterator();
  for (; ; ) {
    boolean n1=iter1.hasNext();
    boolean n2=iter2.hasNext();
    if (n1 != n2) {
      return false;
    }
    if (!n1)     return true;
    int d1=iter1.nextDoc();
    int d2=iter2.nextDoc();
    if (d1 != d2) {
      return false;
    }
  }
}
