{
  final int prime=31;
  int result=1;
  for (int i=0; i < charArray.length; i++) {
    result=prime * result + charArray[i];
  }
  result=prime * result + from;
  result=prime * result + to;
  long temp;
  temp=Double.doubleToLongBits(weight);
  result=prime * result + (int)(temp ^ (temp >>> 32));
  return result;
}
